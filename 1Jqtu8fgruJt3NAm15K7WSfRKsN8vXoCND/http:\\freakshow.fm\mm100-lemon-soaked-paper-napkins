	    jQuery("#post-7070 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
	    // button-stile download-widget
	    jQuery("#post-7070 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/mm/mm100-lemon-soaked-paper-napkins.mp3", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;MP3&nbsp;Audio";
	    // select-style download-widget
	    jQuery("#post-7070 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
	    // button-stile download-widget
	    jQuery("#post-7070 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/mm/mm100-lemon-soaked-paper-napkins.opus", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Opus&nbsp;Audio";
	    // select-style download-widget
