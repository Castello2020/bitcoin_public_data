	<script src="/static/js/jquery.tokeninput.js"></script>
	<script src="/static/js/jquery.tipsy.js"></script>
	<script>
$("document").ready(function () {
    // Eingabefeld wird zu token-input
    $("#id_q").tokenInput("/ingredient", {
      hintText: "Gib eine Zutat ein.",
      noResultsText: "Keine passenden Zutaten gefunden.",
      searchingText: "",
      animateDropdown: false,
      searchDelay: 0,
      minChars: 1,
      relativeDropdown: true,
      preventDuplicates: true,
      prePopulate: [],
      onReady: function () { initTipsy(); },
      onAdd: function () { initTipsy(); },
      onDelete: function () { $(".tipsy").remove(); },
    });

    var included = "";
    var excluded = "";

    $.each(included.split(","), function (index, value) {
        $("#id_q").tokenInput("include", {id: parseInt(value)});
    });
    $.each(excluded.split(","), function (index, value) {
        $("#id_q").tokenInput("exclude", {id: parseInt(value)});
    });


    /*
     * Stichwort-Eingabefeld wurde Ende Mai aus den Suchoptionen entfernt.
     * Absprache nötig, wie weiter damit verfahren wird.

    $("#id_k").tokenInput("/keyword", {
      hintText: "Gib ein Stichwort ein.",
      noResultsText: "Keine passenden Stichworte gefunden.",
      searchingText: "Suche nach Stichwort…",
      animateDropdown: false,
      searchDelay: 0,
      minChars: 1,
      relativeDropdown: true,
      preventDuplicates: true,
      prePopulate: [],
    });
    */

  
    /* Platzhalter einfügen und bei Fokussieren der Suchzeile wieder löschen */
    $("[role=search] .token-input-list li:first").prepend(
        '<span id="token-input-placeholder">Gib hier Deine Zutaten ein.</span>'
    );
    $("#filters-toggle").css("display", "none");
    $("#filters-toggle-placeholder").css("display", "block");
    $(".token-input-input-token").css("width", "100%"); // Mehrzeiliger Platzhalter

    function enter_ingredients_start() {
      $(".token-input-input-token").css("width", "");
      $("#token-input-placeholder").remove();
      $("#filters-toggle").slideDown();
      $("#filters-toggle-placeholder").slideUp();
    }

    /*
     * Ausführen in click und focus ist notwendig, da das focus event nach der Anzeige
     * des Labels "Gib eine Zutat ein" ausgeführt wird. Durch das entfernen des
     * Platzhalters wird die Höhe des Inputs dann noch verändert, und das Label
     * erscheint an einer falschen Position.
     * Focus wird zusätzlich als Fallback ausgeführt, falls z.B. mit Tab durch die
     * Steuerelemente gewechselt wird
     */
    $(".token-input-input-token").click(function () { enter_ingredients_start(); });
    $(".token-input-input-token input").focus(function () { enter_ingredients_start(); });

  
});
</script>

