  <script>
    $("document").ready(function(){
      
      // Rezeptkarten mit MixItUp initialisieren
      setGridPositionAttr();
         
      $('#recipes').mixitup({
          targetSelector: '.grid-item',
          transitionSpeed: 300,
          sortOnLoad: "data-grid-position",
          onMixEnd: function () {
            truncateIngredientList();
            gaLogImpressions(); //Aktuell angezeigte Rezepte an GA übermitteln
            makeIngredientsClickable();
            makeCardsClickable();
            refreshAdSense();
            initTipsy();
          }
      });
            
      
      makeCardsClickable();

      //Chef initialisieren
      updateChief(1);
      
      $(document).on("click",".hint .hide, #chief-toggle", function () {
          if (document.cookie.search("chief=false") == -1) {
            createCookie("chief", "false", 30);
            ga('send', 'event', 'misc', 'hint_hide');
          }
          else {
            createCookie("chief", "true", 30);
            ga('send', 'event', 'misc', 'hint_show');
          }
          updateChief();
      });
    });
  </script>


