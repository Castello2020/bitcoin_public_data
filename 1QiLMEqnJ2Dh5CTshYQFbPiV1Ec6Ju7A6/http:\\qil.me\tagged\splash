            var newHeight = iframe.contentWindow.document.body.offsetHeight;
            if (newHeight) iframe.height = newHeight;
            if (typeof(params.onload) === 'function') params.onload();
        };
        var doc = iframe.contentWindow.document;
        doc.open('text/html', 'replace');
        doc.write([
            '<!DOCTYPE html><html><head><title>Tumblr Video</title></head>',
            '<body class="' + params.body_class + '">',
            params.content,
            '</body></html>'
        ].join(''));
