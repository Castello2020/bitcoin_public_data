<script>
  // Event tracking
  function addListener(element, type, callback)
  {
    if (element.addEventListener)
      element.addEventListener(type, callback);
    else if (element.attachEvent)
      element.attachEvent('on' + type, callback);
  }

  addListener(document.getElementById('dmg_download'), 'click', function() { ga('send', 'event', 'link', 'click', 'dmg_download');});
  addListener(document.getElementById('source_code'), 'click', function() { ga('send', 'event', 'link', 'click', 'source_code');});
  addListener(document.getElementById('report_bug'), 'click', function() { ga('send', 'event', 'link', 'click', 'report_bug');});
</script>
