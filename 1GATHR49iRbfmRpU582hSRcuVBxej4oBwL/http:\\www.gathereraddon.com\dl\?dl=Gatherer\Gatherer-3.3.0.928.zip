<script>
$(function() {
	function go(amount, subs) {
		if (subs) {
			$('#cmd').val('_xclick-subscriptions');
			$('#pay_amount').val('');
			$('#sub_amount').val(amount.toFixed(2));
			$('#sub_period').val('1');
			$('#sub_time').val('M');
			$('#sub_src').val('1');
			$('#sub_sra').val('1');
			$('#item_number').val('sponsor-gatherer');
			$('#item_name').val('Sponsorship - Gatherer AddOn');
		}
		else {
			$('#cmd').val('_xclick');
			$('#pay_amount').val(amount.toFixed(2));
			$('#sub_amount').val('');
			$('#sub_period').val('');
			$('#sub_time').val('');
			$('#sub_src').val('');
			$('#sub_sra').val('');
			$('#item_number').val('donate-gatherer');
			$('#item_name').val('Donation - Gatherer AddOn');
		}

		$('#donate-table').css('opacity', 0.4);
		setTimeout(function (){
			$('#form').submit();
		}, 0);
	}

	$('#donate-custom').on('click.custom', function() {
		var el = $(this),
			amount = $('#amount').val().replace(/[\$,]+/g, ''),
			amt = parseFloat(amount);
		if (!amt || amt < 2) {
			alert("Please enter an amount over $2. Amounts under this get gobbled up with fees, and we'd prefer not to feed the PayPal monster.");
			return;
		}
		go(amt);
	});
	$('a.donate-other').on('click.more', function(e) {
		e.preventDefault();
		e.stopPropagation();
		var el = $(this),
			tag = el.attr('href').substr(1);
		$('#donate-'+tag).slideDown();
	});
	$('a.donate').on('click.donate', function(e) {
		e.preventDefault();
		e.stopPropagation();
		var el = $(this),
			tag = el.attr('href').substr(1),
			fc = tag.substr(0,1),
			subs = false;

		if (fc == 's') {
			tag = tag.substr(1);
			subs = true;
		}
		go(parseInt(tag), subs);
	});
});
</script>


