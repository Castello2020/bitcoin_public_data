      var config = {'blogId': '6147716270535278850', 'postId': '5522553933817841666', 'feed': 'http://www.hackingdaily.com/feeds/5522553933817841666/comments/default', 'authorName': 'Hacking Daily', 'authorUrl': 'http://www.blogger.com/profile/07073204931705868746', 'baseUri': 'http://www.blogger.com', 'maxThreadDepth': 2};

