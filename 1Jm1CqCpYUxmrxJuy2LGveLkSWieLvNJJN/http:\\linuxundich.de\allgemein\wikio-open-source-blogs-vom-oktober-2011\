        $(function() {
            $(".53ca38c5d2575Wraper").carouFredSel({
                prev: $(".53ca38c5d2575NavLeft"),
                next: $(".53ca38c5d2575NavRight"),
                responsive: true,
                width: "100%",
                auto: false,
                items: {
                    width: $("#metroW").width() /4,
