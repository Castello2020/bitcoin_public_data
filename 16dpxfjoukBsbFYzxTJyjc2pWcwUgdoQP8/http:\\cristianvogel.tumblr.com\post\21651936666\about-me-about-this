<canvas id="Plasma" class="shadow" width="600px" height="200px" align="center">
</canvas>


<script type="application/processing" target="Plasma">
//Made with ProcessingLyon Tumblrify:
//http://p5lyon.tumblr.com/ProcessingJSTumblr
// Plasma Banner by CV

int pixelSize=(int) random(10, 35);
float calculation1, calculation2=.5;
float s2, s3, s, s1, xc, yc,t;
int timeDisplacement, pgxy;
boolean d=true;
PGraphics pg;

void setup() {
    frameRate(10);
  size(600, 80);
  // Create buffered image for plasma effect
  pgxy = (int) random(50,100);
  pg = createGraphics(pgxy, pgxy, P2D);
  colorMode(HSB);
  //noLoop(); // dont animate
}

void draw() {
  timeDisplacement = pixelSize+(int) t;
  if (timeDisplacement > 1000) {d=false;} 
else if (timeDisplacement < 0) {d=true;}
 if (d) { t+=1;} else { t-=1;}
  calculation1 = sin( optRadians(timeDisplacement * 0.06));
  calculation2 = sin( optRadians(timeDisplacement * calculation1));
  
  // Output into a buffered image for reuse
  pg.beginDraw();
  pg.loadPixels();

  // Plasma algorithm
  xc=25; //reset pixel x tile coeff
    for (int x = 0; x < pg.width; x++, xc += max(1, pixelSize))
  {
    yc = 25; //reset pixel y tile coeff
    s1 = 128 + 128 * sin(optRadians(xc) * calculation1 );
     xc+=calculation2;
    for (int y = 0; y < pg.height; y++, yc += pixelSize)
    {
       s2 = 128 + 128 * sin(optRadians(yc) * calculation2 );
       s3 = 128 + 128 * sin(optRadians((xc + yc + timeDisplacement * 2) / 2));  
       s  = (s1+ s2 + s3) / 2.0;
      pg.pixels[x+y*pg.width] = color(s , s*0.1, 255 - s / 2.0 );
    }
  }   
  pg.updatePixels();
  pg.endDraw();
  // display the results
  image(pg, 0, 0, width, height);
}

float optRadians (float n) {
  float fPi=3.1415927;
  return  n * (fPi / 180.0) ;
}

</script>



