var target = document.getElementById('spinna');
target.appendChild(spinner.el);



$().ready (function () {
//jquery hides

    $("#content").hide();  
    $("canvas").hide();
    $("iframe").hide();
    
    
// draw all seperators      
    var canvasses=document.getElementsByTagName("canvas");
    for (i in canvasses) {
    if (canvasses[i].id=="niceLine") {
    var ctx=canvasses[i].getContext("2d");
    ctx.beginPath();
    ctx.lineWidth=1;
    ctx.strokeStyle="#dadadd";
    ctx.strokeRect(2,2,600,5);
    ctx.fillStyle="#fefeff";
    ctx.fillRect(3,3,597,3);
        }
    }
// adjust length of tabsContainer
$('.tagsContainer').each(function(index) {
    var idClass="id"+index;
   $(this).addClass(idClass);
    var tagCount = $("."+idClass+" tags").length;
    var xrnd= tagCount*500;
    tagCount = Math.min(tagCount*18,95)
    $(this).css({'width': tagCount+"\%"});
   // var xrnd= Math.round((Math.random() * 2000)+50);    
    $(this).css({"background-size": xrnd+"px 100px"})
})
//tag rollover events
    $("tags").bind('mouseenter mouseleave', function() 
    {
    $(this).toggleClass('tagEntered');
    });   
    

});


