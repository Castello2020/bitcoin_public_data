<script type="text/javascript">
var opts ={
  lines: 13, 
  length: 16, 
  width: 5,
  radius: 10,
  rotate: 0,
  color: '#333', 
  speed: 0.5, // Rounds per second
  trail: 60, 
  shadow: false, 
  hwaccel: false, 
  className: 'spinner', // The CSS class to assign to the spinner
  zIndex: 200, 
  top: 'auto', 
  left: 'auto' 
};
var spinner = new Spinner(opts).spin();
var target = document.getElementById('spinna');
target.appendChild(spinner.el);



$().ready (function () {
//jquery hides

    $("#content").hide();  
    $("canvas").hide();
    $("iframe").hide();
    
    
// draw all seperators      
    var canvasses=document.getElementsByTagName("canvas");
    for (i in canvasses) {
    if (canvasses[i].id=="niceLine") {
    var ctx=canvasses[i].getContext("2d");
    ctx.beginPath();
    ctx.lineWidth=1;
    ctx.strokeStyle="#dadadd";
    ctx.strokeRect(2,2,600,5);
    ctx.fillStyle="#fefeff";
    ctx.fillRect(3,3,597,3);
        }
    }
// adjust length of tabsContainer
$('.tagsContainer').each(function(index) {
    var idClass="id"+index;
   $(this).addClass(idClass);
    var tagCount = $("."+idClass+" tags").length;
    var xrnd= tagCount*500;
    tagCount = Math.min(tagCount*18,95)
    $(this).css({'width': tagCount+"\%"});
   // var xrnd= Math.round((Math.random() * 2000)+50);    
    $(this).css({"background-size": xrnd+"px 100px"})
})
//tag rollover events
    $("tags").bind('mouseenter mouseleave', function() 
    {
    $(this).toggleClass('tagEntered');
    });   
    

});


//jquery faders when all images loaded

$(window).load( function() 
    {
    spinner.stop();
    $("#content").fadeIn(500);
    $("canvas").fadeIn(1000);
    $("iframe").fadeIn(500);

//description box dynamic resize
var footerPos=$('#footer').offset();
var h=footerPos.top+100;
$('#description').css({height: h+'px', background: '#ddd'});    
//stretch swf audio player and video embeds
$('.audio_player embed').attr('width','98%');
$('iframe').attr('width' , '98%');
$('h2').addClass('shadow');
   
    });


</script>
    
     
