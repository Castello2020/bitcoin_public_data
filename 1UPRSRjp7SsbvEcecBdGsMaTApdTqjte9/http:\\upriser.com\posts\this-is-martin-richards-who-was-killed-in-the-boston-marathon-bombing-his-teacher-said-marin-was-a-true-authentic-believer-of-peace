window.onYouTubePlayerAPIReady = function(playerId) {
	var video_id = $("#youtube-player").data('video-id');
	var start = $("#youtube-player").data('start');
  $.each($(".youtube-player"), function(player) {
		player = new YT.Player('youtube-player',{
			videoId: video_id,
      playerVars: { 'start': start },
			events: {
				'onStateChange': onPlayerStateChange
			}
		})
	});
}

