	<script type="text/javascript">

		$('#MainContent_StarLinkButton').click(function (e) {
			e.preventDefault();
			//alert('click detected');
			var selectedButton = $(this);
			var favObject = new Object();
			favObject.UserID = "";
			favObject.FavoriteID = 114 +0;
			favObject.FavoriteType = 1;
			favObject.isDefaultFavorite = 0;
			if (selectedButton.hasClass("star1")) {
				$.ajax({
					url: "http://college-confessions.com/api/AddFavorite/",
					data: JSON.stringify(favObject),
					contentType: 'application/json',
					type: 'POST',
					success: function (data) {
						selectedButton.removeClass("star1").addClass("star2");
						$('#MainContent_StarLabel').html("This school is a favorite");
					},
					error: function (data) {
						alert("error: " + data.responseMessage);
						//alert(JSON.stringify(favObject));
					}
				});

			}
			else {
				$.ajax({
					url: "http://college-confessions.com/api/AddFavorite/",
					data: JSON.stringify(favObject),
					contentType: 'application/json',
					type: 'POST',
					success: function (data) {
						selectedButton.removeClass("star2").addClass("star1");
						$('#MainContent_StarLabel').html("Add this school as a favorite school");
					},
					error: function (data) {
						alert("error: " + data.responseMessage);
						//alert(JSON.stringify(favObject));
					}
				});

			}
			return false;
		});

	</script>
	

