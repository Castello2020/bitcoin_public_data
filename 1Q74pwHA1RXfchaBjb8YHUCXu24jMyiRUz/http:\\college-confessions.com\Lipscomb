	<script type="text/javascript">
		$('.up').click(function (e) {
			e.preventDefault();
			var confID = this.id.split("-")[1];
			var voteObject = new Object();
			voteObject.VoteID = confID;
			voteObject.VoteScore = 1;
			//alert('Up vote clicked for ' + confID);
			$.ajax({
				url: "http://college-confessions.com/api/Vote/",
				data: JSON.stringify(voteObject),
				contentType: 'application/json',
				type: 'POST',
				success: function (data) {
					//alert(data);
					if (data != "err") {
						$('#ScoreDiv-' + voteObject.VoteID).html(data);
					}
				},
				error: function (data) {
					alert("error occurred");
					//alert(JSON.stringify(favObject));
				}
			});
			return false;
		});

		$('.thumbpng').click(function (e) {
			e.preventDefault();
			var confID = this.id.split("-")[1];
			var voteObject = new Object();
			voteObject.VoteID = confID;
			voteObject.VoteScore = -1;
			//alert('Up vote clicked for ' + confID);
			$.ajax({
				url: "http://college-confessions.com/api/Vote/",
				data: JSON.stringify(voteObject),
				contentType: 'application/json',
				type: 'POST',
				success: function (data) {
					//alert(data);
					if (data != "err") {
						$('#ScoreDiv-' + voteObject.VoteID).html(data);
					}
				},
				error: function (data) {
					alert("error occurred");
					//alert(JSON.stringify(favObject));
				}
			});
			return false;
		});
	</script>
</div>
