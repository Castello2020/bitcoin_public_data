        <script  type='text/javascript'>
        $.mynamespace = { 
            count : 0
        };
        $(document).ready(function(){
            $("#andrea_censi_portrait2").hover(function() {
                $.mynamespace.count = $.mynamespace.count + 1;
                //$(this).attr("src","media/portrait1.jpg");
                // if ($.mynamespace.count > 0) {
                //    $('#other_picture').show();
                // }   
                if ($.mynamespace.count > 5) {
                    $('#other_picture2').show();
                }
                if ($.mynamespace.count > 10) {
                    $('#other_picture3').show();
                }
                if ($.mynamespace.count > 15) {
                    $('#thatsit').show();
                }
                if ($.mynamespace.count > 20) {
                    $('#more').show();
                }
            }, function() {
                //$(this).attr("src","media/portrait2.jpg");
            });
        });
        </script>
        
