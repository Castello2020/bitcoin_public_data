													<script language="javascript" type="text/javascript">
$(document).ready(function() {
	$(".answer_link").click(function() {
		var str = $(this).attr('id');
		var parent_id = str.substr(7);
		$("#parent_id").val(parent_id);

		var review_form = $("#review_form");
		if (parent_id != 0)
			$(this).parent().append(review_form.attr('style', 'margin-left:25px'));
		else
			$(this).parent().append(review_form.attr('style', 'margin-left:0'));

		return false;
	});

	$("#add_review").click(function() {
		$.post('http://www.dumaguetereview.com/reviews/add/listings/240/comments/', {review: $("#review_body").val(), parent_id: $("#parent_id").val(), anonym_name: $("#anonym_name").val(), anonym_email: $("#anonym_email").val(), captcha: $("#captcha").val()},
		function (data, status) {
			if(typeof(data.error_msg) != 'undefined') {
				if(data.error_msg != '') {
					alert(data.error_msg);
				} else {
					$("#reviews").html('<div style="margin: 20px 0;">' + $("#ajax_loader").html() + '<'+'/div>');
					$.post('http://www.dumaguetereview.com/reviews/refresh/listings/240/comments/', {},
						function(data) {
							$("#reviews").html(data);
						}
					);
				}
			}
		}, 'json');
	});
});
</script><div id="reviews">
