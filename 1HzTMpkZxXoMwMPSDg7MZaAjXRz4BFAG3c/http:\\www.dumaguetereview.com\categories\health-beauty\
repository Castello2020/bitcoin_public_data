							categories_list = categories_list + $(this).attr('id').replace("category_", "") + ", ";
						});
						$("#search_category").val(categories_list);
					});
	                
	                $("#search_category_button").click( function(){
	                	$("#search_by_category_tree").show();
	                	var pos = $("#search_category_button").offset();
	                	$("#search_by_category_tree").css({'top': parseInt(pos.top)+parseInt(14)+'px', 'left': parseInt(pos.left)+parseInt(5)+'px'});
	                	return false;
	                });
	                
	                // If click outside - hide all menus
					$("body").click( function(event) {
						if(!$(event.target).is('#search_by_category_tree *') && !$(event.target).is('#search_category_button')) {
							$("#search_by_category_tree").hide();
						}
					});

										$("#what_search").blur( function() {
						if ($(this).val() == '' )
							$(this).val(default_what).addClass('highlight_search_input');
					});
					$("#what_search").focus( function() {
						if ($(this).val() == default_what)
							$(this).val('').removeClass('highlight_search_input');
					});
															
					// Form submit event
					$("#search_form").submit( function() {
											if ($("#what_search").val() != '' && $("#what_search").val() != default_what) {
                			global_js_url = global_js_url + 'what_search/' + urlencode($("#what_search").val()) + '/';
                			global_js_url = global_js_url + "what_match" + '/' + $("input[name=what_match]:checked").val() + '/';
                		}
                	                	                		if ($("#search_category").val() != '') {
                			global_js_url = global_js_url + 'search_category/' + $("#search_category").val() + '/';
                		}

                		global_js_url = global_js_url + use_advanced;
                		window.location.href = global_js_url;
						return false;
					});

					var what_advanced_opened = false;
					$("#what_match").click( function() {
						$("#what_advanced_block").toggle(200);
						if (what_advanced_opened) {
							$("#what_match img").attr('src', 'http://www.dumaguetereview.com/themes/default/images/icons/add.png');
							what_advanced_opened = false;
						} else {
							$("#what_match img").attr('src', 'http://www.dumaguetereview.com/themes/default/images/icons/delete.png');
							what_advanced_opened = true;
						}
						return false;
					});
					
					var where_advanced_opened = false;
					$("#where_advanced").click( function() {
						$("#where_advanced_block").toggle(200);
						if (where_advanced_opened) {
							$("#where_advanced img").attr('src', 'http://www.dumaguetereview.com/themes/default/images/icons/add.png');
							where_advanced_opened = false;
						} else {
							$("#where_advanced img").attr('src', 'http://www.dumaguetereview.com/themes/default/images/icons/delete.png');
							where_advanced_opened = true;
						}
						return false;
					});
					
					var advanced_opened = false;
					$(".advanced_search").click( function() {
						if (advanced_opened) {
							$(".advanced_search img").attr('src', 'http://www.dumaguetereview.com/themes/default/images/icons/add.png');
							$("#advanced_search_block").hide();
							advanced_opened = false;
							use_advanced = '';
						} else {
							$(".advanced_search img").attr('src', 'http://www.dumaguetereview.com/themes/default/images/icons/delete.png');
							$("#advanced_search_block").show();
							advanced_opened = true;

							$("#advanced_search_block").html($("#ajax_loader").html());
							$.post('http://www.dumaguetereview.com/build_advanced_search/', {type_id: 0, args: 'null'}, 
							function(data) {
								$("#advanced_search_block").html(parseScript(data));
								evalScripts(data);
								use_advanced = 'use_advanced/true/';
							}
							);
						}
						return false;
					});
                });

                function submit_listings_form()
                {
                	$("#listings_form").attr("action", 'http://www.dumaguetereview.com/admin/listings/' + action_cmd + '/');
                	$("#listings_form").submit();
                }
