            $("#accordion").accordion({
                active: false,
                collapsible: true,
                animate: "easeInOutQuint",
                heightStyle: "content",
                beforeActivate: function(e, ui){
					
					if($.browser.msie && ui.newPanel.html() != undefined)
                    {
                        var frm = ui.newPanel.find("iframe").first();
						
