			fx:    "scrollHorz",
			speed:  "slow",
			timeout: "10000",
			random: 0,
			nowrap: 0,
			pause: 1,
			pager:  "#slider-navigation",
			pagerAnchorBuilder: function(idx, slide) {
				return "#slider-navigation li:eq(" + (idx) + ") a";
