                <script type="text/javascript">
            $(".btn-more-sharing").click(function(e){
                e.preventDefault();
                $(this).css("display","none");
                target = $(this).attr("href");
                $($(this).attr("href")).css("display","block");
            });

            $(window).scroll(function() {
                var top = window.pageYOffset || document.documentElement.scrollTop;
                if(top > $(".article-links").offset().top && top < $("#comments").offset().top) {
                    $("#social-fixed .social-fixed-container").addClass("social-fixed-open");
                }else{
                    $("#social-fixed .social-fixed-container").removeClass("social-fixed-open");
                }
            });

            $(function(){
                $('a.fb-share').on('click', function(){
                    window.open("http://facebook.com/sharer/sharer.php?locale=es_ES&app_id=360028941603&redirect_uri=http://www.genbeta.com/guia-de-inicio/aprendiendo-a-usar-una-moneda-virtual-primeros-pasos-con-el-bitcoin&u=http://www.genbeta.com/guia-de-inicio/aprendiendo-a-usar-una-moneda-virtual-primeros-pasos-con-el-bitcoin",'Sharer','toolbar=0,status=0,width=548,height=325');
                });
            });
        </script>
               </div>
  <!-- .container -->
  </div>
