		<script>
			$("input:file").change(
				function(e){
					var fileName = $(this).val().split("/").pop().split("\\").pop();
					$("#longURL").val(fileName);
					$("#longURL").prop("disabled", true);
					$("#shortenSubmit").val("Go");
					$("#warning").addClass("hidden");
					$("#imageUpLabel").addClass("hide-offscreen");
					$("#imageRemoveLabel").removeClass("hidden");
				});

			$("#imageRemoveLabel").click(function() {
				$(this).closest("form").find("input[type=text]").val("");
				$("#longURL").prop("disabled", false);
				$("#shortenSubmit").val("Shorten");
				$("#warning").addClass("hidden");
				$("#imageUpLabel").removeClass("hide-offscreen");
				$("#imageRemoveLabel").addClass("hidden");
			});
		</script>
		
		<script>
	        $(function() {
		        $("#customOptions").removeClass("hidden");
	        });
	        
        	$("#customOptions").click(function() {
	        	$("#customURLContainer").toggleClass("hide");
	        	if ($("#customOptions").html() == "Custom URL ▼") {
		        	$(this).html("Nevermind ▲");
	        	}
	        	else {
		        	$(this).html("Custom URL ▼");
		        	$("#customURL").val("");
	        	}
	        	// down arrow&#9660;
	        	// up arrow&#9650;
	        	event.preventDefault();
        	});
        </script>
        
        <script>
			$("#shortenSubmit").click(function(event) {
				event.preventDefault();
				
				var urlString = $("#longURL").val();
				var customString = $("#customURL").val();
				var goAhead = true;
				
				if(urlString.length < 1) {
					$("#warning").removeClass("hidden");
					$("#warning").text("Please don't leave it blank");
					$("#longURL").focus();
					$("#longURL").select();
					
					goAhead = false;
					return false;
				}
				
				if (urlString.length < 4) {
					$("#warning").removeClass("hidden");
					$("#warning").text("Please enter a URL longer than 3 characters");
					$("#longURL").focus();
					$("#longURL").select();
					goAhead = false;
					return false;
				}
				
				if (urlString.indexOf(".") === -1) {
					$("#warning").removeClass("hidden");
					$("#warning").text("Please enter a valid URL");
					$("#longURL").focus();
					$("#longURL").select();
					goAhead = false;
					return false;
				}
				
				if (urlString.toLowerCase().indexOf("waa.ai") !== -1) { 
					$("#warning").removeClass("hidden");
					$("#warning").text("Please don't shorten waa.ai links");
					$("#longURL").focus();
					$("#longURL").select();
					goAhead = false;
					return false;
				}

				
				if (goAhead === true) {
					if (customString.length < 1) {
						$("#shortenForm").submit();
					}
					
					$.ajax({
		        		type: "post",
	        			url: "includes/validate.php?lang=en",
	        			data: $("#customURL"),
	        			success: function(response) {
							var result = jQuery.parseJSON(response);
							if (result.success === false) {
								$("#warning").removeClass("hidden");
            					$("#warning").text(result.message);
            					$("#customURL").focus();
								$("#customURL").select();
		          			}
		          			else {
								$("#warning").text("SUCCESS: " + result.message);
								$("#shortenForm").submit();
		        			}
		        		}
					});
				}
			});
		</script>
		
				
