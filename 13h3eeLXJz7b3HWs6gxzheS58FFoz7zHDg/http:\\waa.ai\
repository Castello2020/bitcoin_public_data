		<script type="text/javascript">
			$(function () {
				setActivePage();
			});
			
			function setActivePage() {
				var path = window.location;

				$("#mainNav a").each(function () {
					var href = $(this).attr("href");
					if (path == href) {
						$(this).closest("li").addClass("active");
					}
				});
			}
		</script>


