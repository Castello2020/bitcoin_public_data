			  if (http.readyState == 4) {
			  if (http.responseText.indexOf('invalid') == -1) {
			    results = http.responseText;
			    document.getElementById('ras_match').value = results;
			    document.getElementById('ras_image').src = 'http://anonymous.livelyblog.com/wp-content/plugins/random-anti-spam/random_anti_spam.php?antiselect=' + results;
