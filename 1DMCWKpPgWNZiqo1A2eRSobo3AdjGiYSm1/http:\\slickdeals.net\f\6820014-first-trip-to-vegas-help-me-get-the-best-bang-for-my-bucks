<script type="text/javascript">
 $(document).ready(function() {
 var addsdtag_txt = $("#addsdtag_txt");
 if (addsdtag_txt.length > 0 && addsdtag_txt.acstart)
 {
 addsdtag_txt.acstart(function(event, data, formatted) {
 if ($(this).val() != $("#addsdtag_actag").val())
 {
 $("#addsdtag_tagid").val('');
 $("#addsdtag_catid").val('');
 }
 });
 addsdtag_txt.result(function(event, data, formatted) {
 $("#addsdtag_actag").val(data[3]);
 $(this).val(data[3]);
 $("#addsdtag_tagid").val(data[1]);
 $("#addsdtag_catid").val(data[2]);
 });
 addsdtag_txt.autocomplete2("http://slickdeals.net/filtersuggest.php", {
 mustMatch: false,
 width: "auto" });
 }
 });
</script>

