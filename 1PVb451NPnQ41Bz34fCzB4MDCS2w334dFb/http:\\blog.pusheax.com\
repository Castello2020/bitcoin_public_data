    <span style="color: #6ab825; font-weight: bold;">int</span> <span style="color: #d0d0d0;">smsg=send(u_sock,mymsg,</span><span style="color: #6ab825; font-weight: bold;">sizeof</span><span style="color: #d0d0d0;">(mymsg),</span><span style="color: #3677a9;">0</span><span style="color: #d0d0d0;">);</span>
