    <span style="color: #6ab825; font-weight: bold;">if</span><span style="color: #d0d0d0;">(conn==SOCKET_ERROR){</span>
        <span style="color: #d0d0d0;">std::cout&lt;&lt;</span><span style="color: #ed9d13;">"Error - when connecting "</span><span style="color: #d0d0d0;">&lt;&lt;WSAGetLastError()&lt;&lt;std::endl;</span>
        <span style="color: #d0d0d0;">closesocket(u_sock);</span>
        <span style="color: #d0d0d0;">WSACleanup();</span>
    <span style="color: #d0d0d0;">}</span>

    <span style="color: #999999; font-style: italic;">//Send some message to remote host</span>
    <span style="color: #6ab825; font-weight: bold;">char</span><span style="color: #d0d0d0;">*</span> <span style="color: #d0d0d0;">mymsg=</span><span style="color: #ed9d13;">"GET / HTTP/1.1\r\n\r\n"</span><span style="color: #d0d0d0;">;</span>
