        &#34;tradeId&#34;: &#34;b377ad3c-6b84-4af1-9a09-125be4f70f09&#34;,
        &#34;orderId&#34;: &#34;c379c3e4-c536-4781-aea5-822b09883a81&#34;,
        &#34;timestamp&#34;: 1394359432000,
        &#34;tradedCurrencyFillAmount&#34;: &#34;21014.05&#34;,
        &#34;settlementCurrencyFillAmount&#34;: &#34;550.74&#34;,
        &#34;ccyPair&#34;: &#34;DOGEHKD&#34;,
        &#34;side&#34;: &#34;BUY&#34;
    }]
}</code></pre>
                    </section>
                                                                                    </section>
            </section>
                          <section class="debugger" data-resource-signature="POST /api/2/money/trade/list"></section>
