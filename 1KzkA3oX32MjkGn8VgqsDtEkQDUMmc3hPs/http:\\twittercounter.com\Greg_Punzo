				$('#featured_users_banner').hide();
				var fA = 4; // Featured amount
					fA = ($(window).width() < 779 && $(window).width() > 479 ) ? 1 : fA;
					fA = ($(window).width() > 479) ? fA : 0; // zero featured users on mobile

				if(fA !== 0){
					$('#featured_users_banner').show();
					$.get('/featured/', {amount: fA, country_code: 'US'}, function(data) {
						$('#featured_users_banner').html(data);
