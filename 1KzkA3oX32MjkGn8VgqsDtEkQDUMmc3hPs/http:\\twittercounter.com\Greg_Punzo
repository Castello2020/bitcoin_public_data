<script type="text/javascript">

	var g = ($(window).width() > 479) ? '#graph-bottom' : '#graph-top';
	var tweets = [],
			$lastTweets = $('#lastTweets'),
			$tweetCon = $('#tweetContainer'),
			$graph = $(g),
			$graphTools = $("#graph_tools"),
			newHeight,
			newWidth,
			newOffset;

	tweets['mentions'] = [];
	tweets['retweets'] = [];

	function showTweets(twitter_id, type, date){

		newHeight = $graph.innerHeight() - $graphTools.innerHeight();
		newWidth = $graph.width();
		newOffset = $graph.offset();

		$tweetCon.empty();
		$tweetCon.html('<div style="text-align:center; margin-top:10px;">Loading <img src="http://cdn.twittercounter.com/images/ajax_spinner.gif"/></div>');

		if(typeof tweets[type][date] != 'undefined'){
			fillContainer(tweets[type][date]);
		}else {
			$.ajax({
				url:"/inc-files/showTweets.php",
				type: "GET",
				data: { 'twitter_id': twitter_id, 'type': type, 'date': date },
				ifModified: true,
				success: function(data, textStatus, jqXHR){
					tweets[type][date] = data;
					fillContainer(data);
				}
			});
		}
	}

	function fillContainer(data){
			$tweetCon.empty().html(data);

			/*
			$lastTweets.animate({
				width: newWidth-20,
				height: newHeight,
				top: newOffset.top+$("#graph_tools").innerHeight()+6,
				left: newOffset.left
			},
			800,
			function() {
				$("#tweetContainer").fadeIn(1000);

				$("#lastTweets").append('<a href="#" id="zoom_close"><img src="http://cdn.twittercounter.com/js/images/closebox.png" /></a>')
				$('#zoom_close').css({"position" : "absolute", "left" : "-15px", "top" : "-15px", "z-index" : "1000"})

				$('#zoom_close').click(function() {
					$tweetCon.fadeOut(100)
					$("#lastTweets h2").fadeOut(100);
					$('#zoom_close').unbind('click');
					$('#zoom_close').remove();

					$lastTweets.animate({
						width: 0,
						height: 0,
						top: (newHeight / 2) + newOffset.top,
						left: (newWidth / 2) + newOffset.left,
						fade: 0
					},
					400,
					function() {
						$lastTweets.remove();
					});
				});
			});*/
		}
</script>



