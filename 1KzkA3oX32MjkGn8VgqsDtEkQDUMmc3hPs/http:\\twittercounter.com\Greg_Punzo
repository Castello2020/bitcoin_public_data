<script type="text/javascript">

	ga('set', 'dimension3', 'homepage_original');

	$(document).ready(function(){

		$('#show_lists').on('click', function(e){
			e.preventDefault();
			$('span.hidden_list').show();
			$('.appendage').hide();
		});

		var ownProfile = false;

		
		function showGraphMessage($el, message){

			if (message == 'login') {
				popitup('/remote/login.php?referer=twittercounter');
				return;
			}

			if ($el.find('.graph-message-wrapper').length > 0) {
				$el.find('.graph-message-wrapper').remove();
			}
			var messageTemplate = $('#graph-message-wrapper').clone();
			messageTemplate.find('.btn-close').on('click', function () {
				$(this).closest('.graph-message-wrapper').fadeOut('slow').remove();
			});

			var text = $('#graph-message-text-' + message).html();
			messageTemplate.find('p').html(text);

			$el.append(messageTemplate);
			messageTemplate.show();
		}

		function showHideDatePicker(metric)
		{
			if (metric == 'my_retweets' || metric == 'my_mentions') {
				$('#graph-timeframe').hide();
			} else {
				$('#graph-timeframe').show();
			}
		}
		showHideDatePicker('followers');

		function scrollToGraph () {
			$(jQuery.browser.webkit ? 'body' : 'html').animate({ 'scrollTop': $('.graph-resize').offset().top - 60 });
		}

		var is_premium = false;
		var status = (is_premium ? 'enabled' : 'disabled'),
				link = (is_premium ? undefined : '/pages/premium?highlight=mention#mention');

		var upsellLink = '/pages/premium?highlight=mention#mention';
		var g = ($(window).width() > 479) ? '#graph-bottom' : '#graph-top';
		 $('#metricselector').metricselector(
			{
				'followers': {label: 'Followers', status: 'enabled', type: 'line'},
				'following': {label: 'Following', status: 'enabled', type: 'line'},
				'tweets': {label: 'Tweets', status: 'enabled', type: 'line'},
				'mentions': {label: 'Mentions', status: status, type: 'line', link: link},
				'retweets': {label: 'Retweets', status: status, type: 'line', link: link},
				'favorites': {label: 'Favorites', status: status, type: 'line', link: link},
				'my_retweets': {label: 'My Retweets', status: status, type: 'bar', link: link, singleMetric: true},
				'my_mentions': {label: 'My Mentions', status: status, type: 'table', link: link, singleMetric: true}
			},
			{
				typeIcons: {
					line: '<i class="ss-upwardslinechart"></i>',
					bar: '<i class="ss-horizontalbarchart"></i>',
					table: '<i class="ss-horizontalbarchart"></i>'
				},
								defaultMetric: 'followers'
			},
			function (metric1, metric2) {
				$('input[name="_graph_metric1"]').val(metric1);
				$('input[name="_graph_metric2"]').val(metric2);
				var metrics = metric1;
				if (metric2 != null) {
					metrics += ',' + metric2;
				}
				$('input[name="metrics"]').val(metrics);
				showHideDatePicker(metric1);
				$(g).twcGraphUpdate();
				$.get('/featured/', {amount: 3, country_code: 'US'}, function(data) {
					$('#sidebar_featured_user').html(data);
				});
				scrollToGraph();
			}
		);

		$('#graph-timeframe').daterangepicker(
			{
				ranges: {
					'Today': {type: 'enabled', dates: ['today', 'today']},
					'Last week': {type: 'enabled', dates: [Date.today().add({ days: -6 }), 'today']},
					'Last month': {type: 'enabled', dates: [Date.today().add({ months: -1 }), 'today']},
					'Last 3 months': {type: 'enabled', dates: [Date.today().add({ months: -3 }), 'today']},
										'Last 6 months':{type: 'disabled', message: 'login'},
															'See all':{type: 'disabled', message: 'lockedRange'}
									},
				allRange: false,
				startDate: '07/17/2014',
				endDate: '07/23/2014',
				minDate: '03/05/2010',
				minActiveDate: '04/23/2014'
			},
			function(start, end, message) {
				var g = ($(window).width() > 479) ? '#graph-bottom' : '#graph-top';
				if (message) {
					showGraphMessage($(g), message);
				}
				else {
					$('#reportrange span').html(start.toString('MMM d, yyyy') + ' - ' + end.toString('MMM d, yyyy'));
					$('input[name="start_date"]').val(start.toString('yyyy-MM-dd'));
					$('input[name="end_date"]').val(end.toString('yyyy-MM-dd'));
					$(g).twcGraphUpdate();

				}
				$.get('/featured/', {amount: 3, country_code: 'US'}, function(data) {
					$('#sidebar_featured_user').html(data);
				});
				scrollToGraph();
			}
		);

		/*
			Slightly hacky way of adding tracking, but as far as i can tell the daterangepicker()
			function doesnt support callbacks on range presets
			- Joey
		*/
		$('.range_presets ul li').on('click', function(){
			var galabel = $(this).text();
			ga('send', 'event', 'Graph Profile', 'Date', galabel);
		});
		/* /end hackyness */

		var tagSuggestionTo = null;
		var opts = {source: ["socialmedia","music","technology","marketing","tnk4rt","travel","politics","design","business","life","startups","fashion","seo","food","football","photography","news","sports","health","autofollow24hs","art","education","entrepreneurship","books","writing","leadership","tech","science","entertainment","innovation"],
			items: 4,
		};

		
	});

	TwitterCounter.profile.init();

</script>
	</div> <!-- /inner_main_left -->
</div> <!-- /main_left -->

</div><!-- end container-large -->

