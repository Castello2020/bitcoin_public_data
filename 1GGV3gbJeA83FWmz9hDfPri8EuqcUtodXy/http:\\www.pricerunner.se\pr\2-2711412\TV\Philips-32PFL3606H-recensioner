<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function ($) {
	if (location.hash == '#expertReview') {
		$("#expertReview").addClass("box-t-open");
	} else if (location.hash == '#userReview') {
		$("#userReview").addClass("box-t-open");
	} else if (location.hash == '#videoReview') {
		$("#videoReview").addClass("box-t-open");
	} else {
		//$("#expertReview").addClass("box-t-open");
	}

	$("a.expertReviewLink").click(function() {
		$("#expertReview").addClass("box-t-open");
		$("#userReview").removeClass("box-t-open");
		$("#videoReview").removeClass("box-t-open");
	});

	$("a.userReviewLink").click(function() {
		$("#userReview").addClass("box-t-open");
		$("#expertReview").removeClass("box-t-open");
		$("#videoReview").removeClass("box-t-open");
	});

	$("a.videoReviewLink").click(function() {
		$("#videoReview").addClass("box-t-open");
		$("#expertReview").removeClass("box-t-open");
		$("#userReview").removeClass("box-t-open");
	});

	$('#userReview .pageLinks li a').attr('href', function() { return this.href + '#userReview' } );
	$('#expertReview .pageLinks li a').attr('href', function() { return this.href + '#expertReview' } );
	$('#videoReview .pageLinks li a').attr('href', function() { return this.href + '#videoReview' } );
});
//]]>
</script>
