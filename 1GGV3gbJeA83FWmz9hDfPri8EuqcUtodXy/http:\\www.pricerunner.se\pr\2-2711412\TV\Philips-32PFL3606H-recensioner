<script type="text/javascript">
jQuery(document).ready(function($){     

	var pid = "2711412";
	var cid = "2";
	var redirUrl = "http%3A%2F%2Fwww.pricerunner.se%2Fpl%2F2-2711412%2FTV%2FPhilips-32PFL3606H-priser";
	$(".enlargeGraph").click(function(e){
		e.preventDefault();
		var baseUrl = "/mod_shopping/public/price-watch-graph?pid="+pid+"&cid="+cid+"&rurl="+redirUrl;
		$.modal("", {
			overlay: 50,
			overlayId:   "loginOverlay",
			containerId: "loginContainer",
			onOpen: function(dialog) {
			    try {
			        dialog.data.load(baseUrl,  function() {
			            $(".modalClose", $(dialog.data)).click(function(e) {
						    e.preventDefault();
						    $("#loginContainer").next('div[id^=elycharts_tooltip_]').remove();
						    $.modal.close();
						});
			            loadPriceGraph($(".largetGraph"), 11, 360, 'big');
			    	});
				    dialog.overlay.show();
				    dialog.container.show();
				    dialog.data.show();
				} catch(e) { }
			},
		    close: false
		  }
		);
		
		$("#loginContainer").css({"margin-left": "-271px"});
		$("#loginOverlay").click(function(e) {
		    e.preventDefault();
		    $("#loginContainer").next('div[id^=elycharts_tooltip_]').remove();
		    $.modal.close();
		});
	});
	
	

	function loadPriceGraph(obj, axisNum, maxNum, type){
		var needGrid = false;
		var needLevel = true;
		if(type!="tiny" && type!="small" && type!="link"){
			needGrid = true;
		}else{
			if(type=="link"){
				needLevel = false;
			}
		}
		$.elycharts.templates['line_basic_'+type] = {
				 type : "line",
				 series : { serie1 : {color : "red" } },
				 features : { grid : { draw : [needLevel,needGrid], forceBorder : true ,ny: 4} }
		}
		
		$.get( "/mod_shopping/public/price-history",
			   { cid : cid,
				 pid : pid,
				 axisNum : axisNum,
				 maxNum: maxNum,
				 type:type
			   },
			   function (data){
				   if(data && data.pricelist.length){
					   _getPriceGraph(obj, data.pricelist,data.flagFix,type);
					   _showDateLabel(data.startDate, data.endDate);
						$("#loginContainer").next('div[id^=elycharts_tooltip_]').css({"z-index": 10101});
				   }else{
						$(".wdiget-small-box").remove();
						$(".show-price-history").hide();
				   }
			   },
			   'json'
		    );
	}

	function _getPriceGraph( obj, data, flag, type){

		var prices = [];
		var dates  = [];
		var tooltips  = [];
		for(var i = data.length - 1; i >= 0; i--){
			var j = data.length - 1 -i;
			prices[j] = data[i].value;
			dates[j] =  (data[i].date)? data[i].date : '';
			tooltips[j] = data[i].tooltip;
		}
		if('tiny' != type){
			var magin_bot = ('small' == type) ? 8 : 50;
			var x_rotate = ('small' == type) ? -0.1 : -30;
			var x_laMarg = ('small' == type) ? 22 : -4;
		}else{
			if('link' != type){
				var magin_bot =  3 ;
				var x_rotate = -0.1 ;
				var x_laMarg = 22 ;
			}else{
				var magin_bot =  0 ;
				var x_rotate = 0 ;
				var x_laMarg = 0 ;
			}
		}
		
		var prefix   = (flag == 'prefix') ? 'kr' : '';
		var suffix   = (flag == 'suffix') ? 'kr' : '';
		obj.find('div').remove();
		if(type=="link"){
			obj.chart({
				 template: "line_basic_"+type,
				 axis: {
					    l:{ prefix: prefix, suffix: suffix},
					    
				   },
				 
				 defaultAxis : 	{
					 				labels : false,
					 				labelsProps: {
					 	                fill: "#444"
					 	            }
					 			},
				 margins : [1, 1, 1, 1],
				 labels : dates,
				 
				 tooltips : { serie1 :  tooltips },
				 values : {  serie1 : prices },
				 defaultSeries : {
				  plotProps : {	  
					   	"stroke-width" : 1
					}
				 },
				 width: 20,
				 height:17,	
			});	
		}else if(type=="tiny"){
			obj.chart({
				 template: "line_basic_"+type,
				 axis: {
					    l:{ prefix: prefix, suffix: suffix},
					    
				   },
				 
				 defaultAxis : 	{
					 				labels : false,
					 				labelsProps: {
					 	                fill: "#444"
					 	            }
					 			},
				 margins : [3, 3, magin_bot, 0],
				 labels : dates,
				 
				 tooltips : { serie1 :  tooltips },
				 values : {  serie1 : prices },
				 defaultSeries : {
				  plotProps : {	  
					   	"stroke-width" : 2
					}
				 },
				 width: 96,
				 height:60,	
			});	
		}else{
			obj.chart({
				 template: "line_basic_"+type,
				 axis: { x: { labelsRotate: x_rotate,	
					      labelsMargin: x_laMarg
					    },
					    l:{ prefix: prefix, suffix: suffix},
					    
				   },
				 
				 defaultAxis : 	{
									labels : true,
						 			labelsProps: {
						 	        	fill: "#444"
						 	       	}
					 			},
				 margins : [10, 20, magin_bot, 50],
				 labels : dates,
				 
				 tooltips : { serie1 :  tooltips },
				 values : {  serie1 : prices },
				 defaultSeries : {
				  	plotProps : {
				   		"stroke-width" : 2
					}
				 }
			});	
		}
		
	}

	function _showDateLabel(start, end){
		$(".price-history-date-label .start").text(start);
		$(".price-history-date-label .end").text(end);
	}
		loadPriceGraph($(".price-history-date-micro"), 8, 200, 'link');
		
});

 
</script>
