					<!-- //reviewtab -->
					<script  type="text/javascript">

					jQuery(function(){
						var isLogin = 0;
						//get The Count
						jQuery("#user_review_span").append("("+jQuery("#user_review_count").val()+")");
						jQuery("#expert_review_span").append("("+jQuery("#expert_review_count").val()+")");
						jQuery("#video_review_span").append("("+jQuery("#video_review_count").val()+")");
						//show the blocks
												var show_block = "userReviewDetail",blocks = ['prReviewDetail','userReviewDetail','expertReview','videoReview'],hashArr = location.hash.split("#");
						if(hashArr.length>1){
							var hash = hashArr[hashArr.length-1];
							if(hash !=""){
								for(var i=0;i<4;i++){
									if(jQuery("#"+blocks[i]).css("display") == 'none'){
										if(blocks[i] == hash){
											show_block = blocks[i];
										}
									}
								}
							}
						}
						//show form 
						if(hash == 'write-review' && isLogin){
							jQuery("#overall_review").slideUp();
							jQuery("#simple_form").slideDown();
						}
						jQuery("#review_tabs_ul").children("li").each(function(){
							if(show_block == jQuery(this).attr("data")){
								jQuery(this).addClass("selected");
							}
						});
						jQuery("#"+show_block).show();
						//register the Click event
						jQuery("#review_tabs_ul").children("li").each(function(){
							jQuery(this).click(function(){
								jQuery("#review_tabs_ul").children("li").removeClass("selected");
								jQuery(this).addClass("selected");
								jQuery("#"+show_block).hide();
								show_block = jQuery(this).attr("data");
								jQuery("#"+show_block).fadeIn();
							});
						});
					});
					</script>

