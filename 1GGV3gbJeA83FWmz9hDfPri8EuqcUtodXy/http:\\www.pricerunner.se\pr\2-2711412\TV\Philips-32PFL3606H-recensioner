<!-- writereview-->
<script type="text/javascript">
var default_title = "Skriv rubrik för din recension";
var default_content = "Skriv din recension";
jQuery(function(){
	jQuery(".show_s_form_btn").click(function(){
		jQuery("#overall_review").slideUp();
		jQuery("#simple_form").slideDown();
		});
	jQuery("#simple_form_text").focus(function(){
		if(jQuery(this).val() == default_title){
			jQuery(this).val("");
			jQuery(this).css("font-style","normal");
			}
		});
	jQuery("#simple_form_text").blur(function(){
		if(jQuery(this).val() == ""){
			jQuery(this).val(default_title);
			jQuery(this).css("font-style","italic");
			}
		});	
	jQuery("#simple_form_textarea").focus(function(){
		if(jQuery(this).val() == default_content){
			jQuery(this).val("");
			jQuery(this).css("font-style","normal");
			}
		});
	jQuery("#simple_form_textarea").blur(function(){
		if(jQuery(this).val() == ""){
			jQuery(this).val(default_content);
			jQuery(this).css("font-style","italic");
			}
		});	
	jQuery("#write_one").click(function(){
		jQuery("#overall_review").slideUp();
		jQuery("#simple_form").slideDown();
		return false;
		});
	var errorFormHtml = encodeURI("\x3Cul\x20class\x3D\x22errors\x22\x3E\x0A\x3Cli\x3EObligatoriskt\x20f\u00E4lt,\x20f\u00E5r\x20inte\x20vara\x20tomt.\x3C\x2Fli\x3E\x0A\x3C\x2Ful\x3E\x0A");
	jQuery("#saveFormButton").click(function(e){
		e.preventDefault();
		var ischecked = true;
		var title = jQuery("#simple_form_text");
		var content = jQuery("#simple_form_textarea");
		
		title.siblings('.errors').remove();
		content.siblings('.errors').remove();

		if(!jQuery.trim(title.val()) || title.val()==default_title){
			title.after(decodeURI(errorFormHtml));
			ischecked = false;
		}
		if(!jQuery.trim(content.val()) || content.val()==default_content){
			content.after(decodeURI(errorFormHtml));
			ischecked = false;
		}
		if(ischecked) jQuery("#postreview").submit();
	});
});

</script>
<script type="text/javascript" src="/scripts/jquery.rating.pack.js"></script>					 
