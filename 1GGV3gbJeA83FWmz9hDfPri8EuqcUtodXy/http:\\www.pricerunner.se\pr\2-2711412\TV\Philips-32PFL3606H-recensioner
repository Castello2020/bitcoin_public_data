<script type="text/javascript">
							
jQuery(document).ready(function($){

	$(".open-in-frame").click(function(e){
		e.preventDefault();
		var reviewUrl = $(this).attr("rurl");
		var pfrUrl    = $(this).attr("href");
		$.ajax({
			async : false,
			url   : reviewUrl,
			success : window.open(pfrUrl)
		});
	})
	
});
</script>

	                
	                
