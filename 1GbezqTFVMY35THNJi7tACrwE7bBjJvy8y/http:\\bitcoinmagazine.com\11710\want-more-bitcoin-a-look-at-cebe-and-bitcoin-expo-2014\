    $(this).children('.content').addClass('hover');
}, function() {
    $(this).children('.content').stop(true, false).animate({ width: "0px" }, 150);
    $(this).children('.content').removeClass('hover');
});

		/* Static Box */
		var offtop = jQuery(".bm_sidenav_navigate").offset().top;	
