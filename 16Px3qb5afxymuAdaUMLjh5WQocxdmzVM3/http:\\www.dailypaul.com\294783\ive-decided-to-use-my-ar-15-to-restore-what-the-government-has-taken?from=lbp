      <style>
.aftPager button{
	border:none;
	background:transparent;
	display:inline-block;
	font-size: 20px;
	margin: 0px;
	border: 0px;
	padding: 0px 4px;
	cursor:pointer;
}

.aftPager .waity{display:inline-block}

.aftPager button:hover{color:#900}
.aftPager button.disabled,
.aftPager button.disabled:hover{
	color:#bbb;
}
</style>

<script>
(function(){
//init page
var aftBlock=$("#block-views-activeForumTopics-block_1")
aftBlock.get(0).page=0

aftBlock.find(".item-list .pager").remove()

if(1||roles.match(/sub|standard/i)){
	
	var pag=$("<div />").attr({id:"bpager"}).addClass("aftPager"),
		left=$("<button />").addClass("prev").attr({title:"Previous 20"}).html("&#9664;").attr({disabled:"disabled"}).addClass("disabled").appendTo(pag),//disabled to begin with; inits at page 0
		right=$("<button />").addClass("next").attr({title:"Next 20"}).html("&#9654;").appendTo(pag)
		
		pag.clone().attr({id:"tpager"}).insertBefore(aftBlock.find(".view-content"))
		
		pag.insertAfter(aftBlock.find(".view-content"))
	
	aftBlock.find("#bpager button, #tpager button").click(function(e){
		e.preventDefault()
		e.stopPropagation()

		var blok=aftBlock,
			list=blok.find(".view-content"),
			me=$(this),
			inc=1
			
		if(me.hasClass("prev")) inc=-1
		blok.get(0).page+=inc
		var page=blok.get(0).page
		
		if(page>=0){//not that this should be a problem
			$.ajax({
				url:"/views/ajax",
				data:{
					view_name:"activeForumTopics",
					view_display_id:"block_1",
					view_args:"",
					view_base_path:"",
					view_dom_id:"2",
					pager_element:"0",
					page:page,
					js:"1"
				},
				type:"get",
				dataType:"json",
				beforeSend:function(){
					me.css({display:"none"})
					$("<div />").addClass("waity").insertAfter(me)
					aftBlock.find("#tpager button,#bpager button").attr({disabled:"disabled"}).addClass("disabled")
				},
				success:function(r){
					list.replaceWith(
						$(r.display).find(".view-content")
					)
				},
				error:function(){},
				complete:function(){
					$(".waity").remove()
					me.css({display:"inline-block"})
					
					var them=""
					//only release next arrow
					if(page<=0) them="#tpager .next,#bpager .next"
					//otherwise release both
					else them="#tpager button,#bpager button"
					
					setTimeout(function(){
						aftBlock.find(them).removeAttr("disabled").removeClass("disabled")
					},500)
				}
			})
		}
	})
}
})()
</script>    </div>
  
  
