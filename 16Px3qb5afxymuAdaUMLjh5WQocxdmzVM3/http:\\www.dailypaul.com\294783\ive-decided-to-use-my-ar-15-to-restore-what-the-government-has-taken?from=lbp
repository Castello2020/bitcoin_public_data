margin: .25em .25em .25em 0;
}
#block-views-Features-block_1 .featured h3,
#block-views-Features-block_1 .featured .author {
font-size: 12px;
color:inherit;
font-family:verdana;
margin:0!important;
text-shadow: 0 1px 0px white;
}
