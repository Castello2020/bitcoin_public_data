#block-views-Features-block_1{position:relative;}
#block-views-Features-block_1 .view-content {margin-top: 1em;}
#block-views-Features-block_1 > h2 {
background: linear-gradient(90deg,transparent 0%,white 25%, white 75%,transparent 100%);
display: inline-block;
padding: 0;
position: absolute;
left: 25%;
top: -1.1em;
width: 50%;
