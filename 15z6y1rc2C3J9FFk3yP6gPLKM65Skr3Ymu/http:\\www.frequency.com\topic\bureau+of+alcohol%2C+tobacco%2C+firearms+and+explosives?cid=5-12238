<script>
    'use strict';

    (function () {

        var appId = '219649940739',
            channelFile = '//www.frequency.com/channel.html';

        window.fbAsyncInit = function () {
            FB.init({
                appId      : appId, // App ID
                channelUrl : channelFile, // Channel File
                status     : true, // check login status
                cookie     : true, // enable cookies to allow the server to access the session
                xfbml      : true  // parse XFBML
            });

            FB.Event.subscribe('auth.authResponseChange', function (response) {

                // store the user's info in Freq.Services.Facebook for global use in our app
                if (Freq.Services.Facebook) {
                    Freq.Services.Facebook.loginStatus(response.status);
                    if (response.authResponse) {
                        Freq.Services.Facebook.userID(response.authResponse.userID);
                        Freq.Services.Facebook.accessToken(response.authResponse.accessToken);
                        Freq.Services.Facebook.expires(response.authResponse.expiresIn);
                    }
                }

                // if we have a user object, update the hasFacebook field
                if (Freq.user) {
                    Freq.user.hasFacebook = (response.status === 'connected');
                }

                // Here we specify what we do with the response anytime this event occurs.
                if (response.status === 'connected') {
                    // noop
                } else if (response.status === 'not_authorized') {
                    // noop
                } else {
                    // noop
                }
            });

            // This event fires when the like button is clicked.  We log the Frequency activity.
            FB.Event.subscribe('edge.create', function (url) {
                try {
                    var itemId = url.substring(url.lastIndexOf('/') + 1, url.indexOf('?') > 0 ? url.indexOf('?') : url.length),
                        userId = Freq.user && Freq.user.id ? Freq.user.id : 0;
                    if (url.match('/video/') && !isNaN(itemId)) {
                        Freq.Services.Activity.videoSharedViaFacebook(userId, itemId);
                        // Freq.Services.Favorite.favorite(itemId);
                    }
                } catch (ex) {

                }
            });

            // This event fires when the like button is clicked.  We track the social event with Google Analytics.
            FB.Event.subscribe('edge.create', function (targetUrl) {
                Freq.GA.trackSocial('facebook', 'like', targetUrl);
            });

            // This event fires when the like button is clicked.  We track the social event with Google Analytics.
            FB.Event.subscribe('edge.remove', function (targetUrl) {
                Freq.GA.trackSocial('facebook', 'unlike', targetUrl);
            });

            // This event fires when a comment is created.  We track the social event with Google Analytics.
            FB.Event.subscribe('comment.create', function (response) {
                var href = response ? response.href : '';
                Freq.GA.trackSocial('facebook', 'comment', href);
            });

            // This event fires when the share dialog is submitted.  We track the social event with Google Analytics.
            FB.Event.subscribe('message.send', function (targetUrl) {
                Freq.GA.trackSocial('facebook', 'send', targetUrl);
            });

            // Finally, we notify the rest of our app that Facebook has finished initializing.
            if (window.Freq && window.Freq.Events) Freq.Events.publish('fbAsyncInit');
        };

        // Load the SDK asynchronously
        (function (d) {
            var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
            if (d.getElementById(id)) {return;}
            js = d.createElement('script'); js.id = id; js.async = true;
            js.src = "//connect.facebook.net/en_US/all.js";
            ref.parentNode.insertBefore(js, ref);
        }(document));
    }());
</script>
