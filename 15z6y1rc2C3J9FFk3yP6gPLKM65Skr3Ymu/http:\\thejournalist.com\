        window.page_query = 'journalist';
        update_user_actions = ensure(function(data){
                if(data.username){
                    $('#login').html(data.html);
                    window.username = data.username;
                }
                $ensure(function(){
                    if(data.editable){ // user is owner
                        $('.signup-box').html('<a id="id_save_playlist"  class="sign_up" onclick="return save_playlist(this.href)" href="http://editor.wn.com/save_playlist/journalist/"><span data-trans="Save playlist">Save playlist</span></a>');
