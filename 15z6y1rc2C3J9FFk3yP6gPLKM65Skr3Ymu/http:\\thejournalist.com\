                    }else if(data.username){ // watching other user page
                        $('.signup-box').html('<a onclick="return login_and_save_playlist(\'http://editor.wn.com/save_playlist/journalist/\', this.href)" rel="nofollow" class="sign_up" href="http://editor.wn.com/fork/wn/journalist/"><span data-trans="Save playlist">Save playlist</span></a>');
