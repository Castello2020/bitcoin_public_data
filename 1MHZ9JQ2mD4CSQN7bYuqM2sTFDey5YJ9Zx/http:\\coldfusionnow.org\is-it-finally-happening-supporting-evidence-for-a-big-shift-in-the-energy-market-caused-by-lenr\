                                    }
            },
            before: function(slider) {
                if (slider.slides.eq(slider.currentSlide).find('iframe').length !== 0) {
                    $f(slider.slides.eq(slider.currentSlide).find('iframe')[0] ).api('pause');                                        if(window.yt_vid_exists == true) {
