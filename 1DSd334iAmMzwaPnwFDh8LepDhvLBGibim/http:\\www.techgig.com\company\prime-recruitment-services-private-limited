<script type="text/javascript"></script>


 <script type="text/javascript">function save_techgig_updates_subsription(){var email=$('#subscribe_email').val();if(email=='Enter your email'||email.length==0){alert('Provide an Email first.');$('#subscribe_email').val('');$('#subscribe_email').focus();return false;}
if(email.length>100){alert('Email length can not be more than 100.');$('#subscribe_email').val('');$('#subscribe_email').focus();return false;}
var url=base_url+'/general_ajax_task.php?action=save_techgig_subsription_block';if(email.replace(/ /g,'')==""){alert('Provide an Email first.');$('#subscribe_email').val('');$('#subscribe_email').focus();return false;}
$.post(url,{'email_id':email},function(data){data=$.trim(data);if(data!='NVEMAIL'){if(isNaN(data)){$('#subcribe_response').show();$('#subscribe_email').val('');$('#subcribe_response').html('Error During Subsription. Please try again.');}else{$('#subcribe_response').show();$('#subscribe_email').val('');$('#subcribe_response').html('Thank you for subsribing to TechGig Updates.');}}else{$('#subcribe_response').show();$('#subscribe_email').val('');$('#subcribe_response').html('Invalid Email. Please provide valid Email.');}
setTimeout(function(){$('#subcribe_response').fadeOut('slow');},6000);});return false;}
