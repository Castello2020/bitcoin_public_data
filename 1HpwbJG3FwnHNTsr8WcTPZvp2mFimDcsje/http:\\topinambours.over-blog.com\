        <script type="text/javascript">
        (function()
        {
            var gEBCN = function(classname, root)
                {
                    var root = root || document
                        els = root.getElementsByTagName('*'),
                        hl = [],
                        i = 0, len = els.length;

                    for (i, len; i < len; i++)
                    {
                        if (els[i].className.indexOf(classname) > -1)
                        {
                            hl.push(els[i]);
                        }
                    }

                    return hl;
                },
                buttons = gEBCN('obbar-share')[0],
                fb = gEBCN('obbar-facebook', buttons)[0],
                tw = gEBCN('obbar-twitter', buttons)[0],
                gp = gEBCN('obbar-googleplus', buttons)[0],
                pn = gEBCN('obbar-pinterest', buttons)[0];

            /**
             * Bind onclick on each social share buttons
             */
            (function(els)
            {
                var el, i, lne;

                for (i = 0, len = els.length; i < len; i++)
                {
                    el = els[i];

                    el.getAttribute('data-href') &&
                        el.setAttribute(
                            'data-href',
                            el.getAttribute('data-href')
                            .replace(
                                '{referer}',
                                window.location.href
                            )
                            .replace(
                                '{title}',
                                'Topinambours et Billevesées'
                            )
                        );

                    el.onclick = function(e)
                    {
                        e.preventDefault();
                        window.open(
                            this.getAttribute('data-href'),
                            'intent',
                            'height=400,width=500,location=no,menubar=no,toolbar=no'
                        );
                    };
                }
            })([fb, tw, gp]);

            /**
             * Pinterest has a special behavior
             */
            pn.onclick = function(e)
            {
                e.preventDefault();
                (function(d)
                {
                    var e = d.createElement('script');
                    e.setAttribute('type','text/javascript');
                    e.setAttribute('charset','UTF-8');
                    e.setAttribute(
                        'src',
                        '//assets.pinterest.com/js/pinmarklet.js?r='+Math.random()*99999999
                    );
                    d.body.appendChild(e)
                })(document);
            };

            /**
             * Add class on body
             */
            document.body.className += ' withsharebar';
        })();
    </script></div>
<div class="ob-ads ob-other"></div></body>
