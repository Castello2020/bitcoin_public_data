                      <script type="text/javascript">
/* global $ */
;(function () {
    "use strict";

    var ccshUpdated = false;

    var updateCcshiframe = function () {
        if (ccshUpdated) {
            return;
        }

        // social hub business
        var $ccshiframe = $('.body-event--comic-con-social-hub .iframe-container iframe');

        // put desired url in here
        var ccshiframeSrc = 'http://www.crowdalbum.com/embed/album/53c43280992cc6eb5d003a19/1',
            currentSrc;

        if ($ccshiframe && $ccshiframe.length > 0 && ccshiframeSrc) {
            currentSrc = $ccshiframe.attr('src');

            // correct src if wrong
            if (currentSrc !== ccshiframeSrc) {
                $ccshiframe.attr('src', ccshiframeSrc);
            }
            ccshUpdated = true;
        }
    };

    $(document).ready(updateCcshiframe);

    // just in case we couldn't update the iframe on ready, try again on load
    $(window).on('load', updateCcshiframe);
})();
</script>          
