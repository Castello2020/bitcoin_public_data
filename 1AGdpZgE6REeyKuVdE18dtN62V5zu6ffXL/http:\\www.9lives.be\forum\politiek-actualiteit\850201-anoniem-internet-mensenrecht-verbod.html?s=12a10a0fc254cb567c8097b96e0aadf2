function bannercount(strClass) {
    var elems = document.getElementsByTagName('div'), i, iCount = 0; 
    for (i in elems) {
        if((' ' + elems[i].className + ' ').indexOf(' ' + strClass + ' ')  > -1) {
            iCount ++; 
        }
    }
return iCount;
}
