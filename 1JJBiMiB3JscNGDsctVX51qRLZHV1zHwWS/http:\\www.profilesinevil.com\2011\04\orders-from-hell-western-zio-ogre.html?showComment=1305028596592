#w2b-StoTop {-moz-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 5px; width:100px;background-color: #a09696;background-color: rgba(160, 150, 150, 1.0);filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#99a09696',EndColorStr='#99a09696');text-align:center;padding:5px;position:fixed;bottom:10px;right:6px;cursor:pointer;color:#0e0d12;text-decoration:none;border:3px solid #8e2323;}
