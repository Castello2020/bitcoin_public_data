"http://www.gnbo.com.ng/">Powered by Get Nigerian Business Online</a></div></div></div><div class="webstore-doc-decorator-bottom"></div><div id="googlecart-widget" style=" position: absolute; top: 0; margin-top: 2px;right: 0; margin-right: 4px;"></div><script id='googlecart-script' type='text/javascript' src='/js/v2_2/cart.js?mid=1234' integration='jscart-wizard' post-cart-to-sandbox='true' currency='NGN' productWeightUnits='LB'></script><script type="text/javascript">var failedText = "Failed to send the message.";
      function textboxOnFocus() {
        if (this.value == this.defaultValue) {
          this.value = "";
          this.className = 'webstore-shoppingcart-input';
        }
      }

      function textboxOnBlur() {
        if (this.value == "") {
          this.value = this.defaultValue;
          this.className = 'webstore-shoppingcart-input webstore-input-text-placeholder';
        } else {
          this.className = 'webstore-shoppingcart-input';
        }
        removeErrorHighlight();
      }

      function removeErrorHighlight() {
        $("#webstore_user_email").removeClass("webstore-input-error");
        $("#webstore_user_phone").removeClass("webstore-input-error");
      }

      function placeOrder() {
        removeErrorHighlight();
        $("#webstore-shoppingcart-message").hide();
        var mydata = {};

        var items = googlecart.getItems();
        var itemsArray = [];
        var total = 0;
        for (var i = 0; i < items.length; i++) {
          if (!items[i].isMarkedForRemoval()) {
            var itemQuantity = items[i].getQuantity();
            var itemPrice = items[i].getPricePerUnit();
            var itemTitle = items[i].getTitle();
            itemsArray[i] = itemQuantity + ' X ' + itemTitle +
                ' @ ' + itemPrice + ' = ' + itemQuantity * itemPrice;
            total += itemQuantity * itemPrice;
          }
        }
        mydata["items"] = itemsArray;
        mydata["total"] = total;

        if (itemsArray.length == 0) {
          $("#webstore-shoppingcart-message").html("Your cart is empty.");
          $("#webstore-shoppingcart-message").show();
          return;
        }

        var emailVal = $.trim($("#webstore_user_email").val());
        var emailDefaultVal = document.getElementById('webstore_user_email').defaultValue;
        var phoneVal = $.trim($("#webstore_user_phone").val());
        var phoneDefaultVal = document.getElementById('webstore_user_phone').defaultValue;
        var msgVal = $.trim($("#webstore_user_comments").val());
        var msgDefaultVal = document.getElementById('webstore_user_comments').defaultValue;

        if ((emailVal == '' || emailDefaultVal == emailVal) &&
            (phoneVal == '' || phoneDefaultVal == phoneVal)) {
          $("#webstore-shoppingcart-message").html("Please enter your email or phone number.");
          $("#webstore-shoppingcart-message").show();
          document.getElementById('webstore_user_email').focus();
          document.getElementById('webstore_user_email').className += ' webstore-input-error';
          document.getElementById('webstore_user_phone').className += ' webstore-input-error';
          return;
        }

        mydata["email"] = emailVal == emailDefaultVal ? "" : emailVal;
        mydata["phone"] = phoneVal == phoneDefaultVal ? "" : phoneVal;
        mydata["message"] = msgVal == msgDefaultVal ? "" : msgVal;

        $("#webstore-place-order").attr("disabled", "true");

        $.ajax({
          type: "POST",
          cache: false,
          url: "/_action/shoppingCart",
          data: mydata,
          dataType: "json",
          error: function(request, errorType) {
            // display error message
            $("#webstore-shoppingcart-message").html(failedText);
            $("#webstore-shoppingcart-message").show();
            $("#webstore-place-order").attr("disabled", "");
           },
          success: function(result) {
            if (result.status == "success") {
              $("#webstore-shoppingcart-submit-form").hide();
              $("#webstore-shoppingcart-checkout-control").show();
              googlecart.clear();
              googlecart.expungeMarkedForRemoval();
              googlecartWidget.close();
              googlecartWidget.refresh();
              window.location = "/_thankyou?order_id=" + result.orderId;
            } else if (result.status == "unsuccess") {
              $("#webstore-shoppingcart-message").html("Placing order failed. Please try later.");
              $("#webstore-shoppingcart-message").show();
              $("#webstore-place-order").attr("disabled", "");
            } else if (result.status == "email_invalid") {
              $("#webstore-shoppingcart-message").html("Please enter a valid email address.");
              $("#webstore-shoppingcart-message").show();
              $('#webstore_user_email').focus();
              $('#webstore_user_email').addClass('webstore-input-error');
              $("#webstore-place-order").attr("disabled", "");
            }
          }
        });
      }

      function checkoutClicked() {
        $("#webstore-shoppingcart-submit-form").show();
        $("#webstore-shoppingcart-checkout-control").hide();
      }

      function cancelClicked() {
        $("#webstore-shoppingcart-submit-form").hide();
        $("#webstore-shoppingcart-checkout-control").show();
      }
    </script><script type="text/javascript">
      function googlecartWidgetLoaded() {
        var gcheckoutcontrol = document.getElementById('googlecart-checkout-control');
        var gcheckoutdiv = gcheckoutcontrol.parentNode;
        gcheckoutdiv.removeChild(gcheckoutcontrol);

        var wscheckoutdiv = document.createElement('div');
        wscheckoutdiv.className = 'webstore-shoppingcart-checkout-control';
        wscheckoutdiv.id = 'webstore-shoppingcart-checkout-control';
        gcheckoutdiv.appendChild(wscheckoutdiv);

        var wscheckout = document.createElement('button');
        wscheckout.id = "webstore-checkout-button";
        wscheckout.className = 'webstore-blue-button';
        wscheckout.onclick = checkoutClicked;
        wscheckoutdiv.appendChild(wscheckout);

        wscheckout.appendChild(document.createTextNode('Place order'));

        var foot = document.getElementById('googlecart-widget-foot');
        var wscart = document.createElement('div');
        wscart.id = 'webstore-shoppingcart-footer';
        wscart.className = 'webstore-shoppingcart-footer';
        foot.parentNode.appendChild(wscart);

        var div = document.createElement('div');
        div.id = 'webstore-shoppingcart-submit-form';
        div.className = 'webstore-shoppingcart-submit-form';
        wscart.appendChild(div);

        var message = document.createElement('div');
        message.id = 'webstore-shoppingcart-message';
        message.className = 'webstore-shoppingcart-message';
        div.appendChild(message);

        var checkoutinfo = document.createElement('div');
        checkoutinfo.id = 'webstore-shoppingcart-checkout-info';
        checkoutinfo.className = 'webstore-shoppingcart-checkout-info';
        div.appendChild(checkoutinfo);

        checkoutinfo.appendChild(document.createTextNode('Place your order and Please Pay into this account: sikiru o subairu , 0021312771, access bank.Send us details of your payment through your e-mail to lanrezubair@yahoo.com, or lanrezubair5@gmail.com.\n\nTHANKING YOU FOR YOUR PATRONAGE\n'));

        var emailHeader = document.createElement('div');
        emailHeader.className = 'webstore-shoppingcart-input-header';
        div.appendChild(emailHeader);
        emailHeader.appendChild(document.createTextNode('Your email address'));
        div.appendChild(emailHeader);

        var emailEx = document.createElement('div');
        emailEx.className = 'webstore-shoppingcart-input-example';
        div.appendChild(emailEx);
        emailEx.appendChild(document.createTextNode('Ex: myemail@example.com'));
        div.appendChild(emailEx);

        var email = document.createElement('input');
        email.id = 'webstore_user_email';
        email.className = 'webstore-shoppingcart-input webstore-input-text-placeholder';
        email.setAttribute('name', 'email');
        email.setAttribute('size', '25');
        email.defaultValue = 'Email or phone number is required';
        email.onfocus = textboxOnFocus;
        email.onblur = textboxOnBlur;
        div.appendChild(email);

        var phoneHeader = document.createElement('div');
        phoneHeader.className = 'webstore-shoppingcart-input-header';
        div.appendChild(phoneHeader);
        phoneHeader.appendChild(document.createTextNode('Your phone number'));
        div.appendChild(phoneHeader);

        var phoneEx = document.createElement('div');
        phoneEx.className = 'webstore-shoppingcart-input-example';
        div.appendChild(phoneEx);
        phoneEx.appendChild(document.createTextNode('Ex: +032 322 9879'));
        div.appendChild(phoneEx);

        var phone = document.createElement('input');
        phone.id = 'webstore_user_phone';
        phone.className = 'webstore-shoppingcart-input webstore-input-text-placeholder';
        phone.setAttribute('name', 'phone');
        phone.setAttribute('size', '25');
        phone.defaultValue = 'Email or phone number is required';
        phone.onfocus = textboxOnFocus;
        phone.onblur = textboxOnBlur;
        div.appendChild(phone);

        var commentsHeader = document.createElement('div');
        commentsHeader.className = 'webstore-shoppingcart-input-header';
        div.appendChild(commentsHeader);
        commentsHeader.appendChild(document.createTextNode('Additional notes for the website owner'));
        div.appendChild(commentsHeader);

        var comments = document.createElement('textarea');
        comments.id = 'webstore_user_comments';
        comments.className = 'webstore-shoppingcart-input webstore-input-text-placeholder';
        comments.setAttribute('rows', '6');
        div.appendChild(comments);

        var notes = document.createElement('div');
        notes.id = 'webstore-shoppingcart-footer-notes';
        notes.className = 'webstore-shoppingcart-footer-notes';
        div.appendChild(notes);

        notes.appendChild(document.createTextNode('Clicking "Place order" sends your order to the website owner. You and the merchant then must confirm the final order and arrange payment.'));
        div.appendChild(notes);

        var span = document.createElement('span');
        div.appendChild(span);

        var submit = document.createElement('input');
        submit.id = 'webstore-place-order';
        submit.className = 'webstore-shoppingcart-submit webstore-blue-button';
        submit.setAttribute('type', 'submit');
        submit.setAttribute('value', 'Place order');
        submit.onclick = placeOrder;
        span.appendChild(submit);

        var cancel = document.createElement('a');
        cancel.id = 'webstore-place-order-cancel';
        cancel.onclick = cancelClicked;

        cancel.appendChild(document.createTextNode('Cancel'));

        span.appendChild(cancel);

        // override shopping cart messages for localization purpose
        googlecartWidget.STRINGS.MSG_CART_TITLE =
          'View My Order';
        googlecartWidget.STRINGS.MSG_EMPTY =
          'Your order is empty.';
        googlecartWidget.STRINGS.MSG_SUBTOTAL =
          'Subtotal: ';

        googlecartWidget.refresh();
      }
