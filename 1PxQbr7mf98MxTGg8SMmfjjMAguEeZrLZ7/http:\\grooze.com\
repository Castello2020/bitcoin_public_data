hookEvent('iThumbsBox','mousewheel',thumbsWheelScroll);
hookEvent('iPlaylistsBox','mousewheel',playlistsWheelScroll);
hookEvent('iArtistsBox','mousewheel',artistsWheelScroll);
animateTitle();
if(gRootFound)startTimer();
