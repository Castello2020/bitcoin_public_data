//if(!gRootFound)gScreenSize=gOriginalScreenSize=gPriorChatSize=2;
if(gFmt!=""){
 if(gFmt!="full" && gFmt!="thumbs")gScreenSize=gFmt;
 else gF10=(gFmt=="full")?1:2;
}else{
 gF10=gSaveF10;
}
