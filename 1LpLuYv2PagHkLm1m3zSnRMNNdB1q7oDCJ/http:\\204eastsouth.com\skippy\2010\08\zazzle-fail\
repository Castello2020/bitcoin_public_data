			tellFrameNewParent = function() {
				if ( comm_par ) {
					frame.src = "http://jetpack.wordpress.com/jetpack-comment/?blogid=3883164&postid=1373&comment_registration=0&require_name_email=1&stc_enabled=1&stb_enabled=1&show_avatars=1&avatar_default=monsterid&greeting=Leave+a+Reply&greeting_reply=Leave+a+Reply+to+%25s&color_scheme=dark&lang=en-US&jetpack_version=3.0.2&sig=9d078fb2dd1b0ec3fbdcc5778aac5d6cdda5e73e#parent=http%3A%2F%2F204eastsouth.com%2Fskippy%2F2010%2F08%2Fzazzle-fail%2F" + '&replytocom=' + parseInt( comm_par, 10 ).toString();
