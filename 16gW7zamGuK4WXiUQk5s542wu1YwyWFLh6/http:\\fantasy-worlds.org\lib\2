	function f(i) {
		return function() {
			$('#aut')
				.html('<span id="msgbox" class="messagebox" align="center"><img src="/i/ajax.gif">Загрузка...</span>')
				.fadeIn('slow');
			$('#libr')
				.css('display', 'none');
			$.post('/module/ajax/get_author_abc.php', { letter:$('#letter'+i).text() } ,function(data){
					$('#aut').html(data).fadeIn('slow');
			})
		}
	}
	$(document).ready(function(){
		for (i = 1; i <= 32; i++) {
			$('#letter' + i).click(f(i));
		}
		$('#return').click(function(){
			$('#aut')
				.css('display', 'none');
			$('#libr')
				.css('display', 'block');
		});
	});

