     <script type="text/javascript">
        $(document).ready(function() {
            $('#lcm_wrapper').append('<div id="top">Back to Top</div>');
           $(window).scroll(function() {
            if($(window).scrollTop() != 0) {
                $('#top').fadeIn();
            } else {
                $('#top').fadeOut();
            }
           });
           $('#top').click(function() {
            $('html, body').animate({scrollTop:0},500);
           });
        });
     </script>

