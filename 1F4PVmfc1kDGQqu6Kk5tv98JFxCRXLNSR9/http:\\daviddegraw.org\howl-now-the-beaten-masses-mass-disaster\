					essb_sendmail_ajax2108310535(data.emailfrom, data.emailto, data.emailsubject, data.emailmessage, c);
				}
	}
        });
    });
});
		function essb_sendmail_ajax2108310535(emailfrom, emailto, emailsub, emailmessage, c) {
			//alert(emailfrom + "|" + emailto);
			
			var get_address = "http://daviddegraw.org/wp-content/plugins/easy-social-share-buttons/public/essb-mail.php?from="+emailfrom+"&to="+emailto+"&sub="+emailsub+"&message="+emailmessage+"&t=Howl+Now%3A+The+Beaten+Masses+%26%23038%3B+Mass+Disaster&u=http%3A%2F%2Fdaviddegraw.org&p=http%3A%2F%2Fdaviddegraw.org%2Fhowl-now-the-beaten-masses-mass-disaster%2F&c="+c;
