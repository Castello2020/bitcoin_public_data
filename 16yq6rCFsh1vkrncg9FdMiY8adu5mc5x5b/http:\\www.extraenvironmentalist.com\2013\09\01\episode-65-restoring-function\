    // *** Global Analytics Helpers Must load after Google Analytics***
    //create a poll
    function TOPPoll() {
    var d = new Date(); var h = d.getHours(); var m = d.getMinutes();
    //console.log('Interval Fired');
    _gaq.push(['_trackEvent', 'Time On Page Poll', h + ':' + m])
    }
    //kick off the poll
    function StartTOPPoll() {
    window.setInterval(
    function () { TOPPoll(); }, 600000); }
    //set a threshold for NoBounce
    function StartNoBounce(thresh) {
    window.setTimeout(function () {
    _gaq.push(['_trackEvent', 'Not Bounced', 'Over 30 TOS']);
    //console.log('TimeOutFired')
    StartTOPPoll();
    }, thresh); }

(function (jQuery) {
 jQuery.fn.timedNote = function (options) {
  var defaults = {
   displayAfter: 3000,
  }
  var options = jQuery.extend(defaults, options);
  var tab = jQuery(this), tabLink = jQuery('a', tab), tabShelf = jQuery('.noteShelf', tab);
  //set up behaviors
  tabLink.on('click', function () {
   tabShelf.toggle();
  });
  //common functions
  var TimedNote = {
   StartTimer: function (time) { setTimeout(function () { tab.slideDown() }, time); }
  }
  function init() {
   TimedNote.StartTimer(options.displayAfter);
  };
  init();
 }
})(jQuery);

    jQuery(document).ready(function () {
    //Kick off Timed Note
    jQuery('.noteTab').timedNote({displayAfter:120000});
    //Kick off the bounce timer followed by the Poll timer
    StartNoBounce(30000);

    // Podcast player interaction
    jQuery('.powerpress_player').click(function () {
    _gaq.push(['_trackEvent', 'Podcast', 'Player Interaction', window.location.pathname]);
    });

    // Podcast in a new window
    jQuery('.powerpress_link_pinw').click(function () {
    var elm = jQuery(this);
    _gaq.push(['_trackEvent', 'Podcast', 'Podcast Play in New Window', elm.attr('href')]);
    });

    // Podcast Download
    jQuery('.powerpress_link_d').click(function () {
    var elm = jQuery(this);
    _gaq.push(['_trackEvent', 'Podcast', 'Podcast Download', elm.attr('href')]);
    });

    // Podcast Embed Code
    jQuery('.powerpress_link_e').click(function () {
    var elm = jQuery(this);
    _gaq.push(['_trackEvent', 'Podcast', 'Podcast Embed', elm.attr('href')]);
    });
    
    // Feed Link clicks
    jQuery('.feedlink').click(function(){
    var elm = jQuery(this);
    _gaq.push(['_trackEvent', 'Feed', 'Subscribe', elm.text()]);
    });

