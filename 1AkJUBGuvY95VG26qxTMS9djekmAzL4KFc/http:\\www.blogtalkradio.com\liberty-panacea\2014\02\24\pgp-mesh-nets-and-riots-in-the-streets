<script type="text/javascript">
    window.dontLoadNewJQuery = true;
    
    // This will fix the Rad controls Ajax Upload bar not dissapearing because
    // it is specifying the content-length for the request on browsers that are
    // not nescape. That request header should never be set according to standards 
    // http://www.w3.org/TR/XMLHttpRequest/#the-setrequestheader()-method
    if (typeof(window["AjaxNS"]) !== 'undefined') {
        window["AjaxNS"].IsNetscape = function() {
            return true;
        };
    }
</script>
