            var padLeft = function(s, len, char) { 
                var output = s;
                if (typeof output != 'string'){
                    output = output.toString();
                }
                if (output.length < len) {
                    output = new Array(len - output.length + 1).join(char || ' ') + output;
                }
                return output;
            };
