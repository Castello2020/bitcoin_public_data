    var completedPlaylistItemCount = 0;
    function goToNextEpisode(manualOverride)
    {
        if(completedPlaylistItemCount !== jwplayer().getPlaylist().length && manualOverride!==true) return;
        if(manualOverride!==true) _gaq.push(['_trackEvent', 'player-onsite', 'redirected', 'Liberty Panacea']);
        var settings = {
            url: 'http://www.blogtalkradio.com/api/hosts/275109/episodes/6033083/next',
            method: "GET",
            type: "GET",
            data: "{}",
            contentType: "application/json",
            xhrFields: { withCredentials: true }
        };
        $.ajax(settings).done(function (response) {
            if(response.Id != -1) {
                location.assign(response.FullUrl);
            }
        });
    }
    function goToPrevEpisode(manualOverride)
    {
        var settings = {
