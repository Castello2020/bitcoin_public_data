	else load_cmc();
	</script></div><script type="text/javascript">    window._idl = {};    _idl.variant = "modal";    (function() {        var idl = document.createElement('script');        idl.type = 'text/javascript';        idl.async = true;        idl.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'members.internetdefenseleague.org/include/?url=' + (_idl.url || '') + '&campaign=' + (_idl.campaign || '') + '&variant=' + (_idl.variant || 'modal');        document.getElementsByTagName('body')[0].appendChild(idl);    })();</script></body></html>
