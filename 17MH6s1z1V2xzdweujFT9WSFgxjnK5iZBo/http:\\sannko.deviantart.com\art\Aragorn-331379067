            var google_ad_client = 'ca-pub-2005626271413567'
                ,google_ad_channel = '1325855091' 
                ,google_ad_width = 400 
                ,google_ad_height = 55 
                ,google_ui_version = 1
                ,google_ad_type = "text"                ,google_ad_lines = 2
                ,google_tl = 2
                ,google_font_face = 'verdana'
                ,google_tfs = 16
