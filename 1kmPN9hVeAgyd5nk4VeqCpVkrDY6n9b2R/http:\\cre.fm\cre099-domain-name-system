	    jQuery("#post-2298 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/cre/cre099-domain-name-system.opus", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Opus&nbsp;Audio";
	    // select-style download-widget
	    jQuery("#post-2298 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
	    // button-stile download-widget
	    jQuery("#post-2298 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});    /* --> */</script>			</div><!-- .entry-content -->

