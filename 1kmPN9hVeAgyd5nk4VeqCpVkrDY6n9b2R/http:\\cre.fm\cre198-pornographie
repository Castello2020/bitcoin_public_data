	    jQuery("#post-2199 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/cre/cre198-pornographie.transcript.txt", function(info) {
	  if (info) {
