	    // button-stile download-widget
	    jQuery("#post-2241 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});    /* --> */</script>			</div><!-- .entry-content -->

