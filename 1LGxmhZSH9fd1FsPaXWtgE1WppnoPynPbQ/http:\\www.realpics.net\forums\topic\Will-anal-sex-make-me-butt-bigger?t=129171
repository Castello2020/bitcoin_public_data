<script type="text/javascript" language="Javascript">
var keepActive = function() {new Request({url:'http://www.realpics.net'}).send('ka=1&cl='+window.location.href);}
keepActive.periodical(300000);

var img1 = new Image();
var img2 = new Image();
var img3 = new Image();
var img4 = new Image();
img1.src = '/images/buttons/100x20/newthread_on.gif';
img2.src = '/images/buttons/100x20/postreply_on.gif';
img3.src = '/images/buttons/100x20/refresh_on.gif';
img4.src = '/images/buttons/100x20/post_on.gif';

function change_forum(a){
	if(a == 0) return;
	window.location = a;
}

function change_page(page){
	if(!page) return;
	page = (page >= 2) ? '&p='+page : '';
	window.location = '/forums/topic/Will-anal-sex-make-me-butt-bigger?t=129171'+page;
}

window.addEvent('domready', function() {
	$$('#posts a.externall').set('target', '_blank');
	var voteOptions = $$('.post .vote-options');
	voteOptions.each(function(container){
		var id     = container.get('id').split('_');
		var coSign = container.getElement('.cosign-vote');
		var noSign = container.getElement('.nosign-vote');
		//var like   = container.getElement('.like-vote');
		if(coSign) {
			coSign.addEvent('click',function(e){
				e.stop();
				new Request.JSON({
					url: '/forums/director.php',
					noCache: true,
					timeout: 10000,
					urlEncoded: true,
					onRequest:function(){coSign.blur();},
					onTimeout:function(){alert('Timeout');},
					onError:function(){alert('Error');},
					onFailure:function(xhr){alert('Failure');},
					onSuccess:function(r){
						if(r.status) {
							if(r.previousUserRating) {
								noSign.removeClass('used-neg');
								noSign.toggleClass('selected-neg');
							}
							coSign.toggleClass('selected-pos');
							var cosignUsers = (r.action == 'add' || (r.action == 'switch' && r.currentUserRating == 1)) ? '<span class="darken">by <span>you</span>':'';
							if(r.action == 'remove' && r.cosignNum == 0) {
								coSign.removeClass('used-pos');
							}else if(r.action == 'remove' && r.cosignNum > 0) {
								cosignUsers = '<span class="darken" style="font-weight:normal;">by</span> <span>'+r.cosignNum+'</span>';
							}else if(r.cosignNum > 1 && cosignUsers != '') {
								var plural = (r.cosignNum > 1) ? 's':'';
								cosignUsers += ' and <span>'+(r.cosignNum - 1)+'</span> other'+plural+'</span>';
							}
							var cosignNumLabel = (r.cosignNum == 0) ? '':'\'d <span>'+cosignUsers+'</span>';
							var nosignNumLabel = (r.nosignNum == 0) ? '':'\'d <span><span class="darken" style="font-weight:normal;">by</span> '+r.nosignNum+'</span>';
							coSign.set('html','<div class="arrow-up"></div>Up'+cosignNumLabel);
							noSign.set('html','<div class="arrow-down"></div>Down'+nosignNumLabel);
						}else if(r.error) {
							alert(r.error);
						}
					}
				}).post('ref_id='+id[2]+'&ref_type='+id[1]+'&vote=2');
			});
		}

		if(noSign) {
			noSign.addEvent('click',function(e){
				e.stop();
				new Request.JSON({
					url: '/forums/director.php',
					noCache: true,
					timeout: 10000,
					urlEncoded: true,
					onRequest:function(){noSign.blur();},
					onTimeout:function(){alert('Timeout');},
					onError:function(){alert('Error');},
					onFailure:function(xhr){alert('Failure');},
					onSuccess:function(r){
						if(r.status) {
							if(r.previousUserRating) {
								coSign.removeClass('used-pos');
								coSign.toggleClass('selected-pos');
							}
							noSign.toggleClass('selected-neg');
							var nosignUsers = (r.action == 'add' || (r.action == 'switch' && r.currentUserRating == 0)) ? '<span class="darken">by <span>you</span>':'';
							if(r.action == 'remove' && r.nosignNum == 0) {
								noSign.removeClass('used-neg');
							}else if(r.action == 'remove' && r.nosignNum > 0) {
								nosignUsers = '<span class="darken" style="font-weight:normal;">by</span> <span>'+r.nosignNum+'</span>';
							}else if(r.nosignNum > 1 && nosignUsers != '') {
								var plural = (r.nosignNum > 1) ? 's':'';
								nosignUsers += ' and <span>'+(r.nosignNum - 1)+'</span> other'+plural+'</span>';
							}
							var cosignNumLabel = (r.cosignNum == 0) ? '':'\'d <span><span class="darken" style="font-weight:normal;">by</span> '+r.cosignNum+'</span>';
							var nosignNumLabel = (r.nosignNum == 0) ? '':'\'d <span>'+nosignUsers+'</span>';
							coSign.set('html','<div class="arrow-up"></div>Up'+cosignNumLabel);
							noSign.set('html','<div class="arrow-down"></div>Down'+nosignNumLabel);
						}else if(r.error) {
							alert(r.error);
						}
					}
				}).post('ref_id='+id[2]+'&ref_type='+id[1]+'&vote=1');
			});
		}
	});
});
</script>


