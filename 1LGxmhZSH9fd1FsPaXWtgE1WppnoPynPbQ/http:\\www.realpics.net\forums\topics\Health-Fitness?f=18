<script type="text/javascript">

var TopicListingEvents = function() {
	$$('.posted-img').each(function(e){
		e.addEvent('click', function(event) {
			var lastPostD = this.get('id').substring(this.get('id').search("_") + 1).split('-');

			if(lastPostD.length != 2) return false;

			var topicLink = this.getParent('tr').getElement('a[class^=t_url]').get('href');
			var page      = (lastPostD[1] >= 2) ? '&p='+lastPostD[1] : '';

			window.location = topicLink+page+'#p'+lastPostD[0];
		});
	});

	new CheckOptionsHandler({checkmarks: $$('.posted-img')});
};

var CheckOptionsHandler = new Class({
	Implements: Options,
	options: {
		checkmarks: []
	},

	initialize:function(options) {
		this.setOptions(options);
		this.setBasicEvents(this.options.checkmarks);
	},

	hoverBoxes: [],

	setBasicEvents: function(checks) {
		checks.each(function(check) {
			check.addEvent('mouseenter', this.checkEnter.bind(this, check));
			check.addEvent('mouseleave', this.checkLeave.bind(this, check));
		}, this);
	},

	checkEnter: function(check) {
		var boxId = check.retrieve('hid') || Math.ceil(Math.random() * 10000000);

		if(this.hoverBoxes[boxId]) {
			if(this.hoverBoxes[boxId].shown) {
				$clear(this.hoverBoxes[boxId].hideDelay);
				return false;
			}

			this.hoverBoxes[boxId].showDelay = this.boxShowDelay.delay(300, this, boxId);
			$clear(this.hoverBoxes[boxId].hideDelay);
			return false;
		}

		check.store('hid', boxId);

		var checkCords = check.getCoordinates();
		var checkPos   = check.getPosition();
		var checkTop   = checkCords.top;
		var checkLeft  = (checkCords.left - 16);

		var hoverBox   = new Element('div', {
			'id': 'h' + boxId,
			'class': 'check-delete',
			'html' : '<!-- -->',
			'styles': {
				'top'    : checkTop,
				'left'   : checkLeft,
				'opacity': 0.65
			}
		});

		var hoverBoxFx = new Fx.Tween(hoverBox, {unit: 'px', link: 'chain', duration: 600, transition: 'bounce:out'});

		hoverBox.addEvent('mouseenter', this.hoverBoxEnter.bind(this, boxId));
		hoverBox.addEvent('mouseleave', this.hoverBoxLeave.bind(this, boxId));
		hoverBox.addEvent('click', this.checkRemove.bind(this, [boxId, check]));

		this.hoverBoxes[boxId] = new Hash({
			'box'  : hoverBox,
			'boxFx': hoverBoxFx,
			'left' : checkLeft,
			'shown': false,
			'showDelay': 0,
			'hideDelay': 0
		});

		$(document.body).adopt(hoverBox);

		this.hoverBoxes[boxId].showDelay = this.boxShowDelay.delay(300, this, boxId);
	},

	checkRemove: function(boxId, check) {
		var findId    = /\d+$/;
		var topicData = check.getParent('tr').getElement('.t_url').get('href');
		topicData     = topicData.match(findId);
		topicData     = new Hash({
			'topicid': topicData[0].toInt(),
			'action' : 'remove-check'
		});
		new Request({url: 'http://www.realpics.net/forums/'}).send(topicData.toQueryString());

		check.setStyle('display', 'none');
		this.hoverBoxes[boxId].box.removeEvents();
		this.hoverBoxes[boxId].box.setStyle('display', 'none');
	},

	showHoverBox: function(boxId) {
		var checkLeft = this.hoverBoxes[boxId].left;

		this.hoverBoxes[boxId].shown = true;
		this.hoverBoxes[boxId].box.setStyle('left', checkLeft - 10);
		this.hoverBoxes[boxId].box.setStyle('display', 'block');
		this.hoverBoxes[boxId].boxFx.start('left', checkLeft - 10, checkLeft);
	},

	hoverBoxEnter: function(boxId) {
		$clear(this.hoverBoxes[boxId].hideDelay);
		this.hoverBoxes[boxId].box.set('opacity', '0.85');
	},

	hoverBoxLeave: function(boxId) {
		this.hoverBoxes[boxId].hideDelay = this.boxHideDelay.delay(500, this, boxId);
		this.hoverBoxes[boxId].box.set('opacity', '0.65');
	},

	checkLeave: function(check) {
		var boxId = check.retrieve('hid');

		$clear(this.hoverBoxes[boxId].showDelay);
		this.hoverBoxes[boxId].hideDelay = this.boxHideDelay.delay(500, this, boxId);
	},

	boxShowDelay: function(boxId) {
		this.showHoverBox(boxId);
	},

	boxHideDelay: function(boxId) {
		this.hoverBoxes[boxId].box.setStyle('display', 'none');
		this.hoverBoxes[boxId].shown = false;
	}
});

window.addEvent('domready', function(){new TopicListingEvents();});

function change_forum(a){
	if(!a) return;
	window.location = a;
}

function change_page(page){
	if(!page) return;
	page = (page >= 2) ? '&p='+page : '';
	window.location = '/forums/topics/Health-Fitness?f=18'+page;
}

var keepActive = function() {new Request({url:'http://www.realpics.net'}).send('ka=1&cl='+window.location.href);}
keepActive.periodical(300000);
</script>

