<script lang=JavaScript>
function calc_uf_12(x) {
   var s = uf_12.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_12.V.value;
      s =  Math.pow(V/(1 * ( 8/3 * Math.sqrt(3 * (3-t)/(4*(2-t)) - 1) + Math.sqrt(4 * (3-t)/(4*(2-t)) -2) )),1/3);
      uf_12.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_12.A.value;
      s =  Math.pow(A/(1 * (6 + 8 * Math.sqrt(3))),1/2);
      uf_12.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_12.ri.value;
      s =  ri/( Math.sqrt(Math.abs(1-t)/(4*(2-t))));
      uf_12.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_12.ro.value;
      s =  ro/( Math.sqrt((3-t)/(4*(2-t)))	);
      uf_12.s.value = Math.round(s*rx)/rx;
   }
   uf_12.V.value = Math.round( Math.pow(s,3) * ( 8/3 * Math.sqrt(3 * (3-t)/(4*(2-t)) - 1) + Math.sqrt(4 * (3-t)/(4*(2-t)) -2) )*rx)/rx;
   uf_12.A.value = Math.round( Math.pow(s,2) * (6 + 8 * Math.sqrt(3))*rx)/rx;
   uf_12.ri.value = Math.round( s * Math.sqrt(Math.abs(1-t)/(4*(2-t)))*rx)/rx;
   uf_12.ro.value = Math.round( s * Math.sqrt((3-t)/(4*(2-t)))	*rx)/rx;
   uf_12.ra.value = Math.round((uf_12.ro.value * 1+uf_12.ri.value * 1)/2*rx)/rx;
}
calc_uf_12(0);
</script>
