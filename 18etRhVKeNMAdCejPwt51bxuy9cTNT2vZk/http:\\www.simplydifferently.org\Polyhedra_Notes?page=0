      uf_09.s.value = Math.round(s*rx)/rx;
   }
   uf_09.V.value = Math.round( Math.pow(s,3) / 3 * (21 + 14 * Math.sqrt(2))*rx)/rx;
   uf_09.A.value = Math.round( Math.pow(s,2) * 2 * (6 + 6 * Math.sqrt(2) + Math.sqrt(3))*rx)/rx;
   uf_09.ri.value = Math.round( s / 17 * (5 + 2 * Math.sqrt(2) * Math.sqrt(7 + 4 * Math.sqrt(2)))*rx)/rx;
   uf_09.ro.value = Math.round( s / 2 * Math.sqrt(7 + 4 * Math.sqrt(2))*rx)/rx;
