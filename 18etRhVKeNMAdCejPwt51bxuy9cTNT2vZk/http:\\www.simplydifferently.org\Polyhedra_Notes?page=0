      uf_26.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_26.A.value;
      s =  Math.pow(A/(1 * 5 * (Math.sqrt(3) + 6 * Math.sqrt(5 + 2 * Math.sqrt(5)))),1/2);
      uf_26.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_26.ri.value;
      s =  ri/( 5/488 * (17 * Math.sqrt(2) + 3 * Math.sqrt(10)) * Math.sqrt(37 + 15 * Math.sqrt(5)));
      uf_26.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_26.ro.value;
      s =  ro/(1/ 4 * Math.sqrt(74 + 30 * Math.sqrt(5)));
      uf_26.s.value = Math.round(s*rx)/rx;
   }
   uf_26.V.value = Math.round( Math.pow(s,3) * 5/12 * (99 + 47 * Math.sqrt(5))*rx)/rx;
