   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_22.V.value;
      s =  Math.pow(V/(1 * 5/12 * (3 + Math.sqrt(5))),1/3);
      uf_22.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_22.A.value;
      s =  Math.pow(A/(1 * 20 / 4 * Math.sqrt(3)),1/2);
      uf_22.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_22.ri.value;
      s =  ri/(1/ 12 * (3 * Math.sqrt(3) + Math.sqrt(15)));
      uf_22.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_22.ro.value;
      s =  ro/(1/ 4 * Math.sqrt(10 + 2 * Math.sqrt(5)));
      uf_22.s.value = Math.round(s*rx)/rx;
   }
   uf_22.V.value = Math.round( Math.pow(s,3) * 5/12 * (3 + Math.sqrt(5))*rx)/rx;
   uf_22.A.value = Math.round( Math.pow(s,2) * 20 / 4 * Math.sqrt(3)*rx)/rx;
   uf_22.ri.value = Math.round( s / 12 * (3 * Math.sqrt(3) + Math.sqrt(15))*rx)/rx;
   uf_22.ro.value = Math.round( s / 4 * Math.sqrt(10 + 2 * Math.sqrt(5))*rx)/rx;
   uf_22.ra.value = Math.round((uf_22.ro.value * 1+uf_22.ri.value * 1)/2*rx)/rx;
}
calc_uf_22(0);
</script>
<p>
One variant of a <b><a href="Geodesic_Dome_Notes">geodesic dome</a></b> can be derived from the Icosahedron.
<p>
<a name="Dodecahedron"></a>
<h3 class=header>Dodecahedron</h3>
<table class=fotoborder hspace=16 vspace=4 width=200 align=right><tr><td align=center><a href="/Present/Data/Uniform_Polyhedra/23.jpg" rel=lightbox title="Dodecahedron" subtitle="" ><img width=200 height=200 src="/view/Data/Uniform_Polyhedra/23.jpg" class=foto ></a><br> <a href="/ImageViewer?img=Data/Uniform_Polyhedra/23.jpg"><img align=right vspace=2 hspace=4 src=/imgs/blowup.png border=0></a><font size=1>Dodecahedron</font></td></tr></table>
<ul><li>Uniform Polyhedron: U23
<li>Platonic Solid
<li>Platonic Element: Ether
<li>Vertices: 20
<li>Edges: 30
<li>Faces: 12
<li>Wythoff symbol: 3|2 5
<li>Symmetry Group: icosahedral
<li>Vertex Configuration: &#123;5, 5, 5}
<li>Dual: icosahedron
<li>V: s<sup>3</sup> / 4 * (15 + 7 * &#8730;5)
<li>A: s<sup>2</sup> * 12 / 4 * &#8730;(25 + 10 * &#8730;5)
<li>r<sub>inner</sub>: s / 20 * &#8730;(250 + 110 * &#8730;5)
<li>r<sub>outer</sub>: s / 4 * (&#8730;15 + &#8730;3)
<li><img border=0 src=Data/Uniform_Polyhedra/23net.png>
</ul><p>
<br clear=both>
<form name=uf_23>
s = <input class=in name=s value=1.00 size=5 onchange="calc_uf_23(0)">,
V = <input class=in name=V value="" size=6 onchange="calc_uf_23(1)">,
A = <input class=in name=A value="" size=6 onchange="calc_uf_23(2)">,
r<sub>inner</sub> = <input class=in name=ri value="" size=5 onchange="calc_uf_23(3)">,
r<sub>outer</sub> = <input class=in name=ro value="" size=5 onchange="calc_uf_23(4)">,
r<sub>avg</sub> = <input class=res readonly name=ra value="" size=5 onchange="calc_uf_23(5)">
</form>
<script lang=JavaScript>
function calc_uf_23(x) {
   var s = uf_23.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_23.V.value;
      s =  Math.pow(V/(1 / 4 * (15 + 7 * Math.sqrt(5))),1/3);
      uf_23.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_23.A.value;
      s =  Math.pow(A/(1 * 12 / 4 * Math.sqrt(25 + 10 * Math.sqrt(5))),1/2);
      uf_23.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_23.ri.value;
      s =  ri/(1/ 20 * Math.sqrt(250 + 110 * Math.sqrt(5)));
      uf_23.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_23.ro.value;
      s =  ro/(1/ 4 * (Math.sqrt(15) + Math.sqrt(3)));
      uf_23.s.value = Math.round(s*rx)/rx;
   }
   uf_23.V.value = Math.round( Math.pow(s,3) / 4 * (15 + 7 * Math.sqrt(5))*rx)/rx;
   uf_23.A.value = Math.round( Math.pow(s,2) * 12 / 4 * Math.sqrt(25 + 10 * Math.sqrt(5))*rx)/rx;
   uf_23.ri.value = Math.round( s / 20 * Math.sqrt(250 + 110 * Math.sqrt(5))*rx)/rx;
   uf_23.ro.value = Math.round( s / 4 * (Math.sqrt(15) + Math.sqrt(3))*rx)/rx;
   uf_23.ra.value = Math.round((uf_23.ro.value * 1+uf_23.ri.value * 1)/2*rx)/rx;
}
calc_uf_23(0);
</script>
<p>
<a name="Icosidodecahedron"></a>
<h3 class=header>Icosidodecahedron</h3>
<table class=fotoborder hspace=16 vspace=4 width=200 align=right><tr><td align=center><a href="/Present/Data/Uniform_Polyhedra/24.jpg" rel=lightbox title="Icosidodecahedron" subtitle="" ><img width=200 height=200 src="/view/Data/Uniform_Polyhedra/24.jpg" class=foto ></a><br> <a href="/ImageViewer?img=Data/Uniform_Polyhedra/24.jpg"><img align=right vspace=2 hspace=4 src=/imgs/blowup.png border=0></a><font size=1>Icosidodecahedron</font></td></tr></table>
<ul><li>Uniform Polyhedron: U24
<li>Archimedean Solid: A4
<li>Vertices: 30
<li>Edges: 60
<li>Faces: 32
<li>Wythoff symbol: 2|3 5
<li>Symmetry Group: icosahedral
<li>Vertex Configuration: &#123;3, 5, 3, 5}
<li>Dual: rhombic triacontahedron
<li>V: s<sup>3</sup> / 6 * (45 + 17 * &#8730;5)
<li>A: s<sup>2</sup> * (5 * &#8730;3 + 3 * &#8730;5 * &#8730;(5 + 2 * &#8730;5))
<li>r<sub>inner</sub>: s / 8 * (5 + 3 * &#8730;5)
<li>r<sub>outer</sub>: s * (1 + &#8730;5) / 2
<li><img border=0 src=Data/Uniform_Polyhedra/24net.png>
</ul><p>
<br clear=both>
<form name=uf_24>
s = <input class=in name=s value=1.00 size=5 onchange="calc_uf_24(0)">,
V = <input class=in name=V value="" size=6 onchange="calc_uf_24(1)">,
A = <input class=in name=A value="" size=6 onchange="calc_uf_24(2)">,
r<sub>inner</sub> = <input class=in name=ri value="" size=5 onchange="calc_uf_24(3)">,
r<sub>outer</sub> = <input class=in name=ro value="" size=5 onchange="calc_uf_24(4)">,
r<sub>avg</sub> = <input class=res readonly name=ra value="" size=5 onchange="calc_uf_24(5)">
</form>
<script lang=JavaScript>
function calc_uf_24(x) {
   var s = uf_24.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_24.V.value;
      s =  Math.pow(V/(1 / 6 * (45 + 17 * Math.sqrt(5))),1/3);
      uf_24.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_24.A.value;
      s =  Math.pow(A/(1 * (5 * Math.sqrt(3) + 3 * Math.sqrt(5) * Math.sqrt(5 + 2 * Math.sqrt(5)))),1/2);
      uf_24.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_24.ri.value;
      s =  ri/(1/ 8 * (5 + 3 * Math.sqrt(5)));
      uf_24.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_24.ro.value;
      s =  ro/( (1 + Math.sqrt(5)) / 2);
      uf_24.s.value = Math.round(s*rx)/rx;
   }
   uf_24.V.value = Math.round( Math.pow(s,3) / 6 * (45 + 17 * Math.sqrt(5))*rx)/rx;
   uf_24.A.value = Math.round( Math.pow(s,2) * (5 * Math.sqrt(3) + 3 * Math.sqrt(5) * Math.sqrt(5 + 2 * Math.sqrt(5)))*rx)/rx;
   uf_24.ri.value = Math.round( s / 8 * (5 + 3 * Math.sqrt(5))*rx)/rx;
   uf_24.ro.value = Math.round( s * (1 + Math.sqrt(5)) / 2*rx)/rx;
   uf_24.ra.value = Math.round((uf_24.ro.value * 1+uf_24.ri.value * 1)/2*rx)/rx;
}
calc_uf_24(0);
</script>
