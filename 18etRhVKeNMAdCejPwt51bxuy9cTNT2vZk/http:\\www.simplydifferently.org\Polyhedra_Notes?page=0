r<sub>inner</sub> = <input class=in name=ri value="" size=5 onchange="calc_uf_28(3)">,
r<sub>outer</sub> = <input class=in name=ro value="" size=5 onchange="calc_uf_28(4)">,
r<sub>avg</sub> = <input class=res readonly name=ra value="" size=5 onchange="calc_uf_28(5)">
</form>
<script lang=JavaScript>
function calc_uf_28(x) {
   var s = uf_28.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_28.V.value;
      s =  Math.pow(V/(1 * (95 + 50 * Math.sqrt(5))),1/3);
      uf_28.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_28.A.value;
      s =  Math.pow(A/(1 * 30 * (1 + Math.sqrt(2 * (4 + Math.sqrt(5) + Math.sqrt(15 + 6 * Math.sqrt(6)))))),1/2);
      uf_28.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_28.ri.value;
      s =  ri/( 1/241 * (105 + 6 * Math.sqrt(5) * Math.sqrt(31 + 12 * Math.sqrt(5))));
      uf_28.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_28.ro.value;
      s =  ro/(1/ 2 * Math.sqrt(31 + 12 * Math.sqrt(5)));
      uf_28.s.value = Math.round(s*rx)/rx;
   }
   uf_28.V.value = Math.round( Math.pow(s,3) * (95 + 50 * Math.sqrt(5))*rx)/rx;
   uf_28.A.value = Math.round( Math.pow(s,2) * 30 * (1 + Math.sqrt(2 * (4 + Math.sqrt(5) + Math.sqrt(15 + 6 * Math.sqrt(6)))))*rx)/rx;
   uf_28.ri.value = Math.round( s * 1/241 * (105 + 6 * Math.sqrt(5) * Math.sqrt(31 + 12 * Math.sqrt(5)))*rx)/rx;
   uf_28.ro.value = Math.round( s / 2 * Math.sqrt(31 + 12 * Math.sqrt(5))*rx)/rx;
   uf_28.ra.value = Math.round((uf_28.ro.value * 1+uf_28.ri.value * 1)/2*rx)/rx;
}
calc_uf_28(0);
</script>
<p>
<a name="Snub Dodecahedron"></a>
<h3 class=header>Snub Dodecahedron</h3>
<table class=fotoborder hspace=16 vspace=4 width=200 align=right><tr><td align=center><a href="/Present/Data/Uniform_Polyhedra/29.jpg" rel=lightbox title="Snub Dodecahedron" subtitle="" ><img width=200 height=200 src="/view/Data/Uniform_Polyhedra/29.jpg" class=foto ></a><br> <a href="/ImageViewer?img=Data/Uniform_Polyhedra/29.jpg"><img align=right vspace=2 hspace=4 src=/imgs/blowup.png border=0></a><font size=1>Snub Dodecahedron</font></td></tr></table>
<ul><li>Uniform Polyhedron: U29
<li>Archimedean Solid: A8
<li>Vertices: 60
<li>Edges: 150
<li>Faces: 92
<li>Wythoff symbol: |2 3 5
<li>Symmetry Group: icosahedral
<li>Vertex Configuration: &#123;3, 3, 3, 3, 5}
<li>Dual: pentagonal hexecontahedron
<li>V: s<sup>3</sup> * 3.7543
<li>A: s<sup>2</sup> * &#8730;(15 * (95 + 6 * &#8730;5 + 8 * &#8730;(15 * (5 + 2 * &#8730;5))))
<li>r<sub>inner</sub>: s * 2.03987315
<li>r<sub>outer</sub>: s * 2.15583737
<li><img border=0 src=Data/Uniform_Polyhedra/29net.png>
</ul><p>
<br clear=both>
<form name=uf_29>
s = <input class=in name=s value=1.00 size=5 onchange="calc_uf_29(0)">,
V = <input class=in name=V value="" size=6 onchange="calc_uf_29(1)">,
A = <input class=in name=A value="" size=6 onchange="calc_uf_29(2)">,
r<sub>inner</sub> = <input class=in name=ri value="" size=5 onchange="calc_uf_29(3)">,
r<sub>outer</sub> = <input class=in name=ro value="" size=5 onchange="calc_uf_29(4)">,
r<sub>avg</sub> = <input class=res readonly name=ra value="" size=5 onchange="calc_uf_29(5)">
</form>
<script lang=JavaScript>
function calc_uf_29(x) {
   var s = uf_29.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_29.V.value;
      s =  Math.pow(V/(1 * 3.7543),1/3);
      uf_29.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_29.A.value;
      s =  Math.pow(A/(1 * Math.sqrt(15 * (95 + 6 * Math.sqrt(5) + 8 * Math.sqrt(15 * (5 + 2 * Math.sqrt(5)))))),1/2);
      uf_29.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_29.ri.value;
      s =  ri/( 2.03987315);
      uf_29.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_29.ro.value;
      s =  ro/( 2.15583737);
      uf_29.s.value = Math.round(s*rx)/rx;
   }
   uf_29.V.value = Math.round( Math.pow(s,3) * 3.7543*rx)/rx;
   uf_29.A.value = Math.round( Math.pow(s,2) * Math.sqrt(15 * (95 + 6 * Math.sqrt(5) + 8 * Math.sqrt(15 * (5 + 2 * Math.sqrt(5)))))*rx)/rx;
   uf_29.ri.value = Math.round( s * 2.03987315*rx)/rx;
   uf_29.ro.value = Math.round( s * 2.15583737*rx)/rx;
   uf_29.ra.value = Math.round((uf_29.ro.value * 1+uf_29.ri.value * 1)/2*rx)/rx;
}
calc_uf_29(0);
</script>
<p>
<p>
(Note: the volume for this solid is only available numerically, not symbolical - if you can provide me with the expression for V let me know).
<p>
