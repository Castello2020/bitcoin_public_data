function calc_uf_25(x) {
   var s = uf_25.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_25.V.value;
      s =  Math.pow(V/(1 / 4 * (125 + 43 * Math.sqrt(5))),1/3);
      uf_25.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_25.A.value;
      s =  Math.pow(A/(1 * 3 * (10 * Math.sqrt(3) + Math.sqrt(5) * Math.sqrt(5 + 2 * Math.sqrt(5)))),1/2);
      uf_25.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_25.ri.value;
      s =  ri/( 9/872 * (21 + Math.sqrt(5)) * Math.sqrt(58 + 18 * Math.sqrt(5)));
      uf_25.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_25.ro.value;
      s =  ro/(1/ 4 * Math.sqrt(58 + 18 * Math.sqrt(5)));
      uf_25.s.value = Math.round(s*rx)/rx;
   }
   uf_25.V.value = Math.round( Math.pow(s,3) / 4 * (125 + 43 * Math.sqrt(5))*rx)/rx;
   uf_25.A.value = Math.round( Math.pow(s,2) * 3 * (10 * Math.sqrt(3) + Math.sqrt(5) * Math.sqrt(5 + 2 * Math.sqrt(5)))*rx)/rx;
   uf_25.ri.value = Math.round( s * 9/872 * (21 + Math.sqrt(5)) * Math.sqrt(58 + 18 * Math.sqrt(5))*rx)/rx;
   uf_25.ro.value = Math.round( s / 4 * Math.sqrt(58 + 18 * Math.sqrt(5))*rx)/rx;
   uf_25.ra.value = Math.round((uf_25.ro.value * 1+uf_25.ri.value * 1)/2*rx)/rx;
}
