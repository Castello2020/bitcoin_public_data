function calc_uf_27(x) {
   var s = uf_27.s.value;
   var t = 1.83929;
   var rx = 100;
   if(x==0) {
      ;
   } else if(x==1) {
      V = uf_27.V.value;
      s =  Math.pow(V/(1 / 3 * (60 + 29 * Math.sqrt(5))),1/3);
      uf_27.s.value = Math.round(s*rx)/rx;
   } else if(x==2) {
      A = uf_27.A.value;
      s =  Math.pow(A/(1 * (30 + Math.sqrt(30 * (10 + 3 * Math.sqrt(5) + Math.sqrt(15 * (5 + 2 * Math.sqrt(5))))))),1/2);
      uf_27.s.value = Math.round(s*rx)/rx;
   } else if(x==3) {
      ri = uf_27.ri.value;
      s =  ri/(1/ 41 * (15 + 2 *Math.sqrt(5)) * Math.sqrt(11 + 4 * Math.sqrt(5)));
      uf_27.s.value = Math.round(s*rx)/rx;
   } else if(x==4) {
      ro = uf_27.ro.value;
      s =  ro/(1/ 2 * Math.sqrt(11 + 4 * Math.sqrt(5)));
      uf_27.s.value = Math.round(s*rx)/rx;
   }
   uf_27.V.value = Math.round( Math.pow(s,3) / 3 * (60 + 29 * Math.sqrt(5))*rx)/rx;
   uf_27.A.value = Math.round( Math.pow(s,2) * (30 + Math.sqrt(30 * (10 + 3 * Math.sqrt(5) + Math.sqrt(15 * (5 + 2 * Math.sqrt(5))))))*rx)/rx;
   uf_27.ri.value = Math.round( s / 41 * (15 + 2 *Math.sqrt(5)) * Math.sqrt(11 + 4 * Math.sqrt(5))*rx)/rx;
   uf_27.ro.value = Math.round( s / 2 * Math.sqrt(11 + 4 * Math.sqrt(5))*rx)/rx;
   uf_27.ra.value = Math.round((uf_27.ro.value * 1+uf_27.ri.value * 1)/2*rx)/rx;
}
