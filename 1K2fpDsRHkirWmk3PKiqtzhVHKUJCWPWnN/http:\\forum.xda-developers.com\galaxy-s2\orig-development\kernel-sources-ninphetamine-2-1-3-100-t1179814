	<td class="alt1 postContent" id="td_post_15870000">
	
		
	<!-- message, attachments, sig -->

		

		<!-- message -->
		<div id="post_message_15870000">



			


<div style="margin: 0 0 15px 15px; float: right">


<!-- 300x250 ITF -->
<div id='div-gpt-ad-2864076999006-7' style='width:300px; height:250px;'>

<script type='text/javascript'>
  var sidebar_cookie = jQuery.cookie('xda_sidebar_visible');
  //console.log('Window Width: '+ jQuery(window).width());
  //if (jQuery(window).width() < 1247 || sidebar_cookie == 'false') {
googletag.cmd.push(function() { googletag.display('div-gpt-ad-2864076999006-7'); });
  //}
</script>
