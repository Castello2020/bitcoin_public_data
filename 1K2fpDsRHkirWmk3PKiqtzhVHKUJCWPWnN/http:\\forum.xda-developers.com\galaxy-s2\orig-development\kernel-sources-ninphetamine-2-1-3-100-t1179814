<script type="text/javascript">
jQuery(document).ready(function($)
{

	jQuery('#show_login').click(function(e)
	{
		jQuery('#xda_header_login, #xda_header_google_login').show();
		jQuery(this).hide();
		e.preventDefault();
	});

	jQuery('.google-login').click(function(e)
	{
		e.preventDefault();
		var url = $(this).attr('href');
		
           	var login_window = window.open(url, '1367260649051', 'width=700,height=500,toolbar=0,menubar=0,location=0,status=1,scrollbars=1,resizable=1,left=800,top=300');

            var timer = setInterval(function()
            {
                if (login_window.closed)
                {
                    clearInterval(timer);
                    window.location = '/?nocache=1';
                }
            }, 1000);		
	});	
});
</script>
