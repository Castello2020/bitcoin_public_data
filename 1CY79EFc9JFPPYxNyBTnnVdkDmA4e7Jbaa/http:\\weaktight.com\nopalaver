<script type="text/javascript" language="javascript">
var panel_five;
YAHOO.example.panels = function(){
    panel_five = new YAHOO.widget.Panel("panel_five", 
        { 
            close:true,  
            visible:false,  
            draggable:false,
            modal:true,
            fixedcenter:true,
            effect:{effect:YAHOO.widget.ContainerEffect.FADE, duration: 0.3}
        } 
    ); 
    panel_five.render();
};
YAHOO.util.Event.addListener(window,'load',YAHOO.example.panels);

function cb_FLAG_responseSuccess(x)
{
    if(x.responseText !== undefined)
    {
        o = document.getElementById("panel_contents");
        if (o) o.innerHTML=x.responseText;
                
        o = document.getElementById("status_div");
        if (o) o.style.visibility="hidden";
        
        panel_five.show();
    }
}

function cb_EMAIL_responseSuccess(x)
{
    if(x.responseText !== undefined)
    {
        //o = document.getElementById("userposts");
        //if (o) o.innerHTML=x.responseText;
        if (x.responseText.indexOf("SENT") != -1)
            alert("Email Sent.");
        else
            alert("Error sending Email.");
                
        o = document.getElementById("status_div");
        if (o) o.style.visibility="hidden";
        
    }
}

function cb_CENTER_responseSuccess(x)
{
    if(x.responseText !== undefined)
    {
        o = document.getElementById("contentcenter");
        if (o) o.innerHTML=x.responseText;
                
        o = document.getElementById("status_div");
        if (o) o.style.visibility="hidden";
    }
}

function cb_PROFILE_responseSuccess(x)
{
    if(x.responseText !== undefined)
    {
        o = document.getElementById("userposts");
        if (o) o.innerHTML=x.responseText;
                
        o = document.getElementById("status_div");
        if (o) o.style.visibility="hidden";
    }
}

function cb_PROFILE_responseFailure(o)
{
    // Access the response object's properties in the
    // same manner as listed in responseSuccess( ).
    // Please see the Failure Case section and
    // Communication Error sub-section for more details on the
    // response object's properties.
    
    if(o.responseText !== undefined)
    {
        alert("Error: Response Failure - " + o.statusText);
    }
    else
        alert("Error: Response Failure");
}

function PROFILE_pickFlag()
{
    var callback = { success:cb_FLAG_responseSuccess, failure:cb_PROFILE_responseFailure }
    var postData = "pickflag=1&user_id=34962"; 
        
    var transaction = YAHOO.util.Connect.asyncRequest('POST', "profile/p_async_posts.php", callback, postData);
    o = document.getElementById("status_div");
    if (o) o.style.visibility="visible";
}

function PROFILE_comboUpdate()
{
    var o = document.getElementById("post_filter");
    if (o)
    {
        var value = o.options[o.selectedIndex].value;
        if (value!=0)
        {
            o = document.getElementById("status_div");
            if (o) o.style.visibility="visible";
            var callback = { success:cb_PROFILE_responseSuccess, failure:cb_PROFILE_responseFailure }
            var postData = "update=1&value="+value+"&user_id=34962"; 
                
            var transaction = YAHOO.util.Connect.asyncRequest('POST', "profile/p_async_posts.php", callback, postData);
        }
    }
}

function PROFILE_updatePage(x)
{
    var o = document.getElementById("post_filter");
    if (o)
    {
        var value = o.options[o.selectedIndex].value;
        if (value!=0)
        {
            o = document.getElementById("status_div");
            if (o) o.style.visibility="visible";
            var callback = { success:cb_PROFILE_responseSuccess, failure:cb_PROFILE_responseFailure }
            var postData = "update=1&page="+x+"&value="+value+"&user_id=34962"; 
                
            var transaction = YAHOO.util.Connect.asyncRequest('POST', "profile/p_async_posts.php", callback, postData);
        }
    }
}

function PROFILE_verifyEmail()
{
    var o = document.getElementById("e_verify");
    if (o)
        o.disabled=1;
    var callback = { success:cb_EMAIL_responseSuccess, failure:cb_PROFILE_responseFailure }
    var postData = "emailverify=1&user_id=34962"; 
        
    var transaction = YAHOO.util.Connect.asyncRequest('POST', "profile/p_async_posts.php", callback, postData);
}

function PROFILE_updateEmail()
{
    var callback =
    {
      success:cb_updateDiv,
      failure:cb_responseFailure,
      argument: "u_email"
    };
    
    var o = document.getElementById("status_div");
    if (o) o.style.visibility="visible";

    var formObject = document.getElementById("optform");
    if (formObject)
        YAHOO.util.Connect.setForm(formObject);
    
    
    var postData = "updateemail=1&user_id=-1";
    var transaction = YAHOO.util.Connect.asyncRequest("POST", "profile/p_async_posts.php", callback, postData);
}

function PROFILE_updateChecks()
{
    var callback =
    {
      success:cb_updateDiv,
      failure:cb_responseFailure,
      argument: "u_email"
    };
    
    var o = document.getElementById("status_div");
    if (o) o.style.visibility="visible";

    var formObject = document.getElementById("optform");
    if (formObject)
        YAHOO.util.Connect.setForm(formObject);
    
    var postData = "updatechecks=1&user_id=-1";
    var transaction = YAHOO.util.Connect.asyncRequest("POST", "profile/p_async_posts.php", callback, postData);

}

