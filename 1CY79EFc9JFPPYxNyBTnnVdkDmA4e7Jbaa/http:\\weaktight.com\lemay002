        function TABS_init()
        {
            HANDPOST_tabView = new YAHOO.widget.TabView('wttabctrl' );
            var tabhandler = function(e) {
                var formObject = document.getElementById("pgform");

                if (formObject)
                {
                    // remve newlines

                    if (formObject.readtext)
                    {
                        var str = formObject.readtext.value;

                        var str1 = str.replace(/\n/g, " ");
                        var str2 = str1.replace(/\r/g, " ");
                        formObject.readtext.value = str2;
                    }


                    YAHOO.util.Connect.setForm(formObject);
                }
            };
        
            var tab0 = HANDPOST_tabView.getTab(0);
            tab0.set('dataSrc','');
            tab0.set('cacheData',false);
            tt0 = new YAHOO.widget.Tooltip('tt0',{context:'posts_lk',text:'posts'});
            tab0.addListener('beforeActiveChange', tabhandler);
            
            
            var tab1 = HANDPOST_tabView.getTab(1);
            tab1.set('dataSrc','/profile/p_async_posts.php?listfriends=1&user_id=47827&owner=0');
            tab1.set('cacheData',false);
            tt1 = new YAHOO.widget.Tooltip('tt1',{context:'friends_lk',text:'list of friends'});
            tab1.addListener('beforeActiveChange', tabhandler);
            
            
            var tab2 = HANDPOST_tabView.getTab(2);
            tab2.set('dataSrc','/profile/p_async_posts.php?listgroups=1&user_id=47827&owner=0');
            tab2.set('cacheData',false);
            tt2 = new YAHOO.widget.Tooltip('tt2',{context:'groups_lk',text:'list of groups'});
            tab2.addListener('beforeActiveChange', tabhandler);
            
            
        }

        function TABS_select(name)
        {
            var tabs = HANDPOST_tabView.get('tabs');

            for (var i = 0; i < tabs.length; i++) {
                var t = tabs[i].get('label');
                if (tabs[i].get('label') == name) {
                    HANDPOST_tabView.set('activeIndex', i);
                    break;
                }
            }
        }
        
    </script>						
