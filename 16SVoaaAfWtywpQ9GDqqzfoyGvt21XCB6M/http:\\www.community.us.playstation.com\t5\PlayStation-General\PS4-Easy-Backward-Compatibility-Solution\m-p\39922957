<script>

function ClickOmniTrack(obj,eventVal,secName){ 
 secName = secName+":"+pageNameValHeader ;
  s.events=eventVal;
  s.linkTrackEvents=eventVal;   
  s.prop49 = secName;
  s.eVar49 = secName ;
  s.linkTrackVars='events,prop49,eVar49';
  s.pageName= secName;
  s.tl(obj,'o',secName);  
  s.linkTrackVars="None"
  s.linkTrackEvents="None"
}

var _gaq = _gaq || [];
_gaq.push(['_setAccount', 'UA-9546650-22']);
_gaq.push(['_trackPageview']);

(function() {
var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();

function pageTrackerClass() {
this._trackPageview = function(val) {
_gaq.push(['_trackPageview', val]);
}
this._trackEventThreeArg = function(type, state, val) {
_gaq.push(['_trackEvent', type, state, val]);
}
this._trackEvent = function(type, val) {
_gaq.push(['_trackEvent', type, val]);
}
}
var pageTracker = new pageTrackerClass();


</script>	

