<script language="JavaScript" type="text/javascript" src="http://cdn.us.playstation.com/pscomauth/groups/public/documents/webasset/community_secured_s_code.js"></script>
<script language="JavaScript" type="text/javascript"><!--
		var pageNameValHeader="";
   	    var szURL = document.URL;
		var titleURL = document.title;
		szURL = szURL.replace(/\/t5\//gi,'/');
		var ind = szURL.indexOf("=");            
        var splitPart = "";		 
		var indexforNotfound=titleURL.indexOf("Page Not Found"); 
		var indexforNotfoundN=titleURL.indexOf("Node was not Found"); 
		var indexOfQues = szURL.indexOf("?"); 	
        var indexforSearch = szURL.indexOf("/search/"); 
		var indexforgameList = szURL.indexOf("?type="); 
		var indexforcampaignSr = szURL.indexOf("?SR=");
		var indexforcampaignRef = szURL.indexOf("?ref=");
		var indexforcampaignEmc = szURL.indexOf("ATTR=PSU");
		var indexforonlinename = szURL.indexOf("?onlinename=");
		var indexforreturnUrl = szURL.indexOf("?returnURL=");
		var indexofsitemapPageName=szURL.indexOf("sitemap");
		var indexofsitemapCompainCMP=szURL.indexOf("?CMP");
		var indexofConsoletype=szURL.indexOf("?console");
		var indexofSortOrder=szURL.indexOf("?sortOrder");
        var indexforcampaignEmcpsu=szURL.indexOf("/?psu=");
		var indexforEmailcampaign=szURL.indexOf("?EMC=");
		var cmpUrl = szURL ;
		var emcUrl = szURL ;
		var doubleSlash="";
	    if(ind != -1){
            var indPlus1 = ind + 1;  
            var indPlus3 = szURL.length;       
            splitPart = szURL.substring(indPlus1,indPlus3);
			         } 


		 if(indexOfQues != -1){
		 var toBReplaced = szURL.substring(indexOfQues,szURL.length);
		 szURL = szURL.replace(toBReplaced, "");	
			 
		 }

		componentList = szURL.split('/');	
		var reqString = "";
		for(var j=componentList.length-3; j>0; j--) {
		if(reqString =="") {
		reqString = componentList[componentList.length-j];
		}
		else {
		reqString = reqString+"/"+componentList[componentList.length-j];
		}		
		}		
		var newList = reqString.split("/");		
		for(var i=newList.length; i>0; i--) {		
		}		
		var sectionName = "";
		var sectionProp6 = "";
		var sectionProp7 = "";
		var sectionProp8 = "";
		var sectionProp9 = "";
		var sectionProp56 = "";
		var pageNameVal = "";
		var pageType = "";
		var sectionProp21 = "";		
		if(newList.length !=null && newList.length >0 ){
			sectionName = newList[0];	
			sectionName=sectionName.toLowerCase();		
			
			if(newList[newList.length-1]=="")
			{
				newList.pop();
			}
			if(newList[0] !=null){			
				sectionProp6 = newList[0];
				sectionProp6=sectionProp6.toLowerCase();
				pageNameVal = sectionProp6 ;	
		
			}
			if(newList[1] !=null){			
				sectionProp7 = newList[1];
				sectionProp7=sectionProp7.toLowerCase();
				pageNameVal = sectionName+":"+sectionProp7 ;	
			
			}
				if(newList[2] !=null){
					sectionProp8 = newList[2];
					sectionProp8=sectionProp8.toLowerCase();
					pageNameVal = pageNameVal+":"+sectionProp8 ;					
				}
					if(newList[3] !=null){
						sectionProp9 = newList[3];
						sectionProp9=sectionProp9.toLowerCase(); 
						pageNameVal = pageNameVal+":"+sectionProp9 ;	 		
					}	
					
					if(newList[4] !=null){
						var temp = "";
						sectionProp56 = newList[4].toLowerCase();
						for (i=5 ; i<newList.length; i++ ){
						temp = newList[i].toLowerCase();
						sectionProp56 = sectionProp56 + ":" + temp 
						}
						pageNameVal = pageNameVal+":"+sectionProp56 ;	 		
					}		
		}	 
			if(pageNameVal !=null){
				if(pageNameVal !=""){
					pageNameVal = "pdc:forums:"+pageNameVal;
				}
				else {
					pageNameVal = "pdc:forums";
				}	
			}
			else {
				pageNameVal = "pdc:forums";
			}			
						
			if(sectionProp7=="INDEX.HTM" || sectionName=="INDEX.HTML"){
				sectionName="pdc:forums";				
			}			
		

            pageNameVal=pageNameVal.replace("/INDEX.HTM", "");   
            if(pageNameVal.indexOf("#")!=-1){     
            pageNameVal=pageNameVal.substring(0,pageNameVal.indexOf("#"));          
            }else{
              pageNameVal=pageNameVal.replace("#", "");   
            }                                                      
            
            if(splitPart == null || splitPart == ""){
            pageNameVal = pageNameVal; 
                      
            }  
            else {                       		
            if(indexforSearch != -1 || indexforgameList!=-1 || indexforcampaignSr!=-1 || indexforcampaignRef!=-1 || indexforcampaignEmc!=-1 || indexforonlinename!=-1 || indexforreturnUrl!=-1 || indexforredirectreason!=-1 || indexforfeature!=-1 || indexforfeatureid!=-1 || indexforvideoid!=-1 || indexformeetjoemour!=-1 || indexformlbshow!=-1 || indexofsitemapCompainCMP!=-1 || indexofConsoletype!=-1 || indexofSortOrder!=-1)              
            pageNameVal = pageNameVal;            
            else             
            pageNameVal = pageNameVal+"/"+splitPart;  
            } 
                      
           if(pageNameVal.charAt(pageNameVal.length-1)=="/"){
                  pageNameVal=pageNameVal.substring(0,pageNameVal.length-1); 
                 }              						 
               doubleSlash=pageNameVal.indexOf("//"); 			 
            if(doubleSlash!= -1){
                 pageNameVal = pageNameVal.replace("//","/"); 	
             }   
            pageNameValHeader=pageNameVal.replace("#", "");
			

			
/*new code end*/
	function setEventVars(pageName) {
	var pEvents = new Array();// creating an array
	if(map[pageName] != undefined) // page specific variables exists
		pEvents = map[pageName]['events'].split(',');// split the variable seperated by comma and store them in the pEvents array
		
	var events = map['all']['events'].split(',').concat(pEvents); // add page specifc vars to those common to all
	var sEvents = "";
	var eventsSeparator = "";
	var productsSeparator = "";
	var sProducts = ";;;;";
	var event = null;
	
	for (var i in events) {
		if(events[i] != '' && events.hasOwnProperty(i)) {
			// events is an array, not object
			event = events[i];
			sEvents += eventsSeparator + event; // s.events = 'event90,event92';
			
			if(events_map[event] != null && events_map[event] != '') {
				// If events are just numerics, don't add to s.products
				
				productsSeparator = "|";
				}
			eventsSeparator = ",";
			}
		}

	s.events = sEvents;
	
	}

function setVarsFromJSON(pageName) {
	var all = map['all']; // variables common to all pages
	for(var key in all) { // set variables common to all pages
		if(key != null && all.hasOwnProperty(key) && key != 'events') {
			s[key] = all[key]; //s.prop32, s.eVar33 etc
			}
		}
	
	var custom = map[pageName]; // page specific variables
	for(var customKey in custom) { // set page specific variables
		if(customKey != null && custom.hasOwnProperty(customKey) && customKey != 'events') {
			s[customKey] = custom[customKey]; //s.prop32, s.eVar33 etc
			}
		}
	setEventVars(pageName); // separate function to populate event vars as some  might be numeric events
	}
	
if(indexforNotfound != -1 || indexforNotfoundN != -1){
	s.pageName="pdc:forums:pagenotfound";
	s.pageType="errorpage";
	s.eVar16=s.prop16="pagenotfound";
	s.eVar68=s.prop68=document.URL;
	}
	else
	{
//s.pageName = window.location.href; // page url as page name   //document.getElementsByTagName('Title')[0].innerHTML; //page title
pageName=pageNameValHeader;
s.pageName=pageName;
s.channel="pdc:forums:"+sectionName;
