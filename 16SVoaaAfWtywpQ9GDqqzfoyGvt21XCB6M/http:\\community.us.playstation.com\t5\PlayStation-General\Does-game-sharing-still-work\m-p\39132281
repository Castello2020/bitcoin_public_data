<script language="JavaScript" type="text/javascript">
	var deviceType;
	if( navigator.userAgent.match(/Android/i)){
	deviceType="mobile";
	}else if (navigator.userAgent.match(/webOS/i)){
		deviceType="mobile";
	}else if (navigator.userAgent.match(/iPhone/i)){
		deviceType="mobile";
	}else if (navigator.userAgent.match(/iPad/i)){
		deviceType="mobile";
	}else if (navigator.userAgent.match(/BlackBerry/i)){
		deviceType="mobile";
	}else if (navigator.userAgent.match(/PLAYSTATION 3/i)){
		deviceType="ps3";
	}else if (navigator.userAgent.match(/PLAYSTATION 4/i)){
		deviceType="ps4";
	}else if (navigator.userAgent.match(/Playstation Vita/i)){
		deviceType="psvita";
	}else{
		deviceType="pc";
	}
 
	
	// Prepare breadcrumbs in 'omnituredev / Discuss Genesis Topics / Re: Did I mention that Genesis is way RAD?' format
	var breadcrumbArray = LITHIUM.CommunityJsonObject.WebTracking.path.split('/');// for ssplitting the web track path // use F12 to check them out
	//Eg> Community: omnituredev/Board: Discuss Genesis Topics/Message: Re: Did I mention that Genesis is way RAD?
	/*** CAUTION: 
		WebTracking.path is not expected to have '/' characters other than those delimiting breadcrumb items
	***/
	var newBreadcrumbArray = [];
	var channelcrumb = LITHIUM.CommunityJsonObject.WebTracking.path.split('/');
	
	
	for(var i in breadcrumbArray){
		if(breadcrumbArray.hasOwnProperty(i)) {
			var crumb = breadcrumbArray[i];// for storing the splits from the bread crumb array
			var idx = crumb.indexOf(':') + 1;//  taking the charater number after ':' and adding 1 more space to it
			newBreadcrumbArray[i] = crumb.substring(idx);// taking the part of the string after ':' sign 
			}
		}
	
	// leaf crumb in format 'Message:Re: Did I mention that Genesis is way RAD?'
	var leafCrumb = breadcrumbArray[breadcrumbArray.length - 1];// takes the last msg part , the terminal one 
	var pageurl = window.location.href;
	// Maps page types with variables to be captured and their corresponding values
	// creating the map between the pages and their respective events or props or evar ; all page helps in reducing reduncy
	var map = {
		'all': {  
			//'prop32'	:	newBreadcrumbArray.join(' / '), // Content Hierarchy // join the crumb with the '/' sign in between , it i sopposite of split operation
		//	'prop33'	:	leafCrumb, // Content Detail
			'prop3'	:	LITHIUM.CommunityJsonObject.User.login, // Visitor Id
			'eVar3'	:	LITHIUM.CommunityJsonObject.User.login, // Visitor Id
			'events':	'event1'}, // Page View
		'SearchPage': {
			 // Msg Board Search Term
			'events':	'event95'}, // Msg Board Searches
		'ForumTopicPage': {
			'prop36':	'39132281', // Message Id
			'eVar33' :	'39132281', // Message Id
			'events':	'event90,event91,event97'}, // Sum of Rating Value, Read Msg, Sum of LifeSpan
		'ReplyPage': {
			'events':	'event94'}, // Reply to Msg Form View
		'EditPage': {
			'events':	'event93'}, // Edit Msg Form View
		'PostPage': {
			'events':	'event92'}, // Create Msg Form View
		'UserRegistrationPage' :{
			'events':	'event96'}, // Reg. Form View
		'BlogMessage': {
