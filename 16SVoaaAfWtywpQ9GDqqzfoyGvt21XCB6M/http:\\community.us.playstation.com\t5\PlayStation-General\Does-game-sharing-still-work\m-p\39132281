<script>
	$( document ).ready(function() {
     
	var href = window.location.pathname;
	var indxUrlBdp = href.indexOf("page-id/") +8;
	var length = href.length;
	var f_url = href.substring(indxUrlBdp,length)

	
if(href=="/" ){
//nothing happens
}
else if (f_url =="Official-Game-Communities"){
$("#ps-all-games").addClass('active');
} 

		$("#ps-gen").addClass('active');
		
 
 });
</script>
