	<script type="text/javascript">

			var PDCFunctionQueue = function(){
				this.queue = [];
			};
			PDCFunctionQueue.prototype.addToQueue = function(funcItem){

				this.queue.push(funcItem);
			};
			PDCFunctionQueue.prototype.clearQueue = function(){
				this.queue.length = 0;
			};
			PDCFunctionQueue.prototype.executeQueue = function(){

				var nQueueLength = this.queue.length;
				var sTargetID,
					sMethodName,
					aParams,
					$targetElement,
					fMethod;

				for (var i = 0; i < this.queue.length; i++) {
					try {
						var funcItem = this.queue[i];

						if (typeof funcItem === 'object') {

								sTargetID = funcItem['id'];
								sMethodName = funcItem['method'];
								aParams = funcItem['params'];
								$targetElement = $(document.getElementById(sTargetID));
								fMethod = $targetElement[sMethodName];
								fMethod.apply($targetElement, aParams);

						} else if (typeof funcItem === 'string' && PDC.Func) {

							if (typeof PDC.Func[funcItem] === 'function') {
								PDC.Func[funcItem].call();
							}

						}
						
					} catch(err) {
					
						if (typeof console == "object") {
							console.log("Error running script - " + err + " (Has plugin been included?)");
							console.log({
								"sTargetID" : sTargetID,
								"sMethodName" : sMethodName,
								"aParams" : aParams,
								"$targetElement" : $targetElement,
								"fMethod" : fMethod
							});
							
						}
						
					}
					
				}
				this.clearQueue();
			};
			var PDC = PDC || {};
			PDC.funcQueue = PDC.funcQueue || new PDCFunctionQueue();
	</script>
	
	<script type="text/javascript">
PDC.funcQueue.addToQueue('CM066');

	</script> 


		
	<script type="text/javascript">  
 PDC.funcQueue.executeQueue();
 
$(function(){

    function animateToTop() {
        var $body = $("body,html");
        $body.animate({
            scrollTop: 0
        }, 1000);
    }

    
    $("a#footer-else-button").click(function(){
			$("#footer-more-container").slideDown();
			$("a#footer-else-buttonClose").show();
			$("a#footer-else-button").hide();
			
			
		});
		
		$("a#footer-else-buttonClose").click(function(){
			$("#footer-more-container").slideUp();
			$("a#footer-else-button").show();
			$("a#footer-else-buttonClose").hide();			
			
		});
    	
    $(".to-top").bind("click", function (event) {
        event.preventDefault();
        animateToTop();
    });
var bIsIE8 = navigator.userAgent.toLowerCase().indexOf('msie 8') != -1;
var bIsIE9 = navigator.userAgent.toLowerCase().indexOf('msie 9') != -1;
if(bIsIE8){
$('.footer-content .footer-content-wrapper .footer-border').css({
         "border-top": "none",
         "position": "relative"})
$('.footer-content .footer-content-wrapper .footer-border:before').css({
         "position": "relative",
         "width": "100%",
         "height": "auto",
         "background": "transparent url('http://cdn.us.playstation.com/pscomauth/groups/public/documents/webasset/trans-horizontal-nhasset.png') no-repeat top left" });

$('.footer-content .footer-content-wrapper .footer-more div').css({"background": "transparent url('http://cdn.us.playstation.com/pscomauth/groups/public/documents/webasset/trans-vertical-nhasset.png') repeat-y top left" });
$('.footer-content .footer-content-wrapper .footer-more div:first').css({"background":"none"});
$('html').addClass('lt-ie9');
} 
if(bIsIE9){
$('html').addClass('lt-ie9');
