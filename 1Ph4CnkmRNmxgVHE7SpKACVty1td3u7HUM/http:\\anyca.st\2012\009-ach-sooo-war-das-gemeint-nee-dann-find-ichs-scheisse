});jQuery(function($) {
	torrentByEnclosure("http://download.anyca.st/anycast/any009.opus", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Opus";
	    // select-style download-widget
	    jQuery("#post-185 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
	    // button-stile download-widget
	    jQuery("#post-185 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});    /* --> */</script>		</div>
