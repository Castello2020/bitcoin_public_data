<script language="JavaScript" type="text/javascript">
<!--
function initTicker()
{
	// Define run time values
	currentEvent     = -1;
	currentLength    = 0;
	// Locate base objects
	if (document.getElementById) {
		    theAnchorObject     = document.getElementById("newTicker");
			runTicker();
		 }
	else {
            document.write("<style>.ticker1{display:none;}.tickerText{border:0px; padding:0px;}</style>");
            return true;
	}
}

function runTicker()
{
	var myTimeout;
	// Go for the next story data block
	if(currentLength == 0)
	{
		currentEvent++;
		currentEvent      = currentEvent % theItemCount;
		theStorySummary      = eventItems[currentEvent].replace(/&quot;/g,'"');
		thePrefix 	     = "<span class=\"tickerB\">" + theLeadString + "</span>";
	}
	// Stuff the current ticker text into the anchor
	theAnchorObject.innerHTML = thePrefix +
	theStorySummary.substring(0,currentLength) + whatWidget();
	// Modify the length for the substring and define the timer
	if(currentLength != theStorySummary.length)
	{
		currentLength++;
		myTimeout = theCharacterTimeout;
	}
	else
	{
		theAnchorObject.innerHTML += "&nbsp;|&nbsp;<a href=\"news-detail.asp?id=" + eventIDs[currentEvent] + "\">Read more&hellip;</a>"
		currentLength = 0;
		myTimeout = theStoryTimeout;
	}
	// Call up the next cycle of the ticker
	setTimeout("runTicker()", myTimeout);
}
// Widget generator
function whatWidget()
{
	if(currentLength == theStorySummary.length)
	{
		return theWidgetNone;
	}

	if((currentLength % 2) == 1)
	{
		return theWidgetOne;
	}
	else
	{
		return theWidgetTwo;
	}
}
// -->
</script>


