<script language="javascript">
function flipCommentView(){
	var cf = document.getElementById("comment_form");
	var ca = document.getElementById("comment_arrow");
	if (cf.style.display == 'none'){
	    cf.style.display = 'block';
		ca.src = 'images/arrow_up.gif';
		
	}
	else {
		cf.style.display = 'none';
		ca.src = 'images/arrow_down.gif';
	}
}
</script>
