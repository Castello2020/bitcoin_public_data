    }

mt_srand((double)microtime() * 1000000);
$oprand = mt_rand(1,3);

switch($oprand)
    {
    case 1:
     $gl1 = mt_rand(1,20); //Zufallszahl zwischen 1 und 20
     $op = &quot;+&quot;; //Rechenoperator f�r die Ausgabe sp�ter
     $gl2 = mt_rand(1,10); //Zweite Zufallszahl zwischen 1 und 10
     //Anschlie�end wird das Ergebnis mit PHP berechnet und in einem MD5-String verwandelt. Dieser wird sp�ter im Formular zur n�tigen Kontrolle �bergeben.
     $loesungmd5 = md5($gl1+$gl2);
    break;
    case 2:
     //Entsprechend der ersten Operation. Das Zahlenspektrum der Zufallszahlen wurde jedoch anders gew�hlt.
     $gl1 = mt_rand(10,30);
     $op = &quot;-&quot;;
     $gl2 = mt_rand(1,$gl1);
     $loesungmd5 = md5($gl1-$gl2);
    break;
    case 3:
     //Siehe case 1 und case 2
     $gl1 = mt_rand(2,10);
     $op = &quot;x&quot;;
     $gl2 = mt_rand(2,6);
     $loesungmd5 = md5($gl1*$gl2);
    break;
    }
