var Lst;
media=new XMLHttpRequest();
function getMedia(type){
    media.onreadystatechange=function(){
        if (media.readyState==4 && media.status==200){
            document.getElementById("media1").innerHTML=media.responseText;
    	}
    	else{
            document.getElementById("media1").innerHTML="<div id='loading'>loading...</div>";
    	}
    }
    media.open("GET","getmedia.php?type="+type.id+"&"+Math.random(), true);
    media.send();
}
function searchMedia(str){
if (str==0)
  { 
  location.reload();
  return;
  }
xmlhttp=new XMLHttpRequest();
xmlhttp.onreadystatechange=function()
  {
  if (xmlhttp.readyState==4 && xmlhttp.status==200)
    {
    document.getElementById("media1").innerHTML=xmlhttp.responseText;
    }
  }
xmlhttp.open("GET","search.php?q="+str+"&"+Math.random(),true);
xmlhttp.send();
}
