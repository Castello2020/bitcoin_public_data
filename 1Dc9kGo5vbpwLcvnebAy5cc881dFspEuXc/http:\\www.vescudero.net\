 , currency: "bitcoin"
 , counter: "count"
 , alignment: "bl"
 , qrcode: true
 , auto_show: false
 , lbl_button: "Donate"
 , lbl_address: "My Bitcoin Address:"
 , lbl_count: "donations"
 , lbl_amount: "BTC"
});
</script>
