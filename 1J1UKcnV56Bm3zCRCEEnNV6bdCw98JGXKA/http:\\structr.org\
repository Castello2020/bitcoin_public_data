$(function() {
  
  var startValue = 10;
  $('#paypal_amount').val(startValue);
  $('#paypal_form_amount').val(startValue);
  $('#paypal_slider').slider({
    value: startValue,
    min: 5,
    max: 100,
    step: 5,
    slide: function(event,ui){
      $('#paypal_amount').val(ui.value);
    }
  });
    
  
  $('.paypal form').on('submit', function() {
    $('#paypal_form_amount').val($('#paypal_amount').val());
  });

});</script>
