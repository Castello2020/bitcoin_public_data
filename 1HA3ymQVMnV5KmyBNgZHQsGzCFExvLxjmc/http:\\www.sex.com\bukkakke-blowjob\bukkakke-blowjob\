        <script>
        // check cookie
        if (!getCookie('ad_zone_89')) {
            setTimeout(function() {
                // Load iframe
                $('#openxIMiframe').html("<iframe src='http://delivery.ipvertising.com/afr.php?zoneid=89&amp;cb=31736' frameborder='0' scrolling='no' width='300' height='250'></iframe>");
                // open IM
                $( "#openxIMWrapper" ).slideDown( "slow");
                // flag as seen if it was opened for least 2 seconds
                setTimeout(function() {
                    setCookie('ad_zone_89', 1, 1);
                }, 2000);
            }, 2000);
        }

        function openxCloseIM() {
            setCookie('ad_zone_89', 1, 24);
            $("#openxIMWrapper").hide();
        }
    </script>

