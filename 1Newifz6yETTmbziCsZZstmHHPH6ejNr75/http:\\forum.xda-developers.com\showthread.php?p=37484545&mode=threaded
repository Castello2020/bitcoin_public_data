<div class="header-boxes2" id="header-search2">
			<form method="post" action="search.php?do=process&amp;searchthreadid=2094696" _lpchecked="1">
				<input type="text" id="sidepanel-searchInput" value="Search this thread" onfocus="javascript:if(this.value=='Search this thread') {this.value='';}" onblur="javascript:if(this.value=='') {this.value='Search this thread'}" name="query"/>

<input type="hidden" value="1" name="quicksearch"/>
<input type="hidden" value="1" name="childforums"/>
<input type="hidden" value="1" name="exactname"/>
<input type="hidden" value="" name="s"/>
