<script type="text/javascript" > 



$.support.cors = true;
$(document).ready(function() {

 $('.donate-button-link').on('click', function (e) {
        e.preventDefault();
        $(this).slideUp(100);
        $('.bitcoin-address').slideDown(100);
        $('.addr').select();
    });

    $("#download_form").submit(function() {
        $('#dwn_link').hide();
        // $('#dwn_link').html('');
        // $('#dwn_link').attr('href','');
        $('#loadingBar').empty(); 
        $('.top_warning, .bottom_warning').hide();

        var s_url = $(".input_url_field").val();
        
        if (!checkUrl(s_url)) {
            warning_url();
            return false;
        }   

        $('.wheel').attr('src','/images/converting.gif');
        $('#loadingBar').html("Preparing the link for download. Please wait.");

        setTimeout(function() {



        $.ajax({
        url: "/worker.php",
        type: "GET",
        dataType: "text",
        data: { 's_url': s_url },
        success: function(data) {
            //fileddd(data);
            if (data.indexOf('error: bad url') !== -1 || data.indexOf('error: page not found') !== -1) {
                warning_url();
            } else {
                $('.button').hide();
                $('#dwn_link').html('Right-click here and "Save link as..."');
                $('#dwn_link').attr('href',data);
                
                $('#dwn_link').show('slow').css("display", "inline-block");
                $('.wheel').attr('src','/images/static.png');
                $('#loadingBar').empty();
            }
        }
    });
    }, 4000);
        


        return false;
    }); 

});


function warning_url() {
    $('#loadingBar').html("Please check your soundcloud link!");
    // jQuery.fx.off = true;
    $('.button').show();
    $('.top_warning, .bottom_warning').show('slow').css("display", "block");
    $('#dwn_link').hide();
    $('.wheel').attr('src','/images/static.png');
    // setTimeout( function() {
    //     $('#loadingBar').empty(); 
    //     $('.top_warning, .bottom_warning').hide();
    //     $('.button').show('fast');
    // }, 3000);
}

    
function checkUrl(test_url) {
    url = test_url.toLowerCase();
    if (url == "") { return false; }
    var what;
    if (url.indexOf('soundcloud.com') !== -1) {
        what='Soundcloud';
        return true;
    }else if (url.indexOf('vimeo.com') !== -1) {
        what='Vimeo';
    }else{
        what='None';
    }
    return false;
}
</script>

