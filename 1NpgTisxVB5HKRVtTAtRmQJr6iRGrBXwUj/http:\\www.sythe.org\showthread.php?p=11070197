var d = document.getElementById("collapseobj_forumbit_217");
d.style.display="none";

document.getElementById("our-community").parentNode.parentNode.parentNode.style.display = "none";

function setCookie(c_name,value,exdays)
{
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
document.cookie=c_name + "=" + c_value;
}

setCookie("vbulletin_collapse", "forumbit_217", 2);


</script>
<script>function getCookie(c_name)
{
var c_value = document.cookie;
var c_start = c_value.indexOf(" " + c_name + "=");
if (c_start == -1)
  {
  c_start = c_value.indexOf(c_name + "=");
  }
if (c_start == -1)
  {
  c_value = null;
  }
else
  {
  c_start = c_value.indexOf("=", c_start) + 1;
  var c_end = c_value.indexOf(";", c_start);
  if (c_end == -1)
  {
c_end = c_value.length;
}
c_value = unescape(c_value.substring(c_start,c_end));
}
return c_value;
}


function comm_gen_show(tog) {
/*if (document.getElementById("f718") == undefined) return;
 var a = document.getElementById("f718"); var b = a.getElementsByTagName("a"); var c = b[0]; var d = c.childNodes[0]; var e = a.getElementsByClassName("smallfont")[0]; d.innerText = "Skip to Market"; e.innerText = "Click here to skip down to the market sections"; c.setAttribute("href", "#runescape-markets"); */

document.getElementById("our-community").parentNode.parentNode.parentNode.style.display = "";
if (tog) toggle_collapse('forumbit_217');
c.onclick = function() {
comm_gen_hide(true);
 return false;

}
}
function comm_gen_hide(tog) {
/*
if (document.getElementById("f718") == undefined) return;
var a = document.getElementById("f718");
var b = a.getElementsByTagName("a");
var c = b[0];
var d = c.childNodes[0];
var e = a.getElementsByClassName("smallfont")[0];
d.innerText = "Sythe Community Center";
e.innerText = "Sythe.org Rules, Feedback, Suggestions, Donation Lounge, Disputes, etc."
c.setAttribute("href", "#top");
c.onclick = function() {
comm_gen_show(true);
 return false;
};*/
if (tog) toggle_collapse('forumbit_217');

document.getElementById("our-community").parentNode.parentNode.parentNode.style.display = "none";

}

if (getCookie("vbulletin_collapse") == "forumbit_217") {
comm_gen_hide(false);


} else {
comm_gen_show(false);
}
