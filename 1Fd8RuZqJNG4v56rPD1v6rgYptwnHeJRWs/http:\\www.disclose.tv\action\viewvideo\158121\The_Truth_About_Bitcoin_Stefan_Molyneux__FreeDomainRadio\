<script type="text/javascript"> 
jQuery(document).ready(function() {
  jQuery(".button_newsletter").click( function() {
    // 
    jQuery("#share_boxes_email").hide();
    jQuery("#ajaxNewsletterResult").slideDown();

    jQuery(".ajaxNewsletterResult").html( 'Just a moment...' );
    // 
    var newsletterEmail = jQuery("#subscribe_user_email").val();
    // ajax
    jQuery.ajax({
        type: 'POST',
        url: "/ajax/newsletterExternal",
        data: { email: newsletterEmail },
        dataType: 'json',
        success: 
          function(data) 
          {
            // alert('data = ' + data.result );
            // 
            if(data.result == 'email_invalid')
            {
                //
                jQuery(".ajaxNewsletterResult").html( 'E-mail is invalid!' );
            }
            else if( data.result == 'already_user' )
            {
                // 
                jQuery(".ajaxNewsletterResult").html( 'Already a member!' );
            } 
            else if( data.result == 'already_subscribed' )
            {
                // 
                jQuery(".ajaxNewsletterResult").html( 'Already subscribed!' );
            } 
            else if( data.result == 'success' )
            {
                // 
                jQuery(".ajaxNewsletterResult").html( 'Subscribed. Thanks!' );
            }
          }
    });    
  });
});
</script> 
 
			   
			 
