    document.subscribe.email.focus();
    return false;
  }
  else
  {
    a = document.subscribe.email.value.indexOf('@');
    b = document.subscribe.email.value.indexOf('@', a+1);
    c = document.subscribe.email.value.indexOf('.', a+1);
