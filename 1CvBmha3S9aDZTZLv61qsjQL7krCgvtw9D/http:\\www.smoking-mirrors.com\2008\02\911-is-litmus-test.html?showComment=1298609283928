["http://lesvisible.net/NINA/thumbs/I%20used%20to%20be%20Shining%20and%20New.jpg", "http://deepintoartlifewestgallery.blogspot.com/2011/08/i-used-to-be-shining-and-new.html", "_new", "I used to be shining and new..."],

["http://lesvisible.net/NINA/thumbs/Reincarnation.jpg", "http://deepintoartlifewestgallery.blogspot.com/2009/06/reincarnations.html", "_new", "Reincarnations"],

["http://lesvisible.net/NINA/thumbs/Spa%20in%20Heaven.jpg","http://deepintoartlifewestgallery.blogspot.com/2012/11/spa-in-heaven.html", "_new", "Spa in Heaven..."],

["http://lesvisible.net/NINA/thumbs/Talking%20to%20the%20Sun.jpg", "http://deepintoartlifewestgallery.blogspot.com/2009/06/talking-to-sun.html", "_new", "Talking to the Sun"],
	
["http://lesvisible.net/NINA/thumbs/Ticket%20Depot%20for%20the%20Shambhala%20Express.jpg","http://deepintoartlifewestgallery.blogspot.com/2009/06/ticket-depot-for-shambala-express.html", "_new", "Ticket depot for The Shambala Express"],

["http://lesvisible.net/NINA/thumbs/Window%20of%20Opportunity.jpg", "http://deepintoartlifewestgallery.blogspot.com/2009/06/brief-window-of-opportunity.html", "_new", "Brief window of opportunity"]
	
	],
	autoplay: [false, 3200, 20], //[auto_play_boolean, delay_btw_slide_millisec, cycles_before_stopping_int]
	persist: false, //remember last viewed slide and recall within same session?
	fadeduration: 1000, //transition duration (milliseconds)
	oninit:function(){ //event that fires when gallery has initialized/ ready to run
		//Keyword "this": references current gallery instance (ie: try this.navigate("play/pause"))
	},
	onslide:function(curslide, i){ //event that fires after each slide is shown
		//Keyword "this": references current gallery instance
		//curslide: returns DOM reference to current slide's DIV (ie: try alert(curslide.innerHTML)
		//i: integer reflecting current image within collection being shown (0=1st image, 1=2nd etc)
	}
})

</script>
