/* Body Text */ .wf-loading body { visibility: hidden; } .wf-active body { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,Georgia,"Times New Roman",serif; font-variant: normal; font-size: 15px; } .wf-loading #site-description { visibility: hidden; } .wf-active #site-description { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 16px; } .wf-loading .entry-content { visibility: hidden; } .wf-active .entry-content { font-size: 16px; } .wf-loading .entry-meta .byline,.wf-loading .entry-meta .comments-link,.wf-loading .entry-meta .edit-link,.wf-loading .entry-meta .entry-categories,.wf-loading .entry-meta .entry-date,.wf-loading .entry-meta .entry-tags,.wf-loading .entry-meta .image-info { visibility: hidden; } .wf-active .entry-meta .byline,.wf-active .entry-meta .comments-link,.wf-active .entry-meta .edit-link,.wf-active .entry-meta .entry-categories,.wf-active .entry-meta .entry-date,.wf-active .entry-meta .entry-tags,.wf-active .entry-meta .image-info { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 101%; } .wf-loading .entry-meta a { visibility: hidden; } .wf-active .entry-meta a { font-size: 15px; font-variant: normal; } .wf-loading .entry-meta .byline a,.wf-loading .entry-meta .byline a:hover { visibility: hidden; } .wf-active .entry-meta .byline a,.wf-active .entry-meta .byline a:hover { font-variant: normal; } .wf-loading .wp-caption .wp-caption-text { visibility: hidden; } .wf-active .wp-caption .wp-caption-text { font-size: 101%; } .wf-loading body.two-col-posts .entry-meta span { visibility: hidden; } .wf-active body.two-col-posts .entry-meta span { font-variant: normal; } .wf-loading body.two-col-posts .entry-meta a,.wf-loading body.two-col-posts .entry-meta a:hover { visibility: hidden; } .wf-active body.two-col-posts .entry-meta a,.wf-act