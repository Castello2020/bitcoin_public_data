: hidden; } .wf-active #respond p.comment-subscription-form label { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,inherit; font-variant: normal; } .wf-loading #comment-nav-above a,.wf-loading #comment-nav-below a { visibility: hidden; } .wf-active #comment-nav-above a,.wf-active #comment-nav-below a { font-size: 15px; } .wf-loading .widget-area { visibility: hidden; } .wf-active .widget-area { font-size: 101%; } .wf-loading .widget_rss .rss-date,.wf-loading .widget_rss .rssSummary,.wf-loading .widget_rss cite { visibility: hidden; } .wf-active .widget_rss .rss-date,.wf-active .widget_rss .rssSummary,.wf-active .widget_rss cite { font-size: 101%; } .wf-loading .widget_twitter .timesince { visibility: hidden; } .wf-active .widget_twitter .timesince { font-size: 101%; } .wf-loading #access-footer a { visibility: hidden; } .wf-active #access-footer a { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 15px; } .wf-loading #site-generator { visibility: hidden; } .wf-active #site-generator { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 12px; } .wf-loading #top-link a { visibility: hidden; } .wf-active #top-link a { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Times New Roman",serif; font-variant: normal; font-size: 18px; } 
