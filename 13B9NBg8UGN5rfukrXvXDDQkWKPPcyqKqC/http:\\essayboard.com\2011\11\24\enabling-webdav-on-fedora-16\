/* Body Text */ .wf-loading body { visibility: hidden; } .wf-active body { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,Georgia,"Times New Roman",serif; font-variant: normal; font-size: 15px; } .wf-loading #site-description { visibility: hidden; } .wf-active #site-description { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 16px; } .wf-loading .entry-content { visibility: hidden; } .wf-active .entry-content { font-size: 16px; } .wf-loading .entry-meta .byline,.wf-loading .entry-meta .comments-link,.wf-loading .entry-meta .edit-link,.wf-loading .entry-meta .entry-categories,.wf-loading .entry-meta .entry-date,.wf-loading .entry-meta .entry-tags,.wf-loading .entry-meta .image-info { visibility: hidden; } .wf-active .entry-meta .byline,.wf-active .entry-meta .comments-link,.wf-active .entry-meta .edit-link,.wf-active .entry-meta .entry-categories,.wf-active .entry-meta .entry-date,.wf-active .entry-meta .entry-tags,.wf-active .entry-meta .image-info { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 101%; } .wf-loading .entry-meta a { visibility: hidden; } .wf-active .entry-meta a { font-size: 15px; font-variant: normal; } .wf-loading .entry-meta .byline a,.wf-loading .entry-meta .byline a:hover { visibility: hidden; } .wf-active .entry-meta .byline a,.wf-active .entry-meta .byline a:hover { font-variant: normal; } .wf-loading .wp-caption .wp-caption-text { visibility: hidden; } .wf-active .wp-caption .wp-caption-text { font-size: 101%; } .wf-loading body.two-col-posts .entry-meta span { visibility: hidden; } .wf-active body.two-col-posts .entry-meta span { font-variant: normal; } .wf-loading body.two-col-posts .entry-meta a,.wf-loading body.two-col-posts .entry-meta a:hover { visibility: hidden; } .wf-active body.two-col-posts .entry-meta a,.wf-active body.two-col-posts .entry-meta a:hover { font-variant: normal; } .wf-loading #content nav .nav-next a,.wf-loading #content nav .nav-previous a,.wf-loading #content nav .next-image a,.wf-loading #content nav .previous-image a { visibility: hidden; } .wf-active #content nav .nav-next a,.wf-active #content nav .nav-previous a,.wf-active #content nav .next-image a,.wf-active #content nav .previous-image a { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 17px; } .wf-loading #content #nav-below span { visibility: hidden; } .wf-active #content #nav-below span { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Times New Roman",serif; font-variant: normal; font-size: 25px; } .wf-loading .nocomments { visibility: hidden; } .wf-active .nocomments { font-size: 17px; } .wf-loading #comments .comment-author cite { visibility: hidden; } .wf-active #comments .comment-author cite { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 20px; } .wf-loading #comments .comment-meta { visibility: hidden; } .wf-active #comments .comment-meta { font-size: 12px; } .wf-loading .comment-reply-link { visibility: hidden; } .wf-active .comment-reply-link { font-size: 12px; } .wf-loading #cancel-comment-reply-link { visibility: hidden; } .wf-active #cancel-comment-reply-link { font-size: 15px; } .wf-loading #respond textarea { visibility: hidden; } .wf-active #respond textarea { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,inherit; font-variant: normal; } .wf-loading #respond input[type=submit] { visibility: hidden; } .wf-active #respond input[type=submit] { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,Georgia,"Times New Roman",serif; font-variant: normal; font-size: 16px; } .wf-loading #respond p.comment-subscription-form label { visibility: hidden; } .wf-active #respond p.comment-subscription-form label { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,inherit; font-variant: normal; } .wf-loading #comment-nav-above a,.wf-loading #comment-nav-below a { visibility: hidden; } .wf-active #comment-nav-above a,.wf-active #comment-nav-below a { font-size: 15px; } .wf-loading .widget-area { visibility: hidden; } .wf-active .widget-area { font-size: 101%; } .wf-loading .widget_rss .rss-date,.wf-loading .widget_rss .rssSummary,.wf-loading .widget_rss cite { visibility: hidden; } .wf-active .widget_rss .rss-date,.wf-active .widget_rss .rssSummary,.wf-active .widget_rss cite { font-size: 101%; } .wf-loading .widget_twitter .timesince { visibility: hidden; } .wf-active .widget_twitter .timesince { font-size: 101%; } .wf-loading #access-footer a { visibility: hidden; } .wf-active #access-footer a { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 15px; } .wf-loading #site-generator { visibility: hidden; } .wf-active #site-generator { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Sorts Mill Goudy",Georgia,"Times New Roman",serif; font-variant: normal; font-size: 12px; } .wf-loading #top-link a { visibility: hidden; } .wf-active #top-link a { font-family: ff-basic-gothic-web-pro-1,ff-basic-gothic-web-pro-2,"Times New Roman",serif; font-variant: normal; font-size: 18px; } 
