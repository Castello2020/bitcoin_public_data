		<div id="AdapTvTitleCard"></div>
		<iframe id="AdapTvTitleCard_Mask"></iframe>




		
			
<script type="text/javascript">
	(function(){
		var __multiLevelReferrer = 'multilevel_embed';
		var JSON = window.JSON || {};

		JSON.parse = JSON.parse || $.evalJSON;
		JSON.stringify = JSON.stringify || $.toJSON;

		if (window.sessionStorage) {

			var ursid = window.sessionStorage.getItem('ursid'),
				contentId = '23828775',
				contentType = 'video',
				rsid = false,
				tsDelta;

			if (ursid) {
				ursid = (JSON.parse(ursid)).value;
				rsid = ursid.rsid;
				tsDelta = +new Date() - ursid.ts;

				if (ursid.media !== contentType + '' + contentId) {
					// mas media van, rsid kuka
					window.sessionStorage.removeItem('ursid');
					ursid = rsid = false;
				}

				if (tsDelta > 8000) {
					// tovabb tartott ez mint egy oldalujratoltes, rsid kuka
					window.sessionStorage.removeItem('ursid');
					ursid = rsid = false;
				}
			}

			if (typeof $(window).unload !== 'undefined') {
				$(window).unload(function(){
					//var ursid = window.sessionStorage.getItem('ursid');
					var strg = window.sessionStorage.getItem('ursid'),
						ursid = (strg) ? (JSON.parse(strg)).value : false;

					ursid.ts = +new Date();

					window.sessionStorage.setItem('ursid', JSON.stringify({value: ursid}));
				});
			}
		}

		var isIE = ustream.browser.ie,
			flashMode = isIE ? 'opaque' : 'direct',
			uViewer = {
				id: "UstreamViewer",
				version: "10.3",
				movie: "http://static-cdn1.ustream.tv/swf/live/viewer:290.swf?vrsl=c:662&amp;ulbr=100",
				width: "100%",
				height: "100%",
				attributes: {
					id: "UstreamViewer",
					name: "UstreamViewer"
				},
				params: {
					wmode: flashMode,
					allowfullscreen: true,
					bgcolor: "#000000",
					allowscriptaccess: "always"
				},
				flashvars: {"vid":23828775,"locale":"en_US","sessionid":false,"autoplay":"true","enablejsapi":"1","sv":6}			};

		var __referrer = (function () {
			if (window.parent !== window.top) {
				try {
					if (window.parent.location.host == location.host) {
						if (window.parent.multiLevelEmbed) {
							return __multiLevelReferrer;
						}
						return window.parent.document.referrer || 'unknown';
					}
					return __multiLevelReferrer;
				} catch (e) {
					return __multiLevelReferrer;
				}
			}
			return document.referrer || 'unknown';
		}());

				if (ustream.vars.customEmbed || (ustream.vars.isSuperChannelEmbed && !ustream.browser.ios)) {
			uViewer.flashvars.referrer = encodeURIComponent(__referrer);
		}

				if ($.cookie("siteVisited") == 1) {
			delete uViewer.flashvars.volume;
		}

				if (ustream.vars.isNewHomePage) {
			uViewer.flashvars.hometype = "new";
		}

		$.cookie("siteVisited", 1);

		if (rsid) {
			uViewer.flashvars.rsid = rsid;
		}

		// SITE-10751 Add ts flashvar to player
		uViewer.flashvars.ts = (new Date()).getTime();

		if (!rsid) {
			window.sessionStorage.setItem('ursid', JSON.stringify({value: {
				media: contentType + '' + contentId,
				ts: +new Date()
			}}));
		}

		window.swfobject.embedSWF(
			uViewer.movie,
			"UstreamViewer",
			uViewer.width,
			uViewer.height,
			uViewer.version,
			false,
			uViewer.flashvars,
			uViewer.params,
			uViewer.attributes,
			function (i) {

				if (!i.success) {
					$('#NoFlashContent').show();
				}
				
				if (!ustream.flash) {
					ustream.flash = {};
				}
				if (!ustream.flash.viewer) {
					ustream.flash.viewer = {};
				}
				ustream.flash.viewer.UstreamViewer = {};
				ustream.flash.viewer.UstreamViewer.instance = i.ref;
			});
	})();
	/*
		endof
		SITE-8971 Page Refresh Proto
	*/
</script>

