   function FooterIcon__GenerateColorImage( base_url, alt ) {
      var scheme_key = CURRENT_SITE_SCHEME_KEY;
      var rgb        = SCHEME_COLORS[ scheme_key ];
      if ( typeof( rgb ) == 'undefined' ) {
         rgb = SCHEME_COLORS[ CURRENT_SITE_SCHEME_KEY ];
      }
      var url = base_url +
               '/r=' + rgb.r +
               '/g=' + rgb.g +
               '/b=' + rgb.b;
      document.write( '<img src="' + url + '" border="0" width="12" height="12" alt="' + alt + '" />' );
   }
