<script>

        $(document).ready(function(){


                //SIGN UP MOD
                $('body').click(function() {
                        $('.currency-select-div').hide();

                });
                $('.currency-select').click(function() {
                        $('.currency-select-div').show();
                        return false;
                });

                $('.currency-item').click(function() {
                        $('.currency-select-div').hide();
                        $('.currency-select').text($(this).text());
                        $('.currency-mark').val($(this).attr('mark'));
                        $('.currency-select').removeClass('c_usd c_gbp c_cad c_aud c_eur');
                        $('.currency-select').addClass('c_'+$(this).text().toLowerCase());
                        $('#currencyfield').val($(this).text());



                        return false;
                });
                $('.pricefield-2').click(function () {
                        setTimeout(function () {
                                $('.pricefield-1').focus();
                        }, 100);
                        return false;
                });
                $('.pricefield-2').focusin(function () {
                        setTimeout(function () {
                                $('.pricefield-1').focus();
                        }, 100);
                        return false;
                });

                $('.signupmod-link').click(function () {
                        $('form.signup-mod-form').eq(0).submit();
                        return false;
                });

/*

                //SEARHC MOD
                $('.search-top').click(function() {

                        if($('.search-term-top').val()==$('.search-term-top').attr('title'))
                                return false;

                        top.location='http://www.gofundme.com/mvc.php?route=&term='+$('.search-term-top').val();
                        return false;
                });

                $('.search-term-top').keypress( function(event) {
                        if (event.keyCode == 13)
                        {
                                if($('.search-term-top').val()==$('.search-term-top').attr('title'))
                                        return false;

                                top.location='http://www.gofundme.com/mvc.php?route=&term='+$('.search-term-top').val();

                                return false;
                        }
                });

                $('.search-left').click(function() {
                        var searchVal = $('.search-term-left').val();

                        if(searchVal==$('.search-term-left').attr('title') || searchVal == "")
                                return false;

                        var destination = 'http://www.gofundme.com/mvc.php?route=';

                        var searchTerm = $("#subsearchTerm").val();

                        if(searchTerm !== undefined && !isPotentialPostalCode(searchVal)){
                                searchTerm += " AND " + val;
                        }else{
                                searchTerm = val;
                        }



                        if ($('.search_location_zip_filter').length > 0){
                                destination +="&addterm="+encodeURIComponent(searchTerm)
                                    +"&term="+$('.search_location_zip_filter').val()
                                    +"&country="+$('.search_location_country_filter').val();
                        }else if ($('.search_partner_filter').length > 0) {
                                destination ="http://www.gofundme.com/partner/"+$('.search_partner_filter').val()+"?term="+encodeURIComponent(searchTerm);
                        }else if ($('.search_category_filter').length > 0){
                                destination ="http://www.gofundme.com/"+$('.search_category_filter').val()+"?term="+encodeURIComponent(searchTerm);
                        }else{
                                destination +="&term="+encodeURIComponent(searchTerm);
                        }

                        top.location=destination;
                        return false;
                });

                $('.search-term-left').keypress( function(event) {
                        if (event.keyCode == 13)
                        {
                                $('.search-left').click();
                                return false;
                        }
                });

                $('.search-main').click(function() {


                        if($('.search-term-main').val()==$('.search-term-main').attr('title'))
                                return false;

                        top.location='http://www.gofundme.com/mvc.php?route=&term='+$('.search-term-main').val();
                        return false;
                });

                $('.search-term-main').keypress( function(event) {
                        if (event.keyCode == 13)
                        {
                                if($('.search-term-main').val()==$('.search-term-main').attr('title'))
                                        return false;

                                top.location='http://www.gofundme.com/mvc.php?route=&term='+$('.search-term-main').val();

                                return false;
                        }
                });

*/

        });

</script>

