goapp = false;
$j('body').click(function() {
	$j("#ajax_listOfOptions").hide();  
	$j("#profile").hide();
});
$j("#search_input").on('tap', function(e){this.focus()});
$j("#search_input").on('mousedown', function(e){ this.focus(); if (e.which==2) return false;});
$j("#search_input").on('keydown', function(e){
	if ( e.keyCode=='40' )  
		select_option( $j('.optionDivSelected').next('.optionDiv') );
	else if (e.keyCode=='38')
                select_option( $j('.optionDivSelected').prev('.optionDiv') );
	else if (e.keyCode=='13')
	{
		n = $j('.optionDivSelected a');
		if (n.length) 
		{
			window.location = n.attr('href');
			goapp = true;
		}
	}
});

</script>
