  var count = "2038";
  function limiter(){
    var tex = document.form1.comment.value;
    var len = tex.length;
    if(len > count){
      tex = tex.substring(0,count);
      document.form1.comment.value =tex;
      return false;
    }
  document.form1.limit.value = count-len;
  }
