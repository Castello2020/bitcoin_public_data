            var iterate = function (i) {
                if (imgs.length > i) {
                    var img = new Image();
                    img.src = '/img/' + imgs[i];
//                     console.log("loading: " + img.src);
                    img.onLoad = function () {
                        iterate(i + 1);
                    }();
                };
            };
            iterate(0);
        };
    </script>
