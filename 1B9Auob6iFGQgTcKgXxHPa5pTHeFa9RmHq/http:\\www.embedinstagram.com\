<script type="text/javascript" src="js/jquery.min.js"></script>

<script type="text/javascript">
$(document).ready(function()
{

$(function() {
   var myInput = $('input#customSizeA');
   myInput.change(function() {
       $('input#customSizeB').val(myInput.val());
   });
});


$(".instructions").click(function()
{
	$(".hiddenInstructions").toggle();
	_gaq.push(['_trackEvent', 'UI', 'instructions', 'Embed Code']);
});

$("[name='type']").click(function()
{
	if($("[name='type']:checked").val() == "image"){
		$(".videoFormat").hide();
		_gaq.push(['_trackEvent', 'EmbedType', 'image', 'Embed Code']);
	}
	if($("[name='type']:checked").val() == "video"){
		$(".videoFormat").show();
		_gaq.push(['_trackEvent', 'EmbedType', 'video', 'Embed Code']);
	}
});


$("[name='size']").click(function()
{
	if($("[name='size']:checked").val() == "custom"){
		$(".customSizeContainer").show();
		_gaq.push(['_trackEvent', 'UI', 'customSize', 'Embed Code']);
	}else{
		$(".customSizeContainer").hide();
	}
});

$('.outputContainer').on('click', '#responsiveVersion', function () {
     _gaq.push(['_trackEvent', 'UI', 'responsive', 'Embed Code']);
	console.log("test");
});

$(".advanced").click(function()
  {
	$(".options").toggle();
  });

  $("#submit").click(function()
  {
   // $(".options").toggle();
	var url = $("#url").val();

	if($("[name='size']:checked").val() == "custom"){
		var size = $("#customSizeA").val();
	}else{
		var size = $("[name='size']:checked").val();		
	}

	var format = $("[name='format']:checked").val();
	var type = $("[name='type']:checked").val();
	//var custom = $("#customSizeA").val();



	var n=url.indexOf("http://instagram.com/p/"); 
    if(url.length > 0 && n != -1)
    {
      //A loading or waiting gif image will display in the demo_output div until the extract data will appearing
	  $(".invalid").hide();
      $("#demo_output").html('<img src="images/loading.gif" class="loading">');
      $.ajax
      ({
         type: "POST",
         url: "get_content.php",
         data: "url="+url+"&size="+size+"&format="+format+"&type="+type,
         success: function(option)
         {
           $("#demo_output").html(option);
			_gaq.push(['_trackEvent', 'Embed', 'Generated', 'Embed Code']);
         }
      });
    }else{
		$(".invalid").show();
		_gaq.push(['_trackEvent', 'Error', 'incorrect URL format', 'Embed Code']);	
	}
  });

  	//enter button
	$('#url').keypress(function(e) {
	  // Enter pressed?
	  if(e.which == 10 || e.which == 13) {
		e.preventDefault();
	   // $(".options").toggle();
		var url = $("#url").val();

		if($("[name='size']:checked").val() == "custom"){
			var size = $("#customSizeA").val();
		}else{
			var size = $("[name='size']:checked").val();		
		}

		var format = $("[name='format']:checked").val();
		var type = $("[name='type']:checked").val();
	
	
		var n=url.indexOf("http://instagram.com/p/"); 
		if(url.length > 0 && n != -1)
		{
		  //A loading or waiting gif image will display in the demo_output div until the extract data will appearing
		  $(".invalid").hide();
		  $("#demo_output").html('<img src="images/loading.gif" class="loading">');
		  $.ajax
		  ({
			 type: "POST",
			 url: "get_content.php",
			 data: "url="+url+"&size="+size+"&format="+format+"&type="+type,
			 success: function(option)
			 {
			   $("#demo_output").html(option);
			   _gaq.push(['_trackEvent', 'Embed', 'Generated', 'Embed Code']);
			 }
		  });
		}else{
			$(".invalid").show();
			_gaq.push(['_trackEvent', 'Error', 'incorrect URL format', 'Embed Code']);	
		}
	  }
  });

});
</script>





