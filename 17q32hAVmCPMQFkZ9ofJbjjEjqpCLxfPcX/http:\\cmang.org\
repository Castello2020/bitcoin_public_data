        //console.log("window.firstPosition: %s", window.firstPosition);
        var daemonStartingPoint = $('#rightside').outerHeight(true) / 2;
        //$('#daemonpic').css({top: daemonStartingPoint, right: daemonRightPoint});   // set daemon starting position
        $('#daemonpic').css({top: daemonStartingPoint});   // set daemon starting position
        //setTimeout(fadeInSectionNames, 1000);
        //setTimeout(fadeInDaemon, 300);
        //fadeInDaemon();
        //setTimeout(raiseDaemon, 300);
        raiseDaemon();
