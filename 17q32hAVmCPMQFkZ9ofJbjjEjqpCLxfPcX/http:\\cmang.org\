<script type="text/javascript">
// Kind of our main start of execution. Set timers and stuff to animate stuff.
// setTimeout(fadeInSectionNames, 2000);
// setTimeout(fadeInDaemon, 600);

// Make stuff invisible
// var sec1 = document.getElementById('sec1')
// sec1.style.opacity = "0";
$('#daemonpic').css("opacity", "0");
//$('#sec1').css("opacity", "0");
//$('#sec2').css("opacity", "0");
//$('#sec3').css("opacity", "0");
// Set boring colors so the fades look k-rad
$('p').css("color", "#AAA");
$('a').css("color", "#AAA");
$('h4').css("color", "#AAA");
$('#sec1').css("color", "#C7A");
$('#sec2').css("color", "#C7A");
$('#sec3').css("color", "#C7A");

$('#daemonpic').draggable({
    containment: '#rightside',
    start: function( event, ui ) {
        //console.log("Started dragging.");
        $('#daemonpic').stop(true,false);   // stop animation
        $('#sec1').stop(true,false)
        $('#sec2').stop(true,false)
        $('#sec3').stop(true,false)
        $('p').stop(true,false)
        $('a').stop(true,false)
        $('h4').stop(true,false)
    }
});
//$('#daemonpic').draggable({ containment: '#wholetable' });
// $('#daemonpic').animate({top:"10%"}, 1500);
var rightDiv = document.getElementById("#rightside");
//objDiv.scrollTop = objDiv.scrollHeight;

$('#daemonpic').click(function(){
    //$('#daemonpic').toggle('bounce',rightDiv.scrollHeight)
    clickedDaemon();
})

// functions?
function fadeInDaemon() {
    // fade("daemonpic");
    $("#daemonpic").animate( { opacity: 1.0 }, 3000);
    /* fade("textcontent"); */
    // $("#daemonpic").fadeIn();
}

function fadeInSectionNames() {
    //fade("sec1");
    $("#sec1").animate( { opacity: 1.0 }, 4000);
    $("#sec2").animate( { opacity: 1.0 }, 4000);
    $("#sec3").animate( { opacity: 1.0 }, 4000);
}

function clickedDaemon() {
    // stop any going animation
    $('#daemonpic').stop(true,false)
    // Figure out if the daemon is low in the #rightside element.
    // "low" means past about halfway down.
    var picPosition = $('#daemonpic').position()
    var delta = $('#rightside').outerHeight(true) - $('#rightside').height()
    var lowPoint = ($('#rightside').height() / 2) - delta
    // If so, raise it.
    if (parseInt(picPosition.top) > lowPoint) {
        raiseDaemon();
        fadeInDaemon();
    }
    else {
        changePicture();
        dropDaemon();
    }
}

function changePicture() { // daemon to other animated gif
    var eye = '/ascii/cm-eye.gif';
    var daemon = 'bsd-color.gif';
    var currentPic = $('#daemonpic').attr("src");
    if (currentPic === daemon) {
        $('#daemonpic').attr("src", eye);
    } else {
        $('#daemonpic').attr("src", daemon);
    }
    //console.log("changePicture.");
}

function dropDaemon() {
    //var endPoint = $(window).height() - $('#rightside').height()
    //var endPoint = $(window).height() - $('#rightside').outerHeight(true);
    var endPoint = $('#rightside').outerHeight(true) - $('#daemonpic').height() - 4;
    //endPoint = '-=' + endPoint;
    //console.log("endPoint: %s", endPoint);
    $('#daemonpic').stop(true,false)
    $('#sec1').stop(true,false)
    $('#sec2').stop(true,false)
    $('#sec3').stop(true,false)
    $('p').stop(true,false)
    $('a').stop(true,false)
    $('h4').stop(true,false)
    $('#daemonpic').animate(
        {   // css
            //bottom: "-=50"
            top: endPoint
        }, 200, function() {
            //$('#daemonpic').style.bottom=endPoint;
            bounceDaemon(); // done falling
        });
    //$("#daemonpic").animate( { opacity: 1.0 }, 300); // fade in if not already faded in
    // set a color scheme, either awesome or bogus.
    // random number
    var randnum = 1 + Math.floor(Math.random() * 2);
    //console.log(randnum);
    if (randnum > 1) {
        $('#sec1').animate( { color: '#0F4' }, 300);
        $('#sec2').animate( { color: '#0F4' }, 300);
        $('#sec3').animate( { color: '#0F4' }, 300);
        $('p').animate( { color: '#B0C' }, 300);
        $('a').animate( { color: '#0BD' }, 300);
        $('h4').animate( { color: '#D0E' }, 300);
    } else {
        $('#sec1').animate( { color: '#C6B' }, 300);
        $('#sec2').animate( { color: '#C6B' }, 300);
        $('#sec3').animate( { color: '#C6B' }, 300);
        $('p').animate( { color: '#ACA' }, 300);
        $('a').animate( { color: '#DCA' }, 300);
        $('h4').animate( { color: '#ACA' }, 300);
    }
}

function raiseDaemon() {
    //var endPoint = window.firstPosition;
    //var endPoint = $(window).outerHeight(true) - $('#rightside').outerHeight(true);
    //var endPoint = endPoint - $(footerTable).outerHeight(true);
    var endPoint = $('#rightside').outerHeight(true) - $('#rightside').height();
    //console.log("raiseDaemon destination: %d", endPoint);
    //$('#daemonpic').stop(true,false)
    $('#daemonpic').animate(
        {   // css
            top: endPoint
        }, { duration: 4000, queue: false }, function() {
        // }, 2000, function() {
    });
    // return to original color scheme.
    $('#sec1').animate( { color: '#F2B' }, { duration: 4000, queue: false });
    $('#sec2').animate( { color: '#F2B' }, { duration: 4000, queue: false });
    $('#sec3').animate( { color: '#F2B' }, { duration: 4000, queue: false });
    $('p').animate( { color: '#8CD' }, 4000);
    $('a').animate( { color: '#0D2' }, 4000);
    $('h4').animate( { color: '#07F' }, 4000);
}


function bounceDaemon() {
    $('#daemonpic').effect('bounce', { times: 3 }, "slow");
    
}

//document.write("wtf test");
</script>

