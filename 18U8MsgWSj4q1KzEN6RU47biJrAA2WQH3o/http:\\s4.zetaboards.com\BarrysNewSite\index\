    var topic_number = 9884298;
    function shoutPost(msg) {
    var xc = $("#shoutxss").val();
    $.ajax({
    "type":"POST",
    "url":main_url + "tasks/",
    "data":{ "task":9, "msg":msg, "xc":xc, "last":lastMsgID },
    "dataType":"json",
    "success":function(json) {
    $.ajax({
    "type":"GET",
    "url":main_url + "post/",
    "data":{"mode":"2","type":"1","f":forum_number,"t":topic_number},
    "success":function(d) {
    $.ajax({
    "type":"POST",
    "url":main_url + "post/",
    "data":{"mode":"2","type":"1","f":forum_number,"t":topic_number,"xc":$("input[name=xc]",d).val(),"qhash":$("input[name=qhash]",d).val(),"sd":"1","p":"0","ast":$("input[name=ast]",d).val(),"post":msg},
    "success":function() {
    shoutUpdate(json);
    }
    });
    }
    });
    }
    });
    }
