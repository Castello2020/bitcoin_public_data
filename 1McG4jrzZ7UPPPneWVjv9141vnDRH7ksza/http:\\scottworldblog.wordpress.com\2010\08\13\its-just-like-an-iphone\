if ( typeof GA_googleAddAttr == 'function' ) {
GA_googleAddAttr("AdOpt", "1");
GA_googleAddAttr("Origin", "other");
GA_googleAddAttr("LangId", "1");
GA_googleAddAttr("Domain", "scottworldblog.wordpress.com");
GA_googleAddAttr("BlogId", "7700439");
GA_googleAddAttr("PageURL", "http://scottworldblog.wordpress.com/2010/08/13/its-just-like-an-iphone/");
GA_googleAddAttr("AdSafe", "1");
GA_googleAddAttr("Autotag", "technology");
GA_googleAddAttr("Tag", "scottworld-blog");
GA_googleAddAttr("Tag", "android");
GA_googleAddAttr("Tag", "att");
GA_googleAddAttr("Tag", "blackberry");
GA_googleAddAttr("Tag", "google");
GA_googleAddAttr("Tag", "iphone");
GA_googleAddAttr("Partner", "AOL");
GA_googleAddAttr("theme_bg", "ffffff");
GA_googleAddAttr("theme_text", "333333");
GA_googleAddAttr("theme_link", "772124");
GA_googleAddAttr("theme_border", "eeeeee");
GA_googleAddAttr("theme_url", "58181b");
GA_googleAddAdSensePageAttr("google_page_url", "http://scottworldblog.wordpress.com/2010/08/13/its-just-like-an-iphone/");
GA_googleFillSlot("wpcom_below_post_adsafe");
}
</script>
</div>
