		if ( dropdown.options[dropdown.selectedIndex].value > 0 ) {
			location.href = "http://libertyblitzkrieg.com/?cat="+dropdown.options[dropdown.selectedIndex].value;
		}
	}
	dropdown.onchange = onCatChange;
/* ]]> */
</script>

