<script>
    $(function () {

        //diff chart
        $.getJSON('/ajax/diffchartdata?coinId=6&t=055225180027046132166064141249110007237060022133046091013083211178152108105135030214217105255113', function (data) {
            // Create the chart
            $('#diffcontainer').highcharts('StockChart', {


                rangeSelector: {
                    buttons: [
                    {
                        type: 'day',
                        count: 1,
                        text: '1d'
                    },
                    {
                        type: 'week',
                        count: 1,
                        text: '1w'
                    },
                    {
                        type: 'month',
                        count: 1,
                        text: '1m'
                    },
                    {
                        type: 'month',
                        count: 3,
                        text: '3m'
                    },
                    {
                        type: 'month',
                        count: 6,
                        text: '6m'
                    },
                    {
                        type: 'all',
                        text: 'All'
                    }],
                    selected: 2
                },

                title: {
                    text: 'Peercoin (PPC) Difficulty Chart'
                },

                series: [{
                    name: 'Difficulty',
                    data: data,
                    tooltip: {
                        valueDecimals: 8
                    }
                }]
            });
        });

        //network chart
        $.getJSON('/ajax/networkhashratechartdata?coinId=6&t=055225180027046132166064141249110007237060022133046091013083211178152108105135030214217105255113', function (data) {

            // Create the chart
            $('#networkcontainer').highcharts('StockChart', {


                rangeSelector: {
                    buttons: [
                    {
                        type: 'day',
                        count: 1,
                        text: '1d'
                    },
                    {
                        type: 'week',
                        count: 1,
                        text: '1w'
                    },
                    {
                        type: 'month',
                        count: 1,
                        text: '1m'
                    },
                    {
                        type: 'month',
                        count: 3,
                        text: '3m'
                    },
                    {
                        type: 'month',
                        count: 6,
                        text: '6m'
                    },
                    {
                        type: 'all',
                        text: 'All'
                    }],
                    selected: 2
                },

                title: {
                    text: 'Peercoin (PPC) Network Hashrate Chart'
                },

                series: [{
                    name: 'Network Hashrate',
                    data: data,
                    tooltip: {
                        valueDecimals: 8
                    }
                }],

                yAxis: {
                    labels: {
                        formatter: function() {
                                var hashRate = "";

                                if (this.value == 0)
                                {
                                    hashRate = "0";
                                }
                                else if (this.value > 0 && this.value < 1000)
                                {
                                    hashRate = Number(this.y) + " H/s";
                                }
                                else if (this.value >= 1000 && this.value < 1000000)
                                {
                                    hashRate = Number(this.value / 1000) + " KH/s";
                                }
                                else if (this.value >= 1000000 && this.value < 1000000000)
                                {
                                    hashRate = Number(this.value / 1000000) + " MH/s";
                                }
                                else if (this.value >= 1000000000 && this.value < 1000000000000)
                                {
                                    hashRate = Number(this.value / 1000000000) + " GH/s";
                                }
                                else if (this.value >= 1000000000000 && this.value < 1000000000000000)
                                {
                                    hashRate = Number(this.value / 1000000000000) + " TH/s";
                                }
                                else if (this.value >= 1000000000000000)
                                {
                                    hashRate = Number(this.value / 1000000000000000) + " PH/s";
                                }

                                return hashRate;
                        }
                    }
                },

                tooltip: {
                    formatter: function() {

                            var hashRate = "";

                            if (this.y == 0)
                            {
                                hashRate = "no data for this time period";
                            }
                            else if (this.y > 0 && this.y < 1000)
                            {
                                hashRate = parseFloat(this.y).toFixed(4) + " H/s";
                            }
                            else if (this.y >= 1000 && this.y < 1000000)
                            {
                                hashRate = parseFloat(this.y / 1000).toFixed(4) + " KH/s";
                            }
                            else if (this.y >= 1000000 && this.y < 1000000000)
                            {
                                hashRate = parseFloat(this.y / 1000000).toFixed(4) + " MH/s";
                            }
                            else if (this.y >= 1000000000 && this.y < 1000000000000)
                            {
                                hashRate = parseFloat(this.y / 1000000000).toFixed(4) + " GH/s";
                            }
                            else if (this.y >= 1000000000000 && this.y < 1000000000000000)
                            {
                                hashRate = parseFloat(this.y / 1000000000000).toFixed(4) + " TH/s";
                            }
                            else if (this.y >= 1000000000000000)
                            {
                                hashRate = parseFloat(this.y / 1000000000000000).toFixed(4) + " PH/s";
                            }

                            var date = new Date(this.x);
                            var formattedTime = date.toUTCString();

                            hashRate = '<small style="font-size:7.5pt;">' + formattedTime  + '</small><br /><b>Hashrate: </b>' + hashRate; 

                            return hashRate;
                        }
                },
            });
        });


        //exchange rate chart
        var seriesOptions = [],
		yAxisOptions = [],
		seriesCounter = 0,
		names = [],
		colors = Highcharts.getOptions().colors;

        $.getJSON('/ajax/exchangechartdata?coinId=6&t=055225180027046132166064141249110007237060022133046091013083211178152108105135030214217105255113', function (data) {

            $.each(data, function (i, exchangedata) {
                names[i] = exchangedata.Exchange;

                seriesOptions[i] = {
                    name: exchangedata.Exchange,
                    data: exchangedata.ExchangeSnapshots,
                    tooltip: {
                        valueDecimals: 8
                    }
                }
            });

            // Create the chart
            $('#exchangecontainer').highcharts('StockChart', {

                chart: {
                },

                rangeSelector: {
                    buttons: [
                    {
                        type: 'day',
                        count: 1,
                        text: '1d'
                    },
                    {
                        type: 'week',
                        count: 1,
                        text: '1w'
                    },
                    {
                        type: 'month',
                        count: 1,
                        text: '1m'
                    },
                    {
                        type: 'month',
                        count: 3,
                        text: '3m'
                    },
                    {
                        type: 'month',
                        count: 6,
                        text: '6m'
                    },
                    {
                        type: 'all',
                        text: 'All'
                    }],
                    selected: 2
                },

                yAxis: {
                    labels: {

                    },
                    plotLines: [{
                        value: 0,
                        width: 2,
                        color: 'silver'
                    }]
                },

                plotOptions: {

                },

                tooltip: {
                    pointFormat: '<span style="color:{series.color}">{series.name}</span>: <b>{point.y}</b><br/>',
                    valueDecimals: 8
                },

                series: seriesOptions,

                title: {
                    text: 'Peercoin (PPC) Exchange Rate Chart'
                }

            });
        });

    });

</script>
    </div>
