            $('#lnkCalc').click(function () {
                var url = "/calculators/namecoin-mining-calculator/?h=" + $('#h').val() +
                      "&p=" + $('#p').val() +
                      "&pc=" + $('#pc').val() +
                      "&pf=" + $('#pf').val() +
                      "&d=" + $('#d').val() +
                      "&r=" + $('#r').val() +
                      "&er=" + $('#er').val() +
                      "&btcer=" + $('#btcer').val() +
                      "&hc=" + $('#hc').val();
                $(this).attr('href', url);
            });
