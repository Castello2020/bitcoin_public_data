	<header>
		
			
		
		<h2 class="firehose">
				
				
				<span class="vote">
					
						
							<span id="updown- _ item.id _ " class="vote">
								<a onclick="show_login_box(); return false;" href="/my/login" onclick="return false" class="up" title="Vote this item up">+</a>
