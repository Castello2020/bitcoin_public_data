                    function beginRequest(sender, args) 
                    {
                        var r=args.get_request();
                        if (r.get_headers()["X-MicrosoftAjax"])
                        {
                        b=r.get_body();
                        var a="__MicrosoftAjax=" + encodeURIComponent(r.get_headers()["X-MicrosoftAjax"]);
                        if (b!=null && b.length>0)
                        {
	                    b+="&";
                        }
                        else
	                    b="";
                        r.set_body(b+a);
                        }       
                    }
                    Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(beginRequest);
                </script>
            
 
            <script language="javascript" type="text/javascript">
                function IAmSelected( source, eventArgs ) 
                {
                    //alert( " Key : "+ eventArgs.get_text() +" Value : "+eventArgs.get_value());
                    var hostvar = "";
                    if (window.location.host.match("localhost"))
                    {
                        hostvar = window.location.host + "/keeperleague";
                    }
                    else
                    {
                        hostvar = window.location.host;
                    }

                    var s = eventArgs.get_text();
                    if (s.indexOf("[NHL]") !== -1) {
                        window.location = "http://" + hostvar + "/" + "nhl" + "/playerdetails.aspx?playerid=" + eventArgs.get_value();
                    }
                    else {
                        if (s.indexOf("[NBA]") !== -1) {
                            window.location = "http://" + hostvar + "/" + "nba" + "/playerdetails.aspx?playerid=" + eventArgs.get_value();
                        }
                        else {
                            alert("This is an error... unable to go to selected item");
                        }
                    }
                }
            </script>
