                    }
                    if(data.username){
                        $('.not_editable').hide();
                        $('.is_editable').show();
                    }else{
                        $('.not_editable').show();
                        $('.is_editable').hide();
                    }
                });
        });

        query_user_actions = ensure(function(){
            jQuery.ajax({
                data: {
                    edit: 'solar_panelling',
                    owner: 'wn',
                    ieditor: 'http://editor.wn.com',
                },
                dataType: 'jsonp',
                url: 'http://login.wn.com/actions/i/',
