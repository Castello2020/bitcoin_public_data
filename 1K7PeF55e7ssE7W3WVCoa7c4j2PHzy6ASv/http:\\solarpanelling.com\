			<br clear="left" />
			<div class="detailsBar border_color bgr_color">
			 	
				<a onClick="ensure(load_iframe_url)('modal-player', 'http://www.youtube.com/embed/J3xOUuieOiA?autoplay=1'); return false;" data-reveal-id="video-content"  class="play"><span data-trans="Play">Play</span></a>


