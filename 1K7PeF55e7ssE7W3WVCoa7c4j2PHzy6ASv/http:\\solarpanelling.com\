        <script type="text/javascript"><!--
        window.page_query = 'solar panelling';
        update_user_actions = ensure(function(data){
                if(data.username){
                    $('#login').html(data.html);
                    window.username = data.username;
                }
                $ensure(function(){
                    if(data.editable){ // user is owner
                        $('.signup-box').html('<a id="id_save_playlist"  class="sign_up" onclick="return save_playlist(this.href)" href="http://editor.wn.com/save_playlist/solar_panelling/"><span data-trans="Save playlist">Save playlist</span></a>');
                        $('.button').html('<a id="id_save_playlist"  class="sign_up" onclick="return save_playlist(this.href)" style="float: none !important;" href="http://editor.wn.com/save_playlist/solar_panelling/"><span data-trans="Save this page">Save this page</span></a>');
                    }else if(data.username){ // watching other user page
                        $('.signup-box').html('<a onclick="return login_and_save_playlist(\'http://editor.wn.com/save_playlist/solar_panelling/\', this.href)" rel="nofollow" class="sign_up" href="http://editor.wn.com/fork/wn/solar_panelling/"><span data-trans="Save playlist">Save playlist</span></a>');
