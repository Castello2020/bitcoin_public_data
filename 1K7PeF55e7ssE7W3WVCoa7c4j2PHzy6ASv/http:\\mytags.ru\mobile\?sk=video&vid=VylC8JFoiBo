function query_name(){  
  if ($("#qnisrun").text() === 'true') {
    $.fancybox( "#name" ); }
  else {
    var all = document.getElementById("text").value;
    $('#name').fadeOut(200);     
    $.post( "/components/youtybeGet_mobile/index.php", { url: all },
      function( data ) {
        $( "#name" ).html( data );
      }
    );
    $("#qnisrun").html("true");
    $.fancybox( "#name" );
    $.fancybox.showLoading();
  }
}   
</script>      
