    <script type="text/javascript">
      function weightedprices_calculator(obj) {
        var weightedprices = {"USD": {"7d": "622.94", "30d": "618.61", "24h": "622.72"}, "IDR": {"7d": "6934532.14", "30d": "7115621.77", "24h": "6895618.67"}, "ILS": {"7d": "2054.20", "30d": "2032.61", "24h": "2061.49"}, "GBP": {"7d": "367.55", "30d": "364.90", "24h": "367.45"}, "DKK": {"7d": "3676.92", "30d": "3564.97"}, "CAD": {"7d": "670.96", "30d": "665.97", "24h": "669.55"}, "JPY": {"7d": "63476.67", "30d": "63307.59", "24h": "63386.50"}, "RON": {"7d": "1975.27", "30d": "1902.79", "24h": "1971.19"}, "XRP": {"7d": "113698.90", "30d": "148482.52", "24h": "109205.45"}, "SEK": {"7d": "4367.18", "30d": "4265.72", "24h": "4385.05"}, "SGD": {"7d": "776.57", "30d": "775.48", "24h": "774.17"}, "HKD": {"7d": "4847.57", "30d": "4826.02", "24h": "4842.86"}, "AUD": {"7d": "667.52", "30d": "663.38", "24h": "666.64"}, "CHF": {"7d": "561.28", "30d": "556.66", "24h": "561.58"}, "timestamp": 1406040486, "KRW": {"7d": "645163.35", "30d": "638878.79", "24h": "645299.98"}, "CNY": {"7d": "3840.42", "30d": "3844.53", "24h": "3840.04"}, "LTC": {"7d": "71.89", "30d": "73.22", "24h": "72.25"}, "NZD": {"7d": "719.92", "30d": "712.26", "24h": "719.71"}, "THB": {"7d": "19194.29", "30d": "19256.23", "24h": "19078.05"}, "EUR": {"7d": "461.34", "30d": "455.24", "24h": "461.53"}, "SLL": {"7d": "156481.05", "30d": "154584.33", "24h": "156797.70"}, "ARS": {"7d": "7217.97", "30d": "7095.81", "24h": "7290.06"}, "NOK": {"7d": "3890.03", "30d": "3829.84", "24h": "3954.75"}, "RUB": {"7d": "23215.73", "30d": "22847.96", "24h": "23259.47"}, "INR": {"7d": "38031.13", "30d": "37475.67", "24h": "38601.71"}, "MXN": {"7d": "8269.47", "30d": "8245.92", "24h": "8282.50"}, "CZK": {"7d": "12478.72", "30d": "12534.57", "24h": "12505.11"}, "BRL": {"7d": "1515.25", "30d": "1479.45", "24h": "1550.23"}, "NMC": {"7d": "346.02", "30d": "347.89", "24h": "347.79"}, "PLN": {"7d": "1867.59", "30d": "1847.73", "24h": "1871.12"}, "ZAR": {"7d": "6831.99", "30d": "6610.25", "24h": "6864.00"}}
        var currencies_name = {"USD": "US Dollar", "IDR": "Indonesian rupiah", "BGN": "Bulgarian Lev", "PEN": "Peruvian Nuevo Sol", "ILS": "Israeli New Sheqel", "GBP": "British Pound Sterling", "DKK": "Danish Krone", "CAD": "", "MXN": "Mexican Peso", "HUF": "Hungarian Forint", "RON": "Romanian leu", "GAU": "Pecunix (grams of gold)", "XRP": "Ripple Credits", "UAH": "Ukrainian Hryvnia", "SAR": "Saudi Riyal", "SEK": "Swedish Krona", "SGD": "Singapore Dollar", "HKD": "Hong Kong Dollar", "AUD": "Australian Dollar", "CHF": "", "KRW": "South Korean won", "CNY": "Chinese Yuan", "LTC": "Litecoin", "NZD": "New Zealand Dollar", "CLP": "Chilean Peso", "THB": "Thai Baht", "EUR": "Euro", "SLL": "Linden Dollars", "ARS": "Argentine Peso", "NOK": "Norwegian Krone", "RUB": "Russian Ruble", "INR": "Indian Rupee", "JPY": "Japanese Yen", "CZK": "Czech Koruna", "BRL": "Brazilian Real", "NMC": "Namecoin", "BTC": "Bitcoin", "PLN": "Polish Zloty", "ZAR": "South African Rand"}
        var last_input = null

        var value_a = obj.find("input[name=value_a]")[0]
        var value_b = obj.find("input[name=value_b]")[0]
        var currency_a = obj.find("select[name=currency_a]")
        var currency_b = obj.find("select[name=currency_b]")

        currencies_name["mBTC"] = "Bitcoin"

        function update() {
          currency_a.children().remove()
          currency_b.children().remove()

          fx.base = "BTC"

          fx.rates = {"BTC": 1, "mBTC": 1e3}

          for (d in weightedprices) {
            if (d == "timestamp")
              continue

            var obj = weightedprices[d]

            if (obj.hasOwnProperty("24h"))
              fx.rates[d] = obj["24h"]
          }

          var currencies = []

          for (d in fx.rates) {
            currencies.push(d)
          }

          currencies.sort()

          for (d in currencies) {
            var option = $("<option/>")

            option.text(currencies_name[currencies[d]] + " (" + currencies[d] + ")")
            option.attr("value", currencies[d])

            currency_a.append(option.clone())
            currency_b.append(option.clone())
          }

          currency_a.children("[value=mBTC]").attr("selected", 1)
          currency_b.children("[value=EUR]").attr("selected", 1)
        }

        function calc(e) {
          var ctx = e.data
          var attr = this.getAttribute("name").split("_")
          var direction = attr[1]

          if (attr[0] == "value")
            last_input = attr[1]

          if (attr[0] == "currency" && last_input != direction)
            direction = direction=="a"?"b":"a"

          var from, to

          var currency_a = $(ctx.currency_a).val()
          var currency_b = $(ctx.currency_b).val()

          switch(direction) {
            case "a":
              from = {field: ctx.value_a, currency: currency_a}
              to = {field: ctx.value_b, currency: currency_b}
              break;
            case "b":
              from = {field: ctx.value_b, currency: currency_b}
              to = {field: ctx.value_a, currency: currency_a}
              break;
          }

          from_value = from.field.value.replace(",", ".")

          to.field.value = fx.convert(from_value,
            {from: from.currency, to: to.currency})
        }

        update()

        var objs = {value_a: value_a, value_b: value_b,
                    currency_a: currency_a, currency_b: currency_b,
                    last_input: last_input}

        obj.find("input").change(objs, calc)
        obj.find("select").change(objs, calc)
      }

      weightedprices_calculator($(".cconv").first())
    </script>

