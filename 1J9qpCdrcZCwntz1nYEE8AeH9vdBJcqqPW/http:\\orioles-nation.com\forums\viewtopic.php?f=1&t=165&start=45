function newPopup(url) {
	popupWindow = window.open(
		url,'popUpWindow','height=235,width=299,left=10,top=10,resizable=yes,scrollbars=no,toolbar=no,menubar=no,location=no,directories=no,status=no,titlebar=no')
}
</script>
			
			
