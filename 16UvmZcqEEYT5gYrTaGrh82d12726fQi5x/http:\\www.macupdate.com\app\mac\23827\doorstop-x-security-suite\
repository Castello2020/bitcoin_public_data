		<% 
			}
		%>
		<input type="hidden" id="hdn_bump_count_<%=rc_revid%>" value="<%=rc.total_bumps%>" />
		<img id='smiley_<%=rc_revid%>' class='<%=rc.bump_icon_class%>' name='bump_review' src='<%=rc.bump_icon%>'>
		</div>
		<p class='rcpb-revcom-content'><%=rc.revmsg%></p>
		<div class='yui3-u-2-3 rcpb-reply-button-box'>
			<a id='<%=rc_revid%>' class='rcpb-reply-button'><img src='/img/app_info_reviews_comments_reply_btn.png' class='rcpb-reply-button-image'>Reply</a>
			<%  
				var number_of_replies = 0;
				var reply_text = "&nbsp;replies";
				if(rc.replies){
					if(rc.replies.length){
						number_of_replies = rc_replies.length;
						if(number_of_replies == 1){
							reply_text = "&nbsp;reply";
						}
					}
				}
				var replies = number_of_replies+reply_text;
			%>
			<span class='rcpb-reply-count'><%=replies%></span>
		</div>
		<div class="review_vers yui3-u-1-3">Version&nbsp;<%=rc.app_vers%></div>
	</div>
		<% if(rc_replies){ %>
		<% _.each(rc_replies,function(item,key,list){var rc_reply = item; %>
			<div id="<%=rc_reply.revid%>" class="rcpb-threadreply">
				<div class="rcpb-icon_badge">
					<a href="<%=rc_reply.member_link%>">
						<img src="<%=rc_reply.member_icon%>" alt="<%=rc_reply.member_name%> Member Icon" class="rcpb-usericon" />
						<span class="rcpb-user-rating"><%=rc_reply.member_rating%></span>
					</a>
				</div>
				<div class="yui3-u rcpbt-content">
					<a href="<%=rc_reply.member_link%>" class="rcpb-fromuser"><%=rc_reply.member_name%></a>
					<span class="rcpb-postdate"><%=rc_reply.revdate%></span>
					<div class="rcpb-bump">
						<%
							if(parseInt(rc_reply.total_bumps) < 0){
						%>
							<span class='rcpb-bumpcount' id="total_bump_count_<%=rc_reply.revid%>"><%=rc_reply.total_bumps%></span>
						<% 
							} else {
						%>
							<span class='rcpb-bumpcount' id="total_bump_count_<%=rc_reply.revid%>">+<%=rc_reply.total_bumps%></span>
						<% 
							}
						%>
						<input type="hidden" id="hdn_bump_count_<%=rc_reply.revid%>" value="<%=rc_reply.total_bumps%>" />
						<img id='smiley_<%=rc_reply.revid%>' class="<%=rc_reply.bump_icon_class%>" name="bump_review" src="<%=rc_reply.bump_icon%>">
					</div>
					<p class="rcpbt-revcom-content"><%=rc_reply.revmsg%></p>
				</div>
			</div>
		<%})%>
	<%}%>
