<script id="app-info-tmpl-rc-more_reviews_comments" type="text/x-underscore-template">
<% _.each($.mu.api.data.objects.app.more_reviews, function(item,key,list){
	var rc = item;
	var rc_revid = rc.revid;
	var rc_replies = null;
	var rc_badge_class = "review-badge";
	var rc_badge_text = "Review"
	if(rc.replies){
		if(rc.replies.length){
			rc_replies = item.replies;
		}	
	}
	switch(parseInt(rc.type)){
        case 1:
            rc_badge_text = "Comment";
            rc_badge_class = "comment-badge";
            break;
        case 2:
            rc_badge_text = "Review";
            rc_badge_class = "review-badge";
            break;
        case 3:
            rc_badge_text = "Comment";
            rc_badge_class = "comment-badge";
            break;
        case 5:
            rc_badge_text = "Dev";
            rc_badge_class = "dev-badge";
            break;
    }
%>
