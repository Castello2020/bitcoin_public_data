		<% } %>
		<div class='rcpb-bump'>
		<%
			if(parseInt(rc.total_bumps) < 0){
		%>
			<span class='rcpb-bumpcount' id="total_bump_count_<%=rc_revid%>"><%=rc.total_bumps%></span>
		<% 
			} else {
		%>
