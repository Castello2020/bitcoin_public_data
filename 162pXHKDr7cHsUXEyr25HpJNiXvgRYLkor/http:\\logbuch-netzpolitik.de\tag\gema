  <source src="http://logbuch-netzpolitik.de/podlove/file/1456/s/webplayer/c/website/lnp057-es-kann-nur-einen-geben.oga" type="audio/ogg"/>
  <source src="http://logbuch-netzpolitik.de/podlove/file/1460/s/webplayer/c/website/lnp057-es-kann-nur-einen-geben.mp3" type="audio/mpeg"/>
