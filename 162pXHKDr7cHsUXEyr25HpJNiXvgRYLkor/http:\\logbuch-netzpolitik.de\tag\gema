	    jQuery("#post-1576 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/lnp/lnp057-es-kann-nur-einen-geben.oga", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Ogg&nbsp;Vorbis&nbsp;Audio";
	    // select-style download-widget
	    jQuery("#post-1576 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
	    // button-stile download-widget
	    jQuery("#post-1576 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
