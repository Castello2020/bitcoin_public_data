	torrentByEnclosure("http://meta.metaebene.me/media/lnp/lnp059-no-princess-for-you.opus", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Ogg&nbsp;Opus&nbsp;Audio";
