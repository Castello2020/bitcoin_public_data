});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/lnp/lnp073-wimpel-im-wind.opus", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Ogg&nbsp;Opus&nbsp;Audio";
	    // select-style download-widget
	    jQuery("#post-2038 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
