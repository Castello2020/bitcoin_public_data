			src  = icon.attr('src');
			icon.attr('src', /minus/.test(src) ? src.replace('minus', 'plus') : src.replace('plus', 'minus'));
			jQuery('#comment-'+ id +' div.comment').animate({ height: 'toggle', opacity: 'toggle' }, 1000);
			return false;
		});

		toggleSidebars = function() {
			icon = jQuery('.switch-sidebars img');
