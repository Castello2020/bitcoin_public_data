            holder.postFeed = "http://www.blogger.com/feeds/13785946/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/13785946/comments/default";
          
          holder.currentBlogUrl = "http://martialarm.blogspot.com/";
          holder.currentBlogId = "13785946";
        
