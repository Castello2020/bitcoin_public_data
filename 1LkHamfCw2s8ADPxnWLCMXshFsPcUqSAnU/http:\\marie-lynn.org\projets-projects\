				var get_sizes = null;
									(function($){
						get_sizes = function(id){
							var sizes;
							$.ajax({
								async: false,
								data: {
									action: "flickr-gallery-sizes",
									id: id
								},
								dataType: "json",
								type: "POST",
								url: "http://marie-lynn.org/",
								success: function(rsp) {
									sizes = rsp;
								}
							});
							return sizes;
						};
					})(jQuery);
