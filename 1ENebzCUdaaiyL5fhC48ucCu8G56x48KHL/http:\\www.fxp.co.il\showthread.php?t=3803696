$( window ).scroll(function() {
  var y = $(window).scrollTop(); 
	if (y > 126){
	    $(".tower_right,.tower_left, .tower_left_adse").css( "position", "fixed" );
	    $(".tower_right,.tower_left, .tower_left_adse").css( "top", "0px" );

	}
	else{
		$(".tower_right, .tower_left, .tower_left_adse").css( "position", "absolute" );
	        $(".tower_right, .tower_left, .tower_left_adse").css( "top", "126px" );

	}
  });
</script>
