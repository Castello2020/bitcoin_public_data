                <div class="comment-content">
                                            <div class="comment-voting">
                            <div class="vud-widget vud-widget-thumbsup  up-inactive" id="widget-comment-145664">
            <div class="thumbsup-votes-display"> 0</div>

        <a onclick="TINY.box.show({url:'/sites/all/themes/peak_prosperity/widgets/thumbsup/thumbsup-anon.html',width:600,height:265})" rel="nofollow" class="vud-link-up">                    <div class="up-inactive" title="Vote Up!"></div>
                    <div class="element-invisible">Vote Up!</div>
