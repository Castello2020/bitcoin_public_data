                    }));
                    // load_radio_iframe
                    $('a.load_radio_iframe').click(ensure(function() {
                            if ($('#liveradio_iframe').attr('src') == $(this).attr('href')) {
                                return false;
                            }
                            $('#liveradio_iframe').attr('src', $(this).attr('href'));
                            return false;
                    }));
                    $('.radio-menu li a').click(ensure(function(){
                        $('.radio-menu li').removeClass('active');
                        $(this).parent().addClass('active');
                   }));


                    //show-hide-skins
                    $("#skins").hide();

                    $('#skins_content').click(ensure(function() {
                        $("#skins").slideToggle(500);
                    }));
                    $("body").click(ensure(function(e) {
                        if(e.target.className !== "skins")
                        {
                          $(".skins").hide();
                        }
                    }));
                    //show-hide-dictionary
                    $("#dictionary").hide();

                    $('#dictionary_content').click(ensure(function() {
                        $("#dictionary").slideToggle(500);
                    }));
                    $("body").click(ensure(function(e) {
                        if(e.target.className !== "dictionary")
                        {
                          $(".dictionary").hide();
                        }
                    }));

                    //to the top
                    $('.gotop').click(ensure(function(){
                        $('body,html').animate({ scrollTop: 0}, 800);
                    }));

                    //lyrics toggle
                    $('.lyrics_toggle').click(ensure(function() {
                            $(this).find("div").toggle();
                            $("#lyrics_container").slideToggle(500);
                    // $('#tabcontainer').height($(".playlist-page").height() + 500);
                            updateHeight();
                    }));
                    //lyrics karaoke on/off

                    $('.karaoke_on_off').mouseover(ensure(function() {
                          $('.karaoke').removeClass('off');
                    }));
                    $('.karaoke_on_off').mouseleave(ensure(function(){
                           $('.karaoke').addClass('off');
                    }));

                    $('.artist_data li').hover(ensure(function(){
                        $(this).find('.karaoke').toggleClass('off');
                    }));
                    $('.artist_data li').hover(ensure(function(){
                        $(this).find('.fullscreen').toggleClass('off');
                    }));

                    //timeline news / hide other sections
                    $('.current_year').click(ensure(function() {
                        $("#news_stories").show();
                        $("#timeline_results").hide();
                        // $('#tabcontainer').height($("#expand").height());
                        updateHeight();
                     }));
                    $('.timeline li a').click(ensure(function(){
                        $('.timeline li').removeClass('bold');
                        $(this).parent().addClass('bold');
                    }));


		    		//reloads sms/email iframes	
                    $('.send').click(ensure(function() {
                        $('.reload').attr('src', $('.reload').attr('src'));
                    }));
			


                    // show_synopsis					
                    $('.show_synopsis').hover(ensure(function(){
                        $(this).find('.synopsis-box').show().delay('6000');
                      }, function(){
                        $(this).find('.synopsis-box').hide().delay('6000');
                    }));
					
			


				    var $title_count = $('.page_title').text().length;
					if ($title_count > 37) {
					  $('.page_title').addClass('smaller_font');
					} else {
					  $('.page_title').addClass('bigger_font');
					}

				    $('.weather-data').hover(function() {
				                     $('.forecast-data').animate({ width: 'toggle'}, 900);
				    });



					
                    $('.offine_download').live('click', function() {
                        init_videoload();
                        $(".videoload-container").toggle(500);
                    });
					
					
				
				var fullscreen_interval_caller = false;
				
				jQuery('.play-album-fullscreen').live('click', ensure(function(e) {
					videoplayer_fs.clearPlaylist();
					
					// checking if we have at least one video in the playlist in order to click it
					fullscreen_interval_caller = setInterval(function(){
						first_video = jQuery('#playlistcontainer_fs').find('a.playlistitem:first');
						if (first_video) {
							first_video.find('img.playlist-thumbnail').click();
							triggerFullscreen(1);
							clearInterval(fullscreen_interval_caller);
						}
					}, 1000);
				}));
				
				jQuery('.play-fullscreen').live('click', ensure(function(e) {
					videoplayer_fs.clearPlaylist();
					fullscreen_interval_caller = setInterval(function(){
						first_video = videoplayer_fs.playlistContainer.find('li').first().find('a.playlistitem');
						if (first_video) {
							first_video.click();						
							triggerFullscreen(1);
							clearInterval(fullscreen_interval_caller);
						}
					}, 1000);

				}));


				

        });
    // -->
    </script>

