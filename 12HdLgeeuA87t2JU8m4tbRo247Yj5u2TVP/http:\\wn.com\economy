	<!-- SET LYRICS ARTISTS NAME -->	
	
	
	
  <script type="text/javascript">				
    function load_wiki(el, cb) {
        var el = $(el).closest('.wiki-page').find('.wikiparser');
        var query = el.data('title');
        var main = el.data('main');
        var wikisnippet = el.find('.wikisnippet');
        el.find('.wikipage a').html('Loading... please wait...');
        el.closest('div').load('/api/upge/cheetah-photo-search/wiki_page?main='+main+'&query='+escape(query), 			
		function(){
            wikisnippet.hide();
            updateHeight();
            if(cb) cb();
        });
        return false;
    }				
    function load_playlistlyrics(query) {
		var lyrics_query = query.replace('&', ' ').replace('[', ' ').replace(']', '').replace('@', ' ').replace('live', ' ').replace('Live', ' ').replace('(', ' ').replace(')', ' ').replace('"', ' ').replace('!', ' ')
        jQuery.ajax({
            url: '/api/upge/cheetah-photo-search/lyrics_playlist',
            data: {'query': lyrics_query,
                   'artist': 'Economy'
            },
            dataType: 'text',
            success: ensure(function(text) {
                text = text.trim().split(new RegExp("<"+"!--"),1)[0];
                if(!text) {
                    var song = "";                    					
                }else{
                    var matches = text.trim().match(new RegExp("(.*)\n(.*)\n([^]*)"));
                    var song = matches[3];
                    jQuery('.lyrics_icon').show();
					jQuery('.lyrics_body').html(song);
                }
            })
        });
        return false;
    }
  </script>
  
  <script type="text/javascript"><!--
    bind_videos = ensure(function(vsid, autostart){
					
                        $('a#play'+vsid+'').click(ensure(function(){
                            videoplayers[vsid].addVideo($('#details'+vsid+' .video-info'));
                            videoplayers[vsid].addVideo($('#playlist'+vsid+' .video-info'));
                        }));
                        $('a#remove'+vsid+'').click(ensure(function(){
                            $('#playlist'+vsid+' .mainvideoresults').animate({ opacity: 'hide' }, "slow");
                            updateHeight();
                        }));

                        $('a.list-preview').click(ensure(function(){
                            $('.list-preview').hide('fast');
                            $('.thumb-preview').show('fast');
                        }));
                        $('a.thumb-preview').click(ensure(function(){
                            $('.list-preview').show('fast');
                            $('.thumb-preview').hide('fast');
                        }));
                        $('#playlist_clear'+vsid+'').click(function(){
                            $(this).hide('fast');
                            $('#playlist_restore'+vsid+'').show('fast');
                         });
                        $('#playlist_restore'+vsid+'').click(function(){
                            $('.playlist_clear'+vsid+'').show('fast');
                            $(this).hide('fast');
                        });
                        $('#sortby'+vsid+'').hide();
                        $('a#open_sortby'+vsid+'').click(function(){
                            $('#sortby'+vsid+'').show('slow');
                            $('#open_sortby'+vsid+'').hide('fast');
                            $('#close_sortby'+vsid+'').show('fast');
                        });
                        $('a#close_sortby'+vsid+'').click(function(){
                            $('#sortby'+vsid+'').hide('slow');
                            $('#open_sortby'+vsid+'').show('fast');
                            $('#close_sortby'+vsid+'').hide('fast');
                        });			


        if(!window.videoplayers){
            videoplayers = [];
        }
        videoplayers[vsid] = new Videoplayer({
            playlistContainer: "#playlistcontainer"+vsid,
            playerContainer: "#playercontainer"+vsid,
            playlistItemTemplate: '<li class="playlistitemli thumbnail border_color bgr_color_content font_color">' +
'<a class="playlistitem ellipsis bgr_color" id="<%= id %>" href="#">' +
'<div class="thumb border_color rounded"><div class="clip">' +
'<img width="110" height="82" alt="<%= title %>" src="<%= thumbnailUrl %>" />' +
