    if( window.Trackpipe ) {
        Trackpipe.maybeDoTralbumActions();
    }

    
    if ( window.BandFollow )
        BandFollow.init({"tralbum_id":557655421,"tralbum_type":"a"});
    



    
    if ( window.Share )
        Share.initPanel(false, 1, "http://store.blamstra.in/album/lowblow");


    if ( window.FanControls ) {
        FanControls.init({"show_collect":true});
