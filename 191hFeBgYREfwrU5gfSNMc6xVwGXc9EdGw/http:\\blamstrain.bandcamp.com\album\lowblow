$(document).ready(function() {

    if ( window.LogView ) {
        LogView.show();
    }

    PopupImage.attach( "a.popupImage" );

    if ( window.Cart ){
        Cart.startup();
    }
    if ( window.AutoLyrics ) {
        AutoLyrics.init("_lyrics_", "lyrics_div_", "lyrics_row_", TralbumData.trackinfo.length, 1 );
    }
    if ( window.Player ) {
        Player.init(TralbumData, AlbumPage.registerPlaylistListeners);
    }
    if ( window.TralbumDownload ) {
        TralbumDownload.init();
        
        $(".download-link").click(function(ev) {
            ev.preventDefault();
            TralbumDownload.begin(ev);
        });
        
        $(".send-as-gift > .download-link").unbind("click").click(function(ev) {
            ev.preventDefault();
            TralbumDownload.begin(ev, true);
        });
    }
    if ( window.TralbumUpdater ) {
        TralbumUpdater.init();
    }
    if ( window.PackageOrder ) {
        PackageOrder.init();
        
        $(".order_package_link").click(function(ev){
            ev.preventDefault();
            // PackageOrder.begin takes two params: the package index and an isGift boolean. previously it only took a 
            // package index, but this funtion call was passing event as well. if you need the event in begin(), then add
            // it as an extra param.
            // dh 12-02-2013
            PackageOrder.begin($(this).attr('data-pkg'));
        });
        
        $(".send-as-gift > .order_package_link").unbind("click").click(function(ev){
            ev.preventDefault();
            PackageOrder.begin($(this).attr('data-pkg'), true);
        });
    }
    
    Y.util.Event.on("buy-disc-link", "click", function() {Stats.record({kind:"click", click:"buy_disc"}); return true;});
    Y.util.Event.on("rssFeedAlbum", "click", function() {Stats.record({kind:"click", click:"rss_feed"}); return true;});
    Y.util.Event.on("rssFeedArtist", "click", function() {Stats.record({kind:"click", click:"rss_feed"}); return true;});

    if ( window.AlbumPage ) {
        AlbumPage.registerMouseOverForTrackRows();
    }
    
    
    
    if ( window.TralbumFans ){
        TralbumFans.initialize([{"why":null,"username":"paulsims","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/paulsims","grid":null,"name":"Simmo","thumb":null,"purchase_date":"13 Mar 2014 20:31:21 GMT","item_type":"a","image_id":1831464,"fan_id":93796,"item_id":557655421},{"why":null,"username":"scubadevils","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/scubadevils","grid":null,"name":"Scubadevils","thumb":null,"purchase_date":"12 Mar 2014 21:46:24 GMT","item_type":"a","image_id":2434917,"fan_id":180518,"item_id":557655421},{"why":null,"username":"JMarques","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/JMarques","grid":null,"name":"João Marques","thumb":null,"purchase_date":"16 Aug 2013 18:04:42 GMT","item_type":"a","image_id":1079333,"fan_id":23799,"item_id":557655421},{"why":null,"username":"Trasheater","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/Trasheater","grid":null,"name":"Trasheater","thumb":null,"purchase_date":"16 Aug 2013 17:54:28 GMT","item_type":"a","image_id":1088622,"fan_id":18787,"item_id":557655421},{"why":null,"username":"shaaman","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/shaaman","grid":null,"name":"Lubomir Sugra","thumb":null,"purchase_date":"03 Jan 2011 12:28:22 GMT","item_type":"a","image_id":1760338,"fan_id":59175,"item_id":557655421},{"why":null,"username":"christopherdilkus","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/christopherdilkus","grid":null,"name":"Christopher Dilkus","thumb":null,"purchase_date":"23 Dec 2009 22:41:09 GMT","item_type":"a","image_id":2604212,"fan_id":208813,"item_id":557655421},{"why":null,"username":"chaircrusher","gift_type":null,"featured_track_id":null,"url":"http://bandcamp.com/chaircrusher","grid":null,"name":"kent williams","thumb":null,"purchase_date":"10 Nov 2009 15:06:56 GMT","item_type":"a","image_id":1390712,"fan_id":53563,"item_id":557655421},{"why":null,"use