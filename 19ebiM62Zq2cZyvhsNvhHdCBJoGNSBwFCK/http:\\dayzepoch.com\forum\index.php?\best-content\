if (_isWater) exitWith {DZE_ActionInProgress = false; cutText &#91;localize "str_player_26", "PLAIN DOWN"];};
if (_inVehicle) exitWith {DZE_ActionInProgress = false; cutText &#91;(localize "str_epoch_player_42"), "PLAIN DOWN"];};
