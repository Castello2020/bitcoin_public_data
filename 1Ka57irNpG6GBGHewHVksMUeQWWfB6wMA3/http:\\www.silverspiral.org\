	  	  <!-- Item title -->
	  <h3 class="catItemTitle">
			
	  				<a href="/index.php/home/item/166-the-ancestors-altar">
	  		The Ancestors&#039; Altar	  	</a>
	  	
	  		  </h3>
	  
				<!-- Item Author -->
		<span class="catItemAuthor">
			Written by <a rel="author" href="/index.php/component/k2/itemlist/user/65-nayaaerodiode">Naya Aerodiode</a>
		</span>
		  </div>

  <!-- Plugins: AfterDisplayTitle -->
  
  <!-- K2 Plugins: K2AfterDisplayTitle -->
  
	
