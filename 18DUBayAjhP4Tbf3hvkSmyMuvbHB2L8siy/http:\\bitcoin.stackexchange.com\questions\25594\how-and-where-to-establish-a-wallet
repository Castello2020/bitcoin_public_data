<div id="answer-25601" class="answer" data-answerid="25601"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
    <input type="hidden" name="_id_" value="25601">
    <a class="vote-up-off" title="This answer is useful">up vote</a>
