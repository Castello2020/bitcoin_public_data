                                                            flattr_tle = flattr_tle.slice(0,79);
                          }
                          var flattr_dsc = document.getElementById('flattr_summary_4527100518584779107').innerHTML;  
                                                                   flattr_dsc = flattr_dsc.replace(/<.*?>/g, "");
                          flattr_dsc = flattr_dsc.replace(/\n/g, "\\n");
                          flattr_dsc = flattr_dsc.replace(/\'/g, "\\'");
                                                          if (flattr_dsc.length > 580) {
                            flattr_dsc = flattr_dsc.replace(/(.{0,571}) .*/, "$1 &#8230;");
                                                            flattr_dsc = flattr_dsc.slice(0,579);
                          }
                          var flattr_cat = 'text';  
                          var flattr_lng = 'de_DE';  // change to your language!
                          //        var flattr_tag = 'europa,ezb,politik,wirtschaft';  
                          var flattr_tag = 'wirtschaft';
                          if (flattr_tag.length > 230) {
                            flattr_tag = flattr_tag.replace(/(.{0,230}),/, "$1");
                            flattr_tag = flattr_tag.slice(0,229);
                          }
                          var flattr_url = 'http://www.diewunderbareweltderwirtschaft.de/2014/06/negativerleitzinsezbwiewirkt.html';  
                          // var flattr_btn = ''; // for normal button  
                          var flattr_btn = 'compact'; // for compact button    
                        </script>
<script src='http://api.flattr.com/button/load.js' type='text/javascript'></script>
</span>
<fb:like font='arial' href='http://www.diewunderbareweltderwirtschaft.de/2014/06/negativerleitzinsezbwiewirkt.html' layout='button_count' send='true' show_faces='false' width='150'></fb:like>
