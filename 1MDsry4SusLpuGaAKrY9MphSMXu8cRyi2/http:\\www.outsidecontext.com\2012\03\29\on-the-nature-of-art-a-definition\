				},
				out: function() {
					if(jQuery(this).find('.current_page_item').length == 0 && jQuery(this).hasClass('current_page_item') == false) {
						jQuery(this).find('.children').stop(true, true).slideUp('slow');
					}
				},
				timeout: 500
			});
		}
	
	if(jQuery().eislideshow) {
