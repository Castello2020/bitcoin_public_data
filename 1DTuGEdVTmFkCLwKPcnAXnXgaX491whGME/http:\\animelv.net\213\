<!-- Bottom Panel ends -->





	  
    
    
	</div>
    
    
<style>
    .fixed {
    position: fixed;
    top: 20px;
    background: #f5f5f5;
	padding: 12px 16px;
	width: 244px;
	display: inline-block;
	border-radius:4px;
margin-left: -16px;}
</style>

<script>
var sticky = document.querySelector('#text-3');
var limit = document.querySelector('#bottompanel');
var origOffsetY = sticky.offsetTop;
var limitOffsetY = limit.offsetTop;
var scrolled = window.scrollY;
var finalTop = 0;

function onScroll(e) {
  if(window.scrollY >= origOffsetY && window.scrollY + sticky.offsetHeight < limitOffsetY)
  {
    makeSticky()
    scrolled = window.scrollY;
    finalTop = 0;
  }
  else if (window.scrollY + sticky.offsetHeight >= limitOffsetY)
  { 
    var top = window.scrollY - scrolled;
    finalTop = finalTop + top;
    sticky.style.top =  "-" + finalTop + "px";
    scrolled = window.scrollY;
  }

  else
  {
    unStick();
    scrolled = window.scrollY;
    finalTop = 0;
  }


  /* sticky.classList.add('fixed') :
                                  sticky.classList.remove('fixed');*/
}

function makeSticky()
{
  
  sticky.style.position = "fixed";
  sticky.style.top = "0";
  sticky.style.background = "#F5F5F5";
  sticky.style.padding = "27px 16px 12px 16px";
  sticky.style.margin= " 0 0 0 -16px";
  /*sticky.style.border-radius = "0 0 4px 4px";
  sticky.style.-webkit-border-radius = "0 0 4px 4px"
  sticky.style.-moz-border-radius = "0 0 4px 4px"*/
}

function unStick()
{
  sticky.style.position = null;
  sticky.style.top = null;
  sticky.style.background = null;
  sticky.style.padding = null;
  sticky.style.margin = null;/*
  sticky.style.border-radius = null;
  sticky.style.-webkit-border-radius = null;
  sticky.style.-moz-border-radius = null;*/


}

document.addEventListener('scroll', onScroll);
</script>
</div>

