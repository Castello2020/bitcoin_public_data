<script type="text/javascript">
jQuery(document).ready(function($) {
$('a').smoothScroll();

$('nav ul li li').has('.sub-menu').children('a').addClass('sub-menu-parent');
$('nav li').has('.sub-menu').hover(function(){ $(this).children('.sub-menu').fadeIn();}, function() { $(this).children('.sub-menu').fadeOut();  });


$(".fancybox, article a:has(img)").fancybox({padding: 3, openEffect: 'elastic', closeEffect: 'elastic'});

$(".small").hover(function(){
     $(this).find(".excerpt").css("opacity", 0.95);
}, function(){
     $(this).find(".excerpt").css("opacity", 0);
});

pos = 0;
inClick = false;
$(".slider-next").click(function(e){

     if(inClick) { return; } else {

    inClick = true;

     var children = $(this).parent().find(".slider").find(".slides").children().length;

     if (pos < children-3) {
          pos = pos + 3;
     $(this).parent().find(".slider").find(".slides").css('marginLeft', '-=960px');
     }

     setTimeout(function() { inClick = false; }, 500);

     }
});


$(".slider-prev").click(function(){

     if(inClick) { return; } else {

    inClick = true;

     var children = $(this).parent().find(".slider").find(".slides").children().length;

     if (pos > 0) {
          pos = pos-3;
     $(this).parent().find(".slider").find(".slides").css('marginLeft', '+=960px');
     }
     setTimeout(function() { inClick = false; }, 500);

     }
});



$('.ac-head').click(function() {
  target = $(this).data('ac');
  if ($('#ac-'+target).is(':visible')) {
    $($('#ac-'+target)).slideUp(); 
    $(this).removeClass('expd');
  } else {
  $(this).addClass('expd');
  $('#ac-'+target).slideDown();
}


});



$("#s").attr('placeholder', "Szukaj...");
$("#top").click(function() {
window.location.href = 'http://libertarianin.org';
});

target = $('#font-ctrl');
Ttop = target.offset().top-20;
Tleftt = target.offset().left;
height = target.parent().outerHeight();
$(window).scroll(function () {
     if(Ttop < $(window).scrollTop() && $(window).scrollTop() < height+300) {
          target.addClass('fixed');
          target.css({left: Tleft}); } else {
           target.removeClass('fixed');}
});

if ($.cookie("font")) {
     $("article p").css('font-family',$.cookie("font"));
}

if ($.cookie("font-size")) {
     $("article p").css('font-size',$.cookie("font-size"));
}


$("#sans-serif, #serif").click(function() {

   var font = $(this).css('font-family');
   $("article p").css('font-family', font);
   $.cookie("font", font);

});


$("#txt-plus").click(function() {
     $("article p").css('font-size', '+=3px');
     var size = $("article p").css('font-size');
     $.cookie("font-size", size);
});

$("#txt-minus").click(function() {
     $("article p").css('font-size', '-=3px');
     var size = $("article p").css('font-size');
     $.cookie("font-size", size);
});


$('.fullstory p').append(' <span style="opacity: 0; height: 0px; width: 0px; margin: 0px; padding: 0px; font-size: 1px; line-height: 1px;">Tekst pochodzi ze strony Libertarianin.org</span>');



});



</script>


