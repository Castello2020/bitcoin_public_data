var ewebSitePopUp = function(name, full) {
    if (!full) {
        full = 'yes';
    }
    var popup = window.open("about:blank", name, "height=500,left=15,location=" + full + ",menubar=yes,scrollbars=yes,resizable=yes,status=yes,toolbar=yes,top=15,width=650", true);
    popup.focus();
    return popup;
}
$(document).ready(function(){
    $.fn.maxWidth = function(o) {          
        return this.each(function() {
            var imgwidth = $(this).width();    // Current image width
            // alert('WIDTH = '+imgwidth);
            if(o.max){
                // Check if the current width is larger than the max
                if(imgwidth > o.max){
                    $(this).width(o.max+'px'); // Set new width to max width
                }
            }                      
            if(o.min){
                if(imgwidth < o.min){
                    $(this).width(o.min+'px'); // Set new width to min width
                }
            }          
            $(this).parent().width(this.width);
            $(this).parent().height(this.height);
        });                  
    }
});
    
$(document).ready(function() {
    // Set the variable $width to the width of our wrapper on page load
    $width = $('#mobileContent').width();
    $height = $('#mobileContent').height();
    // Max width makes sure a large image is scaled to fit within the viewport.  This is only needed right before page rendering.
    $('#mobileContent img').css({
        'max-width' : $width , 'height' : 'auto'
        
    });
});

function myfitwall(myfitwallUrl) {
    $.fancybox({
        'href' : myfitwallUrl,
        'type' : 'iframe',
        'width'  : 910,
        'height' : 590,
        'scrolling' : 'no',
        'centerOnScroll': false,
        'padding': 0,
        'autoScale' : false
    });
}
    
</script>
