<script type="text/javascript">

	jQuery(document).ready(function($) {



		jQuery.fn.textWidth = function(){
			var html_org = jQuery(this).html();
			var html_calc = '<span>' + html_org + '</span>';
			jQuery(this).html(html_calc);
			var width = jQuery(this).find('span:first').width();
			jQuery(this).html(html_org);
			return width;
		};

		jQuery('.widget:not(".soc-block .widget")').each(function(){
			var a = jQuery(this);

			a.children().each(function() {

				var b = jQuery(this);

		 	//console.log(b.html() + 'outerWidth: ' + b.parent().outerWidth() + ' textWidth: ' + b.textWidth());
		 	if(b.textWidth() > b.outerWidth()){
		 		//can just change css instead - 
		 	//b.css('width',b.outerWidth);
		 	//b.css('overflow','auto');
		 	b.replaceWith('<textarea readonly autofocus="false" style="background-color: transparent;border:0 none;resize: none;color:rgba(255,255,255,.5)">' + b.html() + '</textarea>')
		 }
		});
		});







		function jjlm_update_stats_header(){

			$.get('http://newsdev.thegenesisblock.com/wp-content/themes/gblock/assets/header_tick_data.php?htick=1', function(data) {
				
				jQuery('.htick_price').html(data);
				// console.log(data);
			});
		}

		jjlm_update_stats_header();

		var jjlmMakeTick = setInterval(jjlm_update_stats_header,5000);

	// shorten slider caption

	jQuery('.slider-caption h1').each(function(){
		var $jthis = jQuery(this).html();

		jQuery(this).html($jthis.substring(0,65) + '...')


	});


});
