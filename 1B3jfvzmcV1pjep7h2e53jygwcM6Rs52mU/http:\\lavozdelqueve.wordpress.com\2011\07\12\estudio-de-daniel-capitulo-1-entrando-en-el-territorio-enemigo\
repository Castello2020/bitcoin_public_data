			new Image().src = stat_gif + "&baba=" + Math.random();
			wpcom_adclk_recorded = true;
			var now=new Date(); var end=now.getTime()+250;
			while(true){now=new Date();if(now.getTime()>end){break;}}
			return true;
		}
	
if ( typeof GA_googleAddAttr == 'function' ) {
GA_googleAddAttr("AdOpt", "1");
GA_googleAddAttr("Origin", "other");
GA_googleAddAttr("LangId", "19");
GA_googleAddAttr("Domain", "lavozdelqueve.wordpress.com");
GA_googleAddAttr("BlogId", "17544520");
GA_googleAddAttr("PageURL", "http://lavozdelqueve.wordpress.com/2011/07/12/estudio-de-daniel-capitulo-1-entrando-en-el-territorio-enemigo/");
GA_googleAddAttr("AdSafe", "1");
GA_googleAddAttr("Tag", "la-voz-de-la-biblia");
GA_googleAddAttr("Tag", "la-voz-del-tercer-angel");
GA_googleAddAttr("Tag", "cap1");
GA_googleAddAttr("Tag", "conocimiento");
GA_googleAddAttr("Tag", "daniel");
GA_googleAddAttr("Tag", "llave");
GA_googleAddAttr("Tag", "profecia");
GA_googleAddAttr("Tag", "temperancia");
GA_googleAddAttr("Partner", "AOL");
GA_googleAddAttr("theme_bg", "333333");
GA_googleAddAttr("theme_border", "444444");
GA_googleAddAttr("theme_text", "eeeeee");
GA_googleAddAttr("theme_link", "cccccc");
GA_googleAddAttr("theme_url", "ee0000");
GA_googleAddAdSensePageAttr("google_page_url", "http://lavozdelqueve.wordpress.com/2011/07/12/estudio-de-daniel-capitulo-1-entrando-en-el-territorio-enemigo/");
GA_googleFillSlot("wpcom_below_post_adsafe");
}
</script>
</div>
