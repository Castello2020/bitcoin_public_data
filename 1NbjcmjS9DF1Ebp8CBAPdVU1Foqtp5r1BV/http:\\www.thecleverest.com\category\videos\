$(window).scroll(function() {
	if (scrolleffect == true) {
		var scrollTop = $(window).scrollTop();
		//console.log(scrollTop);
		position = scrollTop * 2;
		offset1 = scrollTop * -2;
		offset2 = scrollTop * -1.4;
		offset3 = scrollTop * -1.1;
		offset4 = scrollTop * -1.5;
		offset5 = scrollTop * 1.01;
		$('#bigtext').css({'top' : position+'px'});
		$('.t1').css({'top' : offset1+'px'});
		$('.e1').css({'top' : offset2+'px'});
		$('.l1').css({'top' : offset2+'px'});
		$('.c1').css({'top' : offset1+'px'});
		$('.v1').css({'top' : offset3+'px'});
		$('.e3').css({'top' : offset2+'px'});
		$('.r1').css({'top' : offset1+'px'});
		$('.e4').css({'top' : offset4+'px'});
		$('.t2').css({'top' : offset5+'px'});
	}	
});


$(".img-swap").live('click', function() {
	if ($(this).attr("class") == "img-swap") {
	  this.src = this.src.replace("-4s","-3gs");
	} else {
	  this.src = this.src.replace("-3gs","-4s");
	}
	$(this).toggleClass("on");
  });	  
  


$(document).scroll(function() {
    var scrolldist = $(document).scrollTop();
    	console.log("scrolldist is: "+scrolldist);
    var topbounds = $('#bigtext').height() + 90;
    	console.log("topbounds is: "+topbounds);
    var bottombounds = $("#bottom").offset();
    bottombounds = bottombounds.top;
    bottombounds = bottombounds - $(window).height();
		console.log("bottom bounds is: "+bottombounds);    	
    	
    //var offset = $("#bottom").offset();
    //console.log("Bottom is this far down:"+offset.top);
    //var offsettop = offset.top;
    //console.log(offsettop);
    
    if ((scrolldist > topbounds) && (scrolldist < bottombounds)) {
    	//$('.signup-header').css('display','block');
    	//$('.signup-header').fadeIn();
    	$('.signup-header').slideDown('right');
    }
    
    if ((scrolldist > topbounds) && (scrolldist > bottombounds)) {
    	//$('.signup-header').fadeOut();
    	$('.signup-header').slideUp('right');
    }
    
    console.log('\n');
    
})



