        <# if (entities) {
            var padLeft = function(s, len, char) { 
                var output = s;
                if (typeof output != 'string'){
                    output = output.toString();
                }
                if (output.length < len) {
                    output = new Array(len - output.length + 1).join(char || ' ') + output;
                }
                return output;
            };
            var toHoursAndMinutes = function(totalMinutes) {
                return { 
                    hours: padLeft(Math.floor(totalMinutes/60.0),2,'0'), 
                    minutes: padLeft(totalMinutes % 60,2,'0') 
                };
            };
