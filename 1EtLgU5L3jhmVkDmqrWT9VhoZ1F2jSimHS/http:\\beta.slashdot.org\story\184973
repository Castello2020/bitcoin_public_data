        <div class="body-text" >{{{preview_text}}}</div>
        <div class="row form-inline">
                {{#if captcha_update}}
                    <div class="span4 cap-wrap">
                        <img src="{{{captcha_update.image}}}">
                        <button class="btn btn-info btn-audio"><i class="fa  fa-volume-up"></i></button>
                        <audio class="captcha_audio">
                            <source src="{{captcha_update.audio}}"></source>
                        </audio>
                        <div class="captcha-control">
                            <label for="catpcha-input">Prove yourself</label>
                            <input type="text" name="hcanswer" placeholder="prove yourself" />
                        </div>
                    </div>
                    <div class="span4 error-message"></div>
                {{else}}
                    <div class="span8 error-message"></div>
                {{/if}}

            <div class="span4 text-right">
                <button class="btn toggle-view" data-control-id="{{cid}}">Edit Comment</button>
                <button class="btn btn-primary push-comment">Post Comment</button>
            </div>
        </div>
    </form>
</script>
