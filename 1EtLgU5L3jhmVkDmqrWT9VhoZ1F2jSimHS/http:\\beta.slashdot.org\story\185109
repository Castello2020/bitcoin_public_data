<script>
    $(function(){
        $('.for-text').slashModal({
            content     : $('.submission-info'),
            width       : 525,
            hide_overlay: true
        });

        $('.edit-login').on("click", function() {
            editPreview();
        });

        $("#usr-login").validate({
            messages: {
                unickname: {
                    required: 'Please eneter a valid nickname'
                },
                upasswd: {
                    required: 'Please enter a valid password'
                }
            },
            errorClass: 'form-error',
        });

        (function(){
            var $anchor = $('a[href=""], a[href="#"]');
            // IF ON SANDBOX ONLY COLOR ALL EMPTY ANCHOR TAGS RED
            
        })();
    });
</script>

