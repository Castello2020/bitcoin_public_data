    <script type="text/javascript">
    $(function () {
      $(".add-criticas-e-sugestoes").click(function () {
        ga('send', 'event', 'Top Friends', 'Suggest', '1');
        $("#criticas-e-sugestoes").modal();
        return false;
      });
      var formSubmit = function () {
        $(this).unbind('submit');
        $("#message-criticas-e-sugestoes").show().siblings().hide();
        $.post($(this).attr('action'), $(this).serialize(), function (data) {
          $("#criticas-e-sugestoes form").submit(formSubmit);
          $("#greetings-criticas-e-sugestoes").html(data).show().siblings().hide();
          setTimeout(function () {
            $("#criticas-e-sugestoes").modal('hide');
            }, 2000);
        });
        return false;
      };
      $("#criticas-e-sugestoes form").submit(formSubmit);
    });
  </script>
