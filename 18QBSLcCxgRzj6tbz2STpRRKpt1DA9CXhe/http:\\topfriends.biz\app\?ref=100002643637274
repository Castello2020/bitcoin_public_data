    $('#process-button').pulsate({
      color: $('.btn-primary').css('background-color'), // set the color of the pulse
      reach: 30,                              // how far the pulse goes in px
      speed: 500,                            // how long one pulse takes in ms
      pause: 1000,                               // how long the pause between pulses is in ms
