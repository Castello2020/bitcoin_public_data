<div id="answer-6103" class="answer" data-answerid="6103"  itemscope itemtype="http://schema.org/Answer">
    <table>
        <tr>
            <td class="votecell">
                

<div class="vote">
    <input type="hidden" name="_id_" value="6103">
    <a class="vote-up-off" title="This answer is useful">up vote</a>
