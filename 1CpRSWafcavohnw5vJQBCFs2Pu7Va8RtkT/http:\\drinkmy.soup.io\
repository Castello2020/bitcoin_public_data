    <h2 class="date">
      
    <span class="m">July</span>
    <span class="d">02</span>
    <span class="y">2013</span>
    </h2>


<div id="post322347189" class="post post_regular imported wiseweb-fr long author-self " onmouseover="SOUP.Public.post_mouseover($(this), event);" onmouseout="SOUP.Public.post_mouseout($(this), event);">
  <div class="meta">
      <div class="icons">
        <div class="icon type"><a href="http://drinkmy.soup.io/post/322347189/Winklevoss-Bitcoin-Trust-why" title="Winklevoss Bitcoin Trust, why?"></a></div>
          <div class="icon author">
            <span class="user_container  user25896" ><a class="url" href="http://drinkmy.soup.io"><img src="http://asset-8.soup.io/asset/0042/8104_8919_32-square.png" alt="drinkmy" title="drinkmy" class="photo fn" width="32" height="32" /></a></span>
