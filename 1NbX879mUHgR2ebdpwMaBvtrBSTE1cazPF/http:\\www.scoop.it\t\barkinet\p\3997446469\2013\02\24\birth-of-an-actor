<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_1290132").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/1290132?view=json", function(json){
				jQuery("#upv_1290132").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_1290132").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_1290132").append("Not available");
				}
			});
		}
	}
</script>
