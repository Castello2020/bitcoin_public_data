// A wizard to generate this code is at http://www.jottings.com/obfuscator/
{ coded = "Rd9Lc@I9UPc9h.EjR"
  key = "PuNfKBrYCRvqjpZFliM6nVH85gDaoSI0AhdzGE4O71Ls9cbxkym2T3tJQeXWUw"
  shift=coded.length
  link=""
  for (i=0; i<coded.length; i++) {
    if (key.indexOf(coded.charAt(i))==-1) {
      ltr = coded.charAt(i)
      link += (ltr)
    }
    else {     
      ltr = (key.indexOf(coded.charAt(i))-shift+key.length) % key.length
      link += (key.charAt(ltr))
    }
  }
document.write("<a href='mailto:"+link+"'>click email me</a>")
}
