		<object type="application/x-shockwave-flash" data="flashmediaelement.swf">
					<param name="movie" value="flashmediaelement.swf" />
					<param name="flashvars" value="controls=true&file=http://www.podtrac.com/pts/redirect.mp3?http://happyshooting.podzilla.de/hs245_schnittstueck.mp3" />
