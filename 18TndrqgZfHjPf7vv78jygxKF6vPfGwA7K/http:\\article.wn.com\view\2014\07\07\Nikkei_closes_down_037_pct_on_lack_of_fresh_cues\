        $(document).ready(function(){  
			    $('.popup').click(function(){
			     	popup = window.open($(this).attr('href'),'_blank','height=400,width=600, directories=no, status=no, menubar=no, scrollbars=yes, resizable=no,top=200,left=500');
			     	if (window.focus) { popup.focus() }
			     	return false;
			    });
            // switch to the Videos tab and start playing the first video
            jQuery('#main-image-link').on('click', function(e) {
                jQuery('ul#left-nav > li > a.videos').fadeOut('slow').fadeIn('slow');
                jQuery('ul#left-nav > li > a.videos').click();
                // a somewhat aproximate way of playing the default playlist under the "Videos" left menu link
                window.videoplayers[1].playVideo();
            });
        });
		jQuery(function(){
    			var title_count = jQuery('.title_size').text().length;
    			if (title_count > 90)  {
        			jQuery('.title_size').css('font-size', '28px');
    			}
		
		});
