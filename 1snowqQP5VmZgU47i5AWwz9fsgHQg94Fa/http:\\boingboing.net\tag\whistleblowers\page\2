        $(document).ready(function(){
            if($('#ad_footer').height() > 50){
                $('#ad_footer').css('margin-bottom', '20px');
                $('#ad_footer').css('margin-top', '20px');
                $('#ad_footer_label').show();
            }
        });
    </script>
  </div>

  </div><!-- end #posts-loop -->





</div>
</div>

