        var hmdCurrentBrowserUrl = window.location.href;
        if (hmdCurrentBrowserUrl.indexOf("?login") != -1)
            $('[id$=\"hrefDynamicLogin\"]').click();
        else if (hmdCurrentBrowserUrl.indexOf("?register") != -1)
            $('[id$=\"hrefDynamicRegister\"]').click();
