                            $('.thumb-preview').show('fast');
                        });
                        $('a.thumb-preview').click(function(){
                            $('.list-preview').show('fast');
                            $('.thumb-preview').hide('fast');
                        });
                        $('#playlist_clear2').click(function(){
                            $(this).hide('fast');
                            $('#playlist_restore2').show('fast');
                         });
                        $('#playlist_restore2').click(function(){
                            $('.playlist_clear2').show('fast');
                            $(this).hide('fast');
                        });
                        $('#sortby2').hide();
                        $('a#open_sortby2').click(function(){
                            $('#sortby2').show('slow');
                            $('#open_sortby2').hide('fast');
                            $('#close_sortby2').show('fast');
                        });
                        $('a#close_sortby2').click(function(){
                            $('#sortby2').hide('slow');
                            $('#open_sortby2').show('fast');
                            $('#close_sortby2').hide('fast');
                        });
						jQuery(document).keyup(function(e) {
			            if (e.keyCode == 27) {    			
							jQuery('#home2').click();	
			                return false;
			            	}
			        	});	
                });
                // -->
                </script>





	
