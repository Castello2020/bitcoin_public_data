        function initMORE(){
            var theForm = jQuery('#more_202281211');
            theForm.submit(function(event) {
            event.preventDefault();
            var id = jQuery("#id_202281211").val();
            jQuery.ajax({
             url: '/api/upge/worldnews/more',
             data: { id: id },
                     success: function(text) {
                    jQuery('div#search-loading').addClass('show');
                    jQuery('div#search-loading').removeClass('hide');
                    jQuery('#more_box_202281211').append(jQuery(text));

                },
             complete: function() {
                    jQuery('div#search-loading').addClass('hide');
                    jQuery('div#search-loading').removeClass('show');
                    jQuery('.submit_202281211').addClass('hide');
                }
            });
            });
            var theForm = jQuery('#more_202286143');
            theForm.submit(function(event) {
            event.preventDefault();
