    function fill_city_weather(){};
    if(window.location.hostname == 'upgenocache.wn.com'){
        Raven.config('http://42f400e816ee4a56a823bb8113c03f52@sentry2.wn.com/7');
    }else{
        Raven.config('http://865219b3648349058c6c7fe5974fa7b8@sentry2.wn.com/2');
