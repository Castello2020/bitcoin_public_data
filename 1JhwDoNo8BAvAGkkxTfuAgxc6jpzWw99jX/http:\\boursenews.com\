                        $('.signup-box').html('<a onclick="return login_and_save_playlist(\'http://editor.wn.com/save_playlist/bourse_news/\', this.href)" rel="nofollow" class="sign_up" href="http://editor.wn.com/fork/wn/bourse_news/"><span data-trans="Save playlist">Save playlist</span></a>');
