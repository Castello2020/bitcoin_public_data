	<script type="text/javascript">
		WPCOM_sharing_counts = {"http:\/\/cavnews.wordpress.com\/2013\/01\/28\/illegal-immigrants-can-now-apply-for-drivers-licenses-in-illinois\/":3878}	</script>
		<script type="text/javascript">
		jQuery(document).on( 'ready post-load', function(){
