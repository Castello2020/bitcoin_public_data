<script type="text/javascript">
var divnodes = document.getElementsByTagName("div");
var i, j, anode, oldimgnode;
for(j in divnodes) {
    if('post_body' == divnodes[j].className) {
        var imgnodes = divnodes[j].getElementsByTagName("img");
        for(i in imgnodes)
            if(null != imgnodes[i]
                    && parseInt(i) == i
                    &&'A' != imgnodes[i].parentNode.tagName
                    && null != imgnodes[i].src) {
                anode = document.createElement('a')
                anode.href = imgnodes[i].src;
                oldimgnode = imgnodes[i].parentNode.replaceChild(anode, imgnodes[i]);
                anode.appendChild(oldimgnode);
            }
    }
}
</script>
