<script type="text/javascript">
    window.SuperStorage = {};
    window.SuperStorage.storage = {
        save: function (key, jsonData, expirationMin) {
            if (!Modernizr.localstorage) {
                return false;
            }
            var expirationMS = expirationMin * 60 * 1000;
            var record = { value: JSON.stringify(jsonData), timestamp: new Date().getTime() + expirationMS };
            localStorage.setItem(key, JSON.stringify(record));
            return jsonData;
        },
        load: function (key) {
            if (!Modernizr.localstorage) {
                return false;
            }
            var record = JSON.parse(localStorage.getItem(key));
            if (!record) {
                return false;
            }
            return (new Date().getTime() < record.timestamp && JSON.parse(record.value));
        }
    };
    $('#chk_wrn_adb').live('click', function (elm) {
        window.SuperStorage.storage.save('noAdpWarn', 'true', 43829);
        $('#bloc_pub_header').remove();
    });
    function checkload(elm, jqImg) {
        if (!elm.complete || typeof elm.naturalWidth == "undefined" || elm.naturalWidth < 5) {
            var adImg = $("<img />").attr('src', "http://static.pcinpact.com/message-information.png").load(function () {
                showWarning(adImg);
            });
        }
    }

    function showWarning(img) {

        if (window.SuperStorage.storage.load('noAdpWarn') === 'true')
            return;
        var lnkMsg = $('<a></a>').attr('href', 'http://www.nextinpact.com/news/79467-comment-desactiver-adblock-sur-pc-inpact-et-nous-placer-en-liste-blanche.htm');
        lnkMsg.append(img);
        $('#bloc_pub_header_content').html(lnkMsg);
        var wrapper = $('<div> </div>').attr('id', 'wrapper_warn_adp').attr('style', ' position: relative; top: -112px;left: 433px;');

        var imgMasquer = $('<img />').attr('src', "http://static.pcinpact.com/masquer-info-pub-txt.png");
        var lblAdpb = $('<label for="chk_wrn_adb" id="lbl_warn_adb"></label>').html(imgMasquer);
        wrapper.append(lblAdpb);
        var chkAdbp = $('<input type="checkbox" id="chk_wrn_adb" name="chk_wrn_adb" />');
        wrapper.append(chkAdbp);
        $('#bloc_pub_header_content').append(wrapper);
        $("#chk_wrn_adb").uniform();
    }
    (function ($) {
        return;
        try {
            var img = $("<img />").attr('src', "http://pcinpact.blob.core.windows.net/kvgc3/t.png");

           
            img.load(function () {
                checkload(this, img);
            })
            .error(function () {
                checkload(this, img);
            });



        } catch (e) {
            checkload(this, null);
        }


    })(window.jQuery);
</script>


