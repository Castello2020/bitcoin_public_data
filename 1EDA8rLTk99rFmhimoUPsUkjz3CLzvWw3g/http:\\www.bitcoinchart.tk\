<div class='widget-content'>
<script src="https://www.google.com/jsapi"></script>
<script>

$(document).ready(function() {


var json_cpuntry = google.loader.ClientLocation;

if (json_cpuntry) {

var user_country = json_cpuntry.address.country_code;


$.cookie("country", user_country, { expires: 365,path: "/"})

} else if($.cookie("country")) { user_country = $.cookie("country"); } else {

var ajaxfollow = {
		type: 'GET',	
		url: "http://freegeoip.net/json/",
		dataType: 'json',
		success: function (data) { 
		
			user_country = data.country_code;
			$.cookie("country", user_country, { expires: 365,path: "/"})
			
			if (user_country == "US" || user_country == "CA") {	 // USA es: US y canada es CA
			$("#boxpublix").html("<a target=\"_blank\" href=\"http://signaltrader.go2cloud.org/aff_c?offer_id=2&aff_id=1106\"><div id='bannerxx'  style=\"margin: 0 auto;width: 500px;height:32px;text-align: center;background-image:url('http://1.bp.blogspot.com/-hOlyuxUmjYs/UrGGvM7msFI/AAAAAAAAABQ/-0Vm5uvD2VY/s1600/proba.gif');background-repeat:no-repeat;background-position:center; display: table;\"></div></a>").show();
			}

   
		} ,
		 error   : function(data) { System.console(JSON.stringify(data)); } 
		};
 
		$.ajax(ajaxfollow);

}


if (user_country == "US" || user_country == "CA") {	 // USA es: US y canada es CA
	$("#boxpublix").html("<a target=\"_blank\" href=\"http://signaltrader.go2cloud.org/aff_c?offer_id=2&aff_id=1106\"><div id='bannerxx'  style=\"margin: 0 auto;width: 500px;height:32px;text-align: center;background-image:url('http://1.bp.blogspot.com/-hOlyuxUmjYs/UrGGvM7msFI/AAAAAAAAABQ/-0Vm5uvD2VY/s1600/proba.gif');background-repeat:no-repeat;background-position:center; display: table;\"></div></a>").show();

} else {
$("#boxpublix").html("<a target=\"_blank\" href=\"http://www.avatrade.com/lp/trade-bitcoin?tag=40387&tag2=~banner_7733\"><div id='bannerxx'  style=\"margin: 0 auto;width: 500px;height:32px;text-align: center;background-image:url('http://4.bp.blogspot.com/-LHll-nf7wN4/UrGvrPW7rOI/AAAAAAAAABg/dFbYtEPGZSo/s1600/probaava.gif');background-repeat:no-repeat;background-position:center; display: table;\"></div></a>").show();
}




$(".openlogin").toggle(function() {

$("#donate-pop").removeClass('donate-popdown').addClass('donate-popup');
        	
}, function(){
    
$("#donate-pop").removeClass('donate-popup').addClass('donate-popdown');
}
);

var donation_stat = 0; 
$("#donatex").click(function() {  
	if (donation_stat == 0) { 
		$("#donate-pop").removeClass('donate-popdown').addClass('donate-popup');
		donation_stat = 1; 
 	} else { 
		$("#donate-pop").removeClass('donate-popup').addClass('donate-popdown');
     	donation_stat = 0; 
      }
});

});
</script>
</div>
</div></div>
