<script type='text/javascript'>setTimeout(function(){
    hljs.configure({useBR: true});
    $('div.codeBox').each(function(){
        var codebox = $(this);
        codebox.find('li').each(function(){
            var line = $(this).text(),
                hl = hljs.highlightAuto(line).value;
            $(this).text('');
            $(this).html(hl);
        });
    });
}, 500);</script>
