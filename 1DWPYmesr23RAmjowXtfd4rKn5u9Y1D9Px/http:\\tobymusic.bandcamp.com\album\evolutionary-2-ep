$(document).ready(function() {

    if ( window.LogView ) {
        LogView.show();
    }

    PopupImage.attach( "a.popupImage" );

    if ( window.Cart ){
        Cart.startup();
    }
    if ( window.AutoLyrics ) {
        AutoLyrics.init("_lyrics_", "lyrics_div_", "lyrics_row_", TralbumData.trackinfo.length, 1 );
    }
    if ( window.Player ) {
        Player.init(TralbumData, AlbumPage.registerPlaylistListeners);
    }
    if ( window.TralbumDownload ) {
        TralbumDownload.init();
        
        $(".download-link").click(function(ev) {
            ev.preventDefault();
            TralbumDownload.begin(ev);
        });
        
        $(".send-as-gift > .download-link").unbind("click").click(function(ev) {
            ev.preventDefault();
            TralbumDownload.begin(ev, true);
        });
    }
    if ( window.TralbumUpdater ) {
        TralbumUpdater.init();
    }
    if ( window.PackageOrder ) {
        PackageOrder.init();
        
        $(".order_package_link").click(function(ev){
            ev.preventDefault();
            // PackageOrder.begin takes two params: the package index and an isGift boolean. previously it only took a 
            // package index, but this funtion call was passing event as well. if you need the event in begin(), then add
            // it as an extra param.
            // dh 12-02-2013
            PackageOrder.begin($(this).attr('data-pkg'));
        });
        
        $(".send-as-gift > .order_package_link").unbind("click").click(function(ev){
            ev.preventDefault();
            PackageOrder.begin($(this).attr('data-pkg'), true);
        });
    }
    
    Y.util.Event.on("buy-disc-link", "click", function() {Stats.record({kind:"click", click:"buy_disc"}); return true;});
    Y.util.Event.on("rssFeedAlbum", "click", function() {Stats.record({kind:"click", click:"rss_feed"}); return true;});
    Y.util.Event.on("rssFeedArtist", "click", function() {Stats.record({kind:"click", click:"rss_feed"}); return true;});

    if ( window.AlbumPage ) {
        AlbumPage.registerMouseOverForTrackRows();
    }
    
    
    
    if ( window.TralbumFans ){
        TralbumFans.initialize([{"gift_type":null,"fan_id":62695,"grid":null,"featured_track_id":null,"why":null,"url":"http://bandcamp.com/endho","name":"endho","username":"endho","item_type":"a","image_id":1491057,"thumb":null,"purchase_date":"15 Apr 2013 22:26:10 GMT","item_id":2873915444},{"gift_type":null,"fan_id":5252,"grid":null,"featured_track_id":null,"why":null,"url":"http://bandcamp.com/pej","name":"pej","username":"pej","item_type":"a","image_id":1140582,"thumb":null,"purchase_date":"21 Feb 2013 22:25:30 GMT","item_id":2873915444},{"gift_type":null,"fan_id":164027,"grid":null,"featured_track_id":null,"why":null,"url":"http://bandcamp.com/peterschumacher","name":"Peter Schumacher","username":"peterschumacher","item_type":"p","image_id":2324087,"thumb":null,"purchase_date":"09 Dec 2012 19:42:21 GMT","item_id":1896154236},{"gift_type":null,"fan_id":1303,"grid":null,"featured_track_id":4205222994,"why":null,"url":"http://bandcamp.com/supahbuldozer","name":"Michael Court","username":"supahbuldozer","item_type":"p","image_id":830488,"thumb":null,"purchase_date":"23 Nov 2012 14:56:36 GMT","item_id":1896154236},{"gift_type":null,"fan_id":22561,"grid":null,"featured_track_id":null,"why":null,"url":"http://bandcamp.com/hazey82","name":"DJ Hazey 82","username":"hazey82","item_type":"a","image_id":1066913,"thumb":null,"purchase_date":"17 Oct 2012 03:36:48 GMT","item_id":2873915444}], null, null, null, null);
    }
    
    if( window.Trackpipe ) {
        Trackpipe.maybeDoTralbumActions();
    }

    
    if ( window.BandFollow )
        BandFollow.init({"tralbum_id":2873915444,"tralbum_type":"a"});
    



    
    if ( window.Share )
        Share.initPanel(false, 1, "http://tobymusic.bandcamp.com/album/evolutionary-2-ep");


    if ( window.FanControls ) {
        FanControls.init({"show_collect":true});
    }



});

</script>



