			if (resp === 'true') {
				//console.log('fbComments:     Updated and cached Facebook comment count for post with xid=_post401');
			} else {
				//console.log('fbComments:     FAILED to update Facebook comment count for post with xid=_post401');
			}
		});

	};

	FB.Event.subscribe('comments.add', addedComment);
</script>

