		//console.log('fbComments:     Making AJAX call to update Facebook comment count');
		$.post('http://englishrussia.com/wp-content/plugins/facebook-comments-for-wordpress/facebook-comments-ajax.php', { fn: 'addComment', xid: '_post659' }, function(resp) {
			if (resp === 'true') {
				//console.log('fbComments:     Updated and cached Facebook comment count for post with xid=_post659');
			} else {
				//console.log('fbComments:     FAILED to update Facebook comment count for post with xid=_post659');
			}
		});

	};

	FB.Event.subscribe('comments.add', addedComment);
</script>

