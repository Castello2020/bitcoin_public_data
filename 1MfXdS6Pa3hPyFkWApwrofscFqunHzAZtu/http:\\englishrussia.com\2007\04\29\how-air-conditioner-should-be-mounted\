		$.post('http://englishrussia.com/wp-content/plugins/facebook-comments-for-wordpress/facebook-comments-ajax.php', { fn: 'addComment', xid: '_post872' }, function(resp) {
			if (resp === 'true') {
				//console.log('fbComments:     Updated and cached Facebook comment count for post with xid=_post872');
			} else {
				//console.log('fbComments:     FAILED to update Facebook comment count for post with xid=_post872');
			}
		});

	};

	FB.Event.subscribe('comments.add', addedComment);
</script>

