<script type="text/javascript">
       $(document).ready(function() {
            if( $.cookie('showOnlyOne')>1000 ){
                //it is still within the day
                //hide the div
                $('#shownOnlyOnceADay').hide();
            } else {
                //either cookie already expired, or user never visit the site
                //create the cookie
		if ($.cookie('showOnlyOne')) { var cookieVal = parseInt($.cookie('showOnlyOne'))+1;} else 
		var cookieVal = 1;
                $.cookie('showOnlyOne', cookieVal, { expires: 1, path: '/' });

                //and display the div

		        
            }
        });
        


