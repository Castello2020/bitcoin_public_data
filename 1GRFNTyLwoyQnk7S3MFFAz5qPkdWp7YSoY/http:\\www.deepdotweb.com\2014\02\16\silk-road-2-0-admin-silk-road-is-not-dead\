            .JPV_thumbnail > .JPV_thumbnail_img {
                max-width: 125px;
                height: auto;
            }

            .JPV_thumbnail + .JPV_text {
                width: 45%;
                padding-bottom: 10px;
                text-align: center;
                margin-top: -5px;
            }

            .JPV_thumbnail_title {
                float: left;
                width: 21.276596%;
            }

            .JPV_thumbnail_title > .JPV_thumbnail_img {
                max-width: 40px;
                height: auto;
            }

            .JPV_thumbnail_title + .JPV_text {
                float: right;
                width: 73.404255%;
                padding-bottom: 10px;
