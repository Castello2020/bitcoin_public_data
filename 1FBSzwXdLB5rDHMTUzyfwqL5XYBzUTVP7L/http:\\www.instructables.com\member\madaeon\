head.load(
    "/static/js/jqplugins/fancybox/source/jquery.fancybox.pack.js",
    "/static/js/fancybox-photoset.js",
    function(){
        $('.read-more').click(function(){
            if ($(this).hasClass('readless')) {
                // return to "read more" mode
                $(this).removeClass('readless');
                $('.member-details').css({height:'176px'});
                $(this).html('read more &raquo;');
            } else {
                $(this).addClass('readless');
                $('.member-details').css({height:'auto'});
                $(this).html('&laquo; less');
            }
        });

        // force login for non-logged-in-types -- the follow button is taken care of in instruct_jq.js
        $('#messageButton').add('#patchButton').click(function(e){
            if (true) {
                e.preventDefault();
                navoperator.makeLoginByJquery();
            }
        });

        // handle the slider for the little profile images moving around inside their container, if necessary:
        $('#showMoreProfileImages').click(function () {
            var futureSlidePosition =  parseInt($('#member_photo_slider_tray').css('marginLeft')) - 564 ;
            var traysize = parseInt($('#member_photo_slider_tray').css('width'));
            if ( Math.abs(futureSlidePosition) > traysize ) {
                // return to beginning
                $('#member_photo_slider_tray').animate({'margin-left':'0px'});
                $('#showMoreProfileImages').html("&raquo");
            } else {
                $('#member_photo_slider_tray').animate({'margin-left':'-=564px'},function(){
                    // if we arrive at the last tray, turn the arrow leftwards:
                    if ( Math.abs(parseInt($('#member_photo_slider_tray').css('marginLeft')) - 564) > traysize ) {
                        $('#showMoreProfileImages').html('&laquo');
                    }
                });
            }
        });

        // enable clicking on fancybox-image to open image url
        $("#image_action a").fancybox({wrapCSS: 'photoset-fancybox member-photos'});
        $(".photoset-fancybox .fancybox-image").live('click', function(){
          var href = $($.fancybox.group[$.fancybox.current.index]).attr('href');
          if (href) {
            window.location.href = href;
          }
        });        
    }
);
</script></body>
