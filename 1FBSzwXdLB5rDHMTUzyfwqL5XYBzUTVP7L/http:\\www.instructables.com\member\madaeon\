    (function($) {
      var onMenu = false;
      var onButton = false;
      var onMenuContent = false;
      
      function clearMenus() {
        $(".header-nav").each(function(){
          var navElem = $(this),
              menuElem = $('#'+this.id+'-menu');
          navElem.removeClass('selected');
          menuElem.hide();
        });
      }

      function getMenuContent(menuContainer, navId) {
        menuContainer.innerHTML = '<div class="menu-load">Loading...</div>';
        $.ajax("/dynjsp/nav_"+navId+".jsp", {
          success: function(data, textStatus, jqXHR) {
            menuContainer.html(data);
          },
          error: function() {
            menuContainer.html('<div class="menu-load-error">An error occurred while loading this content.</div>');
          }
        });        
      }             
      
      $(function() {
          $(".header-nav").each(function() {
              (function(that) {
                  var navElem = $(that);
                  var menuTop = navElem.height()+1;
                  var menuLeft = navElem.position().left;
                  var menuElem = $("#"+that.id+"-menu");
                  var loaded = (that.id == "create" || that.id == "community");
                  var onThisButton = false;
                  var onThisMenuContent = false;
                  var delay = 10;
                                    
                  navElem.on('click.nav-menu', function(e){
                    if (!onMenu) {
                      e.preventDefault();                    
                      clearMenus();
                      if (!loaded) {
                        loaded = true;
                        getMenuContent(menuElem, that.id);                      
                      }
                      if (!menuElem.is(":visible")) {
                        menuElem.css({top: menuTop+'px', left: menuLeft+'px'});
                        navElem.addClass('selected');
                        menuElem.show();
                        onThisButton = true;
                        onButton = true;
                        onMenu = true;
                      }
                      return false;
                    }
                  });
                  
                  navElem.hover(
                    function() {
                      onThisButton = true;         
                      onButton = true;             
                      if (onMenu) {
                        if (!loaded) {
                          loaded = true;
                          getMenuContent(menuElem, that.id);
                        }
                        setTimeout(function() {
                          if (onThisButton) {
                            menuElem.css({top: menuTop+'px', left: menuLeft+'px'});
                            navElem.addClass('selected');
                            menuElem.show();
                            onMenu = true;
                          }
                        }, delay);
                      }
                    },
                    function() {
                      onThisButton = false;
                      onButton = false;
                      setTimeout(function() {
                        if(!onThisMenuContent) {
                            navElem.removeClass('selected');
                            menuElem.hide();
                        }
                      }, delay);
                      
                      setTimeout(function() {
                        if (!onButton && !onMenuContent) {
                          onMenu = false; 
                        }
                      }, delay+2000);
                    }
                  );
                
                  menuElem.hover(
                    function() {
                        onMenu = true;
                        onThisMenuContent = true;
                        onMenuContent = true;
                        navElem.addClass('selected');
                    },
                    function() {
                        onThisMenuContent = false;
                        onMenuContent = false;
                        setTimeout(function() {
                            if (!onThisButton && !onThisMenuContent) {
                                menuElem.hide();
                                navElem.removeClass('selected');
                            }
                        }, delay);
                        
                        setTimeout(function() {
                          if (!onButton && !onMenuContent) {
                            onMenu = false;
                          }
                        }, delay+2000);                        
                    }
                  );                  
              })(this);
          });
      });
    })(jQuery);

    function makeHelpBubbleLoginHTML(loginContinueParams, doRedirectLocation, source, fb) {
    
        var loginHTML = '<form method="post" action="/account/login?' + loginContinueParams + '">';
        loginHTML += '<input id="RememberME_hb" type="hidden" name="RememberME" />';
        loginHTML += '<table style="margin-top:5px;">';
        loginHTML += '<tr style="margin-top:5px;"><td>Username </td>';
        loginHTML += '<td><input id="username_hb" type="text" style="width:100px;" name="u"/></td></tr>';
        loginHTML += '<tr><td>Password </td>';
        loginHTML += '<td><input id="password_hb" type="password" style="width:67px;" name="p"/>';
        loginHTML += '<input class="ml5" class="button" type="submit" value="go" name="Submit"/></td></tr>';
        loginHTML += '<tr><td colspan="2" class="center">';
        loginHTML += '<a class="fs-1" href="/account/forgot?' + loginContinueParams + '">(forgot username/password?)</a></td></tr>';
        if (typeof fb === "undefined" || fb !== false) {
            loginHTML += '<tr><td colspan="2" class="center"> OR </td></tr>';
            loginHTML += '<tr><td colspan="2" class="center">';
            if (typeof doRedirectLocation === "undefined" || doRedirectLocation === "") {
                loginHTML += '<fb:login-button size="medium" scope="email,user_birthday,publish_stream" onlogin="doRedirect(\'' + loginContinueParams + '\', \'' + source + '\');">';
            } else {
                loginHTML += '<fb:login-button size="medium" scope="email,user_birthday,publish_stream" onlogin="doRedirect(\'' + doRedirectLocation + '\', \'' + source + '\');">';
            }
            loginHTML += 'Log In with Facebook</fb:login-button></td></tr>';
        }
        loginHTML += '</table></form>';

        return loginHTML;
    
    }

    function focusOnHelpBubbleLogin() {
        if (typeof FB !== "undefined") {
            FB.XFBML.parse(document.getElementById("login5"));
        }
        setTimeout(function() {
            $( "username_hb" ).focus();
        }, 10 );
    }
</script><div id="feedback" class="feedback" style="display:none;"></div><script type="text/javascript">
      $(function(){            
          var feedback = {};
          if (feedback.messages || feedback.validation) {
            head.js("/static/js/feedback.jq.2.js", function() {       
              feedBack.addFromJSON(feedback, 'feedback');
            });
          }   
      });       
    </script><div id="omni" class="clearfix"><div class="container"><div class="header-bar"><div class="container"><div id="member-profile-statbar"><h1 id="member-profile-name" title="madaeon">madaeon</h1><ul><li><img src="/static/img/profile/joined.png" alt="Date Joined" title="Date Joined" />Mar 22, 2009</li><li><img src="/static/img/profile/totalViews.png" alt="Total Views" title="Total Views" />110,726</li><li><strong><img src="/static/img/profile/featured.png" alt="Featured" title="Featured" /> %</strong>28%</li></ul></div></div></div><div id="member-profile-fulltitle" class="collapsed"><div class="user-image"><img src="http://graph.facebook.com/1609113306/picture?type=large" /></div><div class="member-details"><div class="member-profile-buttons"><div class="button-zrow"><a id="follow-btn" class="btn btn-yellow followbtn" data-action="follow" data-followid="MZ94MG6FSLF8122" data-flag="0" data-dotxt="Follow" data-donetxt="Following" data-undotxt="Unfollow"><i class="bg-icon plus active"></i>&nbsp; <span>Follow</span></a><span class="callout">55</span></div><div class="button-row"><a id="messageButton" class="btn" tile="Private Message" href="/edit/compose?recipientId=MZ94MG6FSLF8122&linkTo=/member/madaeon/"><i class="bg-icon email"></i>Private Message</a></div><div class="button-row"><a href="/patch/send?recipient=madaeon&action=edit" title="Send Patch" class="btn" id="patchButton"><i class="bg-icon patch"></i>Send Patch</a></div></div><div class="member-profile-content"><div class="member_sidebar_div_9"><strong>About:</strong>Support this instructable! If clicking on the line above does not work, use this info: Pay to:  1FBSzwXdLB5rDHMTUzyfwqL5XYBzUTVP7L Message: Support the author: donate with Bitcoin!</div><div class="member_sidebar_div_11"><strong>Location:</strong>Italy</div><div class="member_sidebar_div_11"><strong>Joined:</strong>Mar 22, 2009</div><div class="member_sidebar_div_14"><strong>Age</strong>:
