	<script>
	
	function DropDown(el) {
		this.dd = el;
		this.initEvents();
	}
	DropDown.prototype = {
		initEvents : function() {
			var obj = this;

			obj.dd.on('click', function(event){
				$(this).toggleClass('active');
				event.stopPropagation();
			});	
		}
	}
	
	$(document).ready(function(){
			var totalcost = 0.00;
			function calculate() {
				
				var message = $("#message").val();
				var messagelength = message.length;
				var ppc = 0.00001;
				var exchange = 620.112005;
				//$("#message").attr('maxlength');
				$("#count").html(messagelength);
				totalcost = messagelength * ppc;
				totalcost = totalcost.toFixed(5);
				$("#totalcost").html(totalcost);
				usd = exchange*totalcost;
				usd = usd.toFixed(2);
				$("#usd").html(usd);
			}
			
			$('#message').keyup(function(){
				calculate();
			});
			
			/*
			$('#sortby').change(function() {
			  // set the window's location property to the value of the option the user has selected
			  window.location = $(this).val();
			});
			*/
			
			var dd = new DropDown( $('#dd') );

			$(document).click(function() {
				// all dropdowns
				$('.wrapper-dropdown-5').removeClass('active');
			});
			
			
						$('.popup-modal').magnificPopup({
				type: 'inline',
				preloader: false,
				focus: '#username'
			});
			$(document).on('click', '.popup-modal-dismiss', function (e) {
				e.preventDefault();
				$.magnificPopup.close();
			});
			$(".toggle-qr").click(function(e){
				e.preventDefault();
				$(".qr-code").toggleClass('hide');
			});
			$(".meter > span").each(function() {
				$(this)
					.data("origWidth", $(this).width())
					.width(0)
					.animate({
						width: $(this).data("origWidth")
					}, 600);
			});
			
			
						
			
						
			
							

				//var cookie = false;
				var cookieValue = $.cookie("bubbleclose2");
				
				if (cookieValue == 'yes') {
					//do nothing (keep it hidden)
				} else {
					//show the cookie
					$('.whatisthis').removeClass('hide');
				}
				
				//Hard code to show cookie for testing
				//$('.whatisthis').removeClass('hide');
			
				$("#alert-close").click(function(e){
					e.preventDefault();
					//Set the cookie
					$.cookie('bubbleclose2', 'yes', { expires: 60, path: '/' });
					//Hide the bubble
					$('.whatisthis').addClass('hide');
				});
			
						
						
			
		});
		
	</script>

