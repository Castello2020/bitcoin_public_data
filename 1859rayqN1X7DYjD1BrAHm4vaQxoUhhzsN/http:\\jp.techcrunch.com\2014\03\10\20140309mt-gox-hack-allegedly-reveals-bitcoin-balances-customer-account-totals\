        jQuery(document).ready(function($) {

            $("img[data-src]").bind("scrollin", function(){

                var img = this,
                    $img = $(img);

                $img.unbind("scrollin"); // clean up binding
                img.src = $img.attr( "data-src" );

            });

            //
            // PAGE-SPECIFIC SCRIPT HERE
