  $('#comment_message').focus(function() {
    _form = $(this).closest('.comment_form')
    if($('body').hasClass('signed_in')) {
      _form.html("");
      _form.spin();
      $.ajax({
        url: '/bitcoin/pages/18819012-bitdazzle-the-largest-bitcoin-marketplace-to-date/comments/new',
        success: function(data) {
          _form.replaceWith(data);
        },
        error: function(data) {
          location.reload();
        }
      });
    } else {
      _form.html("Please <a href=\"/users/sign_in?redirect_to=http%3A%2F%2Fopenruby.com%2Fbitcoin%2Fpages%2F18819012-bitdazzle-the-largest-bitcoin-marketplace-to-date\">sign in<\/a> or <a href=\"/users/sign_up\">sign up<\/a> to comment.");
