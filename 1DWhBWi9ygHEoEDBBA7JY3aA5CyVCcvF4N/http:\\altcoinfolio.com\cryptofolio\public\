  <script>

  var ref = new Firebase("https://publicdata-bitcoin.firebaseio.com/");
  ref.child("bid").on("value", showBidPrice);
  ref.child("ask").on("value", showAskPrice);

  function showBidPrice(snapshot) {
    console.log(snapshot.name() + ": " + snapshot.val());
    $(".get-BTC").html(snapshot.val());
  }

  function showAskPrice(snapshot) {
    console.log(snapshot.name() + ": " + snapshot.val());    
  }

  $(function(){
    $("ul#ticker01").liScroll();
  });

  function updateCurrentValuesFromJSON() {
    console.log("Hello from interval timer coinprices.js");
    $.getJSON('/cryptofolio/public/js/coins.json', function(data) {
          var output="<ul>";
          var str;
          for (var i in data.coins) {
              str = 'get-' + data.coins[i].sym.toUpperCase();
              $("td." + str).html(data.coins[i].price); //  $("td.get-DOGE").html(data.coins[i].price;
              $("a#" + str).html(data.coins[i].price); //  $("td.get-DOGE").html(data.coins[i].price;

              //<li><span>Bitcoin [BTC]</span><a class="curVal get-BTC" href="#" style="color:red;"> 462.71</a> <a class="trend-BTC" style="color:red;">-3.01 %</a></li>

             // output+=str + '"' + " " + data.coins[i].coinname + " -- " + data.coins[i].price+ " -- " + data.coins[i].delta +" ...  ";
             output+="'"+data.coins[i].sym.toUpperCase()+"' => '"+data.coins[i].sym.toUpperCase()+" - "+data.coins[i].coinname+"',";

          }
          console.log ("uncomment out the next line to see coins.json output");
          // console.log (output);
    });
  }

  function updateShowEditValues() {

    $("div.show").each(function () {
       var amtOwned = $(this).find(".holdingVal").html();
       var paidPerCoin = $(this).find(".paidVal").html();
       var curCoinValue = $(this).find(".curVal").html();
       var totalPaidForAsset = (amtOwned * 1) * (paidPerCoin * 1);
       var assetsCurrentWorth = (amtOwned * 1) * (curCoinValue * 1);
       $(".assetInvestedVal").html(totalPaidForAsset);
       $(".assetCurrentVal").html(assetsCurrentWorth);
       $(".assetNetChg").html(assetsCurrentWorth - totalPaidForAsset);
    });
  }

  function updateAssetTableValues() {

    var mult1 = 0;
    var mult2 = 0;
     // for each row:
     $("tr.oneAsset").each(function () {
       // get the values from this row:
       var amtOwned = $(this).find(".holdingVal").html();
       var paidPerCoin = $(this).find(".paidVal").html();
       var curCoinValue = $(this).find(".curVal").html();
       var totalPaidForAsset = (amtOwned * 1) * (paidPerCoin * 1);
       var assetsCurrentWorth = (amtOwned * 1) * (curCoinValue * 1);
       // set total for the row
       //console.log("total:" + assetsCurrentWorth);
       $('.multTotal', this).text((assetsCurrentWorth-totalPaidForAsset).toFixed(2) + " (" + (100*((curCoinValue/paidPerCoin)-1)).toFixed(2) + "%)");
       mult1 += totalPaidForAsset;
       mult2 += assetsCurrentWorth;
     });


     netchg = mult2 - mult1;
     if (netchg < 1){
      $(".netChg").html("<b>Net Chg: " + netchg.toFixed(2) + " USD (" + (100*((mult2/mult1)-1)).toFixed(2) + "%)</b>").css("color","red");
     } else {
      $(".netChg").html("<b>Net Chg: " + netchg.toFixed(2) + " USD (" + (100*((mult2/mult1)-1)).toFixed(2) + "%)</b>").css("color","green");
     }
     $(".invested").html("<b>Invested: " + mult1.toFixed(2) + " USD</b>");
     $(".investedVal").html(mult1.toFixed(2));
     $(".grandTotal").html("<b>Portfolio Value: " + mult2.toFixed(2) + " USD</b>");
     $(".grandTotalVal").html(mult2.toFixed(2));
     $(".tabTotal").text(mult2.toFixed(2) + " USD");     

  }

////////////////////////////////////////////////////////////////////////////////////////////////////
  'use strict';
  $(document).ready(function () {

    $("#ticker01").css("visibility","visible");
    updateCurrentValuesFromJSON();

    setTimeout(function(){
      updateAssetTableValues();
      updateShowEditValues();
    },1000);

    setInterval(function(){
      updateCurrentValuesFromJSON();
      updateAssetTableValues();
    },15000);

  });
</script>


</body>
</html>
