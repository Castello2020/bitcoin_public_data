                                #wrapper #sidebar #top #buttons div {
                                    padding-bottom: 10px;
                                }
                            
                            #wrapper #sidebar #top #buttons .button#button-rss {
                                background-image: url('http://assets.tumblr.com/themes/redux/button-rss.png?2');
                                text-transform: capitalize;
                            }
                            
