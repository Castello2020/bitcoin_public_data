                        <div id="page-title">
                                <h1 class="single-pagetitle">CloudHashing.com Prepares $20 Million-a-Month Bitcoin Mining Operation</h1>
                        </div>
                        <!-- end page-title --> 
                    </div>
                    <!-- end page-content-header -->
