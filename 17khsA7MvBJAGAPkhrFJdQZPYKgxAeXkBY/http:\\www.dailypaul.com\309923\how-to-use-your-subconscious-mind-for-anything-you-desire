.ftags .blogpost .bp>a{display:block}
.ftags .blogpost .portrait{
 float:left;
 margin-right:5px;
 display:block;
 width:32px;
 height:32px;
 background-size:cover;
 background-position:center center;
}
.ftags .blogpost .headline{font-weight:bold;font-size:12px;}

.ftags .tagtab li>*{
cursor:pointer;
/*display:inline-block;*/
color: white;
border-radius:1em 1em 0 0;
padding: .5em;
font-family: verdana;
/*box-shadow: inset 0 0 1em white;*/
box-shadow: inset 0 0 1em #55A;
background:#006;
font-weight: bold;
font-size: 11px;
}

.ftags .view-content{
 border:solid #999 1px;
 border-width:1px 1px 0px 1px;
}
.ftags .view-footer{
 border:solid #999 1px;
 border-width:0px 1px 1px 1px;
}
.ftags .tagtab{
 padding:0px;
 font-size:0px;
/* background: #009;*/
}

.ftags .tagtab li{
 opacity:.5;
/* display:inline;*/
 float:left;
}

.ftags .tagtab li:hover{
opacity:.75
}

.ftags .tagtab li.active{opacity:1;}

.ftags .pgr .aro{
 font-size:1.75em;
 display:inline-block;
 cursor:pointer;
}

.ftags .pgr .aro.disabled{
 color:gray;
}

.ftags .pgr .aro:hover{
 color:#900;
}

.ftags .view-content .waity{position:absolute;}
.ftags .view-content{position:relative;}

</style>
