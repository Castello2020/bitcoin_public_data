var blk=$("#block-views-Features-block_1")
   ,nodesPerPg=5
   ,pgr=blk.find(".pgr:first").empty().attr({pg:0}).append(
	 $("<div class='aro prev disabled' title=previous />").html(String.fromCharCode(9664))
	,$("<div class='aro next' title=next />").html(String.fromCharCode(9654))
   )
   ,touched=0
pgr.find(".aro").click(function(e,auto){
	if(auto!="auto") touched=1
	var me=$(this)
	if(!me.hasClass("disabled")){
		var pg=Math.max(0,pgr.attr("pg")*1+(me.hasClass("next")?1:-1))
		pgr.find(".aro").addClass("disabled")
		$("<div class=waity />").css({display:"inline-block"}).insertAfter(me.hide())
		$.getJSON("/json/features",{page:pg},function(r){
			r=r.nodes
			var rl=r.length
			//if @ end, loop pg back to beginning
			if(!rl || rl<nodesPerPg) pg=-1
			
			pgr.attr({pg:pg})
			var content=blk.find(".view-content:first")//.empty()
				,t=.01

			for(var i in r) +function(i){
				var n=r[i].node
					,nu=$("<a />").addClass("featured ub uid"+n.uid).attr({href:"/"+n.nid}).append(
						 $("<div class=portrait />").css({backgroundImage:"url('http://images.weserv.nl/?url="+n.portrait+"&w=32&h=32&t=square')"})
						,$("<h3 class=title />").html(n.title)
						,$("<span class=author />").html(n.name)
					).css({opacity:0})
				setTimeout(function(){
					var ol=content.children(":eq("+i+")")
					if(ol.length) ol.animate({opacity:0},t,function(){
						nu.insertAfter(this).animate({opacity:1},t)
						$(this).remove()
					})
					else nu.appendTo(content).animate({opacity:1},t)
				},i*t/2)
					
			}(i)
			
			pgr.find(".aro").removeClass("disabled")
			if(pg<=0) pgr.find(".aro.prev").addClass("disabled")
			pgr.find(".waity:first").remove()
			me.show()
		})
	}
})

}()</script>    </div>
  
  
</div> </div>
 </div>
  <div class="block block-block" id="block-block-19">
