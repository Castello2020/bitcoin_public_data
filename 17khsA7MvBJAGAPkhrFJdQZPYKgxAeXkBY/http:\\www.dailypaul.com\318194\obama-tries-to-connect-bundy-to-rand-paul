window.playYT=function(x) {
        var me = $(x),
            id = me.attr("id") ? me.attr("id") : me.css("background-image").replace(/^.*vi\/([\w-]{11})\/.*$/, "$1"),
            wide = me.hasClass("wide"),
            sp = id.split("#t"),
            t = ""
        if (sp.length > 1) {
            id = sp[0]
            t = "&start=" + eval(
            sp[1].replace(/=((\d*)h)?((\d*)m)?((\d*)s)?/i, "(($2-0)*3600+($4-0)*60+($6-0))"))
        }
        me.replaceWith($('<iframe title="YouTube player" class="youtube-player ' + (wide ? "wide" : "full") + 'screen" type="text/html" src="http://www.youtube.com/embed/' + id + '?autoplay=1' + t + '" frameborder="0" allowFullScreen></iframe>').css({
            margin: "0 auto"
        }))
    }
} else {
window.playYT=function(x) {
        var me = $(x),
            id = me.attr("id") ? me.attr("id") : me.css("background-image").replace(/^.*vi\/([\w-]{11})\/.*$/, "$1"),
            wide = me.hasClass("wide"),
            sp = id.split("#t"),
            time = 100
            t = ""
        if (sp.length > 1) {
            id = sp[0]
            t = "&start=" + eval(
            sp[1].replace(/=((\d*)h)?((\d*)m)?((\d*)s)?/i, "(($2-0)*3600+($4-0)*60+($6-0))"))
        }
        me.width(me.width()).height(me.height()).removeClass("wide").removeClass("full")
        var skrim = $("<div />").css({
            background: "#000",
            width: "100%",
            height: "100%",
            opacity: 0
        }).appendTo(me)
        skrim.animate({
            opacity: 1
        }, time, "linear", function () {
            me.css({
                background: "black"
            });
            $(this).remove()
        })
		if($.browser.mozilla) me.css({"border-radius":"0px 0px 0px 0px"})
        me.animate({
            width: "600px",
            height: (wide ? 363 : 475) + "px",
            "border-radius": "0px"
        }, time, "swing", function () {
            $(this).append('<iframe title="YouTube player" class="youtube-player ' + (wide ? "wide" : "full") + 'screen" type="text/html" src="http://www.youtube.com/embed/' + id + '?autoplay=1' + t + '&wmode=transparent" frameborder="0" allowFullScreen allowtransparency=always style="opacity:0" ></iframe>')
            me.find("iframe").load(function () {
                me.find(".play").animate({
                    opacity: 0
                }, 1000, "linear", function () {
                    $(this).remove()
                })
                $(this).animate({
                    opacity: 1
                }, 1000, "linear")
            })
        })
    }
}




