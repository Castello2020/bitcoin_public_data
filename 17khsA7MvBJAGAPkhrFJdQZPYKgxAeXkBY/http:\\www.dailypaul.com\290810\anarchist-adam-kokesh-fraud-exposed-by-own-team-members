<script>
window.colorizeComments=function(){
var userMax=59000

function HueToRgb(m1, m2, hue) {
	var v;
	if (hue < 0) hue += 1;
	else if (hue > 1) hue -= 1;

	if (6 * hue < 1) v = m1 + (m2 - m1) * hue * 6;
	else if (2 * hue < 1) v = m2;
	else if (3 * hue < 2) v = m1 + (m2 - m1) * (2/3 - hue) * 6;
	else v = m1;

	return Math.floor(255 * v);
}
function HSL2RGB(h, s, l) {
	var m1, m2, hue;
	var r, g, b
	s /=100;
	l /= 100;
	if (s == 0) r = g = b = (l * 255);
	else {
		if (l <= 0.5) m2 = l * (s + 1);
		else m2 = l + s - l * s;
		m1 = l * 2 - m2;
		hue = h / 360;
		r = HueToRgb(m1, m2, hue + 1/3);
		g = HueToRgb(m1, m2, hue);
		b = HueToRgb(m1, m2, hue - 1/3);
	}
	return [r,g,b];
}
function RGB2Hex(rgb) {return "#"+toHex(rgb[0])+toHex(rgb[1])+toHex(rgb[2])}
function toHex(n) {
 n = parseInt(n,10);
 if (isNaN(n)) return "00";
 n = Math.max(0,Math.min(n,255));
 return "0123456789ABCDEF".charAt((n-n%16)/16)
      + "0123456789ABCDEF".charAt(n%16);
}
function HSL2Hex(h,s,l){
	return RGB2Hex(HSL2RGB(h,s,l));
}

var b=navigator.userAgent.match(/msie|webkit|opera|mozilla/ig).pop().toLowerCase(),
	g=	b=="webkit"?	["background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,","), color-stop(100%,","))"]:
		b=="mozilla"?	["background:-moz-linear-gradient(top, "," 0%, "," 100%)"]:
		b=="opera"?		["background:-o-linear-gradient(top, "," 0%, "," 100%)"]:
		b=="msie"?		["filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='","', endColorstr='","', GradientType=0)"]:
		false

$("#comments").css({display:"block"})

//if(!navigator.userAgent.match(/trident/i)||userNum==15029)
if(g)
	$("#main_content .comment:not(.comment-unpublished)").each(function(){
		var me=$(this),
			id=me.attr("class").replace(/\D+/g,'')
			hue=Math.floor(266*(userMax-id)/userMax),
			nx=me.next(),
			c1=HSL2Hex(hue,100,97),
			c2=HSL2Hex(hue,100,93)
		me.attr({style:g[0]+c1+g[1]+c2+g[2]})
		if(nx.hasClass("indented"))
			nx.attr({style:g[0]+c2+g[1]+c1+g[2]})
	})

}
</script></div>
