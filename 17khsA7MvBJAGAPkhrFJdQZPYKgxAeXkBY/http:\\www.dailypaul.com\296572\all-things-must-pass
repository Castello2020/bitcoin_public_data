/* background: #009;*/
}

.ftags .tagtab li{
 opacity:.5;
/* display:inline;*/
 float:left;
}

.ftags .tagtab li:hover{
opacity:.75
}

.ftags .tagtab li.active{opacity:1;}

.ftags .pgr .aro{
 font-size:1.75em;
 display:inline-block;
 cursor:pointer;
}

.ftags .pgr .aro.disabled{
 color:gray;
}

.ftags .pgr .aro:hover{
 color:#900;
}

.ftags .view-content .waity{position:absolute;}
.ftags .view-content{position:relative;}

</style>
