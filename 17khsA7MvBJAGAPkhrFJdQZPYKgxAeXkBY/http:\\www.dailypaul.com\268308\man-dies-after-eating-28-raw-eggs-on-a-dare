    <div class="content"><script type="text/javascript" src="/files/jjs/postContentScript.js"></script>
<script>
function parseYTE(){
	var yte=$("#main b.yte")
	if(yte.length){
		var keys=[],
		 er=1000/563
		yte.each(function(){
			var me=$(this),
				yid=me.attr("yid").split(/[#?]/),
				id=yid[0],
				t=yid[1],
				width=me.attr('width'),
				height=me.attr('height')
			// me.css({background:"url(http://images.weserv.nl/?url=img.youtube.com/vi/"+id+"/0.jpg&trim=50&t=fit) center",backgroundSize:"cover"})
			keys.push("b.yte[yid*='"+id+"']{background-image: url(http://images.weserv.nl/?url=img.youtube.com/vi/"+id+"/0.jpg&trim=40&t=fit&q=100&w=480);}")
			me.click(function(){
				var w=me.width()
				if(t && t.length) t=eval((t+'').replace(/t=((\d*)h)?((\d*)m)?((\d*)s)?/i,"(($2-0)*3600+($4-0)*60+($6-0))").replace(/[^0-9\-\+\(\)\*]/g,''))

				me.replaceWith('<iframe width="'+w+'" height="'+Math.floor(w/er)+'" src="http://www.youtube.com/embed/'+id+'?autoplay=1'+(t?'&start='+t:'')+'" frameborder="0" allowfullscreen></iframe>')
			})
			if(width) me.css({width:width+(!width.match(/[^0-9]/)?"px":"")})
			if(height) me.css({height:height+(!height.match(/[^0-9]/)?"px":"")})
		})

		$("<style />").html(
		  "b.yte{position:relative;display:block;width:100%;height:333px;background-size: contain;background-position: center;background-repeat: no-repeat;background-color: transparent;cursor:pointer;}\n"
		 +"b.yte:after{content:''; position:absolute; left:50%; top:50%; margin:-22px 0 0 -32px; background:url('https://lh6.googleusercontent.com/-Vn2KjK_Oq6w/T4kIp7GBOgI/AAAAAAAACqY/ZHNEOjqnt14/s128/') top center no-repeat;  background-size:cover; width:64px; height:45px;opacity:.5;}\n"
		 +"b.yte:hover:after{background-position:bottom center;opacity:1;}\n"
		 +"b.yte.open:after{display:none;}\n"
		 +keys.join("\n")
		).appendTo("head")

	}
}
parseYTE()
</script></div>
