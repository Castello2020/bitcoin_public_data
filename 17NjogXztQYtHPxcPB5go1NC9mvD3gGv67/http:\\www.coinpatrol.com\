<script>
  $(function() {
    $fadeWrap = $("#ticker-fade-wrap");
    $fadeElms = $fadeWrap.find(".ticker-fade");
  
    var totalElms = $fadeElms.length;
    var counter = 0;
    var $lastElm;
  
    var fadeNext = function() {
      var $currentElm = $($fadeElms[counter]);
  
      if($lastElm) {
        $lastElm.fadeOut(400, function() {
          $currentElm.fadeIn();
        });
      } 
  
      if((counter + 1) > (totalElms - 1)) {
        counter = 0;
      } else {
        counter += 1;
      }
  
      $lastElm = $currentElm;
    }
  
    setInterval(function() {
      fadeNext();
    }, 3000);
  });
</script>

