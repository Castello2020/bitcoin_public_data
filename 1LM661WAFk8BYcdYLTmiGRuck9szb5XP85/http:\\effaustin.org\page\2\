function rotateEvery(sec)
{
	var Quotation=new Array()

	// QUOTATIONS
	Quotation[0] = 'Keep your mitts off my bits!';
	Quotation[1] = 'Keep Austin Wired';
	Quotation[2] = 'Keep your paws off my packets!';
