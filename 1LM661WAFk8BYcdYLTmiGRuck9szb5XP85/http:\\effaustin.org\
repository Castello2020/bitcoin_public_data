function rotateEvery(sec)
{
	var Quotation=new Array()

	// QUOTATIONS
	Quotation[0] = 'Keep your mitts off my bits!';
	Quotation[1] = 'Keep Austin Wired';
	Quotation[2] = 'Keep your paws off my packets!';
        Quotation[3] = 'Support EFF-Austin!';
        Quotation[4] = 'Support the Open Internet';

	var which = Math.round(Math.random()*(Quotation.length - 1));
	document.getElementById('textrotator').innerHTML = Quotation[which];
	
	setTimeout('rotateEvery('+sec+')', sec*1000);
}
</script>
</head>
<body onload="rotateEvery(10)">
