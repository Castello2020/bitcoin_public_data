      'publisher'       : 'ShootingTheBull410',
      'width'           : 550,
      'height'          : 250,
      'sid'             : "wordpress-plugin above",
      'color_site_link' : '0000CC',
      'color_title'     : '0000CC',
      'color_text'      : '000000',
      'color_bg'        : 'ffffff',
      'font_title'      : '',
      'font_text'       : '',
      'impsrc'          : 'wordpress',
      'calltype'        : 'async[2]'
