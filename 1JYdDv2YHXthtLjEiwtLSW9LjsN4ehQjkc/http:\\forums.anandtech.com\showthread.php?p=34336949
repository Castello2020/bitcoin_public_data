 <script language="javascript">

        var hitLogger = 'http://tracker.anandtech.com/imgtrack.aspx';
        
        function insertimg() {
	          var uniqueID = GetUidCookie();
            // Create an image node to invoke the hit logger
            trackLink = document.createElement('img');
            var sourceUrl = location.href;
            var forumId = 024;
                        
            if (forumId > 0 && sourceUrl.toLowerCase().indexOf('f=')==-1) {
            	sourceUrl = sourceUrl + ';f|' + forumId;
            }
            
            sourceUrl = sourceUrl.replace('=','|');
				    sourceUrl = sourceUrl.replace('&',';');
				    sourceUrl = sourceUrl.replace('?','&queryString=');				    
            sourceUrl = hitLogger + '?url=' + sourceUrl;
            sourceUrl = sourceUrl + '&referrer=' + document.referrer;
            sourceUrl = sourceUrl + '&uid=' + uniqueID; 
            sourceUrl = sourceUrl + '&sid=2';
            trackLink.setAttribute('src', sourceUrl);
            trackLink.setAttribute('width', 1);
            trackLink.setAttribute('height', 1);
            trackLink.setAttribute('alt', '');
            //trackLink.setAttribute('style', 'display: none;');
            document.body.appendChild(trackLink);
        }       

        function set_cookie(name, value, exp_y, exp_m, exp_d, path, domain, secure) {
            var cookie_string = name + "=" + escape(value);

            if (exp_y) {
                var expires = new Date(exp_y, exp_m, exp_d);
                cookie_string += "; expires=" + expires.toGMTString();
            }

            if (path)
                cookie_string += "; path=" + escape(path);

            if (domain)
                cookie_string += "; domain=" + escape(domain);

            if (secure)
                cookie_string += "; secure";

            document.cookie = cookie_string;
        }


        function get_cookie ( cookie_name )
        {
          var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );

          if ( results )
            return ( unescape ( results[2] ) );
          else
            return null;
        }

        function GetUidCookie() {
            var cookie = get_cookie('myuid');
            var uid = '2db45948-f307-0994-7162-4f4e986d7c42';
            if (cookie == null) {
                set_cookie('myuid', uid, 2020, 1, 1, '/', '.anandtech.com', false);
            } else {
                uid = cookie.valueOf();
            }
            return uid;
        }
        
        YAHOO.util.Event.onDOMReady(insertimg);

    </script>



