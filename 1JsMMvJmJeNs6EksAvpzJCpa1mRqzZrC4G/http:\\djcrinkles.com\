	function sk_timer899882() {
		var sk_timer_div = document.getElementById('sk_timer899882');
		if(sk_timer_div.value) clearTimeout(sk_timer_div.value);
		timer_id=setTimeout( 'sk_refresh899882(true);' , 0 );
		sk_timer_div.value=timer_id;
	}
	
	function sk_refresh899882(timer) {
		var sk_timer_div = document.getElementById('sk_timer899882');
		sk_feed( document.getElementById('sk_page899882').value, 899882, sk_semaphore899882, false, timer);
	}
	
	function for_delete899882() {
		document.getElementById('sk_for_id899882').value='0';
		document.getElementById('sk_for_name899882').innerHTML='';
		document.getElementById('sk_for_tr899882').className='sk-for-nai';
	}
	
	function for_set899882(id, name) {
		document.getElementById('sk_for_id899882').value=id;
		document.getElementById('sk_for_name899882').innerHTML=name;
		var text=document.getElementById('sk_text899882').value;
		if(false) {
			document.getElementById('sk_text899882').value='Answer: '+text;
		} else {
			document.getElementById('sk_text899882').value='For '+name+' - '+text;
		}
		document.getElementById('sk_for_tr899882').className='sk-for-yep';
	}
	
	function sk_replyDelete899882(id, text) {
		var aux = document.getElementById('sk-899882-'+id);
		aux.setAttribute('class', 'sk-reply sk-reply-on');
		aux.setAttribute('className', 'sk-reply sk-reply-on');
		if(confirm(text)) {
			sk_action(id, 'delete', 899882, sk_semaphore899882);
		} else {
			aux.setAttribute('class', 'sk-reply');
			aux.setAttribute('className', 'sk-reply'); //IE sucks
		}
	}
	
	function sk_pressButton899882() {
		var alias=document.getElementById('sk_alias899882').value;
		var text=document.getElementById('sk_text899882').value;
		var email=document.getElementById('sk_email899882').value;
		var skfor=document.getElementById('sk_for_id899882').value;
		if(text.length>225) {
			alert('The lenght of the message is bigger than the allowed size.');
			return false;
		}if(email_intext ( text ) ) {
			check=confirm('To prevent identification theft, we recomend\nthat you do not include e-mail adresses.\nDo you want to continue?');
			if(!check) {
				return false;
			}
		}
		document.getElementById('th_sk_alias899882').innerHTML = alias;
		document.getElementById('th_sk_text899882').innerHTML = text.replace(/\n/g,"<br>");;
		if(true){
			document.getElementById('throbber-page899882').setAttribute('class','throbber-page-on');
			document.getElementById('throbber-page899882').setAttribute('className','throbber-page-on'); //IE sucks
		} else {
			document.getElementById('throbber-img899882').setAttribute('class','throbber-img-on');
			document.getElementById('throbber-img899882').setAttribute('className','throbber-img-on'); //IE sucks
			document.getElementById('throbber-img899882').style.visibility='visible';
		}
		for_delete899882();
		document.getElementById('sk_page899882').value=1;
		document.getElementById('sk_text899882').value='';
		div_sk_allowed = document.getElementById('sk_allowed899882');
		if(div_sk_allowed.value > 0) {
			div_sk_allowed.value = div_sk_allowed.value - 1;
		}
		
		sk_add( alias, email, text, skfor, 899882, sk_semaphore899882 );
	}
