#w2b-StoTop {-moz-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 5px; width:100px;background-color: #f5f5f5;background-color: rgba(245, 245, 245, 1.0);filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#99f5f5f5',EndColorStr='#99f5f5f5');text-align:center;padding:5px;position:fixed;bottom:10px;right:6px;cursor:pointer;color:#238e23;text-decoration:none;border:3px solid #238e23;}
</style>
<script src='https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js' type='text/javascript'></script>
<script type='text/javascript'>
    $(function() {
        $.fn.scrollToTop = function() {
            $(this).hide().removeAttr("href");
            if ($(window).scrollTop() != "0") {
                $(this).fadeIn("slow")
            }
            var scrollDiv = $(this);
            $(window).scroll(function() {
                if ($(window).scrollTop() == "0") {
                    $(scrollDiv).fadeOut("slow")
                } else {
                    $(scrollDiv).fadeIn("slow")
                }
            });
            $(this).click(function() {
                $("html, body").animate({
                    scrollTop: 0
                }, "slow")
            })
        }
    });
    $(function() {
        $("#w2b-StoTop").scrollToTop();
    });
</script>
<a href='#' id='w2b-StoTop' style='display:none;'><strong><span style='font-family:verdana;font-size:85%;'>Scroll to Top</span></strong></a>
