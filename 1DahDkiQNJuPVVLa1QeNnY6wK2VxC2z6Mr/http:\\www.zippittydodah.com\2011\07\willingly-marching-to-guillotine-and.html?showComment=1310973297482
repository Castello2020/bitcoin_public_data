border-top:0px solid #238e23;
border-right:0;
border-bottom:0px solid #238e23;
border-left:0px solid #238e23;
color:#238e23;
background-color: #ecd0c5;
}
input.gsc-search-button {
width:68px; !important;
height: 26px !important;
border-top:4px solid #238e23;
border-right:3px solid #238e23;
border-bottom:4px solid #238e23;
border-left:0;
margin: 1px 2px 1px 2px;
background-color: #238e23;
color: #f5f5f5;
}
#uds-searchControl .gs-result .gs-title,
      #uds-searchControl .gs-result .gs-title *,
      #uds-searchControl .gsc-results .gsc-trailing-more-results,
