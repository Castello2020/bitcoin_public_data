<script type="text/javascript" >
function quotedb_doget() {
	$.ajax({
		type: "GET",
		url: "plugins/quotedb/quotedb.php",
		dataType: "html",
		success: function(stuff) {
			$("#quotedb_div").html(stuff);
		}
	});
}

// get the first quote
quotedb_doget();

</script>



