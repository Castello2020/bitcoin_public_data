<script type="text/javascript" >
function kountr_doget() {
	var filepath = window.location.pathname;
	var pagename = filepath.substring(filepath.lastIndexOf('/') + 1);
	$.ajax({
		type: "GET",
		url: "plugins/kountr/kountr.php?page=" + pagename,
		dataType: "html",
		success: function(stuff) {
			$("#kountr_div").html(stuff);
		}
	});
}

// get the page kount
kountr_doget();

</script>



