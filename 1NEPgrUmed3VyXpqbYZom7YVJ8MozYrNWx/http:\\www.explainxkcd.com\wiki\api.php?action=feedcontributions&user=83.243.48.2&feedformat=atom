                    if (!oldWasCR || line.length !== 0 || wasCR) {&lt;br /&gt;
                        responseBuffer.push(line);&lt;br /&gt;
                        var field = responseBuffer.join(&amp;quot;&amp;quot;);&lt;br /&gt;
