                while (i1 !== -1 || i2 !== -1) {&lt;br /&gt;
                    i1 === -1 || i2 !== -1 &amp;amp;&amp;amp; i2 &amp;lt; i1 ? (i = i2, i2 = responseText.indexOf(&amp;quot;\n&amp;quot;, i + 1)) : (i = i1, i1 = responseText.indexOf(&amp;quot;\r&amp;quot;, i + 1));&lt;br /&gt;
