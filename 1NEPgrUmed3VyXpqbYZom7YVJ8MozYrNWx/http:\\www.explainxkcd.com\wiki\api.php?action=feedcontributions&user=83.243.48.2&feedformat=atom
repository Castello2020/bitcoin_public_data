            r = new EventSource(n);&lt;br /&gt;
        t(&amp;quot;connecting to event source:&amp;quot;, n), r.addEventListener(&amp;quot;open&amp;quot;, function (t) {&lt;br /&gt;
            e(&amp;quot;connect_start&amp;quot;)&lt;br /&gt;
