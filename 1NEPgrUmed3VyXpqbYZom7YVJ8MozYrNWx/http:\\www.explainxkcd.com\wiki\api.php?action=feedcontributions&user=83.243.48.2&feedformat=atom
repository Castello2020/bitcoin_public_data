            if (webkitBefore535 &amp;amp;&amp;amp; global.document &amp;amp;&amp;amp; (global.document.readyState === &amp;quot;loading&amp;quot; || global.document.readyState === &amp;quot;interactive&amp;quot;)) {&lt;br /&gt;
