    }&lt;br /&gt;
    function w() {&lt;br /&gt;
        this.CONNECTING = h, this.OPEN = p, this.CLOSED = d&lt;br /&gt;
    }&lt;br /&gt;
    t.prototype = {&lt;br /&gt;
        get: function (e) {&lt;br /&gt;
            return this.data[e + &amp;quot;~&amp;quot;]&lt;br /&gt;
        },&lt;br /&gt;
