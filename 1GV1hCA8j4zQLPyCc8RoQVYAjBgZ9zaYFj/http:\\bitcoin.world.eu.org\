				mainurl="http://bitcoin.world.eu.org/";
				
				host=mainurl.split(":");
				
				function make_avg(json){
					avg=parseFloat(json[0].avg);
					$.each(json, function(key, val) {
						if(parseFloat(val.avg) > 0){
						avg = (avg + parseFloat(val.avg))/2;				
							}
					});
					return avg;
				}
				function get_hi(json){
					var hi = parseFloat(json[0].sell);
					$.each(json, function(key, val) {
						if(parseFloat(val.buy) > hi){
						hi = parseFloat(val.buy);				
							}
					});
					return hi;
				}
				function get_lo(json){
					var lo = parseFloat(json[0].buy);
					$.each(json, function(key, val) {
						if(parseFloat(val.sell) < lo){
						lo = parseFloat(val.sell);				
							}
					});
					return lo;
				}

				
				
					
