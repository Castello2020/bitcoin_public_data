		<rdf:Description rdf:about="http://irdial.com/blogdial/?p=1114"
    dc:identifier="http://irdial.com/blogdial/?p=1114"
    dc:title="Modern trends in crushed calcium"
    trackback:ping="http://irdial.com/blogdial/wp-trackback.php?p=1114" />
