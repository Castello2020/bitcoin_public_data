$(document).ready(function() {

    if ( window.LogView ) {
        LogView.show();
    }

    PopupImage.attach( "a.popupImage" );

    if ( window.Cart ){
        Cart.startup();
    }
    if ( window.AutoLyrics ) {
        AutoLyrics.init("_lyrics_", "lyrics_div_", "lyrics_row_", TralbumData.trackinfo.length, 1 );
    }
    if ( window.Player ) {
        Player.init(TralbumData, AlbumPage.registerPlaylistListeners);
    }
    if ( window.TralbumDownload ) {
        TralbumDownload.init();
        
        $(".download-link").click(function(ev) {
            ev.preventDefault();
            TralbumDownload.begin(ev);
        });
        
        $(".send-as-gift > .download-link").unbind("click").click(function(ev) {
            ev.preventDefault();
            TralbumDownload.begin(ev, true);
        });
    }
    if ( window.TralbumUpdater ) {
        TralbumUpdater.init();
    }
    if ( window.PackageOrder ) {
        PackageOrder.init();
        
        $(".order_package_link").click(function(ev){
            ev.preventDefault();
            // PackageOrder.begin takes two params: the package index and an isGift boolean. previously it only took a 
            // package index, but this funtion call was passing event as well. if you need the event in begin(), then add
            // it as an extra param.
            // dh 12-02-2013
            PackageOrder.begin($(this).attr('data-pkg'));
        });
        
        $(".send-as-gift > .order_package_link").unbind("click").click(function(ev){
            ev.preventDefault();
            PackageOrder.begin($(this).attr('data-pkg'), true);
        });
    }
    
    Y.util.Event.on("buy-disc-link", "click", function() {Stats.record({kind:"click", click:"buy_disc"}); return true;});
    Y.util.Event.on("rssFeedAlbum", "click", function() {Stats.record({kind:"click", click:"rss_feed"}); return true;});
    Y.util.Event.on("rssFeedArtist", "click", function() {Stats.record({kind:"click", click:"rss_feed"}); return true;});

    if ( window.AlbumPage ) {
        AlbumPage.registerMouseOverForTrackRows();
    }
    
    
    
    if ( window.TralbumFans ){
        TralbumFans.initialize([{"gift_type":null,"fan_id":4311,"grid":null,"featured_track_id":null,"item_type":"a","image_id":1707333,"why":null,"url":"http://bandcamp.com/michaelgarfield","name":"Michael Garfield","item_id":4201067221,"thumb":null,"purchase_date":"01 Apr 2011 05:27:52 GMT","username":"michaelgarfield"}], null, null, null, null);
    }
    
    if( window.Trackpipe ) {
        Trackpipe.maybeDoTralbumActions();
    }

    
    if ( window.BandFollow )
        BandFollow.init({"tralbum_type":"a","tralbum_id":4201067221});
    



    
    if ( window.Share )
        Share.initPanel(false, 1, "http://therealphatkid.bandcamp.com/album/wars-on-ideas");


    if ( window.FanControls ) {
        FanControls.init({"show_collect":true});
    }



});

</script>



