<script type="text/javascript">
	$(function() {
		$("#t").tabs({
			show: function(event, ui) {
				if (ui.index == 1) {
					$('#dnsInput').focus();
				}
				if (ui.index == 3) {
					$('#whoisInput').focus();
				}
				if (ui.index == 2) {
					$('#mxInput').focus();
				}
			}
		});
	});
	
	$('#dnsInput').keypress(function(e) {
        if(e.which == 13) {
            $(this).blur();
			$('#dnsButton').click();
        }
    });
	$('#whoisInput').keypress(function(e) {
        if(e.which == 13) {
            $(this).blur();
			$('#whoisButton').click();
        }
    });
	$('#reverseipInput').keypress(function(e) {
        if(e.which == 13) {
            $(this).blur();
			$('#reverseipButton').click();
        }
    });
    $('#mxInput').keypress(function(e) {
        if(e.which == 13) {
            $(this).blur();
			$('#mxButton').click();
        }
    });

	function dnsLookup() {
		$('#dnsSpinner').show();
		$.get('/lookup.php?type=dns&hash=hash&'+$('#dnsInput').serialize(), function(data){
			$('#dnsSpinner').hide();
			$('#dnsText').hide();
			$('#dnsText').html('<pre>'+data+'</pre>');
			$('#dnsText').show('fast');
		});
	}
	function mxLookup() {
		$('#mxSpinner').show();
		$.get('/lookup.php?type=mx&hash=hash&'+$('#mxInput').serialize(), function(data){
			$('#mxSpinner').hide();
			$('#mxText').hide();
			$('#mxText').html('<pre>'+data+'</pre>');
			$('#mxText').show('fast');
		});
	}
	function whoisLookup() {
		$('#whoisSpinner').show();
		$.get('/lookup.php?type=whois&hash=hash&'+$('#whoisInput').serialize(), function(data){
			$('#whoisSpinner').hide();
			$('#whoisText').hide();
			$('#whoisText').html('<pre>'+data+'</pre>');
			$('#whoisText').show('fast');
		});
	}
	function reverseipLookup() {
		$('#reverseipSpinner').show();
		$.get('/lookup.php?type=reverseip&hash=hash&'+$('#reverseipInput').serialize(), function(data){
			$('#reverseipSpinner').hide();
			$('#reverseipText').hide();
			$('#reverseipText').html('<pre>'+data+'</pre>');
			$('#reverseipText').show('fast');
		});
	}
	
	$('#resolution').text(screen.width + 'x' + screen.height).closest('tr').show();
	try {
		if (ui.bit != '') {
			os_string = ui.os + ' ' + ui.bit;
		} else {
			os_string = ui.os;
		}
		$('#os').html(os_string).closest('tr').show();
	} catch (exception) {
		$('#os').html('<i>Failed Detection</i>').closest('tr').show();
	}
	$('#browser').text(ui.browser + ' ' + ui.version).closest('tr').show();
	$('#timezone').text(timezone = jstz.determine().name());
	
	function showTime() {
		$('#time').html(moment().format('MMMM Do YYYY[<br/>] h:mm:ss A'));
	}
	showTime();
	setInterval(function() {
		showTime();
	}, 1000);

	function showGeoLocation() {
		$.getJSON("/geo.php", function(response) {
			$('[data-location-label]').text(response.city + ', ' + response.state + ', ' + response.country);
			$('[data-coordinates-label]').html('Coordinates: <a href="https://maps.google.com/?q=' + response.latitude + ',%20' + response.longitude + '" target="_blank">' + response.latitude + ', ' + response.longitude + '</a>');
		});
	}
	jQuery(function() {
		showGeoLocation();
	});

	jQuery('#jsenabled').text('Yes');
	
</script>


	</div>


	</body>
	</html>
