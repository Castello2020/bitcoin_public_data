$(function(){
	if($("#adTop").length > 0 && $("#adTop").css("display") != "none"){
		ga('send', 'event', 'Ads', 'load', adsLoaded.join(", "));
		var hoverAd = function(){
			if($("#adTop:in-viewport").length == 0 && $("#adSide:in-viewport").length == 0 && $("#adHoverMarker:in-viewport").length == 0){
				if(adHoverStatus == 0){
					var ob = $("#adHover");
					ob.css({
						"position": "fixed",
						"bottom": "-" + ob.find("div").height() + "px",
						"width": "100%"
					});
					ob.animate({
						"bottom": "0px"
					}, 300);
					ob.find("#ad-hover").css("border", "4px solid rgba(0, 0, 0, 0.3)");
					ob.find("#ad-hover-mobile").css("border-top", "2px solid rgba(0, 0, 0, 0.3)")
					adHoverStatus = 1;
				}	
			}else{
				if(adHoverStatus == 1){
					var ob = $("#adHover").css({
						"position": "static",
						"bottom": null,
						"width": null,
					});					
					ob.find("#ad-hover").css("border", "0px");
					ob.find("#ad-hover-mobile").css("border", "0px");
					adHoverStatus = 0;
				}
			}
		};
		hoverAd();
		$(window).scroll(hoverAd);
	}else{
		ga('send', 'event', 'Ads', 'block', 'All');
	}
});




</script>
