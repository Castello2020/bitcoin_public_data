<script type="text/javascript">
$(".minecraftHead").each(function(index){
	var canvas = this;
	var name = $(canvas).attr("title");
	var img = new Image();
	img.src = "http://s3.amazonaws.com/MinecraftSkins/" + name + ".png";
	img.onload = function(){
		var ctx = canvas.getContext("2d");
		ctx.imageSmoothingEnabled = false;
		ctx.webkitImageSmoothingEnabled = false;
		ctx.mozImageSmoothingEnabled = false;
		ctx.drawImage(img, 8, 8, 8, 8, 0, 0, canvas.getAttribute("width"), canvas.getAttribute("height"));
		ctx.drawImage(img, 40, 8, 8, 8, 0, 0, canvas.getAttribute("width"), canvas.getAttribute("height"));
	};
	img.onerror = function(){
		img.src = "https://minecraft.net/images/char.png";
	}
});
</script>
