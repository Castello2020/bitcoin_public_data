+&lt;!-- Barra lateral --&gt;
+&lt;div id="barra_lateral"&gt;
+	&lt;div id="menu"&gt;
+		&lt;ul id="menu_principal"&gt;
+			&lt;li onclick="window.location='../index.html'"&gt;
+				&lt;a href="../index.html"&gt;es.openoffice.org&lt;/a&gt;&lt;/li&gt;
+			&lt;li onclick="window.location='../comunidad/index.html'"&gt;
+				&lt;a href="../comunidad/index.html"&gt;Comunidad&lt;/a&gt;&lt;/li&gt;
+			&lt;li class="current"&gt;
+				&lt;a href="#"&gt;Ayuda&lt;/a&gt;&lt;/li&gt;
+			&lt;li onclick="window.location='../programa/index.html'"&gt;
+				&lt;a href="../programa/index.html"&gt;Descargas&lt;/a&gt;&lt;/li&gt;
+					&lt;li&gt;&lt;a href="../comunidad/irc.html"&gt;IRC&lt;/a&gt;&lt;/li&gt;
+			&lt;li&gt;&lt;a href="lecturas_0019.html"&gt;Dona Aqu&amp;iacute;!&lt;/a&gt;&lt;/li&gt;
