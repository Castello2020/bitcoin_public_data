    <script type="text/javascript">
      function ToggleLoginRow(){
        if(document.getElementById('loginRow').style.display == '')
          document.getElementById('loginRow').style.display = 'none';
        else
          document.getElementById('loginRow').style.display = '';
      }
      function ToggleGuide(id){
        if(id == 'nowherecast'){
          if(document.getElementById('nowhereCastGuide').style.display == '')
            document.getElementById('nowhereCastGuide').style.display = 'none';
          else
            document.getElementById('nowhereCastGuide').style.display = '';
        }else{
          if(document.getElementById('nowhereTVGuide').style.display == '')
            document.getElementById('nowhereTVGuide').style.display = 'none';
          else
            document.getElementById('nowhereTVGuide').style.display = '';
        }
      }
