    <script type="text/javascript">
		$(document).ready(function() {
			$("#frm").height($(window).height()-40);	  
		});
		$(window).resize(function() {
			$("#frm").height($(window).height()-40);	  
		});
		$(".links a").click(function(event) {
			
				$("#frm").fadeOut(500);
				if($(this).data("target") == 'blank')
					window.open($(this).data("href"),'_newtab');
				else
					$("#frm").attr("src",$(this).data("href"));
				
				if($(this).data("disabled") != 'N')
				{
					$(this).parent().addClass("disabled");
					$(this).unbind('click');
				}
				$("#frm").load(function() 
				{
					$(".faq").fadeOut(500);
					$("#frm").fadeIn(500);					
				});
				
						
			
			
		});
    </script>
    
    
