  $('#comment_message').focus(function() {
    _form = $(this).closest('.comment_form')
    if($('body').hasClass('signed_in')) {
      _form.html("");
      _form.spin();
      $.ajax({
        url: '/bitcoin/pages/10998257-for-all-those-fancy-number-bitcoin-people-we-now-accept/comments/new',
        success: function(data) {
          _form.replaceWith(data);
        },
        error: function(data) {
          location.reload();
        }
      });
    } else {
      _form.html("Please <a href=\"/users/sign_in?redirect_to=http%3A%2F%2Fopenruby.com%2Fbitcoin%2Fpages%2F10998257-for-all-those-fancy-number-bitcoin-people-we-now-accept\">sign in<\/a> or <a href=\"/users/sign_up\">sign up<\/a> to comment.");
