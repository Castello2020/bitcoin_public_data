        $adContainer.addClass('cnAdIsEmpty');
        $(function() {
            log('removing cnAdId-4');
            if($adContainer != null && $adContainer.length > 0)
                $adContainer.remove();});
        setTimeout(function(){
            if($adContainer != null && $adContainer.length > 0)
                $adContainer.remove();
        }, 1000);
    }
}else {
    if (window.top && window.top.log) 
        window.top.log('advert', 'Ad was *not* blank (label: adLabel_300x250-3x3-420x160_2)');
    if (window.$) 
        $('.cnAdId-4').addClass('cnAdIsNotEmpty');  // Obsolete
}
</script><noscript>
