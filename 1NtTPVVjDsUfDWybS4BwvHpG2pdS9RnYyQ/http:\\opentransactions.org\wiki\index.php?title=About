						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading"><span dir="auto">About</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From Open Transactions</div>
				<!-- /tagline -->
