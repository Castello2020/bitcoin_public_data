			// use fancybox for hi-res images
			$(document).ready(function() {
				$("a.image-link").fancybox();
				noIndent();
			});

			// remove image indentation in paragraphs
			function noIndent() {
				$("a.image-link").fancybox();
				$(".post.text p:has(img)").addClass("no-indent");
				$(".post.text p a:has(img)").addClass("no-indent");
			}

			// set height of background div
			function setHeight() {
			    var viewport = $(window).height();
			    var background = $("#background").height();

			    if (viewport > background) {
					$("#background, #background-inner").css("height", viewport);
				}
			}

			$(document).ready(function() {
			    setHeight();
			    $(window).bind('resize', setHeight);
			});

