$(document).ready(function($){
	$('#mega-menu-tut').dcMegaMenu({
		rowItems: '1',
		speed: 'fast',
        effect: 'slide'
	});
	
	
		$('.sub-container').hover(function(){ 
		varArea = $(this).closest('li');
		//alert(varArea);
		if ($(varArea).hasClass('HorParentHover') ) {
				$(varArea).removeClass('HorParentHover');
		}else{
		 $(varArea).addClass('HorParentHover');
		}
	});
	
});	

</script>	

