<script type="text/javascript">
	function spoilHandler( cspid ) {
/*	
		//-- will hold it's vertical space
		curState = document.getElementById( cspid ).style.visibility;
		if ( curState == 'visible' )
			document.getElementById( cspid ).style.visibility = 'hidden';
		else
			document.getElementById( cspid ).style.visibility = 'visible';
*/
		//-- will fold in and open up
		curState = document.getElementById( cspid ).style.display;
		if ( curState == 'none' )
			document.getElementById( cspid ).style.display = 'block';
		else
			document.getElementById( cspid ).style.display = 'none';
	}
</script>


