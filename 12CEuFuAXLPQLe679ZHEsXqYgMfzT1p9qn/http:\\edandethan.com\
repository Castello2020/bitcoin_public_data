	$(document).ready(function(){
		$('.view-show-notes').click(function(e){
			e.preventDefault();

			if ($(this).parents('li').children('.show-notes').hasClass('hidden')) {
				$('ul#podcasts .show-notes').addClass('hidden');
				$(this).parents('li').children('.show-notes').removeClass('hidden');
				scrollToElement('#'+$(this).parents('li').attr('id'));
			} else {
				$('ul#podcasts .show-notes').addClass('hidden');
				$(this).parents('li').children('.show-notes').addClass('hidden');
			}
		});
	});

	function scrollToElement(element) {
		$('html, body').animate({ scrollTop: $(element).offset().top - 32 }, 500);
	}
</script>

