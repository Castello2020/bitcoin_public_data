		<script type="text/javascript">
			$(function(){
				var pager = $('.banner-block');
				var pages = $('.banner-block > ul > li');
				var buttons = $('.banner-list > li');
				var initDone = false;
				$.history.bind(function(hash){
					var button;
					var page;
					if (hash == 'welcome' || hash == '') {
						page = pages[0];
						button = $(buttons[0]);
					}
					else if (hash == 'benefits') {
						page = pages[1];
						button = $(buttons[1]);
					}
					else if (hash == 'features') {
						page = pages[2];
						button = $(buttons[2]);
					}
					else if (hash == 'how-to-use') {
						page = pages[3];
						button = $(buttons[3]);
					}
					else if (hash == 'home-games') {
						page = pages[4];
						button = $(buttons[4]);
					}
					else {
						return;
					}
					if (initDone) {
						pager.scrollTo(page, 900, {
							easing: 'easeOutExpo'
						});
					}
					else {
						initDone = true;
						pager.scrollTo(page, 0);
					}
					buttons.removeClass('active');
					button.addClass('active');
				});
			});
		</script>
		
