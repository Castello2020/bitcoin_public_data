<script type="text/javascript">
(function($){
	var auth = function () {
	}
	
	auth.prototype.init = function() {
		var self = this;
		this.callbacks = {};
		this.success = false;
		this.authDialog = $("#dialog-auth");
		this.loginBox = $("#LoginForm");
		this.registerBox = $("#RegisterForm");
		this.captcha = $(".captcha");
		this.captchaRefresh = $("#captchaRefresh");
		this.nextUrl = null;
		this.binds = {};
		this.baseUrl = document.location.href;
		this.loginButton = $('#login-box-submit');
		this.registerButton = $('#register-box-submit');
		this.closeLink = $('#close-link');
		this.errorDialog = $('#errorDialog');
		this.welcomeDialog = $('#welcomeDialog');
		
		this.loginButton.click(function() {
			self.login();
		});
		this.registerButton.click(function() {
			self.register();
		});
		this.closeLink.click(function() {
			self.close();
		});

		$('a[target=_blank][href^=#]').each(function(){
			$(this).attr('target', '_self').attr('href', $(this).attr('href') + ':blank');
		});

		$.history.bind(function(hash){
			if (hash == '') {
				self.close();
			}
			else if (hash.substr(0, 5) == 'auth:') {
				self.show();
				var url = hash.substr(5);
				if (url) {
					self.nextUrl = url;
				}
			}
			else if (hash.substr(0, 5) == 'login') {
				self.showAuth();
			}
			else if (hash.substr(0, 8) == 'register') {
				self.showAuth();
			}
		});
		
		this.loginBox.find('input').keyup(function(e){
			if (e.keyCode == '13') {
				var allFilled = true;
				self.loginBox.find('input').each(function(){
					if (!$(this).val().length) {
						allFilled = false;
					}
				});
				if (allFilled) {
					self.login();
				}
			}
		})
		.wijtextbox();

		this.registerBox.find('input').keyup(function(e){
			if (e.keyCode == '13') {
				var allFilled = true;
				self.registerBox.find('input').each(function(){
					if (!$(this).val().length) {
						allFilled = false;
					}
				});
				if (allFilled) {
					self.register();
				}
			}
		})
		.wijtextbox();
		
		this.authDialog.dialog({
			autoOpen: false,
			resizable: false,
			modal: true,
			width: 854,
			open: function() {
				$('div[aria-labelledby="ui-dialog-title-dialog-auth"]')
					.css('background', 'transparent')
					.css('border', '0px');
				$(".ui-dialog-titlebar").hide();
				self.loginBox.find('input:first').focus();
			},
			close: function(){
				if (!self.authDialog.dialog('isOpen')) {
					$.history.load('');
					if (!self.success) {
						self.trigger('fail');
					}
				}
			}
		});		

		this.captchaRefresh.click(function(){
			var url = '/common/securimage/securimage_show.php?' + Math.random();
			console.log(url);
			self.captcha.attr('src', url);
		});

	},

	auth.prototype.bind = function (name, callback) {
		if (!this.binds[name]) {
			this.binds[name] = [];
		}
		this.binds[name][this.binds[name].length] = callback;
	}

	auth.prototype.trigger = function (name) {
		var res;
		if (this.callbacks && this.callbacks[name] && !this.callbacks[name]()) {
			return false;
		}
		if (this.binds[name]) {
			for (var i = 0 ; i < this.binds[name].length ; i++) {
				res = this.binds[name][i]();
				if (!res) {
					return false;
				}
			}
		}
		return true;
	}

	auth.prototype.error = function (message, close) {
		this.errorDialog.dialog({
			modal: true,
			autoOpen: true,
			resizable: true,
			width: 'auto',
			close: close,
			open: function() {
				$('#error-close-button').blur();
				$('div[aria-labelledby="ui-dialog-title-errorDialog"]')
					.css('background', 'transparent')
					.css('border', '0px');
				$(".ui-dialog-titlebar").hide();
				$('#error-message').html(message);
				$('#error-close-button').click(function() {
					$('#errorDialog').dialog('close');
				});				
			}			
		});
	}
	
	auth.prototype.welcome = function (username, close) {
		this.welcomeDialog.dialog({
			modal: true,
			autoOpen: true,
			resizable: true,
			width: 'auto',
			close: close,
			open: function() {
				$('#welcome-close-button').blur();
				$('#welcome-username').html(username);
				$('div[aria-labelledby="ui-dialog-title-welcomeDialog"]')
					.css('background', 'transparent')
					.css('border', '0px');
				$(".ui-dialog-titlebar").hide();
				$('#welcome-close-button').click(function() {
					$('#welcomeDialog').dialog('close');
				});				
			}			
		});
	}	

	auth.prototype.show = function (callbacks) {
		this.callbacks = callbacks;
		if ($.cookie('is_user')) {
			this.showAuth();
		}
		else {
			this.showAuth();
		}
	}

	auth.prototype.showAuth = function () {
		this.authDialog.dialog('open');
	}

	auth.prototype.close = function () {
		var fail = false;
		if (this.authDialog.dialog('isOpen')) {
			fail = !this.success;
			this.authDialog.dialog('close');
		}
		if (fail) {
			this.trigger('fail');
		}
	}

	auth.prototype.login = function () {
		var self = this;
		$.ajax({
			url: '/auth/login?active_lang=eng',
			data: this.loginBox.find(":input").serialize(),
			type: "POST",
			dataType: "json",
			success: function(data) {
				if (data && data.type == 'error') {
					self.error(data.message, function(){
						self.loginBox.find('input:first').focus();
					});
				}
				else {
					self.success = true;
					self.authDialog.dialog('close');
					if (self.trigger('success')) {
						if (self.nextUrl) {
							if (self.nextUrl.substr(-6) == ':blank') {
								window.open(self.nextUrl.substr(0, self.nextUrl.length - 6));
								location.reload();
							}
							else {
								document.location = self.nextUrl;
							}
						}
						else {
							document.location = document.location.href.replace('#', '');
						}
					}
				}
			}
		});
	}

	auth.prototype.register = function () {
		var self = this;
		$.ajax({
			url: '/auth/register?active_lang=eng',
			data: this.registerBox.find(":input").serialize(),
			type: "POST",
			dataType: "json",
			success: function(data) {
				self.registerBox.find('.error-icon').remove();
				if (data && data.type == 'error') {
					if (typeof(data.message) == 'object') {
						var msg = [];
						$.each(data.message, function(key, val){
							self.registerBox.find("#" + key).after('<span class="error-icon"></span>');
							msg.push('<li>' + val + '</li>');
						});
						data.message = '<ul class="error-list">' +  msg.join('') + '</ul>';
					}
					self.error(data.message, function(){
						self.registerBox.find('input:first').focus();
					});
				}
				else {
					self.authDialog.dialog('close');
					if (self.trigger('success')) {
							if (self.nextUrl) {
								if (self.nextUrl.substr(-6) == ':blank') {
									window.open(self.nextUrl.substr(0, self.nextUrl.length - 6));
									location.reload();
								}
								else {
									document.location = self.nextUrl;
								}
							}
							else {
								document.location = document.location.href.replace('#', '');
							}
					}
				}
			}
		});
	}

	$.auth = new auth();

	$(document).ready(function() {
		$.auth.init();
	});


	auth.prototype.error = function (message, close) {
		$('<div title="Error">' + message + '</div>').dialog({
			modal: true,
			autoOpen: true,
			resizable: true,
			width: 'auto',
			close: close,
			buttons: {
				Ok: function() {
					$(this).dialog('close');
				}
			}
		});
	}
	
})(jQuery);

function disableSubmit() {
	return false;
}
</script>

