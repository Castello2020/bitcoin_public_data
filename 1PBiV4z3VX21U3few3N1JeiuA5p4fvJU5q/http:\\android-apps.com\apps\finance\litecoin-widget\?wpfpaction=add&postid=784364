$(document).ready(function() { 
	var ajaxurl = 'http://android-apps.com/wp-admin/admin-ajax.php';
 	var nonce = "5e3950be8b";
 
 	jQuery('#mobile-menu').click(function(e) {
		e.preventDefault();
		jQuery('#menu-main-menu').toggle(300);
	});


// ajax to fetch the load me data.
 $(".vote a").click(function(){
var pid = $(this).data('pid');
$.post(
   ajaxurl, 
   {
 	dataType: 'json',
	'action':'vote',
	'post_id': pid,
	'vote_nonce': nonce
   }, 
   function(data){
 	var obj = JSON.parse(data);
	if(obj.vote_count){
	var new_vote_count = obj.vote_count;
	$('td.votes-display').text(new_vote_count + ' votes');
	$('.vote').hide();
	$('label.thanks').show();
	$('.ratings label').text(new_vote_count);
	$('li.votes-display').text(new_vote_count + ' votes');
	} 
   }
);
});

$('#menu-item-755743 a').prepend('<i class="icon-star"></i>&nbsp;');
