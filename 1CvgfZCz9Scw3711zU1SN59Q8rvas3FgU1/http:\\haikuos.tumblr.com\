			// use fancybox for hi-res images
			$(document).ready(function() {
				$("a.image-link").fancybox();
				noIndent();
			});

			// remove image indentation in paragraphs
			function noIndent() {
				$("a.image-link").fancybox();
				$(".post.text p:has(img)").addClass("no-indent");
				$(".post.text p a:has(img)").addClass("no-indent");
			}

			// set height of background div
			function setHeight() {
			    var viewport = $(window).height();
			    var background = $("#background").height();

			    if (viewport > background) {
					$("#background, #background-inner").css("height", viewport);
				}
			}

			$(document).ready(function() {
			    setHeight();
			    $(window).bind('resize', setHeight);
			});

			/*
			// display theme anorak help on customize screen
			$(document).ready(function () {
			    if(window.location.href.indexOf("tumblr.com/customize") > -1) {
			       // display bottom right credit and support link later
			    }
			});
			*/

		</script>

		
		<script>
			// enable masonry
			$(function(){
				var $container = $('body.index-page #blog');
				$container.imagesLoaded(function(){
					$container.masonry({
						itemSelector: '.post'
					})
				});
			});
		</script>
		

		
		<script type="text/javascript">

			// scroll to top
			$(function () {
				$(window).scroll(function () {
					if ($(this).scrollTop() > 700) {
						$('#to-top').fadeIn();
					} else {
						$('#to-top').fadeOut();
					}
				});
				// scroll body to 0px on click
				$('#to-top a').click(function () {
					$('body,html').animate({
						scrollTop: 0
					}, 800);
					return false;
				});
			});

		</script>
		

		
		<script type="text/javascript">
			// enable infinite scroll
			$(document).ready(function() {

				$('section#blog').infinitescroll({
					navSelector  : "p#pagination",
					nextSelector : "p#pagination a#next",
					itemSelector : "#blog .post",
					loading: {	img : "http://static.tumblr.com/fjltjbq/wgUm4m75u/loading.gif",
								msgText: "",
								finishedMsg: "" }
					},
					function( newElements ) {

						var $newElems = $( newElements ).css({ opacity: 0 });

						var $container = $('body.index-page #blog');

					    var $newElemsIDs = $newElems.map(function () {
					        return this.id;
					    }).get();

						$newElems.imagesLoaded(function(){

							$newElems.animate({ opacity: 1 });
							$container.masonry( 'appended', $newElems, true );

							// enable likes
							Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
							$("a#like").css("line-height" , "12px");

							// re-enable fancybox and indent code
							noIndent();

						});
					}
				);
			});
		</script>
		

		


