<script>
$('.donateButton').click(function(){
        var url = "https://www.gofundme.com/LincolnOffice/donate";

        
        if($(this).data("wl") !== undefined){
                url += "&wl="+$(this).data("wl");
        }else if($(this).data("amt") !== undefined){
                url += "&amt="+$(this).data("amt");
        }

        location.href = url;

        
        return false;
});

$('.share-fb').click(function() {
        $(".followContinue").show();
        $('.followLoading').hide();
        try              { GFM.Follower.post2feed.apply($(this)); }
        catch (notready) { setTimeout(function() {GFM.Follower.post2feed.apply($(this))}, 1000); }
        return false;
});

