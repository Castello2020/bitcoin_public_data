        var $printImg = $('#printsign_img');

        if ($printImg.attr('src') == "" ){
                $printImg.attr('src','http://funds.gofundme.com/index.php?route=printmedia/donationposter&url=LincolnOffice&r=6648');
        }

        $('#print_sign_lb').showAsLightbox(true);
        return false;
});


$(".printSignButton").click(function(){
        var defaultParams = {
                "width":       "800",   // Window width
                "height":      "1010",   // Window height
                "directories": "no",    // Show directories/Links bar?
                "location":    "no",    // Show location/address bar?
                "resizeable":  "yes",   // Make the window resizable?
                "menubar":     "no",    // Show the menu bar?
                "toolbar":     "no",    // Show the tool (Back button etc.) bar?
                "scrollbars":  "yes",   // Show scrollbars?
                "status":      "no"     // Show the status bar?
        };

        var w = window.open("http://funds.gofundme.com/index.php?route=printmedia/donationposter&print=1&url=LincolnOffice&r=49", "printable", defaultParams);
        $('#print_sign_lb').closeLightbox();
        return false;
});

