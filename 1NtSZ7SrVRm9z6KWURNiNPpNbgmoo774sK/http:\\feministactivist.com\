            $('.autoplay').attr('checked', $(event.target).attr('checked'));
            videoplayers[1].onAutoplayClick();
        });

        // this will add all available videos to playlist
        $ensure(function() {
                videoplayers[3].addVideo($('#video-results-3 .video-info').slice(0, 14));
            videoplayers[3].loadPlaylist();

            jQuery('#videoplayer-play-all3').click(function(event) {
                event.preventDefault();
                videoplayers[3].clearPlaylist();
                videoplayers[3].addVideo(jQuery('#video-results-3 .video-info').slice(0, 23));
                videoplayers[3].playPlaylist();
            });
            jQuery('#videoplayer-play-next3').click(function(event) {
                event.preventDefault();
                videoplayers[3].playNextPlaylistVideo();
            });
            jQuery('#playlist_restore3').click(function(event) {
                    event.preventDefault();
                    videoplayers[3].clearPlaylist();
                    videoplayers[3].addVideo(jQuery('#video-results-3 .video-info').slice(0, 14));
                    videoplayers[3].playPlaylist();
            });

						 
        });
        videoplayers[4] = new Videoplayer({
            playlistContainer: "#playlistcontainer4",
            playerContainer: "#playercontainer4",
            playlistItemTemplate: '<li class="playlistitemli thumbnail border_color bgr_color_content font_color">' +
'<a class="playlistitem ellipsis bgr_color" id="<%= id %>" href="#">' +
'<div class="thumb border_color rounded"><div class="clip">' +
'<img width="110" height="82" alt="<%= title %>" src="<%= thumbnailUrl %>" />' +
'<div class="duration opacity"><%= durationStr %></div></div></div>' +
'<span class="video-title"><%= title %></span>' +
'<span class="playlistitemremove" title="Click to remove from playlist" data-title="Click to remove from playlist"></span></a>' +
'<span class="description-content" style="display:none;"><%= tooltipContentBody %></span>' +
'</div></li>',
            playpositionTemplate: '<%= elapsedTime %> / <%= totalTime %>',
            emptyItemHtml: '<li class="empty"><span data-trans="Add any items to Create Playlist by dragging or using the \"Add\" Icon on each Video thumbnail or selecting more Videos from new Searches to build your Playlist">Add any items to Create Playlist by dragging or using the "Add" Icon on each Video thumbnail or selecting more Videos from new Searches to build your Playlist</span></li>',
            inputOrdering: true,
            descriptionsholder: '.descriptionsplace',
            enableLogging: false,
            playTrigger: '.playvideo4',
            addTrigger: '.addvideo4',
            playlistRepeat: '#playlistrepeat4',
            playlistShuffle: '#playlistshuffle4',
            playlistClear: '.playlist_clear4',
            playposition: '#playposition4',
            autoplayCheckbox: '#autoplay4',
            replaycurrentCheckbox: '.replaycurrent4',
            autoplayCookie: '',
            playlistFormatCookie: 'wn-videoplayer-playlist-format',
            controlNext: '.videoplayer-control-next',
            controlPrev: '.videoplayer-control-prev',
            controlPlay: '.videoplayer-control-play',
            controlPause: '.videoplayer-control-pause',
            playlistLink: '.playlist-link'
        });
        videoplayers[4].playerContainer.bind('videoplayer.player.videoloaded', function(e, videoInfo){
             load_playlistlyrics(videoInfo.title);
        });


        jQuery('.autoplay').attr('checked', jQuery('#autoplay1').attr('checked'));
        jQuery('.autoplay').click(function(event){
            $('.autoplay').attr('checked', $(event.target).attr('checked'));
            videoplayers[1].onAutoplayClick();
        });

        // this will add all available videos to playlist
        $ensure(function() {
                videoplayers[4].addVideo($('#video-results-4 .video-info').slice(0, 14));
            videoplayers[4].loadPlaylist();

            jQuery('#videoplayer-play-all4').click(function(event) {
                event.preventDefault();
                videoplayers[4].clearPlaylist();
                videoplayers[4].addVideo(jQuery('#video-results-4 .video-info').slice(0, 23));
                videoplayers[4].playPlaylist();
            });
            jQuery('#videoplayer-play-next4').click(function(event) {
                event.preventDefault();
                videoplayers[4].playNextPlaylistVideo();
            });
            jQuery('#playlist_restore4').click(function(event) {
                    event.preventDefault();
                    videoplayers[4].clearPlaylist();
                    videoplayers[4].addVideo(jQuery('#video-results-4 .video-info').slice(0, 14));
                    videoplayers[4].playPlaylist();
            });

						 
        });

        // adding more actions
        $ensure(function() {
            jQuery('.submenu li a').click(function(event) {
                var id = $(event.target).attr('id');
                if(id.substring(0, 9) == '#playlist'){
                    window.videoplayer = videoplayers[parseInt(id.substring(9))];
                }
            });
            // hyphenate video details
            // only use this for english language
            jQuery('.video-info').each(function() {
                Hyphenator.hyphenate(this, 'en');
            });

            window.onPlayVideo = function(event) {
                videoplayer.log('onPlayVideo');
                event.preventDefault();
                var videoData = videoplayer.extractVideoData($(event.currentTarget));
                videoplayer.log(videoData);
                videoplayer.playFromPlaylist(videoData, this.AUTOSTART);
            };

            window.onAddVideo = function(event) {
                videoplayer.log('onAddVideo');
                event.preventDefault();
                var videoData = videoplayer.extractVideoData($(event.currentTarget));
                var playlist = videoplayer.getPlaylist();
                playlist.push(videoData);
                videoplayer.setPlaylist(playlist);
                videoplayer.log('onAddVideo done.');
            };

            jQuery('.addvideo').livequery(function() {
                $(this).click(window.onAddVideo);
            });
            jQuery('.playvideo').livequery(function() {
                $(this).click(window.onPlayVideo);
            });
        });
    //-->
    </script>

    </div>
    <!-- upper-block ends -->



    <script type="text/javascript"><!--
        function ie_find_flash(){
            for(var i=8; i>0; i--){
                flashVersion = 0;
                try{
                    var flash = new ActiveXObject("ShockwaveFlash.ShockwaveFlash." + i);
                    flashVersion = i;
                    return i;
                }
                catch(e){
                }
            }
            return 0;
        }
        function is_istuff() {
            var agent=navigator.userAgent.toLowerCase();
            var is_iPhone = (agent.indexOf('iphone')!=-1);
            var is_iPad = (agent.indexOf('ipad')!=-1);
            return (is_iPhone || is_iPad);
        }
        istuff = is_istuff();
        // hide the upper-block if Flash is not supported, for iPhone etc.
        var divname = 'upper-block';
        var element;
        var flash_supported = 0;
        if( document.getElementById ) // this is the way the standards work
            element = document.getElementById( divname );
        else if( document.all ) // this is the way old msie versions work
            element = document.all[divname];
        else if( document.layers ) // this is the way nn4 works
            element = document.layers[divname];

        // works on netscape and all sensible browsers
        if( navigator.mimeTypes["application/x-shockwave-flash"] ){
            flash_supported = 1;
        }else{
            flash_supported = ie_find_flash()
        }
        // iphone or ipad youtube videos can be embedded too

        if(!flash_supported && !istuff){
            element.style.display = 'none';
        }
        //-->
    </script>

</div>
<!-- main ends -->
















