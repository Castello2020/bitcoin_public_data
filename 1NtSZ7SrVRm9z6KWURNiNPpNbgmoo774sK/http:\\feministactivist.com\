                function scrollToAnchor(aid){
                    var aTag = $("a[name='"+ aid +"']");
                    $('html,body').animate({scrollTop: aTag.offset().top},'slow');
                }

                $ensure(function(){
		
				    $(".rslides").responsiveSlides({
				        auto: false,
				        pagination: true,
				        nav: true,
				        fade: 500,
				        maxwidth: 800
					});		
			
