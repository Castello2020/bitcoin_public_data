        var last_count = 22;
        $ensure(function() {
            jQuery('.more_related').click(function(e) {
                find_more_videos();
                e.preventDefault();
            })
            jQuery('.more_videos').click(function(e) {
                more_videos();
                e.preventDefault();
            })

        jQuery('#news-search').click(function(event) {
                var search_string = jQuery("#video_query").val();
                search_string = search_string.toLowerCase();

                // We cannot handle certain characters in searches:
                if(search_string.indexOf('.') >= 0)
                    return true; // submit normally
                if(search_string.indexOf('/') >= 0)
                    return true; // submit normally
                if(search_string.indexOf(':') >= 0)
                    return true; // submit normally

                window.location = 'http://wn.com/' + search_string.replace(' ', '_');
                return false; // do not submit the form
            });
        jQuery('#form-news-search').submit(function(event) {
                var search_string = jQuery("#video_query").val();
                search_string = search_string.toLowerCase();

                // We cannot handle certain characters in searches:
                if(search_string.indexOf('.') >= 0)
                    return true; // submit normally
                if(search_string.indexOf('/') >= 0)
                    return true; // submit normally
                if(search_string.indexOf(':') >= 0)
                    return true; // submit normally

                window.location = 'http://wn.com/' + search_string.replace(' ', '_');
                return false; // do not submit the form
            });
            //jQuery('#multiple-video-search').click(function(e) {
            //    find_multiple_video();
            //    event.preventDefault();
            //});
        });
        //-->
    </script>






    <script type="text/javascript"><!--

	
        function triggerFullscreen(vsid){
		jQuery('#playercontainer'+vsid).css('position', 'fixed');
		jQuery('#playercontainer'+vsid).css('top', '0');
		jQuery('#playercontainer'+vsid).css('left', '0');
		jQuery('#playercontainer'+vsid).css('z-index', '113');
		jQuery('#playercontainer'+vsid).css('width', '100%');
		jQuery('#playercontainer'+vsid).css('height', $(window).height() - 16);
		jQuery('#playercontainer'+vsid).css('background-color', '#000000');
		jQuery('#playercontainer'+vsid).find('div.active').css('position', 'fixed');
		jQuery('#playercontainer'+vsid).find('div.active').css('width', '100%');
		jQuery('#playercontainer'+vsid).find('div.active').css('height', $(window).height() - 16);
		jQuery('#playercontainer'+vsid).find('div.active').css('top', '0');
		jQuery('#playercontainer'+vsid).find('div.active').css('left', '0');
		jQuery('#playercontainer'+vsid).find('object').attr('width', '100%');
		jQuery('#playercontainer'+vsid).find('object').attr('height', $(window).height() - 16);
		jQuery('#playercontainer'+vsid).find('embed').attr('width', '100%');
		jQuery('#playercontainer'+vsid).find('embed').attr('height', $(window).height() - 16);
		jQuery('#home'+vsid).show();
		jQuery('#panelButtons').show().animate({'top': '0px'}, 900).delay(5000).animate({'top': '-27px'}, 900);				
		jQuery('.tools').removeClass('player').addClass('fc');
		jQuery('.tools li').hide();
		jQuery('.tools.fc li.replay-button').show();	
		jQuery('#playlistformating'+vsid).addClass('fsmode').css('height', $(window).height() - 90).css('position', 'fixed').css('right', '-365px');
		jQuery('.playlist-box').hide();

		if($(window).width() >= 1152) {
		jQuery('#nav').removeClass('body_bgr').addClass('fsmode').css('position', 'fixed').css('height', $(window).height() - 120).css('left', '-180px');		
		jQuery('#main-menu').removeClass('body_bgr');
		} 		
		jQuery('#panelSlider').show();	
		jQuery('body').css('overflow', 'hidden');	
		load_suggestions();
       	return false;
        }
        function doneFullscreen(vsid){
                jQuery('#playercontainer'+vsid).css('position', 'relative');
                jQuery('#playercontainer'+vsid).css('width', '640px');
                jQuery('#playercontainer'+vsid).css('height', '390px');
                jQuery('#playercontainer'+vsid).css('z-index', '102');
                jQuery('#playercontainer'+vsid).find('div.active').css('position', 'absolute');
                jQuery('#playercontainer'+vsid).find('div.active').css('width', '640px');
                jQuery('#playercontainer'+vsid).find('div.active').css('height', '390px');
                jQuery('#playercontainer'+vsid).find('object').attr('width', '640px');
                jQuery('#playercontainer'+vsid).find('object').attr('height', '390px');
                jQuery('#playercontainer'+vsid).find('embed').attr('width', '640px');
                jQuery('#playercontainer'+vsid).find('embed').attr('height', '390px ');				
		jQuery('#home'+vsid).hide();    
  
		jQuery('#playlistformating'+vsid).removeClass('fsmode').css('height', '390px').css('position', 'relative').css('right', '0px').css("background-image", "none").show();   
		jQuery('.playlist-box').show();
		// hide player tools
                jQuery('.tools').removeClass('fc').addClass('player');
                jQuery('.tools li').show();				
		jQuery('body').css('overflow', 'auto');		
		jQuery('#playlistcontainer_fscontainer').hide();	
		jQuery('#panelSlider').hide();	
		if($(window).width() >= 1152) {
		jQuery('#nav').removeClass('fsmode').addClass('body_bgr').css('position', 'absolute').css('left', '0px').css('top', '1px').css("background-image", "none");
		jQuery('#main-menu').addClass('body_bgr');	
		}
		jQuery('#panelButtons').hide();
		return false;
        }
	jQuery(document).keyup(function(e) {
            if (e.keyCode == 27) {
		jQuery('.slimScrollDiv').css('position', 'relative');
                jQuery('.slimScrollDiv').css('width', '100%');
                jQuery('.slimScrollDiv').css('height', '360px');				
		jQuery('#autoscroll').css('height', '360px');
                jQuery('.slimScrollDiv').css('z-index', '102');
		jQuery('.slimScrollDiv').css('position', 'relative');	
		jQuery('#scrolling-play-pause').removeClass('fs');	
		jQuery('.slimScrollDiv').removeClass('fs');		
                return false;
            }
        });

    //-->
    </script>
    <script type="text/javascript"><!--
    if(!window.videoplayers){
        videoplayers = [];
