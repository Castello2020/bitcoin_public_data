'<span class="description-content" style="display:none;"><%= tooltipContentBody %></span>' +
'</div></li>',
            playpositionTemplate: '<%= elapsedTime %> / <%= totalTime %>',
            emptyItemHtml: '<li class="empty"><span data-trans="Add any items to Create Playlist by dragging or using the \"Add\" Icon on each Video thumbnail or selecting more Videos from new Searches to build your Playlist">Add any items to Create Playlist by dragging or using the "Add" Icon on each Video thumbnail or selecting more Videos from new Searches to build your Playlist</span></li>',
            inputOrdering: true,
            descriptionsholder: '.descriptionsplace',
            enableLogging: false,

            playTrigger: '.playvideo'+vsid+'',
            addTrigger: '.addvideo'+vsid+'',
            playlistRepeat: '#playlistrepeat'+vsid+'',
            playlistShuffle: '#playlistshuffle'+vsid+'',
            playlistClear: '.playlist_clear'+vsid+'',
            playposition: '#playposition'+vsid+'',
            autoplayCheckbox: '#autoplay'+vsid+'',
            replaycurrentCheckbox: '.replaycurrent'+vsid+'',
            autoplayCookie: (vsid==1)?'wn-videoplayer-autoplay':'',
            playlistFormatCookie: 'wn-videoplayer-playlist-format',
            controlNext: '.videoplayer-control-next',
            controlPrev: '.videoplayer-control-prev',
            controlPlay: '.videoplayer-control-play',
            controlPause: '.videoplayer-control-pause',
            playlistLink: '.playlist-link'
        });
        jQuery('.autoplay').attr('checked', jQuery('#autoplay1').attr('checked'));
        jQuery('.autoplay').click(function(event){
            $('.autoplay').attr('checked', $(event.target).attr('checked'));
            videoplayers[1].onAutoplayClick();
        });
        videoplayers[vsid].playerContainer.bind('videoplayer.player.videoloaded', function(e, videoInfo){
            load_playlistlyrics(videoInfo.title);
        });

        jQuery('.autoplay').attr('checked', jQuery('#autoplay1').attr('checked'));
        jQuery('.autoplay').click(function(event){
            $('.autoplay').attr('checked', $(event.target).attr('checked'));
            videoplayers[1].onAutoplayClick();
        });
        if(vsid == 1){
            window.videoplayer = videoplayers[1];
        }

        // this will add all available videos to playlist
            vp = videoplayers[vsid];
            vp.addVideo($('#video-results-'+vsid+' .video-info').slice(0, 14));
            if(vp.getActualPlaylist().length){
                vp.playFromPlaylist(vp.getActualPlaylist()[0], vp.autoplay || autostart);
            }

            jQuery('#videoplayer-play-all'+vsid+'').click(function(event) {
                event.preventDefault();
                videoplayers[vsid].clearPlaylist();
                videoplayers[vsid].addVideo(jQuery('#video-results-'+vsid+' .video-info').slice(0, 23));
                videoplayers[vsid].playPlaylist();
            });
            jQuery('#videoplayer-play-next'+vsid+'').click(function(event) {
                event.preventDefault();
                videoplayers[vsid].playNextPlaylistVideo();
            });
            jQuery('#playlist_restore'+vsid+'').click(function(event) {
                    event.preventDefault();
                    videoplayers[vsid].clearPlaylist();
                    videoplayers[vsid].addVideo(jQuery('#video-results-'+vsid+' .video-info').slice(0, 14));
                    videoplayers[vsid].playPlaylist();
            });

			jQuery(document).keyup(ensure(function(e) {
	            if (e.keyCode == 27) {    			
					jQuery('#home'+vsid+'').click();	
	                return false;
	            }
	        }));
			
    });
