    function reposition () {
        var w = $(window).width();
        if (w <= 786) {
            $('#title').attr('src','img/title-786.png');
        } else
        if (w <= 1572) {
            $('#title').attr('src','img/title-1572.png');
        } else {
            $('#title').attr('src','img/title-3144.png');
        };
        
        if (w < 760)
            $('#head q').hide ();
        else
            $('#head q').show ();
    };

    $(window).resize(reposition);
    $('#head').ready(reposition);
