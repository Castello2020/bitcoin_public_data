        return false;
});

// refresh social counts
$(function() {
        var top_bar = $('#top-share-bar');
        var bottom_bar = $('#bottom-share-bar');

        var counts = {
                fb: top_bar.data('facebook_count'),
                fb_pre: top_bar.data('facebook_pre'),
                tw: top_bar.data('twitter_count'),
                total: top_bar.data('total_shares')
        };

        var getFacebookCount = function(url, successFunc) {
                $.getJSON('https://api.facebook.com/method/fql.query?query=select%20total_count%20from%20link_stat%20where%20url=%27'+url+'%27&format=json', successFunc);
        };

        var saveSocial = function(t, c) {
                $.ajax({
                        dataType: 'json',
                        contentType: 'application/json',
                        url: "/mvc.php?route=donate/reset&t="+t+"&c="+c+"&u="+$('#hidden_fund_url').val()
                });
        };

        var setSocialCount = function(type, count) {
                var type_class = 'share-' + type;
                var count_div = top_bar.find('.' + type_class).next();
                var top_counts_div = top_bar.find('.big');
                var bottom_counts_div = bottom_bar.find('.big');

                count = isNaN(parseInt(count)) ? 0 : count;

                counts.total = counts.total + (count - counts[type]);
                counts[type] = count;

                if(type == 'fb') {
                        counts.total += counts.fb_pre;
                        counts.fb += counts.fb_pre;
                }

                count_div.text(prettyNumbers(counts.fb));
                top_counts_div.text(prettyNumbers(counts.total));
                bottom_counts_div.text(top_counts_div.text());

                saveSocial(type, count);
        };

        var prettyNumbers = function(x) {
                if(x >= 1000000)
                        return (Math.floor(x/100000)).toFixed(1)/10 + 'M';

                if(x >= 10000)
                        return Math.floor(x/1000) + 'K';

                if(x >= 1000)
                        return(Math.floor(x/100)).toFixed(1)/10 + 'K';

                return x;
        };

        // should refresh
        if(top_bar.hasClass('refresh-counts')) {
                getFacebookCount($('#hidden_fund_url').val(), function(data) {
                        var total = data[0].total_count;
                        if(total < 0 || total == null) {
                                total = 0;
                        }

                        setSocialCount('fb', total);
                });
        }
});
</script>
