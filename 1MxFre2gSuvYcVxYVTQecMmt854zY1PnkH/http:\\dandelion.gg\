            <div class="grid-container">
                <!-- hero holder -->
                <div class="hero-holder grid-100 mobile-grid-100 tablet-grid-100 ut-hero-style-3">
                    <div class="hero-inner" style="text-align:center;">                
                        
                                                    <p><img class="alignnone size-full wp-image-2947" alt="dandelion_logo_white" src="http://dandelion.gg/wp-content/uploads/2014/02/dandelion_logo_white.png" width="441" height="319" /></p>                                                
                                                    <div class="hdh"><span class="hero-description">One Community, Infinite Potential</span></div>
                                                                
                                                    <div class="hth"><h1 class="hero-title"><span>Join Us</span></h1></div>
                                                
                                                    <span class="hero-btn-holder"><a id="to-about-section" class="hero-btn ">Get Involved</a></span>
                                                
