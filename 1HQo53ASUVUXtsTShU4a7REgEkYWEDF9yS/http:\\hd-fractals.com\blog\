<!--jQuery Navigation Slider-->

<script type="text/javascript">

jQuery(document).ready(function(){

    lastBlock = jQuery(".page_item li:last");

    
    maxWidth = 130;

    minWidth = 100;	

    jQuery("#main-nav ul li a").hover(

    
      function(){

        jQuery(lastBlock).animate({width: minWidth+"px"}, { queue:false, duration:400 });

	jQuery(this).animate({width: maxWidth+"px"}, { queue:false, duration:400});

	lastBlock = this;

      },

      function() {

      jQuery(this).animate({width: minWidth+"px"}, { queue:false, duration:400 });

      }

    );

});

</script>



