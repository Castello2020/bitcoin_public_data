        var disqus_shortname = 'geekwire';
        (function () {
            var nodes = document.getElementsByTagName('span');
            for (var i = 0, url; i < nodes.length; i++) {
                if (nodes[i].className.indexOf('dsq-postid') != -1) {
                    nodes[i].parentNode.setAttribute('data-disqus-identifier', nodes[i].getAttribute('rel'));
                    url = nodes[i].parentNode.href.split('#', 1);
                    if (url.length == 1) url = url[0];
                    else url = url[1]
                    nodes[i].parentNode.href = url + '#disqus_thread';
                }
            }
            var s = document.createElement('script'); s.async = true;
            s.type = 'text/javascript';
            s.src = '//' + disqus_shortname + '.disqus.com/count.js';
            (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
        }());
    //]]>
    </script>
    <script>
        function check_if_slidecontent_loaded_before() {
            var cookie_ad_slidecontent = jQuery.cookie('ad_slidecontent');
            if ('loaded' == cookie_ad_slidecontent) {
                return true;
            } else {
                return false;
            }
        }
        $.dfp({
            'dfpID': '16932871',
            enableSingleRequest: false,
            afterEachAdLoaded: function ($adUnit) {

            },
            afterAllAdsLoaded: function (adUnits) {
                if (false == check_if_slidecontent_loaded_before()) {
                    $('.ad_slidecontent').dfp({
                        'dfpID': '16932871',
                        enableSingleRequest: false,
                        afterEachAdLoaded: function ($adUnit) {
                            $('.ad_slidecontent').css('margin-top', '-40px');
                            jQuery.cookie('ad_slidecontent', 'loaded', { expires: 2, path: '/' });
                            setTimeout(function () {
                                $('.ad_slidecontent').slideUp();
                            }, 15000);
                        },
                        afterAllAdsLoaded: function (adUnits) {
                            $('.ad_pencilcontent').dfp({
                                'dfpID': '16932871',
                                enableSingleRequest: false
                            })
                        }
                    })
                } else {
                    $('.ad_pencilcontent').dfp({
                        'dfpID': '16932871',
                        enableSingleRequest: false
                    });
                }

                jQuery('#adspace-160x600_left').fixTo('body', { mind: '#top-bar' });
                jQuery('#adspace-160x600_right').fixTo('body', { mind: '#top-bar' });
            }
        });
    </script>
