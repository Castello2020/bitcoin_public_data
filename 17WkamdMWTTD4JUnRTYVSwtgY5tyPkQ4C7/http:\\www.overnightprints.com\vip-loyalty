                <script type="text/javascript">
            var j = jQuery.noConflict();
        </script>
                    <script src="//www.onpstatic.com/compiled/js/router.js?02b0b02"></script>
                <script type="text/javascript" src="//www.onpstatic.com/compiled/js/onp_foot.js?02b0b02"></script>
            
    <script>
        jQuery(document).ready( function(){

            jQuery("ul.prod-tabs a").click(function(e) {
                e.preventDefault();
                var currTab = jQuery(this);
                if (!currTab.hasClass("active")) {
                    var posLeft = currTab.parents("li").position().left;
                    var boxWidth = currTab.width();
                    var highSpan = jQuery("span.hlight");
                    highSpan.width(boxWidth * 0.8);
                    var floatBoxWidth = highSpan.width();
                    var newPos = posLeft + ( (boxWidth-floatBoxWidth)/2);
                    highSpan.stop(true, true).animate({"left":newPos}, 300, function() {
                        jQuery("ul.prod-tabs a").removeClass("active");
                        currTab.addClass("active");
                    });
                    // update tab content
                    var speed = 400;
                    var tabId = currTab.attr("data-tab");
                    var currInfo = jQuery(".tab-content-container .tab-content:visible");
                    var newInfo = jQuery(".tab-content-container #"+tabId);
                    jQuery(".tab-content-container").animate({height:newInfo.height()}, speed, function() {
                        jQuery(this).css("height","auto");
                    });

                    currInfo.fadeOut(speed);

                    newInfo.css("position", "absolute").fadeIn(speed, function() {
                        newInfo.addClass("active").css("position","static");
                    });
                }
            });
        });
    </script>

   <script>
       onp_require(['jQuery'], function($) {
