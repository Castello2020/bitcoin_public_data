<script>
        jQuery('.first-level-categories').each(function() {
        var $this = jQuery(this);
        var $linksBlock = $this.find('.show-all-links');

                var isOpen = $this.find('.additional-category.open').size() > 0;
        var isChosen = $this.find('.additional-category.chosen').size() > 0;
        if (isOpen || isChosen) {
            $linksBlock.siblings('.additional-category').show();
            var $link = $linksBlock.find('.show-all');
            $link.hide();
            $link.siblings().show();
        }

                if ($this.find('.gallery-category').size() <= 8) {
            $linksBlock.remove();
        }
    });

    (function($) {
        var $body = $('body');

        $body.on('click', '.toggle-sub-categories', function(e) {
            var $this = $(this);
            $this.toggleClass('open closed');
            $this.siblings('.gallery-sub-categories').slideToggle();
        });

        $body.on('click', '.show-all-links a', function(e) {
            e.preventDefault();
            var $this = $(this);
            $this.hide();
            $this.siblings().show();
        });

        $body.on('click', '.show-all', function() {
            $(this).parent().siblings('.additional-category').slideDown();
        });

        $body.on('click', '.hide-all', function() {
            $(this).parent().siblings('.additional-category').slideUp();
        });
    })(jQuery);
</script>

        </div>
    </section>

