<script type="text/javascript">

    function CheckForm()
    {
        var frm = document.addfrm;

        if (frm.torrent.value.length == 0)
        {
            alert("Choose torrent file please");
            frm.torrent.focus();
            return false;
        }

        if (!frm.torrent.value.match(/[\.]torrent$/i))
        {
            alert("Torrent file must have .torrent extension");
            frm.torrent.focus();
            return false;
        }

        if (parseInt(frm.cat.value) == 0 || frm.cat.value == '')
        {
            alert("Choose category please");
            frm.cat.focus();
            return false;
        }

        if (parseInt(frm.s_subcat.value) == 0 || frm.s_subcat.value == '')
        {
            alert("Choose subcategory please");
            frm.s_subcat.focus();
            return false;
        }

        if (!frm.c_name.checked && frm.name.value.length == 0)
        {
            alert("Enter name please");
            frm.name.focus();
            return false;
        }

        if (!frm.c_rules.checked)
        {
            alert("Please read rules and terms of use");
            return false;
        }

        document.getElementsByName("sbtn")[0].disabled = true;
        frm.submit();

        return false;
    }

</script>


