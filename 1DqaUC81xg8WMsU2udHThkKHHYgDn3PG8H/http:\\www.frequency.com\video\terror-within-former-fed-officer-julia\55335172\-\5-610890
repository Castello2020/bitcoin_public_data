    (function () {
        
        var userParams = $.cookie('freqUiData');
        try {
            userParams = JSON.parse(userParams.replace(/(^")|("$)/g, '').replace(/\\"/g, '"'));
        } catch (ex) {}
        if (typeof userParams !== 'object') {
            $.cookie('freqUiData', null, {path: '/', expires: -1});
            userParams = null;
        }
        if (userParams) {
            if (typeof user == 'undefined') {
                user = {};
            }
            user.id = $.cookie('userId') || null;
            user.username = $.cookie('username') || null;
            user.userRealName = (userParams.fn + ' ' + userParams.ln) || null;
            user.navTitle = user.userRealName || user.username || null;
            user.avatar = userParams.a;
            user.firstName = userParams.fn;
            user.lastName = userParams.ln;
            Freq.user = Freq.user(user);
        } else if (typeof user != 'undefined') {
            Freq.user = Freq.user(user);
        } else {
            Freq.user = Freq.user(undefined);
        }
    }());
</script>







