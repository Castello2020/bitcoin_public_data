						function clearText(defaultText, theText, theID){
							if(defaultText == theText){
								document.getElementById(theID).value = "";
							}
						}

						function fillText(defaultText, theText, theID){
							if(theText == ""){
								document.getElementById(theID).value = defaultText;
							}
						}
