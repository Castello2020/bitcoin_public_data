var curHoverState = "";

var color1 = "#8FD8D8";
var color2 = "#0B244D";

$(function(){
	 $("#lowMap").vectorMap({
		map: 'usa_en',
        values: taxRatesLow,
		color: '#C0C0C0',
        hoverOpacity: 0.7,
        hoverColor: false,
        scaleColors: [color1, color2],
        normalizeFunction: 'polynomial',
		backgroundColor: '#FFFFFF',
		onRegionClick: showLocation,
		onRegionOut: reset_lowMap,
        hoverOpacity: 0.7,
        hoverColor: false,
        onLabelShow: function(event, label, code){
		 curHoverState = label.text();
		 var state = label.text();
		 var rank = getRank(taxRatesLow,taxRatesLow[code], code);
		 $("#infoBox").html("<div>#"+rank+" - "+state+"</div> <p>"+state+" has a "+taxRatesLow[code]+"% <i>minimum state income tax rate</i>. "+(rank-1)+" states have higher maximum tax rates.</p>");
		 label.text(label.text()+' - '+taxRatesLow[code]+'%');
		 $(this).css("cursor", "pointer");
		}
	 });
	 
	 $("#lowMap").hide();

	 $("#highMap").vectorMap({
		map: 'usa_en',
        values: taxRatesHigh,
		color: '#C0C0C0',
        hoverOpacity: 0.7,
        hoverColor: false,
		scaleColors: [color1, color2],
        normalizeFunction: 'polynomial',
		backgroundColor: '#FFFFFF',
		onRegionClick: showLocation,
		onRegionOut: reset_highMap,
        onLabelShow: function(event, label, code){
		 curHoverState = label.text();
		 var state = label.text();
		 var rank = getRank(taxRatesHigh,taxRatesHigh[code], code);
		 $("#infoBox").html("<div>#"+rank+" - "+state+"</div> <p>"+state+" has a "+taxRatesHigh[code]+"% <i>maximum state income tax rate</i>. "+(rank-1)+" states have higher maximum tax rates.</p>");
		 label.text(label.text()+' - '+taxRatesHigh[code]+'%');
		 $(this).css("cursor", "pointer");
		}
	 });
	 
	$("#highMap").hide();
	 
	$("#averageMap").vectorMap({
		map: 'usa_en',
        values: taxRatesAverage,
		color: '#C0C0C0',
        hoverOpacity: 0.7,
        hoverColor: false,
        scaleColors: [color1, color2],
        normalizeFunction: 'polynomial',
		backgroundColor: '#FFFFFF',
		onRegionClick: showLocation,
		onRegionOut: reset_averageMap,
        onLabelShow: function(event, label, code){
		 curHoverState = label.text();
		 $(this).css("cursor", "pointer");
		 label.text(label.text()+' - '+taxRatesAverage[code]+'%');		 
		 $("#infoBox").html("<div>#"+getRank(taxRatesAverage,taxRatesAverage[code], code)+" - "+label.text()+"</div> <p>"+taxRatesAverage[code]+"% Average Tax Rate <br /> Ranked #"+getRank(taxRatesAverage,taxRatesAverage[code], code)+" nationwide</p>");
		 //$("#infoBox").css("background", $("#jvectormap3_"+code).attr("fill"));
		}
	 });

	// Set legend colors
	$("#color1").css("background", color1);
	$("#color2").css("background", color2);
});

	 
// Activation functions

function showLocation(event,label){
 activate(curHoverState);
}

function getRank(array, item, label)
{
 count = 1;
 for(index in array) 
  {
   if(array[index] > item) count++;   
   else if( (array[index] == item) && (index < label) ) count++;
  }
 return count;
}

// Switcher
function switchMap(id)
{
 $('.map').hide();
 $('.mapButton').removeAttr('disabled');
 $('.'+id+'Button').attr('disabled', 'disabled');
 $('#'+id).show();
 eval( 'reset_' + id + "()" ); // Reset the info box
}

// Lowest tax bracket

function reset_lowMap(code)
{
 $("#infoBox").html("<div>State Income Taxes</div> <p>Compared by <b>lowest</b> income tax rate. Use to compare tax rates for lower-income individuals</p>");
 reset();
}
	 
// Average tax bracket (default)

function reset_averageMap(code)
{
 $("#infoBox").html("<div>State Income Taxes</div> <p>Compared by <b>average</b> income tax rate</p>");
 reset();
}

// Highest tax bracket

function reset_highMap(code)
{
 $("#infoBox").html("<div>State Income Taxes</div> <p>Compared by <b>highest</b> income tax rate. Use to compare tax rates for high-income individuals</p>");
 reset();
}

function reset()
{
 $("#infoBox").css("background", "white");
 $(".map").css("cursor", "default");
}

// Click handler

function activate(state)
{
 window.location = "http://www.tax-rates.org/"+state.toLowerCase().replace(" ", "_")+"/income-tax";
 $("#"+state).next("div").css("background", "red;");
}
</script>
 
