<form id="YMDselector" name="YMDselector" action="archive.php" method="post">
<select name="YrSel" onChange="MonLen(YrSel, MoSel, DySel)"><option>?</select> -
<select name="MoSel" onChange="MonLen(YrSel, MoSel, DySel)"><option>?</select> -
<select name="DySel"><option>?</select>
<input type="image" border="0" src="http://imgs2.kavkazcenter.com/imgs/archive_btn_ar.gif" alt="&#1573;&#1592;&#1607;&#1575;&#1585;&#32;&#1575;&#1604;&#1571;&#1585;&#1588;&#1610;&#1601;" style="margin:10px"/>
</form>
<script type="text/javascript">
Month = ["\u064A\u0646\u0627\u064A\u0631", "\u0641\u0628\u0631\u0627\u064A\u0631", "\u0645\u0627\u0631\u0633", "\u0623\u0628\u0631\u064A\u0644", "\u0642\u062F", "\u064A\u0648\u0646\u064A\u0648", "\u064A\u0648\u0644\u064A\u0648", "\u0623\u063A\u0633\u0637\u0633", "\u0633\u0628\u062A\u0645\u0628\u0631", "\u0623\u0643\u062A\u0648\u0628\u0631", "\u0646\u0648\u0641\u0645\u0628\u0631", "\u062F\u064A\u0633\u0645\u0628\u0631"]
function Lz(x) { return (x<0||x>=10?"":"0") + x }
function LengthOfMonth(Y, M) { 
	with (new Date(Y,M,1,12)) { setDate(0) ; return getDate() } }
var D_Opt = new Array() // 29..31
function MonLen(Yr, Mo, Dy) { var Dol, K, DiM, NsI
  DiM = LengthOfMonth(+Yr.options[Yr.selectedIndex].text,
    Mo.selectedIndex+1)
  Dol = Dy.options.length
  if ( (NsI = Dy.selectedIndex) >= DiM ) NsI = DiM-1
  for (K=Dol   ; K >DiM ; K--)
    { D_Opt[K] = Dy.options[K-1] ; Dy.options[K-1] = null }
  for (K=Dol+1 ; K<=DiM ; K++)   Dy.options[K-1] = D_Opt[K]
  Dy.selectedIndex = NsI }
function InitYMDselector(Yr, Mo, Dy, Anni, Base, SetObj) { var J
  for (J=0; J<Anni; J++) Yr.options[J] = new Option(Base+J)
  for (J=0;  J<12 ; J++) Mo.options[J] = new Option(Month[J])
  for (J=0;  J<31 ; J++) Dy.options[J] = new Option(J+1)
  if (SetObj) with (SetObj) {
    Yr.selectedIndex = getFullYear()-Base
    Mo.selectedIndex = getMonth()
    Dy.selectedIndex = getDate()-1 }
  else Yr.selectedIndex = Mo.selectedIndex = Dy.selectedIndex = 0
  MonLen(Yr, Mo, Dy) }
var Now = new Date(),
  	BaseYear = 2001,
	YearSpan = new Date().getFullYear() - BaseYear + 1
with (document.forms['YMDselector'])
  InitYMDselector(YrSel, MoSel, DySel, YearSpan, BaseYear, Now)
</script>
<noscript> In order to see selectors correctly you need to have a javascript enabled browser. </noscript>
 
