function draw(gid) {
	$('#graph_' + gid).highcharts({
		chart: {
			type: 'areaspline',
			backgroundColor: "#f0f0f0",
			zoomType: "x",
		},
		plotOptions: {
			areaspline: {
				stacking: gid ? "normal" : null,
				marker: {
					enabled: false,
					symbol: 'circle',
					radius: 2,
					states: {
						hover: {
							enabled: true,
						},
					},
				},
			},
		},
		title: null,
		tooltip: {
			shared: true,
		},
		xAxis: {
			type: 'datetime',
			tickPixelInterval: 150,
		},
		yAxis: {
			title: null,
			max: 100,
			labels: {
				format: "{value}%",
			},
		},
		credits: {
			enabled: false,
		},
		exporting: {
			enabled: false,
		},
		series: gdata[gid],
	});
}

function i6tOnLoad() {
	for (var i = 0; i <= 5; i++) {
		draw(i);
	}
}
