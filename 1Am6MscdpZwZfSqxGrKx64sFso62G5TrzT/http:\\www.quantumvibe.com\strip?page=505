	var name = "#floatMenu";
	var menuYloc = null;
 
	$(document).ready(function(){
	    menuYloc = parseInt($(name).css("top").substring(0,$(name).css("top").indexOf("px")))
	    $(window).scroll(function () { 
	        var os = parseInt($(document).scrollTop());
		if (os < menuYloc) {
			os = menuYloc;
		}
	        var offset = (os)+"px";
	        $(name).animate({top:offset},{duration:0,queue:false});
	    });
	});
