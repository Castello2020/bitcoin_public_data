Number.prototype.padLeft=function(base,chr){var len=(String(base||10).length-String(this).length)+1;return len>0? new Array(len).join(chr||'0')+this:this;}
function UTCtoLocal(){$("small, .utc").each(function(){str=$(this).html();if(str.substr(str.length-3)=="UTC"){d=new Date(str.replace(/-/g,"/"));dformat=[d.getFullYear(),(d.getMonth()+1).padLeft(), d.getDate().padLeft()].join('-')+' '+[d.getHours().padLeft(),d.getMinutes().padLeft(),d.getSeconds().padLeft()].join(':');if(!isNaN(d)) $(this).html(dformat)}});}
