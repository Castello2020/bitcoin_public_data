var lastMod = "";
function mRefresh(){	$.ajax({ url:'index_c.html', type:"head", success:function(res,code,xhr) { if(lastMod != xhr.getResponseHeader("Last-Modified")){ lastMod = xhr.getResponseHeader("Last-Modified"); $("#main_body").load( "index_c.html", function() { UTCtoLocal(); $("#main_body").css("background-color", "rgba(0, 128, 0, 0.06)"); setTimeout(function(){$("#main_body").css("background-color", "")},300); }); } } })
}
