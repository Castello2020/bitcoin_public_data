	    // button-stile download-widget
	    jQuery("#post-2698 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://cdn.podseed.org/einschlafen/ep0288.m4b", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;m4b";
	    // select-style download-widget
