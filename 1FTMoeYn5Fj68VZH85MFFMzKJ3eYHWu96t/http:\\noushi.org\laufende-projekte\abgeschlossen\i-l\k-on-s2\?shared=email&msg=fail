              if(true){
                $("#ac47b6d").append("Du hast wohl AdBlock an. Stell es bitte aus, damit die Seite richtig dargestellt werden kann und natürlich um uns zu unterstützen. ♥  <a id=\"show-adblock-message\" href=\"#\"></a>").hide().fadeIn();
