	<script type="text/javascript">
		function base_url() {
			return "http://www.download-station-extension.com/";
		}
		
		function site_url(segments) {
			var url = "http://www.download-station-extension.com/";
			
			if(segments) {
				if(segments.charAt(0) == "/")
					segments = segments.substring(1);
				if(url.charAt(url.length-1) == "/")
					url = url.substring(0, url.length-1);
				
				return url + "/" + segments;
			}
			else
				return url;
		}
	</script>
	
