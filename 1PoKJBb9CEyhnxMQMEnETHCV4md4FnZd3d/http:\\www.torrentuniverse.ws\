	 $("#login_form").bind("submit", function() {

	if ($("#username").val().length < 1)  
	   ($("#password").val().length < 1)
		{
	    $("#login_error").show();
	    $.fancybox.update();
	    return false;
	}
	$.fancybox.showLoading();
    $.ajax({
		type	: "POST",
		cache	: false,
		url		: "/loginbox.php",
		data	: $(this).serializeArray(),
		success: function(data) {
			$.fancybox(data);
		}
	});
	return false;
});
