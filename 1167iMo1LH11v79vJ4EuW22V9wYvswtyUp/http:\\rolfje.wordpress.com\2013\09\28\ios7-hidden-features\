			return true;
		}
	
if ( typeof GA_googleAddAttr == 'function' ) {
GA_googleAddAttr("AdOpt", "1");
GA_googleAddAttr("Origin", "other");
GA_googleAddAttr("LangId", "1");
GA_googleAddAttr("Domain", "rolfje.wordpress.com");
GA_googleAddAttr("BlogId", "1859125");
GA_googleAddAttr("PageURL", "http://rolfje.wordpress.com/2013/09/28/ios7-hidden-features/");
GA_googleAddAttr("AdSafe", "1");
GA_googleAddAttr("Autotag", "technology");
GA_googleAddAttr("Tag", "apple");
GA_googleAddAttr("Tag", "fun");
GA_googleAddAttr("Tag", "apple");
GA_googleAddAttr("Tag", "fun");
GA_googleAddAttr("Tag", "iphone");
GA_googleAddAttr("Tag", "opinion");
GA_googleAddAttr("Tag", "usability");
GA_googleAddAttr("Partner", "AOL");
GA_googleAddAttr("theme_bg", "ffffff");
GA_googleAddAttr("theme_text", "333333");
GA_googleAddAttr("theme_link", "0066cc");
GA_googleAddAttr("theme_border", "5581C0");
GA_googleAddAttr("theme_url", "114477");
GA_googleAddAdSensePageAttr("google_page_url", "http://rolfje.wordpress.com/2013/09/28/ios7-hidden-features/");
GA_googleFillSlot("wpcom_below_post_adsafe");
}
</script>
</div>
