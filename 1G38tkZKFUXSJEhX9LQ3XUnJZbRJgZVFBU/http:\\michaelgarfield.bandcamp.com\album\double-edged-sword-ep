        (function(){
            function snickerdoodle(which) {
                var pairs = document.cookie.split(/;\s*/);
                for (var i = 0; i < pairs.length; ++i) {
                    var pair = pairs[i].split("=");
                    if (pair[0] == which) {
                        return pair[1];
                    }
                }
                return null;
            }


            // pull id out of the cookies
            window.ClientID = snickerdoodle("client_id");

        })();
    </script>


