		function displayHistory(id, displayTime, currentTime, fullText) {
			function format() {
				var diff = currentTime - displayTime;
				var isFuture = false;
				if(diff < 0) {
					diff = -diff;
					isFuture = true;
				}
				var prefix = isFuture ? "in " : "";
				var postfix = isFuture ? "" : " ago";

				if(diff <         60) return "<b>"+prefix+""+Math.floor(diff % 60)+" seconds"+postfix+"</b>";
				if(diff <       1*60) return "<b>"+prefix+"1 minute"+postfix+"</b>";
				if(diff <      90*60) return "<b>"+prefix+""+Math.round(diff / 60)+" minutes"+postfix+"</b>";
				if(diff <     1*3600) return "<b>"+prefix+"1 hour"+postfix+"</b>";
				if(diff <    24*3600) return "<b>"+prefix+""+Math.round(diff / 3600)+" hours"+postfix+"</b>";
				if(diff <  2*24*3600) return "<b>"+(isFuture?"tomorrow":"yesterday")+"</b>";
				if(isFuture || diff <  4*24*3600) return ""+prefix+Math.round(diff / 86400)+" days"+postfix+"";
				return fullText;
			}
			document.getElementById(id).innerHTML = format();
		}

		function displayCountdown(deadlineTime, currentTime) {
			var id = "countdown_"+(new Date().getTime());
			document.write("<span id=\""+id+"\"></span>");
			var elem = document.getElementById(id);
			function f() {
				var remainingTime = (deadlineTime - currentTime);
				if(remainingTime < 0) {
					elem.innerHTML = "Deadline expired.";
					return;
				}

				var ss = Math.floor(remainingTime /     1) % 60;
				var mm = Math.floor(remainingTime /    60) % 60;
				var hh = Math.floor(remainingTime /  3600) % 24;
				var dd = Math.floor(remainingTime / 86400);

				ss = (ss < 10 ? "0" : "")+ss;
				mm = (mm < 10 ? "0" : "")+mm;
				hh = (hh < 10 ? "0" : "")+hh;
				var time = hh+":"+mm+":"+ss;

				var days = "";
				     if(dd == 0) days = "";
				else if(dd == 1) days = dd+" day &";
				else if(dd == 2) days = dd+" days &";
				else           { days = dd+" days"; time=""; }

				elem.innerHTML = days+" "+time+" remaining";
				currentTime += 1;
			}
			f();
			setInterval(f, 1000);
		}

		function shrinkImage( intoId, url, maxW, maxH, onError, doClip ) {
			if(typeof doClip == "undefined") {
				doClip = false;
			}

			var img = document.createElement("img");

			img.onerror = onError;

			img.onload = function() {
				var ratio = maxW / img.width;
				var imgW = img.width*ratio;
				var imgH = img.height*ratio;
				var scale = 1.0;
				if(doClip) {
					if(imgW > maxW) {
						scale = maxW / imgW;
					}
					if(imgH > maxH) {
						scale = maxH / imgH;
					}
				}
				else {
					if(imgH < maxH) {
						scale = maxH / imgH;
					}
					else if(imgW < maxW) {
						scale = maxW / imgW;
					}
				}
				imgW = Math.floor(imgW * scale);
				imgH = Math.floor(imgH * scale);

				if(imgW > img.width) {
					imgW = img.width;
					imgH = img.height;
				}

				var imgX = Math.floor((maxW-imgW)/2);
				var imgY = Math.floor((maxH-imgH)/2);

				var cvs = document.createElement("canvas");
				cvs.width = maxW;
				cvs.height = maxH;
				var ctx = cvs.getContext("2d");
				ctx.fillStyle = "#000";
				ctx.fillRect(0, 0, maxW, maxH);
				ctx.drawImage(img, imgX, imgY, imgW, imgH);
				ctx.strokeRect(0, 0, maxW, maxH);

				document.getElementById(intoId).appendChild(cvs);
			}

			img.src = url;
		}

		function getCompatibleComputedStyle(oElm, strCssRule){
			var strValue = "";
			if(document.defaultView && document.defaultView.getComputedStyle){
				strValue = document.defaultView.getComputedStyle(oElm, "").getPropertyValue(strCssRule);
			}
			else if(oElm.currentStyle){
				strCssRule = strCssRule.replace(/\-(\w)/g, function (strMatch, p1){
					return p1.toUpperCase();
				});
				strValue = oElm.currentStyle[strCssRule];
			}
			return strValue;
		}

		function checkAdBlocker(elem) {
			var w = getCompatibleComputedStyle(elem, "width");
			var h = getCompatibleComputedStyle(elem, "height");
			if(w=="auto" || h=="auto") {
				detectedAdBlocker();
			}
		}

		var isAdblockerDetected = false;
		function detectedAdBlocker() {
			if(isAdblockerDetected) {
				return;
			}
			isAdblockerDetected = true;

			var msg = "You seem to be using an ad blocker. :o(";

			if(false) {
				setTimeout(function() {
					setInterval(function() {
						if(typeof this.offset == "undefined") {
							this.offset = 0;
						}
						this.offset += 1;
						if(this.offset > msg.length) {
							//notifyBigger("<a href='http://puppygames.net/' target='_blank'><img src='http://static.puppygames.net/images/09/banners/home-banner-titan-attacks.gif'></a>");
							return;
						}
						notify(msg.substring(0, this.offset));
					}, 1000/10);
				}, 2000);
			}

			if(false) {
				setTimeout(function() {
					var messWithBody = document.getElementsByTagName("body")[0];
					setInterval(function() {
						if(typeof this.offset == "undefined") {
							this.offset = 0;
						}
						this.offset += 1;
						messWithBody.style.marginTop = "-"+this.offset+"px";
					}, 1000/25);
				}, 2000);
			}
		}

