		function upopFloat(upop) {
			upop_arr[upop_arr.length] = this;
			var upoppointer     = eval(upop_arr.length-1);
			this.pagetop        = 0;
			this.cmode          = (document.compatMode && document.compatMode!="BackCompat") ? document.documentElement : document.body;
			this.upopsrc        = document.all? document.all[upop] : document.getElementById(upop);
