		function upopGetOffsetY(upop) {
			var upopTotOffset = parseInt(upop.upopsrc.offsetTop);
			var parentOffset = upop.upopsrc.offsetParent;
			while ( parentOffset != null ) {
				upopTotOffset += parentOffset.offsetTop;
				parentOffset  = parentOffset.offsetParent;
			}
			return upopTotOffset;
		}
		function upopFloatInit(upop) {
			upop.pagetop = upop.cmode.scrollTop;
			upop.upopsrc.style.top = upop.pagetop - upop.upopoffset + "px";
		}
