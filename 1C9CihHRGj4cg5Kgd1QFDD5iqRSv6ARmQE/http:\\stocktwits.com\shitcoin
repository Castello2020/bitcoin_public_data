<script>
  $(document).ready(function(){
  	if($.browser.iphone || $.browser.ipad || $.browser.android){
  		$('a#sign-larry-up').text('Get the App');
  		$('#sign-larry-up + span').html('Or <a href="javascript:;" id="mobile-sign-larry-up">Sign Up</a>');
  	}
  });
  
  
  $('#sign-larry-up, #sign-larry-in').click(function(e){
  	e.preventDefault();
  
  	if($.browser.android){
  		window.open('https://play.google.com/store/apps/details?id=org.stocktwits.android.activity');
  		return false;
  	}
  	if($.browser.iphone || $.browser.ipad){
  		window.open('https://itunes.apple.com/us/app/stocktwits/id389157776?mt=8');
  		return false;
  	}
  
  	if($(e.target).is("#sign-larry-in")){
  		SignInModal.open();
  	} else{
  		SignInModal.open();
  		SignInModal.switchPane('#signin-modal-signup');
  		$('#modal-full-name').focus();
  	}
  });
  
  $(document).on('click', '#mobile-sign-larry-up', function(){
  			SignInModal.open();
  			SignInModal.switchPane('#signin-modal-signup');
  			$('#modal-full-name').focus();
  });
</script>

