	function loadShow(show, season, langs, hd, hi)
	{
		$("episodes").innerHTML = '<img src="http://cdn.addic7ed.com/images/loader.gif" />';
		new Ajax('/ajax_loadShow.php?show='+show+'&season='+season+'&langs='+langs+'&hd='+hd+'&hi='+hi, {
			method: 'get',
			update: $("episodes")	
		}).request();
	}
