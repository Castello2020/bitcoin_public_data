			holder=me.find(".soc"),
			title=false,
			path=location.pathname,
			nidr=/\/(\d+)\//,
			urlnid=location.pathname.match(nidr),
			base="http://www.dailypaul.com",
			nid=me.attr("id").replace(/nid/,'')
			
		if(!holder.hasClass("filled")){
			if(location.pathname=="/"){//frontpage
				var a=me.find("h2.title a"),
					href=a.attr("href")
				title=a.text()
				path=href
			}
			else if(urlnid){//node
				title=$("#main h1.title").text()
			}
			else{//custom url
				title=$("#main h1.title").text()
			}
			
			var url=base+path,
				eurl=encodeURIComponent(url),
				etitle=escape(title.replace(/Ron Paul/gi,"#RonPaul").replace(/[^\w\s\~\!\@\#\$\%\^\&\*\(\)\[\]\|\;\:\'\"\,\<\.\>\/\?]/g,'')),
				opts=eurl+"&text="+etitle
				//,miniurl="http://dailypaul.com/"+nid,

			for(var i in badges)
				$("<iframe />")
					.attr({
						"src":badges[i].src+opts,
						frameborder:0,
						scrolling:"no",
						allowtransparency:"true"
					})
					.width(badges[i].w).height(sh)
					.appendTo(holder)
			holder.addClass("filled")
			me.unbind("mouseover")
		}
	})

}
})()

