    <h2 class="title"></h2>
    <div class="content"><style>
#block-block-123{margin:0px 0px 0px 0px;}
</style>
<div id=sim></div>
<script>
//if(!console) window.console={log:function(){return false}}
var nid=location.pathname.split("/",2)[1]
if(!nid.match(/\D/)){

	function gsearch(q,callback){
		$.ajax({
				dataType:"jsonp",
				url:"http://ajax.googleapis.com/ajax/services/search/web",
				data:{
					v:"1.0",
					q:q,
					rsz:"8"
					//,callback:c
				}
				//,jsonp:c
				,success:callback
			})
	}
	function parseIntoList(x){
		//console.log(x)
		var a=x.responseData.results,
			o={},
			a2=[],
			s="<ul>",
			months={"jan":1,"feb":2,"mar":3,"apr":4,"may":5,"jun":6,"jul":7,"aug":8,"sep":9,"oct":10,"nov":11,"dec":12}
		for(var i in a)
			if(	a[i].url.match(/\.com\/\d+|\.com\/node\/\d+/gi)
				&&		
				!(a[i].url+"/").match("/"+nid+"/")
			){
				var nid2=a[i].url.split(".com/")[1].replace(/\/.*/g,''),
					aDate=a[i].content.split(" ",3)
				if(aDate[2].match(/20[\d]{2}/)){//if it matches a year, it's probably a date
					//now make date small as possible: m/d/yy
					var nu=[
						months[aDate[0].toLowerCase()],
						d=aDate[1].replace(/\D/g,''),
						y=aDate[2].replace(/20(..)/,"$1")
					]
					aDate="<span style='color:gray;'>"+nu.join("/")+"</span>"
				}
				else aDate="",nu=[1,1,2000]
				
				if(new Date(new Date()*1-new Date(nu)*1).getDate()>2)//get around noisy recent search results
					o[nid2]=[
						new Date(nu.join("/"))*1,
						"<li><a href='"+a[i].url.replace(/^.*\.com/,'')+"&sss=1'>"+a[i].titleNoFormatting.replace(/\|.*/,'').replace(/\.?\.?\.? ?- Ron Paul/,'...')+"</a> "+aDate+"</li>"
					]
			}
		
		var a2=[]
		for(var i in o) a2.push(o[i])
	
		var give=false
		if(a2.length){
			//sort by newest
			a2.sort(function(a,b){return b[0]-a[0]})
			for(var i in a2) s+=a2[i][1]
			s+="</ul>"
			give=$(s).css({"font-size":"11px","line-height":"1.25em",padding:"0px",margin:"0px 0px 0px 1.5em"})
		}
		return give;
	}
	
	
	//search for similar subjects
	//was .com/*, removed asterick
	var q="site:dailypaul.com/ "+$("h1.title").text().replace(/\W/g,' ').replace(/\s+/g,' ')
	gsearch(q,function(data){
		var found=parseIntoList(data)
		if(found){
			$("<div />")
				.append($('<h2 class="title" title="(sometimes)">Similar Subjects <span style="color:transparent">(Sometimes)</span></h2>'))
				.append(found)
				.appendTo("#sim")
		}
	})
	
	//if one link in OP, check if it's ever been linked to on the DP before
	//ok so this isn't always accurate, because google indexes a lot of data in the sidebar, which results in false positives.
	//I'll restrict it to YT for now.
	var a=$("#main_content .node>.content>*:not(.da2a,.vud-widget,script)").find("a[href*='youtu']")
	if(a&&a.length==1){
		var href=a.attr("href"),
			q2=" link:"+href
		//if YT only use YT key
		if(href.match(/youtu\.be\/[\w-]{11}|youtube\.com\/\S*[\w-]{11}/i)&&href.replace(/^.*(v=|[^r]\/|be\/)([\w-]{11}).*$/,"$2").length==11){
			q2=""+href.replace(/^.*(v=|[^r]\/|be\/)([\w-]{11}).*$/,"$2").replace(/^-/,'')//-search means "subtract from search--will yield completely false results
			var q="site:dailypaul.com "+q2
			gsearch(q,function(data){
				var found=parseIntoList(data)
				if(found){
					$("<div />")
						.append($('<h2 class="title">Also linked at</h2>'))
						.append(found)
						.prependTo("#sim")
				}
			})
		}
	}
}
</script></div>
