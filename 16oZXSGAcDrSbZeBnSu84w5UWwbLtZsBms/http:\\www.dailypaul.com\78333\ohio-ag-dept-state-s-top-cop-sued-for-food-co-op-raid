  <div class="block block-block" id="block-block-175">
    <h2 class="title">Book Talk</h2>
    <div class="content"><style>
#vbkshlf{
 -webkit-columns: 2;
 -moz-columns: 2;
 -ms-columns: 2;
 -o-columns: 2;
 columns: 2;
}
#vbkshlf>li{
 display: inline-block;
 width:100%;
 margin-bottom:2.25em;
}
#vbkshlf .book{
 display: block;
 text-align:center;
}
#vbkshlf .mention{
 display: block;
 vertical-align: top;
 height: 100%;
 padding-left:0px;
}
#vbkshlf .mention:before{
 content: "\2022 ";
 color: black;
 display: inline-block;
 padding-right: 0.25em;
}
#vbkshlf .usr {
 display: block;
 font-size:10px;
 color:#aaa;
}
#vbkshlf .book img {
 box-shadow: 0px 10px 15px -10px #aaa;
 margin-bottom: 3px;
 max-width:100%;
}
