			holder=me.find(".soc"),
			title=false,
			path=location.pathname,
			nidr=/\/(\d+)\//,
			urlnid=location.pathname.match(nidr),
			base="http://www.dailypaul.com",
			nid=me.attr("id").replace(/nid/,'')
			
		if(!holder.hasClass("filled")){
			if(location.pathname=="/"){//frontpage
				var a=me.find("h2.title a"),
					href=a.attr("href")
				title=a.text()
				path=href
			}
			else if(urlnid){//node
				title=$("#main h1.title").text()
			}
			else{//custom url
				title=$("#main h1.title").text()
			}
			
			var url=base+path,
				eurl=encodeURIComponent(url),
				etitle=escape(title.replace(/Ron Paul/gi,"#RonPaul").replace(/[^\w\s\~\!\@\#\$\%\^\&\*\(\)\[\]\|\;\:\'\"\,\<\.\>\/\?]/g,'')),
				opts=eurl+"&text="+etitle
				//,miniurl="http://dailypaul.com/"+nid,

			for(var i in badges)
				$("<iframe />")
					.attr({
						"src":badges[i].src+opts,
						frameborder:0,
						scrolling:"no",
						allowtransparency:"true"
					})
					.width(badges[i].w).height(sh)
					.appendTo(holder)
			holder.addClass("filled")
			me.unbind("mouseover")
		}
	})

}
})()

if(0&&$("body.jtheme").length){
	$("body").addClass("taller")
	$("<style />").html(""
		+".thinvote{background-imadge: url(https://lh4.googleusercontent.com/-Pr_3M4e-XTQ/TiuWI7KhPII/AAAAAAAACNU/AoRTl2mHmhQ/s28/); background-attachment: initial; background-origin: initial; background-clip: initial; background-color: #006; width: 65px; height: 20px; position: absolute; left: 00x; top: -16px; background-position: 50% 50%; background-repeat: no-repeat no-repeat; color:white;font-size: 1.2em;line-height: 1em;text-align: center;font-family: Georgia;border: solid silver 1px;box-shadow: 0px 1px 6px rgba(0,0,0,.5);border-radius: 1em;background: -webkit-linear-gradient(left,#006 0%,#33A 50%,#006 100%);}"
		+".thinvote .up{float:right}"
		+".thinvote .down{float:left}"
		+".thinvote .aro{padding:.1em;}"
		+".thinvote .sum{font-size:1.0em;letter-spacing: -.1em;font-weight: bold;}"
	).appendTo("head")

	$(".vud-widget").each(function(){
		var was=$(this),
			n=was.parent().parent(),
			sum=was.find(".updown-current-score").text()*1,
			nu=$("<div />").addClass("thinvote").html(""
				+"<span class='sum'>"+sum+"</span>"
				+"<a class='down aro'>&#9660;</a>"
				+"<a class='up   aro'>&#9650;</a>"
			)
				
		was.remove()
		nu.prependTo(n)
	})
}
</script></div>
 </div>
</div>							</div>
		</div>
