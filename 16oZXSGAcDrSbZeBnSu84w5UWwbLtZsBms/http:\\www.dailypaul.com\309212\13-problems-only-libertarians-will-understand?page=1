 list-style:none!important;
 margin:0 0 0 0!important;
}
.ftags .blogpost{
 font-size:12px;
 min-height:32px;
 padding: 2px;
 line-height: 1.33em;
}
.ftags .blogpost .bp>a{display:block}
.ftags .blogpost .portrait{
 float:left;
 margin-right:5px;
 display:block;
 width:32px;
 height:32px;
 background-size:cover;
 background-position:center center;
}
.ftags .blogpost .headline{font-weight:bold;font-size:12px;}

.ftags .tagtab li>*{
cursor:pointer;
/*display:inline-block;*/
color: white;
border-radius:1em 1em 0 0;
padding: .5em;
font-family: verdana;
/*box-shadow: inset 0 0 1em white;*/
box-shadow: inset 0 0 1em #55A;
background:#006;
