#block-views-Features-block_1 .pgr .aro.disabled{color:gray;}
#block-views-Features-block_1 .pgr .aro:hover{color:#900;}
#block-views-Features-block_1{
border: solid #999 1px;
padding-bottom: 0;
}
</style>    </div>
  
  
  
