function getRandomNid(it){
	it=$(it)
	$.ajax({
		url:"/randombutton",
		dataType:"json",
		beforesend:function(){it.html("").css({background:"url('http://www.dailypaul.com/misc/throbber.gif') 0 75%;"})},
		success:function(r){location.pathname="/"+r.n[0].n.nid+"&rbtn=1"}
	})
}
