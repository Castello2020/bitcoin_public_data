    <div class="content"><script>
(function(){if(1){

var rs1=Math.random().toString().replace(/\./g,'z'),
    rs2=Math.random().toString().replace(/\./g,'y'),
    badges={
		tw:{
			src:"http://platform.twitter.com/widgets/tweet_button.html?via=dailypaul&url=",
