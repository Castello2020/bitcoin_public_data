<script language="JavaScript">

//-- show comments block
var comments_block = document.getElementById("comments_block_div");
comments_block.style.display="block";
comments_block.style.visibility = "visible";

//-- clobal vars

//var curr_page = 0;
var curr_page = 0
var page_qty = 0;
var ruler_qty = 10; //-- number of pages in ruler

var ie=(document.all&&!window.opera)?true:false;
var mozilla=(document.getElementById&&!ie&&!window.opera)?true:false;

var MAX_COMMENT_LENGTH = 1500;



function goPage(page) {



	var encoding = "utf-8";
	var toReceive = 60000;
	var url = "/comments.asp?vid=6784091&page=" + page;

	 var comment_area = document.getElementById("comment_area");
	//if(comment_area && typeof(comment_area) == "object") {
	var txt = httpRequest(url, encoding, toReceive);
	if (txt > "") {
		document.getElementById("tr1").style.display = "";
		document.getElementById("tr2").style.display = "";
		document.getElementById("tr3").style.display = "";
		document.getElementById("tr4").style.display = "";
		document.getElementById("tr5").style.display = "";
	}
	// alert(url);
	comment_area.innerHTML = String(txt);
	comment_area.style.overflow = "auto";
	//}

	var header_area1 = document.getElementById("header_area1");
	var header_area2 = document.getElementById("header_area2");
	if(txt > "") { header_area1.style.display = "block"; header_area2.style.display = "none"; } else { header_area1.style.display = "block"; header_area2.style.display = "block"; }

	var cnt_member_comments= document.getElementById("cnt_member_comments");
	var txt = httpRequestPost(url+ "&doit=com_cnt", encoding, toReceive,"");
	cnt_member_comments.innerHTML = String(txt);	
	
	var ruler_area = document.getElementById("ruler_area");
	var ruler_area2 = document.getElementById("ruler_area2");
	
	var txt = httpRequest(url + "&doit=pageruler&ruler_qty=" + ruler_qty, encoding, toReceive);
	ruler_area.innerHTML = txt;
	ruler_area2.innerHTML = txt;	
	

	curr_page = page;	
		
}


function doReport(id) {
	window.open('/comments.asp?doit=report&vid=6784091&id='+id,'report','width=500,height=420,status=no,toolbar=no,scrollbars=no');
}

function doSubmit() { 
	var f = document.comments;
	var doit = f.doit;

	if(arguments.length>0) doit.value = arguments[0]; 

	var fcom = f.comment.value;

	var params = "";
	for(ii=0;ii<f.length;ii++)
	{
		if(f[ii].name == "comment") f[ii].value = String(f[ii].value).replace(/\r\n|\r|\n/g,"<br>").replace(/&/gi,"%26");
		params += f[ii].name + "=" + f[ii].value + "&";					
	}	

	if (params == "") return;
	
	var url = "/comments.asp";
	var encoding = "utf-8";
	var toReceive = 60000;
	
	if(!f.username.value > "" || (!f.member_id.value > 0 && !f.password.value > "")) { alert("Type both user name and password"); f.username.focus(); return; }
	if(!f.comment.value > "") { alert("Type comment."); f.comment.focus(); return; }
	if(String(f.comment.value).length > MAX_COMMENT_LENGTH) {
		alert("Comments are limited to 1500 characters.\n\nYour comment is " + String(f.comment.value).length + " characters.");
		f.comment.value = fcom;
		f.comment.focus();
		return;
	}
	//if(!f.captchaString.value > "") { alert("Type validation code."); f.captchaString.focus(); return; }
	
	
				
	var submit_area = document.getElementById("submit_area"); 
	var pre_var = "";
	
	chasiki(1);
	if(submit_area && typeof(submit_area) == "object") {
			var str  = httpRequestPost(url, encoding, toReceive, params.replace(/\+/gi,"%2B").replace(/ /gi,"+") );
			submit_area.innerHTML = str;
			if (str.indexOf("Thank your for your comment. It has been successfully posted.") > -1) {
				var rt = httpRequest("/pic-popup-for-comment.js.asp", encoding, toReceive);
				var scr = document.createElement ('script');
				scr.defer = true;
				scr.text = rt;
				document.body.appendChild (scr); 
			}
	}
			
	chasiki();
	
	if(doit.value == "submit") goPage(0);
	
}

function getForm() {

	var url = "/comments.asp?vid=6784091&doit=submitform";
	var encoding = "utf-8";
	var toReceive = 60000;
	
	var submit_area = document.getElementById("submit_area");
	
	if(submit_area && typeof(submit_area) == "object") {
		submit_area.innerHTML = httpRequestPost(url, encoding, toReceive,"");
	}

}


function getCookie(name) {

	var regExp 	= new RegExp(name + "=([^;]*)","gi");
	var ck 		= regExp.exec(String(document.cookie));	
	ck 			= RegExp.$1;

	return ck;
}

function chasiki(hide) { 
	var submit_area = document.getElementById("submit_area");
	var chasiki_area = document.getElementById("chasiki_area");
	
	if(hide) { 
		submit_area.style.display = "none";
		chasiki_area.style.display = "block";	
	} else {
		submit_area.style.display = "block"; 
		chasiki_area.style.display = "none";
	}
}

function comment_count() {
var cmntbr = String(document.getElementById("comment").value).replace(/\r\n|\r|\n/g,"<br>").replace(/&/gi,"%26");
	var comment = cmntbr.length;
	var comment_cnt = document.getElementById("comment_cnt");
	
	comment_cnt.innerHTML = comment > MAX_COMMENT_LENGTH ? '<b style="color:red;">' + comment + '</b>' : '<b>' + comment + '</b>';

}
goPage(0);
getForm(false);

</script>


    <br>
    			</div>
