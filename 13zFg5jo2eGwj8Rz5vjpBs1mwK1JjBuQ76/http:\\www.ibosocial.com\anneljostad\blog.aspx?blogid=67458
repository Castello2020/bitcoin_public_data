        <tr>
            <td>

                
<script type="text/javascript">




    function togglecomment(id) {
        var commentimage;
        commentimage = document.getElementById('commentshowimage_' + id);
        if (document.getElementById('comments_' + id).style.display == 'none') {
            commentimage.src = "/images/wallcomment_hide.jpg";
        }
        else {
            commentimage.src = "/images/wallcomment_show.jpg";
        }
        Effect.toggle('comments_' + id, 'slide', { delay: 0.2 });

    }


