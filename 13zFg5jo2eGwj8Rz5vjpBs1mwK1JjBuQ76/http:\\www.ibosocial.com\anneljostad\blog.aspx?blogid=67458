<script type="text/javascript">




    function scrollToBottom(elm_id) {
        //eo_Callback('callback_chat');
        var elm = document.getElementById(elm_id);
        try {
            elm.scrollTop = elm.scrollHeight;
        }
        catch (e) {
            var f = document.createElement("input");
            if (f.setAttribute) f.setAttribute("type", "text")
            if (elm.appendChild) elm.appendChild(f);
            f.style.width = "0px";
            f.style.height = "0px";
            if (f.focus) f.focus();
            if (elm.removeChild) elm.removeChild(f);
        }
    }



    function closehudchat() {

        new Effect.SlideUp('hud_chat', { duration: .2 });
        //document.getElementById('hudchat_refreshtype').value = "stop";
       // eo_Callback('callback_hud_chat');
        //document.getElementById('hud_chat').style.display = "none";
    }

    function endhudchat() {

        new Effect.SlideUp('hud_chat', { duration: .2 });
        document.getElementById('hudchat_refreshtype').value = "end";
        eo_Callback('callback_hud_chat');
        //document.getElementById('hud_chat').style.display = "none";
    }

    function beginchat(userid) {

        jqcc(document).ready(function () {
            jqcc.cometchat.chatWith(userid);
            jqcc.cometchat.sendMessage(userid, 'Requesting Chat from IBOsocial..');
            //document.getElementById('hud_chat').style.display = "none";
        });
    } 

    function openhudchat() {

        new Effect.SlideDown('hud_chat', { duration: .2 });

        // document.getElementById('hud_chat').style.display = "block";
    }

    function hudchatbusy_on() {
        document.getElementById('hudchat_busy').style.display = "block";
        return true;
    }

    function hudchatbusy_off() {
        document.getElementById('hudchat_busy').style.display = "none";
        if (document.getElementById('hudchat_refreshtype').value == "success") {
            window.location = "/chatcheck.aspx";
        }
        if (document.getElementById('hudchat_refreshtype').value == "ended") {
            window.location = "/chatcheck.aspx";
        }

        return true;
    }

    function openchat() {
        document.getElementById('hudchat_refreshtype').value = "startnew";
        eo_Callback('callback_hud_chat');
        openhudchat();
        return true;
    }

    function requestchat() {
        document.getElementById('hudchat_refreshtype').value = "request";
        eo_Callback('callback_hud_chat');
        return false;
    }


</script>
