n#include &lt;string.h&gt;<br />\n<br />\nsize_t MEM_SIZE = 1024*1024*1024;<br />\nfloat MEM_USE = 2;<br />\nsize_t ALLOC_SIZE = 4*1024*1024;<br />\n<br />\nint main() {<br />\n        char **lists;<br />\n        char *list;<br />\n        size_t i,j;<br />\n        size_t lists_len = (MEM_SIZE*MEM_USE) / ALLOC_SIZE;<br />\n<br />\n        lists = malloc(lists_len * sizeof(list));<br />\n        for (i = 0; i &lt; lists_len; i++)<br />\n                lists[i] = malloc(ALLOC_SIZE);<br />\n<br />\n        while (1) {<br />\n                for (i = 0; i &lt; lists_len; i++)<br />\n                    memcpy(lists[i], lists[(i+1)%lists_len], ALLOC_SIZE);<br />\n        }<br />\n        return 0;<br />\n}\r\n				</blockquote>\r\n			</div>\r\n\r\n			\r\n\r\n\r\n		</div>\r\n			\r\n				\r\n			\r\n			\r\n			\r\n		</div>\r\n	</div>\r\n	<div class="postfoot">\r\n		<div class="textcontrols floatcontainer">\r\n			<span class="postcontrols">\r\n				<img style="display:none" id="progress_141035" src="images/misc/progress.gif" alt="" />\r\n				\r\n				\r\n				\r\n					<a id="qrwq_141035" class="newreply" href="newreply.php?s=e98812dbaa5a173411a623ce48472064&amp;do=newreply&amp;p=141035" rel="nofollow" title="Reply With Quote"><img id="quoteimg_141035" src="clear.gif" alt="Reply With Quote" />  Reply With Quote</a> \r\n				\r\n				\r\n				\r\n			</span>\r\n			<span class="postlinking">\r\n				\r\n					\r\n				\r\n				\r\n\r\n				 \r\n				\r\n\r\n					\r\n\r\n					\r\n					 \r\n\r\n					\r\n					\r\n					\r\n					\r\n					\r\n			</span>\r\n		</div>\r\n	</div>\r\n	<hr />\r\n</li>\r\n';
