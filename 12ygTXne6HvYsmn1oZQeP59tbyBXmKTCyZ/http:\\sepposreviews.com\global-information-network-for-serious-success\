					if(!jQuery().dcSocialFloater) {
						jQuery.getScript('http://sepposreviews.com/wp-content/plugins/slick-social-share-buttons/js/jquery.social.float.1.3.js', function(){
							jQuery('#dc-dcssb').dcSocialFloater(config);
						}); 
					} else {
						jQuery('#dc-dcssb').dcSocialFloater(config);
					}
								});
			</script>
		
			<script type='text/javascript' src='http://sepposreviews.com/wp-includes/js/comment-reply.min.js?ver=3.9.1'></script>
</body>
</html>

