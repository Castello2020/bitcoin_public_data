    <script type="text/javascript">
      jQuery(function($) {
        var $linkset = $('.m-linksets');
        var $links = $linkset.find('a');

        var interval = setInterval(function() {
          if (Util.inViewport($linkset[0])) {
            ga('t1.send', 'event', 'Next Clicks', 'View', document.title);
            clearInterval(interval);
          }
        }, 500);

        $links.on('click', function(e) {
          // not cmd/ctrl/right/middle-click
          if (event.which === 1 && !event.metaKey && !event.ctrlKey) {
            var $link = $(this);
            ga('t1.send', {
              hitType: 'event',
              eventCategory: 'Next Clicks',
              eventAction: 'Click',
              eventLabel: $link.text(),
              eventValue: $links.index($link),
              hitCallback: function() {
                console.log('next click navigating');
                document.location = $link.attr('href');
              }
            });
            e.preventDefault();
          }
        });
      });
    </script>
</div>
<style type="text/css">.next-clicks-main-container {display: none}</style>

            <div class="m-linksets__label">SB Nation Featured Video</div>
  <div class = "m-linksets__playlist_wrapper">
  <div id="playlistContainer" ></div>
  <script type="text/javascript" src="//player.ooyala.com/v3/e7e908eebffd4efeb96a3096aa9b4bd0?platform=html5-fallback&namespace=ArticlePlaylistPlayer"></script>

    <script type="text/javascript">
      var playlistPlayer;
      ArticlePlaylistPlayer.ready(function() {
        var playerConfiguration = 
{
        autoplay: false,
        loop: false,
        height: '',
        width: '',
        useFirstVideoFromPlaylist: true
}
;
        playerConfiguration.playlistsPlugin = {"data":["54d85399a5c74f7cb333ceb98b232770"]};

        playerConfiguration.onCreate = function(player){
          window.PLMB = player.mb;
        }

        playlistPlayer = ArticlePlaylistPlayer.Player.create('playlistContainer', '', playerConfiguration);
        var lastLogTitle = "";
        window.PLMB.subscribe(ArticlePlaylistPlayer.EVENTS.PLAYING, 'ArticlePlaylist', function(eventName){
          var title = playlistPlayer.getCurrentItemTitle();
          if(title != lastLogTitle) {
            ga('send', 'event', 'ArticlePlaylist', 'Play', title);
            lastLogTitle = title;
          }
        });

    var $embed = jQuery('#playlistContainer');
    var interval = setInterval(function() {
      if (Util.inViewport($embed[0])) {
        var title = playlistPlayer.getCurrentItemTitle();
        if(title) {
          ga('send', 'event', 'ArticlePlaylist', 'View', title);
          clearInterval(interval);
        }
      }
    }, 500);

  });

    </script>
  </div>



      
