<!-- modal QR -->
<script type="text/javascript">
//if a QR link is clicked, prevent the default action and open the modal window
jQuery('a.qrLink').each(function(QRLink) {
	jQuery(this).on('click', function() {
          var attachmentID = jQuery(this).attr('id');
          openQR(attachmentID);
        });
});
function openQR(attachmentID) {
	jQuery('#qrCode').load('/qrcode.php?attachmentid='+attachmentID);
	jQuery('#qrModal').show();
	jQuery('#qrOverlay').show();
}
function closeQR() {
	jQuery('#qrModal').hide();
	jQuery('#qrOverlay').hide();
}
</script>
<!-- end modal QR -->
<!-- QR modal -->
<div id="qrOverlay" style="display:none;background:rgba(0,0,0,0.7);width:100%;height:100%;position:fixed;top:0;left:0;z-index:90;">
	<a href="javascript:closeQR();" style="display:block;width:100%;height:100%;z-index:91;position:absolute;top:0;left:0;"></a>
	<div id="qrModal" style="display:none;width:250px;height:250px;z-index:99;position:relative;margin-top:-125px;margin-left:-125px;top:50%;left:50%;opacity:1;">
		<a href="javascript:closeQR();" style="position:absolute;margin-top:0.5em;margin-left:235px;"><img src="data:image/png;base64,%3D%3D" /></a><br />
		<div id="qrCode"></div>
	</div>
</div>
<!-- end QR modal -->



