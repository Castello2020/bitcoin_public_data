    window.onload = function () {
        // Reading the cookie separately for google analytics since at the time google analytics is called
        // Application.currentUser might not be initalized.
        function readCookie (name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i=0;i < ca.length;i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') {
                    c = c.substring(1,c.length);
                }
                if (c.indexOf(nameEQ) == 0) {
                    return c.substring(nameEQ.length,c.length);
                }
            }
            return null;
        }
        var user = (readCookie('__cu') !== null ? JSON.parse(decodeURIComponent(readCookie('__cu'))) : {});
        _gaq = typeof _gaq === 'undefined' ? [] : _gaq;
        _gaq.push(
            ['_setAccount', 'UA-1169997-20'],
            ['_setCustomVar', 5, 'Session Type', (!user.id ? 'Logged Out' : 'Logged In'), 2],
            ['_setCustomVar', 3, 'Plan Type', getPlan(user), 1],
            ['_setCustomVar', 4, 'Account_id', (!user.id ? 'Unknown' : user.id.toString()), 2],
            ['_trackPageview']
        );
        function getPlan (user) {
            var plan;
            switch (true) {
                case !user.id:
                    plan =  'Unknown';
                    break;
                case user.is_free:
                    plan = 'Free';
                    break;
                case user.is_basic:
                    plan = 'Basic';
                    break;
                case user.is_premium:
                    plan = 'Premium';
                    break;
                case user.is_enterprise == true:
                    plan = 'Enterprise';
                    break;
                default:
                    plan = 'error'
                    break;
            }
            return plan;
        }
    };

