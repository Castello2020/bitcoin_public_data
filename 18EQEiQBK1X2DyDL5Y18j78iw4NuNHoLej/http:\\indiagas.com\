    function fill_city_weather(){};
    if(window.location.hostname == 'upgenocache.wn.com'){
        Raven.config('http://42f400e816ee4a56a823bb8113c03f52@sentry2.wn.com/7');
    }else{
        Raven.config('http://865219b3648349058c6c7fe5974fa7b8@sentry2.wn.com/2');
    }
function ensure(fn) {
    return fn;
    return function() {
        try {
            return(fn.apply(this, arguments));
        } catch(e) {
            if(window.console && console.log) console.log("Reporting: "+e);
            Raven.captureException(e);
            // throw e;
        }
    };
}

$ensure = function(f){
    return jQuery(ensure(f));
};

