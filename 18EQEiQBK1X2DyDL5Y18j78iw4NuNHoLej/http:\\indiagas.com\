  <script type="text/javascript">				
    function load_wiki(el, cb) {
        var el = $(el).closest('.wiki-page').find('.wikiparser');
        var query = el.data('title');
        var main = el.data('main');
        var wikisnippet = el.find('.wikisnippet');
        el.find('.wikipage a').html('Loading... please wait...');
        el.closest('div').load('/api/upge/cheetah-photo-search/wiki_page?main='+main+'&query='+escape(query), 			
		function(){
            wikisnippet.hide();
            updateHeight();
            if(cb) cb();
        });
        return false;
    }				
    function load_playlistlyrics(query) {
		var lyrics_query = query.replace('&', ' ').replace('[', ' ').replace(']', '').replace('@', ' ').replace('live', ' ').replace('Live', ' ').replace('(', ' ').replace(')', ' ').replace('"', ' ').replace('!', ' ')
        jQuery.ajax({
            url: '/api/upge/cheetah-photo-search/lyrics_playlist',
            data: {'query': lyrics_query,
                   'artist': 'India'
            },
            dataType: 'text',
            success: ensure(function(text) {
                text = text.trim().split(new RegExp("<"+"!--"),1)[0];
                if(!text) {
                    var song = "";                    					
                }else{
                    var matches = text.trim().match(new RegExp("(.*)\n(.*)\n([^]*)"));
                    var song = matches[3];
                    jQuery('.lyrics_icon').show();
					jQuery('.lyrics_body').html(song);
                }
            })
        });
        return false;
    }
  </script>
  
  <script type="text/javascript"><!--
    bind_videos = ensure(function(vsid, autostart){
					
