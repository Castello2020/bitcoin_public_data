    <script type="text/javascript">
    function hum(s){
        if(s>=1048576) return ((s/1048576)|0) + ' MB';
        if(s>=1024) return ((s/1024)|0) + ' KB';
        return s+' bytes'
    }
    var maSpeed = 0;
    var maSpeed2 = 0;
    var lastLoaded = 0;
    updateStats = ensure(function(){
        var pl = [];
        var sss = 0;
        var ttt = 0;
        var loaded = 0;
        for(i in videoplayers){
            var iv = videoplayers[i].players;
            for(j in iv){
                try{
                    iv[j].div.css('top', '0px');
                    var jv = iv[j].player.ytplayer;
                    var s = jv.getVideoBytesLoaded();
                    var t = jv.getVideoBytesTotal();
                    if(t){
                        pl.push(jv);
                        if(s == t) loaded ++;
                        sss += s;
                        ttt += t;
                    }
                }catch(e){
                }
            }
        }
        var percent = (sss+0.001)*100/(ttt+0.001);
        var diff = (sss-lastLoaded) * 4;
        lastLoaded = sss;
        maSpeed = maSpeed * 0.95 + (diff<0?0:diff) * 0.05;
        maSpeed2 = maSpeed2 * 0.995 + (diff<0?0:diff) * 0.005;
        if(sss == ttt) maSpeed = 0;
        if(maSpeed<0) maSpeed = 0;
        if(maSpeed2<0) maSpeed2 = 0;
        var eta = (ttt-sss)/(maSpeed2+0.001);
        if(eta > 86400) eta = 'never';
        else if(eta > 3600) eta = ((eta/3600)|0)+' hours';
        else if(eta > 60) eta = ((eta/60)|0)+' minutes';
        else eta = (eta|0)+' seconds';
        $('.videoload').html("<span>players:</span> "+loaded+' of '+pl.length+" loaded<br/>"+hum(sss)+" of "+hum(ttt)+"<br /><span>loaded:</span> "+(percent|0)+"%<br/><span>loading:</span >"+hum(maSpeed|0)+"/sec<br/><span>ETA:</span> "+eta);
    });
    function init_videoload(){
        if(!window.videoload){
            window.videoload = true;
            for(i in videoplayers){
                videoplayers[i].setMultiplePlayers(true);
            }
            $ensure(function(){
                setInterval(updateStats, 250);
            });
        }
    }
    </script>

