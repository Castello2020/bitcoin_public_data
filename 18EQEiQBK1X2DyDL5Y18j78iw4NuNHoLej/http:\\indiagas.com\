	<script type="text/javascript"><!--
	$ensure(function() {
		showAddress();	
		loadContinent();			   
	});	
	showAddress = ensure(function() {
			jQuery.ajax({
				data: {
			  		'q': 'India',
			  	},
			  	dataType: 'jsonp',
			  	url: 'http://wn.com/api/upge/cheetah-photo-search/find_city',
			  	success: ensure(function(resource) {
			  			lat = resource.lat;
			  			lng = resource.lng;
			  			country = resource.country;
			  			city_name = resource.name;
						forecast_div = jQuery('.forecast');
			  			jQuery('.geocoded-lat').html(lat);
			  			jQuery('.geocoded-lon').html(lng);
			  		if ((lat != 0) && (lng!=0) && forecast_div.length) {
			  			get_weather_data(false, lat, lng);
						get_weather_details(forecast_div, lat, lng, 'India');
						
			  		}
			  	})
			});
	});
	loadContinent= ensure(function() {		
		jQuery.ajax({
			data: {	
				'query': 'asia',
				'city' : 'Kolkata',
			},
		  	url: '/api/upge/cheetah-weather/cities',
		  	success: ensure(function(text) {
	            if(text){		
				    $('.cities-list').html(jQuery(text));
		  		}
		    })
		});
	});
	
	//-->
	</script>
	
<script type="text/javascript"><!--	
    first_geo_data = false;
    today = false;

    get_weather_data = ensure(function(geo_obj, lat, lng){
        if (first_geo_data) {
            return;
        }
        first_geo_data = true;
        jQuery.ajax({
            data: {
                lat: jQuery.trim(lat),
                lng: jQuery.trim(lng),
                cnt: 1,
                APPID: '0374ae89689b7ac3ba53376fa99409a0'
            },
            dataType: 'jsonp',
            url: 'http://upge.wn.com/api/upge/cheetah-photo-search/weather_findcity',
            success: ensure(function(data) {
                //weather_data = data.list[0];
                
                //weather_info = '<div class="location-weather font_color">'
                weather_info = ''
                for (idx in data.list) {
                    weather_data = data.list[idx];

                    if (weather_data) {
                        weather = weather_data.weather[0];
                        clouds = weather_data.clouds;
                        m = moment(weather_data.dt);
                        d = new Date(weather_data.dt*1000);

                        today = d.getMonth()+1 + '-' + d.getDate() + '-' + d.getFullYear();
                        weather_info += '<div class="weather-instance">'
                        weather_info += '<div class="temp">'+parseInt(parseFloat(weather_data.main.temp)-273.15)+' &deg;C</div>'


                        if (weather) {
                       	//weather_info += '<img src="http://openweathermap.org/img/w/'+weather.icon+'" alt="" />'
						weather_info += '<img src="http://cdn0.wn.com/or/weather50/'+weather.icon+'.png" alt="" />'
                        }
                        weather_info += '</div>'                        
                    }
                }

               weather_info += ''
               jQuery('.weather-data').html(weather_info);       
               get_forecast_data(data.list[0].id, 3);
			   jQuery('.weather-container').show();   
               return;
            })
        });
    });

    get_forecast_data = ensure(function(city_id, days_count) {
		jQuery.ajax({
            data: {
                city: city_id
            },
            dataType: 'jsonp',
            url: 'http://upge.wn.com/api/upge/cheetah-photo-search/weather_forecast_city',
            success: ensure(function(data) {

		 	if(!data) { text = ('weater data temporarily not available'); }
            // loop through the list of weather info
            weather_info = '';
            var weather_day_loop = 0;
            jQuery.each(data.list, function(idx, value) {
                if (idx < 1) {
                    return;
                }

                if (weather_day_loop >= days_count) {
                    return false;
                }

                weather = value.weather[0]
                clouds = value.clouds
                d = new Date(value.dt*1000)
                t = d.getMonth()+1 + '-' + d.getDate() + '-' + d.getFullYear()

                moment.lang('en', {
                    calendar : {
                        lastDay : '[Yesterday]',
                        sameDay : '[Today]',
                        nextDay : '[Tomorrow]',
                        lastWeek : '[last] dddd',
                        nextWeek : 'dddd',
                        sameElse : 'L'
                    }
                });                        
                mobj = moment(value.dt*1000)
                
                // skip today
                if (t == today) {
                    return;
                }
                // get temperature for right time of day
                if (d.getHours() < 14) {
                	return;
                }
                today = t;
                weather_day_loop += 1;
				
                    weather_info += '<div class="weather-instance"><div class="current_weather">'+mobj.calendar()+'</div>'
	                weather_info += '<div class="temp">'+parseInt(parseFloat(value.main.temp)-273.15)+' &deg;C</div>'
                    if (weather) {
						weather_info += '<img src="http://cdn0.wn.com/or/weather50/'+weather.icon+'.png" alt="" />'
                    }
                    weather_info += '</div>'
                });
                // add the fresh html to the rest of the prediction
                //jQuery('.weather-data > .weather-placeholder-2').replaceWith(weather_info);
                jQuery('.forecast-content').html(weather_info);
            })
        });
    });

	// -->
	</script>
		
	
	<script type="text/javascript"><!--	
	get_weather_details = ensure(function(geo_obj, lat, lng, city_name){
			  first_geo_data = true;
			  var global_geo_obj = geo_obj;
			  var city = city_name.toUpperCase();
				  
	 			jQuery.ajax({
	            data: {
	                lat: jQuery.trim(lat),
	                lng: jQuery.trim(lng),
	                cnt: 1,
	                APPID: '0374ae89689b7ac3ba53376fa99409a0'
	            },
	            dataType: 'jsonp',
	            url: 'http://upge.wn.com/api/upge/cheetah-photo-search/weather_findcity',
	            success: ensure(function(data) {		
				            weather_info = ''
				            for (idx in data.list) {
				                weather_data = data.list[idx]
				
				                if (weather_data) {
				                    weather = weather_data.weather[0]
				                    clouds = weather_data.clouds
				                    m = moment(weather_data.dt)
				                    d = new Date(weather_data.dt*1000)
				
				                today = d.getMonth()+1 + '-' + d.getDate() + '-' + d.getFullYear()
				                weather_info += '<div class="weather-head"><h2 class="font_color h2_head">' +city+ ' Weather Forecast</h2></div><div class="weather-inst">'
				                weather_info += '<div class="current_weather">Today</div>'
								weather_info += '<div class="rounded border_color icon"><img src="http://cdn0.wn.com/or/weather50/'+weather.icon+'.png" alt="" /></div>'
				                    weather_info += '<div class="temp">'+parseInt(parseFloat(weather_data.main.temp)-273.15)+' &deg;C</div>'
				                    weather_info += '<ul class="data rounded"><li><div class="label">Pressure:</div>'+weather_data.main.pressure+' hPa</li>'
				                    if (weather_data.main.humidity) { weather_info += '<li><div class="label">Humidity:</div>'+weather_data.main.humidity+' %</li>'; }
				                   
				                    if (clouds) {
				                        weather_info += '<li><div class="label">Clounds:</div>'+clouds.all+'%</li>'
				                    }
				                    if (weather) {
				                        weather_info += '<li>'+weather.description+'</li>'                                                   
				                    }                    
				                    weather_info += '</ul></div>'
				                    
				                }
				      }
			          global_geo_obj.html(weather_info);
					  get_forecast_details(data.list[0].id, 4, global_geo_obj)
			      })
			  });
	});

	get_forecast_details = ensure(function(city_id, days_count, global_geo_obj){
      jQuery.ajax({
            data: {
                city: city_id
            },
            dataType: 'jsonp',
		url: 'http://upge.wn.com/api/upge/cheetah-photo-search/weather_forecast_city',
		success: ensure(function(data)  {
		 	if(!data) { text = ('weater data temporarily not available'); }
            // loop through the list of weather info
            weather_info = '';
            var weather_day_loop = 0;
            jQuery.each(data.list, function(idx, value) {
                if (idx < 1) {
                    return;
                }

                if (weather_day_loop >= days_count) {
                    return false;
                }

                weather = value.weather[0]
                clouds = value.clouds
                d = new Date(value.dt*1000)
                t = d.getMonth()+1 + '-' + d.getDate() + '-' + d.getFullYear()

                moment.lang('en', {
                    calendar : {
                        lastDay : '[Yesterday]',
                        sameDay : '[Today]',
                        nextDay : '[Tomorrow]',
                        lastWeek : '[last] dddd',
                        nextWeek : 'dddd',
                        sameElse : 'L'
                    }
                });                        
                mobj = moment(value.dt*1000)
                
                // skip today
                if (t == today) {
                    return;
                }
                // get temperature for right time of day
                if (d.getHours() < 14) {
                	return;
                }
                today = t;
                weather_day_loop += 1;

                weather_info += '<div class="weather-inst"><div class="current_weather">'+mobj.calendar()+'</div>'
				weather_info += '<div class="rounded border_color icon"><img src="http://cdn0.wn.com/or/weather50/'+weather.icon+'.png" alt="" /></div>'
                weather_info += '<div class="temp">'+parseInt(parseFloat(value.main.temp)-273.15)+' &deg;C</div>'
                weather_info += '<ul class="data rounded"><li><div class="label">Pressure:</div>'+value.main.pressure+' hPa</li>'
                if (value.main.humidity) { weather_info += '<li><div class="label">Humidity:</div>'+value.main.humidity+' %</li/>'; }                
                if (clouds) {
                    weather_info += '<li><div class="label">Clounds:</div>'+clouds.all+'%</li>'
                }
                if (weather) {
                    weather_info += '<li>' +weather.description+'</li>'
                   
                }
                weather_info += '</ul></div>'
            });
            weather_info += '<br style="clear:left;" />'
            global_geo_obj.append(weather_info);
        })
    });
});

        //-->
    </script>

