<script type="text/javascript">
jQuery(document).ready(function($)
{
    $(document).keydown(function(e)
    {
        // Add left/right arrow sliding for thumbnails
        if(jQuery("table[id^='lightbox']").is(':visible'))
        {
            if(e.keyCode == 37)
            { 
               jQuery("table[id^='lightbox'] input.lightboxbutton:visible:nth(1)").click();

               return false;
            }
            else if(e.keyCode == 39)
            {
               jQuery("table[id^='lightbox'] input.lightboxbutton:visible:nth(2)").click();

               return false;
            }
        }
    });
});
</script>

