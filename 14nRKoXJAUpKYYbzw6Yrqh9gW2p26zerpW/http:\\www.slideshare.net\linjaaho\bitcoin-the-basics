              e = document.createElement('script');
              e.type = 'text/javascript';
              e.async = true;
              e.src =  '//public.slidesharecdn.com/b/slideview/scripts/combined_loggedout.js?d13af5e3713de681722929850e271b7fe3c1ad7b';
              var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(e, s);
            };
            if(document.readyState === 'complete'){
              loadAdditionalFunctionality();
            }else{
              if(window.addEventListener){
                window.addEventListener('load', loadAdditionalFunctionality, false);
              }else if(window.attachEvent){
                window.attachEvent('onload', loadAdditionalFunctionality);
              }
            }
          });
        // ]]>
        </script>
    
  </body>
</html>

