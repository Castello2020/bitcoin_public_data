  <script type="text/javascript">
    $('.photo_image').each(function(){
      var self = this;
      $(self).bind('load', function() {
        var width = self.clientWidth;
        if ($(self).width() > 400) {
          $(self)
            .attr('width',597)
            .css('max-width',597)
            .addClass("rounded_gray_border large_offset");
          if (window.PIE) {
            PIE.attach(self);
          }
        }
      });
    });
    $(".item_content img").not(".photo_image").each(function() {
      var self = this;
      $(self).bind('load', function() {
        var maxWidth = $(self).parent().width();
        if ($(self).width() > maxWidth) {
          $(self).attr('width', maxWidth);
        }
      });
    });
    $("iframe.photoset").each(function() {
      var src = $(this).attr('src').replace(/\/500$/, '/577');
      $(this).attr('src', src).css('width',577);
    });
  </script>
  
  
  
