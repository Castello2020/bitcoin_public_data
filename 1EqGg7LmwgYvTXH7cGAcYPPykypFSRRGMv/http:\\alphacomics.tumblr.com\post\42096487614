  <script type="text/javascript">
    if (window.PIE) {
      $("iframe").attr('allowtransparency',true);
      $(".audio_player").each(function(i,e) { PIE.attach(e); });
      $(".rounded_gray_border").each(function(i,e) { PIE.attach(e); });
    }
  </script>
  <script type="text/javascript">
    $('.photo_image').each(function(){
      var self = this;
      $(self).bind('load', function() {
        var width = self.clientWidth;
        if ($(self).width() > 400) {
          $(self)
            .attr('width',597)
            .css('max-width',597)
            .addClass("rounded_gray_border large_offset");
          if (window.PIE) {
            PIE.attach(self);
          }
        }
      });
    });
    $(".item_content img").not(".photo_image").each(function() {
      var self = this;
      $(self).bind('load', function() {
        var maxWidth = $(self).parent().width();
        if ($(self).width() > maxWidth) {
          $(self).attr('width', maxWidth);
        }
      });
    });
    $("iframe.photoset").each(function() {
      var src = $(this).attr('src').replace(/\/500$/, '/577');
      $(this).attr('src', src).css('width',577);
    });
  </script>
  
  
  
<!-- BEGIN TUMBLR CODE --><iframe scrolling="no" width="1" height="1" frameborder="0" style="background-color:transparent; overflow:hidden; position:absolute; top:0; left:0; z-index:9999;" id="ga_target"></iframe><script type="text/javascript">
    (function(){
        var analytics_frame = document.getElementById('ga_target');
        var analytics_iframe_loaded;
        var user_logged_in;
        var blog_is_nsfw = 'No';

        var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
        var eventer = window[eventMethod];
        var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";
        eventer(messageEvent,function(e) {
            var message = (e.data && e.data.split) ? e.data.split(';') : '';
            switch (message[0]) {
                case 'analytics_iframe_loaded':
                    analytics_iframe_loaded = true;
                    postCSMessage();
                    postGAMessage();
                    break;
                case 'user_logged_in':
                    user_logged_in = message[1];
                    postGAMessage();
                    break;
            }
        }, false);

        analytics_frame.src = "http://assets.tumblr.com/analytics.html?3e91d63d5994bffc6ac211967f138b2c#http://alphacomics.tumblr.com";
        function postGAMessage() {
            if (analytics_iframe_loaded && user_logged_in) {
                var is_ajax = false;
                analytics_frame.contentWindow.postMessage(['tick_google_analytics', is_ajax, user_logged_in, blog_is_nsfw, '/post/42096487614?route=%2Fpost%2F%3Aid'].join(';'), analytics_frame.src.split('/analytics.html')[0]);
            }
        }
        function postCSMessage() {
            COMSCORE = true;
            analytics_frame.contentWindow.postMessage('enable_comscore;' + window.location, analytics_frame.src.split('/analytics.html')[0]);
        }
    })();
