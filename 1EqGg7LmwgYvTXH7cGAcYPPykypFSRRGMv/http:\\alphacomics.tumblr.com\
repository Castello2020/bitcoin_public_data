        #content #left .item.audio .track_info .artist_desc p:first-child {
          margin-top: 0px; }
        #content #left .item.audio .track_info .artist_desc p:last-child {
          margin-bottom: 0px; }
    #content #left .item.audio .audio_player {
      width: 210px;
