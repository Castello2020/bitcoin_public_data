	<script type="text/javascript">
	
		// footer columns same height
		function equalHeight(group) {
		   tallest = 0;
		   group.each(function() {
			  thisHeight = $(this).height();
			  if(thisHeight > tallest) {
				 tallest = thisHeight;
			  }
		   });
		   group.height(tallest);
		}
		$(document).ready(function() {
		   equalHeight($(".widget-egal"));
		});	
		
		// pages drop down effect
		jQuery(document).ready(function() {  
		jQuery("#pagenav ul").css({display: "none"}); // Opera Fix  
		jQuery("#pagenav li").hover(function(){  
		jQuery(this).find('ul:first').css({visibility: "visible",display: "none"}).show(268);  
		},function(){  
		jQuery(this).find('ul:first').css({visibility: "hidden"});  
		});  
		});
		
		// last li menu border 
		$(document).ready(function() {
		$("#dropmenu li:last").addClass("last");
		});	
		
		// colorbox
		$(document).ready(function(){
			$('.gallery a:has(img)').colorbox();
			$('.articol-inner a:has(img)').colorbox();
			$(".colorbox").colorbox();
		});		
		
	</script>

