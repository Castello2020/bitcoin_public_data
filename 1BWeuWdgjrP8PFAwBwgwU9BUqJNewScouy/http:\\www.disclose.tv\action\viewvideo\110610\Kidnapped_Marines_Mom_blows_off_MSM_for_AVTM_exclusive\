<script type="text/javascript">
jQuery(document).ready( function(){
(function() {
    var $win = jQuery(window);
    var $bot = jQuery('#addthis_float');
    var $fbf = jQuery('#facebook_sidebar');
    var shown = false;
    var windowSize  = window.innerHeight;
    var bodyHeight     = document.body.offsetHeight;
    var DistFromBottom = Math.max(bodyHeight - ($win.scrollTop() + windowSize), 0);

    if($win.width() > 1279) 
    {
     $fbf.show();
     shown_fbf = true;
    }
   
    function checkScroll() {
        if ($win.scrollTop() > 250) {
            if (!shown) {
                if($win.width() > 1124) 
                    {
                    shown = true;
                    $bot.stop().animate({ opacity:1 }, 200);
                    }
            }
        } else {
            if (shown) {
                shown = false;
                $bot.stop().animate({ opacity:0 }, 200);
            }
        }

        if ($win.height() + $win.scrollTop() > jQuery(document).height()-100) 
        {
          jQuery("#adSidebar").css("margin-top", "0px");
          jQuery("#adSidebar").css("position", "inherit");
        }
        else if ($win.scrollTop() > 490) {  
          newDist2 = Math.max(0 - $win.scrollTop(), -490);
          jQuery("#adSidebar").css("margin-top", newDist2+"px");
          jQuery("#adSidebar").css("position", "fixed");
        }
        else 
        {
          jQuery("#adSidebar").css("margin-top", "0px");
          jQuery("#adSidebar").css("position", "inherit");
        } 

        
    }  
    $win.scroll(checkScroll);
})();
});
</script>
  
