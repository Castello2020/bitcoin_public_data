		var embedCodeOld = '<object id="dtvplayer" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" width="560" height="315"><param name="movie" value="http://www.disclose.tv/swf/player.swf" /><param name="wmode" value="transparent" /><param name="allowFullScreen" value="true" /><param name="allowscriptaccess" value="always" /><param name="flashvars" value="config=http://www.disclose.tv/videos/config/flv/110610.js" /><embed type="application/x-shockwave-flash" width="560" height="315" src="http://www.disclose.tv/swf/player.swf" flashvars="config=http://www.disclose.tv/videos/config/flv/110610.js"/></object>';
		
		// 
		var valSize = jQuery("input[name='iframeSize']:checked").val();
		var sizeArray = valSize.split('x');
		var cbOldEmbedCode = jQuery("#oldEmbedCode").is(':checked');
		var cbShowTextAndLink = jQuery("#showTextAndLink").is(':checked');
		// 
		if( cbOldEmbedCode ) 
		{
			// 
	    var finalEmbedCode = embedCodeOld;
		}
		else
		{
			// 
			var finalEmbedCode = embedCode;
			finalEmbedCode = finalEmbedCode.replace( '__WIDTH__', sizeArray[0] );
			finalEmbedCode = finalEmbedCode.replace( '__HEIGHT__', sizeArray[1] );
			// 
			if( !cbShowTextAndLink )
			{
				finalEmbedCodeArray = finalEmbedCode.split( '</iframe>' );
				finalEmbedCode = finalEmbedCodeArray[0] + '</iframe>';
			}
		}
		// 
		jQuery("#tMedialink").val(finalEmbedCode);
	});

	 //---------------------
	 // send 2 friend
	 //---------------------
	 jQuery("input[name='send2friend_submit']").live( 'click', function() {
		  // 
		  var send2friend = true;

	    // check message
	    if( jQuery("#send2friend_message").val() == "" )
	    {
	      jQuery("#sharebox_status").html('Message must not be empty!');
	      send2friend = false;
	    }
	      		  
      // check email
      if( !isValidEmailAddress( jQuery("#send2friend_email").val() ) )
      {
        // 
        jQuery("#sharebox_status").html('Invalid email address!');
        send2friend = false;
      }
      
      // 
      if( send2friend )
      {
        // clear status
        jQuery("#sharebox_status").html('');

        // 
        var thisEmail = jQuery("#send2friend_email").val();
        var thisMessage = jQuery("#send2friend_message").val();

        // 
        jQuery.ajax({
        	  type: "POST",
        	  url: "/ajax/send2friend",
        	  data: { email: thisEmail, message: thisMessage, mid: 110610 },
