				<script type="text/javascript">
function le(doBlog) {
    var binfo_actions = null;
    $.ajax({
        url:'/ajax/leblog.ajax.php',
        type:'get',
        data:{ blog_id : '11711', 'doBlog' : doBlog  },
        dataType:'json',
        success:function (data) {
            $.growlUI('Notification', data['notify']);
            if (data.state != 'unautorized') {
                $("#status").html(data['status']);
            }
            var binfo_actions = data.binfo_actions;
            var blog_status = 'O';
            switch(data.state) {
                case 'connected': {
                    $.ajax({
                        url:'/ajax/blog_connect.ajax.php',
                        type:'post',
                        data:{ blog_id : '11711', blog_alias : 'bestarchive' },
                        dataType:'text',
                        success:function (data) {
                            $('.binfo_actions').html(binfo_actions);
                            $('.private_blog_message').remove();
                            if (blog_status != 'O') {
                                $('.blog_info').after(data);
                                $('.blog_news_container').fadeIn();
                            }
                        }
                    });
                    break;
                }
                case 'disconnected': {
                    $.ajax({
                        url:'/ajax/blog_connect.ajax.php',
                        type:'post',
                        data:{ blog_id : '11711', blog_alias : 'bestarchive' },
                        dataType:'text',
                        success:function (data) {
                            $('.blog_info').after(data);
                            $('.binfo_actions').html(binfo_actions);
                            if (blog_status != 'O') {
                                $('.blog_news_container').remove();
                            }
                        }
                    });
                    break;
                }
                case 'awaiting': {
                    break;
                }
                case 'unautorized': {
                    break;
                }
            }
        }
    });

    return false;
};

$(document).ready(function() {
    $('.connect_to').live('click',function() {
        le('enter');
        return false;
    });
    $('.disconnect_from').live('click',function() {
        le('leave');
        return false;
    });
});



</script>

