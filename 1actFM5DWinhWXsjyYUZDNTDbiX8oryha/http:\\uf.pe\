	function verify_form_input(form)
	{
	  if(form.file.value=="")
	  {
	   alert("Make sure to select a file on your local drive!");
		return false;
	  }
	  else if(form.name.value=="" || form.name.value=="Type your file name here")
	  {
		  $('#upna').val('no name');
		  return true;
	   // alert("Please fill in the tag");
		//$('#upna').focus();$('#upna').select();
		//return false;
	  }
	  else if (!form.group.value)
	  {
	    alert("Category needs to be filled in!");
	    return false;
	  }
	  /*else if (form.des.value.length < 10)
	  {
	    alert("Description need to more than ten charater! You now have "+form.des.value.length+" only!");
	    return false;
	  }*/
	  else if(!form.agree.checked)
	  {
	    alert("You need to agree to the 'Terms of Service' prior to uploading!");
		return false;
	  }
	  else
	  {
		  
		
			
			
			
$('#upload_frame').show();
				

		form.post.value="Currently uploading....please Wait";
		form.post.disabled=true;
		
		



		return true;
	  }
	} //end function verify_form_input(this)
	
	</script>



