if (location.href.indexOf("/fr/") != -1)
{
  $("div.signature").hide();
  if (location.href.indexOf("/fr/news") != -1)
    window.location.replace("http://www.hedgewars.org/news-fr");
}
else if (location.href.indexOf("/news-fr") != -1)
{
  $("h2.page-title").hide();
  document.title = "Les news ... en français !";
}
