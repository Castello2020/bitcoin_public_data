jQuery(document).ready(function() {

	/* Slide To */
	$('a.slide').click(function(){
		$('html, body').animate({
			scrollTop: 0
		}, 500);
		return false;
	});
	/* end Slide To */

$(".bm_sidenav_navigate .icon").hover(function(){
    $(this).children('.content').stop(true, false).animate({ width: "150px" }, 150);
    $(this).children('.content').addClass('hover');
}, function() {
    $(this).children('.content').stop(true, false).animate({ width: "0px" }, 150);
    $(this).children('.content').removeClass('hover');
});

		/* Static Box */
		var offtop = jQuery(".bm_sidenav_navigate").offset().top;	
		jQuery(window).scroll(function(){
			var st = jQuery(this).scrollTop() +40;		
