<script type='text/javascript'>
jQuery(document).ready(function() {

	/* Slide To */
	$('a.slide').click(function(){
		$('html, body').animate({
			scrollTop: 0
		}, 500);
		return false;
	});
	/* end Slide To */

$(".bm_sidenav_navigate .icon").hover(function(){
    $(this).children('.content').stop(true, false).animate({ width: "150px" }, 150);
    $(this).children('.content').addClass('hover');
}, function() {
    $(this).children('.content').stop(true, false).animate({ width: "0px" }, 150);
    $(this).children('.content').removeClass('hover');
});

		/* Static Box */
		var offtop = jQuery(".bm_sidenav_navigate").offset().top;	
		jQuery(window).scroll(function(){
			var st = jQuery(this).scrollTop() +40;		
			if( st > offtop ){
				jQuery(".bm_sidenav_navigate").addClass("following");
                                jQuery(".goup").show();
			} else {
				jQuery(".bm_sidenav_navigate").removeClass("following");
                                jQuery(".goup").hide();
			}
		});
		/* end Static Box */

$("body").keydown(function(e) {
  if(e.keyCode == 37) { // left
    $("#bm_go_left").addClass('active');
    var link_left = $("#bm_go_left").children('a');
    link_left.click();
    window.location.href = link_left.attr("href");
  }
  else if(e.keyCode == 39) { // right
    $("#bm_go_right").addClass('active');
    var link_right = $("#bm_go_right").children('a');
    link_right.click();
    window.location.href = link_right.attr("href");
  }
});

});
