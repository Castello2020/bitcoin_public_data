/* <![CDATA[ */
    var DsqLocal = {
        'trackbacks': [
        ],
        'trackback_url': "http:\/\/bitcoinmagazine.com\/9218\/ebay-files-patent-application-programmable-money\/trackback\/"    };
/* ]]> */
</script>

