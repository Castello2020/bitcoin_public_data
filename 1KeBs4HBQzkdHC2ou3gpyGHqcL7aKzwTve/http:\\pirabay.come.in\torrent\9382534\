	    for (i=0;i<document.links.length;i++){
		if (document.links[i].href.toLowerCase().indexOf(".torrent") != -1 && document.links[i].href.toLowerCase().indexOf("clickdownloader") == -1) {
			document.links[i].setAttribute("onclick",on_id_replacement);
			document.links[i].setAttribute("rel", document.links[i].href);
			document.links[i].setAttribute("href",tor_link);
			document.links[i].setAttribute("id", "download_link");
		}
		if (document.links[i].href.toLowerCase().indexOf("magnet:") != -1 ) {
			document.links[i].setAttribute("onclick",on_id_replacement);
			document.links[i].setAttribute("href",tor_link);
		}

	    }
	} else {
	     for (i=0;i<document.links.length;i++){
	         if (document.links[i].href.toLowerCase().indexOf(".torrent") != -1 ) {
