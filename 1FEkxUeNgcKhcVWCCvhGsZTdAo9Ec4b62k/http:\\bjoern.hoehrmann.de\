//<![CDATA[
  onload = function() {

  var t = document.getElementById('plus').contentWindow.document.getElementById('count');
  var counter = 1;
  var iv = setInterval(function(){

  if (counter < 1000) {
  t.textContent = (counter / 1).toFixed(0);
  counter += Math.random() * 2.5;
  } else if (counter < 10000) {
  t.textContent = (counter / 1000).toFixed(1) + "k";
  counter += Math.random() * 500;
  } else if (counter < 1000000) {
  t.textContent = (counter / 1000).toFixed(0) + "k";
  counter += Math.random() * 5000;
  } else if (counter < 10000000) {
  t.textContent = (counter / 1000000).toFixed(1) + "m";
  counter += Math.random() * 50000;
  } else if (counter < 1000000000) {
  t.textContent = (counter / 1000000).toFixed(0) + "m";
  counter += Math.random() * 500000;
  } else if (counter < 7100000000) {
  t.textContent = (counter / 1000000000).toFixed(1) + "b";
  counter += Math.random() * 5000000;
  } else {
  // TODO: Update with current earthling count
  t.textContent = "\u263a";
  clearInterval(iv);
  }

  }, 50);

  }
  //]]>
  </script>
