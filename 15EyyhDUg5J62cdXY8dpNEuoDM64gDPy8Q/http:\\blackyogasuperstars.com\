                                                <div id="tumblr_video_container_92663818480" class="tumblr_video_container" style="width:700px;height:700px;"></div><script class="inline_embed" type="text/javascript">
    (function(params){
        var container = document.getElementById(params.container);
        if (!container) return;
        var iframe = document.createElement('iframe');
        iframe.width = parseInt(params.width, 10) || 500;
        iframe.height = parseInt(params.height, 10) || Math.floor(iframe.width*0.75); // 4x3
        iframe.src = 'about:blank';
        for (var attr in params.attr) {
            if (params.attr.hasOwnProperty(attr)) {
               iframe.setAttribute(attr, params.attr[attr]);
            }
        }
        iframe.setAttribute('frameborder', '0');
        iframe.setAttribute('scrolling', 'no');
        iframe.setAttribute('webkitAllowFullScreen', '');
        iframe.setAttribute('mozallowfullscreen', '');
        iframe.setAttribute('allowFullScreen', '');
        iframe.setAttribute('seamless', '');
        iframe.setAttribute('style', 'display:block;background-color:transparent;overflow:hidden;');
        container.appendChild(iframe);
        iframe.onload = function() {
            var newHeight = iframe.contentWindow.document.body.offsetHeight;
            if (newHeight) iframe.height = newHeight;
            if (typeof(params.onload) === 'function') params.onload();
        };
        var doc = iframe.contentWindow.document;
        doc.open('text/html', 'replace');
        doc.write([
            '<!DOCTYPE html><html><head><title>Tumblr Video</title></head>',
            '<body class="' + params.body_class + '">',
            params.content,
            '</body></html>'
        ].join(''));
        doc.close();
    })({
        attr  : {
            'id'          : 'tumblr_video_iframe_92663818480',
            'class'       : 'tumblr_video_iframe has_lightbox',
            'data-origin' : 'blackyogasuperstars.com',
            'data-width'  : '700',
            'data-height' : '700'
