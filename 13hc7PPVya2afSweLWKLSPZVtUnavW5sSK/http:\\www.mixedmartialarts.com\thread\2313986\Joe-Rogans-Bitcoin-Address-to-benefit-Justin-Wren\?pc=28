	<script type="text/javascript">
		function toggleInput(e) {
			var alertMessage = "You will lose any content in the current editor. Press Ok to switch editors, Cancel to return to the current editor.";

			
			Event.stop(e);

			if(confirm(alertMessage)) {
				if(Event.element(e).id == "ToggleInputText") {
					validator.setRuleEnabled(["bodyHtml"], false);
					validator.setRuleEnabled(["bodyText"], true);
					$("currentEditor").value = "text";
				} else {
					validator.setRuleEnabled(["bodyHtml"], true);
					validator.setRuleEnabled(["bodyText"], false);
					$("currentEditor").value = "html";
				}

				$("HtmlInput").toggle();
				$("TextInput").toggle();

				/* Toggle the switch buttons */
				if(Auth.forumPostHtml){
					$("ToggleInputHtml").toggle();
					$("ToggleInputText").toggle();
				}
			}
		}

		Event.observe("ToggleInputHtml", "click", toggleInput);
		Event.observe("ToggleInputText", "click", toggleInput);

		
		if(Auth.forumPostHtml){

			if(Auth.forumPreferHtml){
				$("HtmlInput").show();
				$("TextInput").hide();
				$("currentEditor").value = "html";
			}else{
				$("HtmlInput").hide();
				$("TextInput").show();
				$("currentEditor").value = "text";
			}
		}else{
			$("HtmlInput").hide();
			$("ToggleInputText").show();
			$("currentEditor").value = "text";
		}
	</script>




