// Initialize Validator
var validator = new Validator("createPost");
// Register Validation Handlers

validator.addValidationHandlers(new BasicValidation());


// Add Form event listener
Event.observe("createPost", "submit", function(event) {
	validator.onFormSubmit(event);
});

// Add Rules
validator.addRule("bodyHtml", true, "", "A Post Body is required.", "bodyHtml", false);
validator.addRule("bodyText", true, "", "A Post Body is required.", "bodyText", false);
validator.addRule("videoSource", true, "", "A video Source is required when creating a video post", "videoSource", false);
</script>
