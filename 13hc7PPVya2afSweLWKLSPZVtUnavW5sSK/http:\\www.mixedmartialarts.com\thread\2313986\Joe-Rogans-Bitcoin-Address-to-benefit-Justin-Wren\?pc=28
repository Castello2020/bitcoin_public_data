<script type="text/javascript">
	var cookieString = Cookie.read('MMA_FORUM1');

	if(cookieString != '1'){
		
		var autoCloseDuration = 6;
		if(Client.isMobileClient(navigator.userAgent) == 'iphone'){
			$('defaultAd').hide();
			$('iPhoneAd').show();
			$('closePopupAd').setStyle({'font-size': '20px'});
			autoCloseDuration = 0;
		}else if(Client.isMobileClient(navigator.userAgent) == 'blackberry'){
			$('defaultAd').hide();
			$('blackBerryAd').show();
			$('closePopupAd').setStyle({'font-size': '20px'});
			autoCloseDuration = 0;
		}else if(Client.isMobileClient(navigator.userAgent) == 'android'){
			$('defaultAd').hide();
			$('androidAd').show();
			$('closePopupAd').setStyle({'font-size': '20px'});
			autoCloseDuration = 0;
		}

		var popupWindow = new PopupContent("popupCoverAd", .75, autoCloseDuration);

		document.observe("dom:loaded", function() {
			popupWindow.show();
			Cookie.create('MMA_FORUM1', '1', 1);
		});
	}

	Event.observe("closePopupAd", "click", function (event) { event.stop();popupWindow.hide(); });
</script>

