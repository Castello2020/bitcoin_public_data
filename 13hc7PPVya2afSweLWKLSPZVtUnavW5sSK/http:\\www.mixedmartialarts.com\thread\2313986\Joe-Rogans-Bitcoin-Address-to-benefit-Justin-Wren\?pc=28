		<script type="text/javascript">
		var showForum = 0;
		var hideForum = 0;
		var showDelayLock = 0;
		var hideDelayLock = 0;

		
		function showSubDelay(forumId) {
			
			if(!showDelayLock && !showForum){
				showDelayLock = 1;
				showForum = forumId;
				
				if(showForum == hideForum)
					hideForum = 0;
				else
					setTimeout("showSubForums();", 250);
			}
		}
		function showSubForums() {
			
			if(showDelayLock && showForum) {
				$("subforums" + showForum).setOpacity(.98);
				$("subforums" + showForum).show();
			}
			showDelayLock = 0;
		}
		
		function hideSubDelay(forumId) {
			showDelayLock = 0;
			hideDelayLock = 1;
			hideForum = forumId;
			
			if(hideForum == showForum)
				showForum = 0;
			if(hideForum)
				setTimeout("hideSubForums();", 75);
		}
		function hideSubForums(){
			
			if(hideDelayLock && hideForum)
				$("subforums" + hideForum).hide();
			hideForum = 0;
			hideDelayLock = 0;
		}
		
			
			Event.observe("forum1", "mouseover", function(e){ showSubDelay(1); Event.stop(e); });
			Event.observe("forum1", "mouseout", function(e){ hideSubDelay(1); Event.stop(e); });
			Event.observe("subforums1", "mouseout", function(e){ hideSubDelay(1); Event.stop(e); });
			
			Event.observe("subforums1", "mouseover", function(e){ hideForum = 0; Event.stop(e); });
			
		
			
			Event.observe("forum2", "mouseover", function(e){ showSubDelay(2); Event.stop(e); });
			Event.observe("forum2", "mouseout", function(e){ hideSubDelay(2); Event.stop(e); });
			Event.observe("subforums2", "mouseout", function(e){ hideSubDelay(2); Event.stop(e); });
			
			Event.observe("subforums2", "mouseover", function(e){ hideForum = 0; Event.stop(e); });
			
		
			
		
		</script>

		<div class="Clear" style="line-height: 0px;">&nbsp;</div>
	</div>

	




	

	

	
		
		
	

					
