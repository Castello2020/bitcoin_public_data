	<script>
	$.ajax({
	   url: '/js/tags.json',
	   dataType: 'json',
	   success:function (data){
	      $('#search-tag').autocomplete({
			source: data,
			minLength: 2,
			autoFocus: true			
		});
	      for(var i = 0; i < 5; i++) {
	        entry = data[Math.floor(Math.random()*data.length)];
	        if (entry != "") {
	        content = '<li><a href="/tag/'+entry+'">#'+entry+'</a></li>';
	      $(content).appendTo('#random-tags');
	  		}
	  	}
	   }
	});

	$( "#search-tag" ).bind( "autocompleteselect", function(event, ui) {
 	  window.location = "/tag/" + ui.item.value;
	  return false;
	});

	$('#search-box').submit(function() {
	  $("#search-tag").autocomplete("search",$("#search-tag").val());
	  return false;
	});

	$(".nsfw").hover(
  	function () {
    $(this).toggleClass("nsfw")
 	 }
	);

	(function() {
	    var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
	    po.src = 'https://apis.google.com/js/plusone.js';
	    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
	})();

	function blogView() {
		$(".row").each(function (i) {
		if ($("div > a > div", this).length > 0) {
		bimage = $("div > a > div", this).css("background-image");
		burl = bimage.replace(/"/g,"").replace(/url\(|\)$/ig, "");
		$("div > a > div", this).html("<img class='gif-full' src='"+burl+"'/>").css('background-image', '').attr('class', '');
		$(this).children().attr('class', '');
		}
		});
	}

	</script>
	<script>
$(document).ready(function() {
	$('.submit-date').each(function(index) {
	var date = moment($(this).text()+'-0700',"YYYY-MM-DD HH:mm:ss Z").fromNow();
	$(this).text(date);			
	});
});
</script>

