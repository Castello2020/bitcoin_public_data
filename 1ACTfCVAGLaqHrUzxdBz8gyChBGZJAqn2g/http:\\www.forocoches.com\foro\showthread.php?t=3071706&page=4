    {
alert('Por favor introduce tu nombre de usuario y clave de acceso.');
        return false;
    }
 
if (document.forms.log.vb_login_password.value == '')
    {
alert('Por favor introduce tu clave de acceso.');
        return false;
    }
 
    sdsubmit2=0
 
}
else {return false}
 
}
 
//-->
</script>
 
