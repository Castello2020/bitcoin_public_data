    if ((!immersion_adcode && !immersionCompliment)  || (immersion_adcode && immersionCompliment)) {
        document.write('<div id="div-gpt-ad-1393435113141-0" ></div>');
        googletag.cmd.push(function() {
            googletag.display('div-gpt-ad-1393435113141-0');
        });
    }
})();
</script>
</div>
