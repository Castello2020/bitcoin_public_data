<script type="text/javascript">
    $(function() {
        /*global addCommas */
        var $top_widget = $('#top-downloaded-widget');
        $.ajax('/top/top_data', {
            global: false,
            success: function(data) {
                if(data.download_alltime.length) {
                    $.each(data.download_alltime, function(index, p) {
                        $top_widget.find('ul').
                        append('<li><span>{downloads}</span><a href="/projects/{shortname}">{name}</a></li>'.format({
                            shortname: p.shortname,
                            name: p.name,
                            downloads: SF.Humanize.intWord(p.downloads)
                        }));
                    });
                    $top_widget.show();
                }
            }
        });
    });
</script>
