			<script>
			function changePlayer(a,feedId) {
				var link;
				var image;
				idToUpdate = 'l-' + a.id;
				image = "http://s.broadcastify.com/i/button-play.png";
				if (a.value == 1) {
				  	link = "/scripts/playlists/1/" + a.id + "/-5624794064.asx";
				}
				else if (a.value == 2) {
					link = "/scripts/playlists/1/" + a.id + "/-5624794064.ram";
				}
				else if (a.value == 3) {
					link = "/scripts/playlists/1/" + a.id + "/-5624794064.m3u";
				}
				else if (a.value == 4) {
					link = "/scripts/playlists/1/" + a.id + "/-5624794064.m3u";
				}
				else if (a.value == 5) {
					link = "javascript:LaunchWindow('/listen/feed/" + a.id + "/web', 'WebFeed" + a.id + "','height=725,width=780,scrollbars=no');";
				}
				else if (a.value == 6) {
					link = "javascript:LaunchWindow('/listen/feed/" + a.id + "/webjava', 'WebFeed" + a.id + "','height=725,width=780,scrollbars=no');";
				}
				else {
					link = "javascript:LaunchWindow('/listen/feed/" + a.id + "/web', 'WebFeed" + a.id + "','height=725,width=780,scrollbars=no');";
				}
				var theContent = '<a href=\"' + link + '\"><img src=\"' +image+ '\"></a>';
				document.getElementById(idToUpdate).innerHTML = theContent;
			}
			</script>
