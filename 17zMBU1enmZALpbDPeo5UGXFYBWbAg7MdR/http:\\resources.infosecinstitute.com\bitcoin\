<script type="text/javascript">
	/*@cc_on
	  @if (@_jscript_version == 10)
	    document.write(' <link type= "text/css" rel="stylesheet" href="http://2we26u4fam7n16rz3a44uhbe1bq2.wpengine.netdna-cdn.com/wp-content/themes/Avada/css/ie10.css" />');
