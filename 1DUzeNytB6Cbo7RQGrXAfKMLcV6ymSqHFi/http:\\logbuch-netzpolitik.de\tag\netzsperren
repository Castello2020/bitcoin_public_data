	    jQuery("#post-107 [name='download_media_file']").append("<option value='" + url + "' data-raw-url='" + url + "'>" + title + "</option>")
	    // button-stile download-widget
	    jQuery("#post-107 .episode_download_list").append("<li><a href='" + url + "'>" + title + "<span class='size'></span></a></li>")
	  }
	});
});jQuery(function($) {
	torrentByEnclosure("http://meta.metaebene.me/media/lnp/lnp005-unter-42-minuten.oga", function(info) {
	  if (info) {
	    var url   = info.sources[0].torrent,
	        title = "Torrent:&nbsp;Ogg&nbsp;Vorbis&nbsp;Audio";
