  <source src="http://logbuch-netzpolitik.de/podlove/file/463/s/webplayer/c/website/lnp010-were-doin-it-live.oga" type="audio/ogg"/>
  <source src="http://logbuch-netzpolitik.de/podlove/file/464/s/webplayer/c/website/lnp010-were-doin-it-live.mp3" type="audio/mpeg"/>
  <object type="application/x-shockwave-flash" data="flashmediaelement.swf">
