				jQuery(document).ready(function(){
					jQuery('.miny').click(function(){
						var eeny = jQuery(this).siblings('.eeny').html();
						var meeny = jQuery(this).siblings('.meeny').html();

						jQuery(this).fadeOut('normal', function(){
							if ( jQuery(this).hasClass('sp-button-1') ) { var type = 'sp-button-1' } else { var type = 'link' }
							jQuery(this).siblings('.moe').html('<a class="'+ type +'" href="mailto:'+eeny+'@'+meeny+'">'+eeny+'@'+meeny+'</a>');
						});

