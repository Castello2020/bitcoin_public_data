						$.fn.center = function () {
							this.css("position","absolute");
							this.css("top", (($(window).height() - this.outerHeight()) / 2) + $(window).scrollTop() + "px");
							this.css("left", (($(window).width() - this.outerWidth()) / 2) + $(window).scrollLeft() + "px");
							return this;
						}
						$(".happy_share").live('click',function(){
							var type = $(this).attr('data-type');
							var title = $(this).parent('.social').find('b').html();
							var content = $(this).parent('.social').find('p').html();
							var img = $(this).attr('data-image');
							var url = $(this).attr('data-url');
							$.ajax({
							   url: "http://www.freenewspos.com/italia/a/community_share.php", 
							   dataType: "html",
							   type: 'POST', 
							   data: 'type=' + type + '&title=' + rawurlencode(title) + '&content=' + rawurlencode(content) + '&img=' + img + '&url=' + url, 
							   success: function(data){ 
								  $("#community_share").center();
								  $("#community_share").css('display','block').html(data);
							   }
							});
						});	
						$('.share-close').live('click',function(){
							$("#community_share").css('display','none').html('');
						});
						$(".share-with-us").live('click',function(){
							var type = $(this).attr('data-type');
							var title = $(this).find('.share-post-title').html();
							var content = $(this).find('.share-post-content').html();
							var img = $(this).attr('data-image');
							var url = $(this).attr('data-url');
							$("#community_share_send").center();
							if($(this).attr('data-type')=='twitter'){
								$("#community_share_send").fadeIn("slow").html('Questa informazione è stata Condividi a POS Twitter. grazie.');
							}
							if($(this).attr('data-type')=='facebook'){
								$("#community_share_send").fadeIn("slow").html('Questa informazione è stata Condividi a POS Facebook. grazie.');
							}
							$.ajax({
							   url: "http://www.freenewspos.com/italia/a/community_send.php", 
							   dataType: "html",
							   type: 'POST', 
							   data: 'type=' + type + '&title=' + title + '&content=' + content + '&img=' + img + '&url=' + url, 
							   success: function(data){ 
									$("#community_share_send").fadeOut("slow").html('');
							   }
							});
						});
						$('.rss_to_facebook').live('click',function(){
							var thurl = rawurlencode($(location).attr('href'));
							if(thurl.match('#')){
								var cuurl = 'http%3A%2F%2Fwww.freenewspos.com' + thurl.split('#')[1].replace('pos=','');
							}else{
								var cuurl = rawurlencode(thurl);
							}
															$.ajax({
									url: "http://www.freenewspos.com/english/login-term",
									dataType: "html",
									type: 'POST',
									data: 'term=1&url='+cuurl,
									success: function(data){ 
										$("#community_share").center();
										$('#community_share').css('display','block').html(data);
									}
								});	
										
						});
						$('.embed-video').live('click',function(){
							var vid = $(this).attr('data-id');										
							$('.par-pw h1').append('<p class="share-embed-code" style="float:left;overflow:hidden;width:620px;"><textarea style="float:left;overflow:hidden;width:590px;">&lt;iframe width="640" height="360" src="//www.youtube.com/embed/'+vid+'" frameborder="0" allowfullscreen&gt;&lt;/iframe&gt;</textarea><span id="close-share-embed" style="float:left;width:20px;height:20px;display:inline-block;text-indent:-9999px;margin:8px 0 0 8px;cursor:pointer;background:url(//www.freenewspos.com/wp-content/themes/freenewspos/background/video.png) no-repeat -400px -60px transparent;">X</span></p>');
							$(this).attr('class','hidden-embed-video');
							$(".share-embed-code textarea").select();
						});
						$('.hidden-embed-video').live('click',function(){								
							$(".share-embed-code").remove();
							$(this).attr('class','embed-video');
						});
						$('#close-share-embed').live('click',function(){								
							$(".share-embed-code").remove();
							$(".hidden-embed-video").attr('class','embed-video');
						});
						$(".morelink").live('click',function(){
							if($(this).hasClass("less")) {
								$(this).removeClass("less");
								$(this).html(moretext);
							} else {
								$(this).addClass("less");
								$(this).html(lesstext);
							}
							$(this).parent().prev().toggle();
							$(this).prev().toggle();
							return false;
						});
