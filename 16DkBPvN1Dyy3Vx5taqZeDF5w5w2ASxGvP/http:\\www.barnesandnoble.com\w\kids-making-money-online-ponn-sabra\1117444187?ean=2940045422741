(function(Y) {
Y.all('.fade-to-more').each(function(ftmEl) {
if (!ftmEl.processed) {
Y.on('domready', function() {
var contentEl = ftmEl.one('.content');
if (contentEl.get('scrollHeight') === contentEl.get('offsetHeight')) {
ftmEl.all('.fade, .trigger').remove(true);
}
});
ftmEl.all('.trigger').on('click', function(e) {
e.preventDefault();
ftmEl.toggleClass('expanded');
if (ftmEl.getY() < (document.body.scrollTop || window.pageYOffset || document.documentElement.scrollTop)) {
Y.ScrollTo.Node(ftmEl, {padding: 50});
}
});
ftmEl.processed = true;
}
});
})(Y);
