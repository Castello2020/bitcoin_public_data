    <script>
      $(function(){
        $('.searchbar').typeahead({
          name: 'searches',
          limit: 10,
          remote: {
            url: 'http://www.androidapps.com/searches/autocomplete?q=%QUERY',
            filter: (function(data) {
              for (var i = 0; i < data.length; i++) {
                if (data[i].type == 'search') {
                  data[i].value = data[i].term;
                  data[i].url = data[i].url.replace(
                    "/searches/site?",
                    "/searches?"
                  ).replace(
                    /&?searchbeta=true&?/,
                    function(x) {
                      if ("&" == x[0] && "&" == x[x.length - 1]) {
                        return "&";
                      } else {
                        return "";
                      }
                    }
                  );
                } else {
                  data[i].value = data[i].name;
                }
              }
              return data;
            })
          },
          template: (function(datum) {
            if (datum.type == 'search') {
              return '<p style="white-space:normal;">' + datum.value + '</p>';
            } else {
              return '<p style="white-space:normal;">' + '<img src="' + datum.icon_url  + '" style="width:30px; margin-right:10px;"/>' + datum.value + '</p>';
            }
          })
        });
        $('.searchbar').on('typeahead:selected', function (object, datum) {
          window.location = datum.url;
        });
        $('.searchbar').on('typeahead:autocompleted', function (object, datum) {
          window.location = datum.url;
        });
      });
    </script>
