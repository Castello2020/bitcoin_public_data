      function s_toggleDisplay(his, me, show, hide) {
        if (his.style.display != 'none') {
          his.style.display = 'none';
          me.innerHTML = show;
					me.className += ' collapsed';
        } else {
          his.style.display = '';
          me.innerHTML = hide;
					me.className = me.className.replace(' collapsed', '');
        }
      }
