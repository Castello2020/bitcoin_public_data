var backtohome=true;


function toggleDiv(flagit) {

  if (flagit=="1")
  {
   document.getElementById('same_sites').style.visibility = "visible"
  }
  else
  {
   document.getElementById('same_sites').style.visibility = "hidden"
  }


}

function toggleFAQ(flagit2) {

  if (flagit2=="1")
  {
   document.getElementById('faqdiv').style.visibility = "visible"
  }
  else
  {
   document.getElementById('faqdiv').style.visibility = "hidden"
  }


}

function dontGobackHome() {
  backtohome=false;
}

function unloadpage() {
  if(backtohome)
   location.replace('http://');
}


function showit(id) {
	var ele = document.getElementById(id);
	ele.style.display = "block";
} 

function hideit(id) 
{
	var ele = document.getElementById(id);
    ele.style.display = "none";
} 
