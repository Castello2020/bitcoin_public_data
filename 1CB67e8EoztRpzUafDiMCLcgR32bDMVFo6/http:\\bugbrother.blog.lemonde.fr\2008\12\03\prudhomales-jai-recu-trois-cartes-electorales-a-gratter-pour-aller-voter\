         function resizeJelec() {
            // on test 10 fois en 1s la taille de l'iframe car elle peut changer au chargement
            var idx = 0, jelec_timer = setInterval(function () {
               if (idx >= 10) {
                  window.clearInterval(jelec_timer);
                  return;
               }
               var the_height = document.getElementById('iframe_jelec').contentWindow.document.body.clientHeight;
               document.getElementById('iframe_jelec').height = the_height;
               idx++;
            }, 100);
         }
      </script>
