        <div class="comment_teaser"></div>
        <div class="no_comment_bugs_exist_yet" style="display:none;">
            <a href="#comment_reply" class="scroll_to post_first_comment">Comment Now</a>
			<div class="comments_follow followable_block">




    <div class="followable_block initializing" data-target-type="article" data-target-id="blogAndPostId/blog/post/948-12689" data-object-json="{&quot;title&quot;:&quot;Google Lets Slip That It's Exploring Possible Bitcoin Integration Plans&quot;,&quot;avatar_src&quot;:&quot;&quot;,&quot;follow_count&quot;:&quot;&quot;}" data-type-advoice="false" data-target-name="" data-tracking-link-name="articleLink_ArticleCommentsFollow">

    <div class="follow_unireg">
	<a href="javascript://follow" class="follow_unireg_link">
		<span class="option_follow">
			<span class="icon"></span><span class="text">Follow Comments</span>
		</span>

		<span class="option_following">
			<span class="icon"></span><span class="text">Following Comments</span>
		</span>

		<span class="option_unfollow">
			<span class="icon"></span><span class="text">Unfollow Comments</span>
		</span>
	</a>
	<span class="follow_count"></span>
	<span class="loadingstate"><img class="loadingstateimg" src="http://i.forbesimg.com/assets/img/loading_spinners/16px_on_transparent.gif" alt="" width="16" height="16" /></span>
    </div>
