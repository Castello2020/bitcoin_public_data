    <!-- Navigation -->
    
    
    <!-- Main Title -->
    <H2 class="caption">
        <span class="bg">
			<span class="left">            
				<span class="right">About BigRedBrent&#x27;s SpellFlash: Warrior</span>
			</span>
		</span>				
    </H2>         
