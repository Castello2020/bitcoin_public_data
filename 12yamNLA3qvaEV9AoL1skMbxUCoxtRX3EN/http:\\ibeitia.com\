	<script type="text/javascript">
			$("#hero-headline").fitText(2, { minFontSize: 24, maxFontSize: '75px' });
			$("#hero-tagline").fitText(2, { minFontSize: 20, maxFontSize: '30px' });
			$(".action_button").fitText(1, { minFontSize: 12, maxFontSize: '14px' });
			
			$('#download_cv').hover(function(){
			    $('#download_cv_icon').css('display', 'none');
			    $('#download_cv_icon_swapped').css('display', 'inline');
			    $('#download_cv_icon_swapped').css('padding-left', '6px');
			    $('#download_cv_icon_swapped').css('padding-right', '9px');
			}, function() {
			    $('#download_cv_icon').css('display', 'inline');
			    $('#download_cv_icon_swapped').css('display', 'none');
			});
	</script>
	
