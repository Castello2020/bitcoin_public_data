  <ul class="field-items">

    
      <li class="field-item even" rel="dc:subject">
        <a href="/sildid/euroopa-parlament" typeof="skos:Concept" property="rdfs:label skos:prefLabel">euroopa parlament</a>      </li>

    
  </ul>

</section>
  </div>

