<script>
$(window).load(function() {
	$('#my-disqus-block iframe').css({
        'height': '327px',
        'height': 'auto !important',
        'min-height': '327px',
      });
  // Get the #comments div
  var commentsDiv = $('#disqus_thread');

  // Only do this work if that div isn't empty
  if (commentsDiv.length) {

    // Hide the comments div by default
    $(commentsDiv).addClass('hiddendisqus');

    // Append a link to show/hide
    $('<a/>')
      .attr('class', 'toggle-comments')
      .attr('href', '#')
	  .css({'clear': 'left',
		'display' : 'block'})
      .text('Show Comments')
      .insertBefore(commentsDiv);

    // when show/hide is clicked
    $('.toggle-comments').on('click', function(e) {
      e.preventDefault();
		if ($(commentsDiv).hasClass('hiddendisqus')) {
			$(commentsDiv).removeClass('hiddendisqus', 500);
		}
		else if (!$(commentsDiv).hasClass('hiddendisqus')){
			$(commentsDiv).addClass('hiddendisqus', 500);
		}
      // show/hide the div using jquery's toggle()

        var anchorText = $(this).text() == 'Show Comments' ? 'Hide Comments' : 'Show Comments';
        $(this).text(anchorText);
		
    });

  } // End if commentsDiv.length

});
</script>


