	<script>
		$(document).ready(function() {
		
			
		Highcharts.setOptions({
   			global: {
        			useUTC: false
    			}
		});
		
		$("#candle_daybutton").addClass("selected");
	$(function() {
	$.getJSON('./?action=get.chart.data&coin_id=2&t=candle&r=0&i=0&callback=?', function(data) {

		// split the data set into ohlc and volume
		var ohlc = [],
			volume = [],
			dataLength = data.length;
			
		for (i = 0; i < dataLength; i++) {
			ohlc.push([
				data[i][0], // the date
				data[i][1], // open
				data[i][2], // high
				data[i][3], // low
				data[i][4] // close
			]);
			
			volume.push([
				data[i][0], // the date
				data[i][5] // the volume
			])
		}
		
		// set the allowed units for data grouping
		var groupingUnits = [[
			'minute',
			[1, 2, 5, 10, 15, 30]
		], [
			'hour',
			[1, 2, 3, 4, 6, 8, 12]
		]];

		// create the chart
		$('#chart2').highcharts('StockChart', {
		    
			rangeSelector : {
				enabled: false
			},
		/*
			navigator: {
				enabled: false
			},*/
			scrollbar : {
				enabled : false
   			},

		    
		    /*
		    title: {
		        text: 'MinCoin - ',
		        style: {
		        	fontWeight: 'bold'
		        }
		    },
		    */
		    
		    yAxis: [{
		        title: {
		            text: 'Price in BTC'
		        },
		        height: 150,
		        lineWidth: 2
		    }, {
		        title: {
		            text: 'Volume'
		        },
		        top: 170,
		        height: 100,
		        offset: 0,
		        lineWidth: 2
		    }],
		    	plotOptions: {
				candlestick: {
					color: 'red',
					upColor: 'green'
				}
			},
		    
		    series: [{
		        type: 'candlestick',
		        name: 'MNC Price',
		        data: ohlc,
		        dataGrouping: {
				units: groupingUnits
		        }
		    }, {
		        type: 'column',
		        name: 'Volume',
		        data: volume,
		        color: '#7DF9FF',
		        yAxis: 1,
		        dataGrouping: {
				units: groupingUnits
		        }
		    }]
		});
	});
