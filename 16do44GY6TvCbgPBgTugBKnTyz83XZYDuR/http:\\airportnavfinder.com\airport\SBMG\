<script language="JavaScript">
function GetTime() {
	window.setTimeout( "GetTime()", 1000 );
	var currentTime=new Date(),hours=currentTime.getUTCHours(),minutes=currentTime.getUTCMinutes(),seconds=currentTime.getUTCSeconds();
	if(minutes < 10) minutes="0"+minutes; if(seconds < 10) seconds="0"+seconds;
	var ztime = document.getElementById('ztime'); ztime.innerHTML ="<b>" +hours +":" +minutes +":" +seconds +" " +"</b> GMT";
}
GetTime();
(function() {
	var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
	po.src = 'https://apis.google.com/js/plusone.js';
	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
})();
var fdelay;
function lookup(inputString){
	$('#find').addClass('fworking');
	clearTimeout(fdelay);
	fdelay = setTimeout(function() {
		if(inputString==null || inputString.length<3){$('#suggestions').fadeOut(); 	$('#find').removeClass('fworking');
		}else{$.post("/ajax/search.php",{find: "" + inputString + ""}, function(data){ $('#suggestions').html(data).fadeIn(); }); 	$('#find').removeClass('fworking');}
	}, 400);	
}
</script>
