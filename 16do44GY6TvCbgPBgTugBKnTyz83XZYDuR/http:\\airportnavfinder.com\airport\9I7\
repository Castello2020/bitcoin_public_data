	if(mapbounds){for (var i=0; i < mapbounds.length; i++){bounds.extend( new google.maps.LatLng(mapbounds[i][1], mapbounds[i][2]) );} map.fitBounds(bounds);}
}
function toggleClouds() {cloudLayer.setMap(cloudLayer.getMap() ? null : map);}
