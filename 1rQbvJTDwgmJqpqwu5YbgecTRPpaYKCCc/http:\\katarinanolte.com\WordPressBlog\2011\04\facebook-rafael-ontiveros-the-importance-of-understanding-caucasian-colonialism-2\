				var GC_Message_Bar_OpenTop_opened = GC_Message_Bar_OpenTop + parseInt(jQuery("#gc_message_bar_open").height(),10);
				var GC_Message_Bar_OpenBottom = parseInt(jQuery("#gc_message_bar_open").css("bottom"),10);
