jQuery(function($) {
	$(function() {

		$(".navigation .menu li a").blur(function() {
			$(this).parent().removeClass("focus");
		});

		$(".navigation .menu li a").focus(function(){
			$(this).parent().addClass("focus");

			if (!$(this).parents(".sub-menu").length) { // if not a child, hide all subs
				$(".sub-menu").removeClass("visible");
			};
			
			if ($(this).parent().has("ul.sub-menu")) { // if a parent, show child sub
				$(this).parent().children(".sub-menu").addClass("visible");
			};
		});
	});
 });
	</script>

