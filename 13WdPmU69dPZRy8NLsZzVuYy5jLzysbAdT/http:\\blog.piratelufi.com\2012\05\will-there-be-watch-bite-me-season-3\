<script type="text/javascript">
function modal_dialog_open() {
jQuery("a.inline").trigger('click')
}
function modal_dialog_close() {
jQuery.colorbox.close();}
jQuery(document).ready(function() {
jQuery("a.inline").colorbox({
inline: true,overlayClose: false,
closeButton: true,
fixed: true,
width: '80%',
height: '80%',
overlayOpacity: 0.3
});
	var cookievalue = jQuery.cookie('piratelufi-webhost-offer');
	if (cookievalue == null) cookievalue = 0;
	if (cookievalue < 1)
	{
		cookievalue++;
		jQuery.cookie('piratelufi-webhost-offer', cookievalue, { expires: 1, path: '/'});
		setTimeout(
			function(){
				modal_dialog_open(); 			}, 15000);
		};
});
