	if (typeof socialEventsDone === 'undefined') {
		window.socialEventsDone = true;
		$(function() {
			$('.socialShare.customButtons .facebook').click(function() {
				if (typeof window._gaq !== 'undefined') {
					_gaq.push(['_trackSocial', 'facebook', 'like', {}]);
				} else if (typeof window.dataLayer !== 'undefined') {
					dataLayer.push({
						'event': 'socialInt',
						'socialNetwork': 'facebook',
						'socialAction': 'share',
						'socialTarget': {}
					});
				}
			});
			$('.socialShare.customButtons .twitter').click(function() {
				if (typeof window._gaq !== 'undefined') {
					_gaq.push(['_trackSocial', 'twitter', 'tweet', {}]);
				} else if (typeof window.dataLayer !== 'undefined') {
					dataLayer.push({
						'event': 'socialInt',
						'socialNetwork': 'twitter',
						'socialAction': 'tweet',
						'socialTarget': {}
					});
				}
			});
			$('.socialShare.customButtons .googlePlus').click(function() {
				if (typeof window._gaq !== 'undefined') {
					_gaq.push(['_trackSocial', 'Google', '+1', {}]);
				} else if (typeof window.dataLayer !== 'undefined') {
					dataLayer.push({
						'event': 'socialInt',
						'socialNetwork': 'Google',
						'socialAction': '+1',
						'socialTarget': {}
					});
				}
			});
		});
	}
</script>

		
<div id="ad-supersquare">


	<!-- start slave -->
	<div id="adoceanczvhnhnrgkjj"></div>
	<script type="text/javascript">
	/* (c)AdOcean 2003-2009, iinfo_cz.Root.cz.Square banner */
	ado.slave("adoceanczvhnhnrgkjj", {
