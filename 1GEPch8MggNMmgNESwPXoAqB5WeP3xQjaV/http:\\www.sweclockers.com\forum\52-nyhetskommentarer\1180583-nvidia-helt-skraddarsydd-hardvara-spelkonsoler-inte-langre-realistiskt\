		<script type="text/javascript">
		function moveDeferredAd(id)
		{
			var target = document.getElementById(id);
			var source = document.getElementById("defer-source-" + id);
			
			if(target && source)
			{
				// Scripts have already been executed
				var scripts = $('script',source);
				
				if(scripts)
				{
					scripts.remove();
				}
				
				while(source.childNodes.length > 0)
				{
					target.parentNode.insertBefore(source.childNodes[0], target);
				}
				
				source.parentNode.removeChild(source);
				target.parentNode.removeChild(target);
			}
		}
		</script>
