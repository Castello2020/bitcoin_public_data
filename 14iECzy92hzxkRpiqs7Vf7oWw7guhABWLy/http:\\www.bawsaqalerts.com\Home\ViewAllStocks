    <script type="text/javascript">
        $(document).ready(function () {
            $("#stockTable").tablesorter();

            var timezone = "Greenwich Standard Time";

            if ($.cookie("timezone")) {
                timezone = $.cookie("timezone");
            }

            $("select#dropDownTimezone").val(timezone);
        });

        $("#gotIt").click(function () {
            $("#cookies").fadeOut("slow", function () {
            });
            $.cookie("first", "", { expires: 365, path: '/' });
        });

        $(".codeTH").click(function () {

            if ($(".codeTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingCode", { expires: 1 });
            }
            else if ($(".codeTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingCode", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingCode", { expires: 1 });
            }
        });

        $(".nameTH").click(function () {

            if ($(".nameTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingName", { expires: 1 });
            }
            else if ($(".nameTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingName", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingName", { expires: 1 });
            }
        });
        $(".currentTH").click(function () {

            if ($(".currentTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingCurrent", { expires: 1 });
            }
            else if ($(".currentTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingCurrent", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingCurrent", { expires: 1 });
            }
        });
        $(".highTH").click(function () {

            if ($(".highTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingHigh", { expires: 1 });
            }
            else if ($(".highTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingHigh", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingHigh", { expires: 1 });
            }
        });
        $(".lowTH").click(function () {

            if ($(".lowTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingLow", { expires: 1 });
            }
            else if ($(".lowTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingLow", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingLow", { expires: 1 });
            }
        });
        $(".movTH").click(function () {

            if ($(".movTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingMovement", { expires: 1 });
            }
            else if ($(".movTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingMovement", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingMovement", { expires: 1 });
            }
        });
        $(".movPercentTH").click(function () {

            if ($(".movPercentTH").hasClass('headerSortUp')) {
                $.cookie("sortBy", "ascendingMovementPercent", { expires: 1 });
            }
            else if ($(".movPercentTH").hasClass('headerSortDown')) {
                $.cookie("sortBy", "descendingMovementPercent", { expires: 1 });
            }
            else {
                $.cookie("sortBy", "ascendingMovementPercent", { expires: 1 });
            }
        });

        $("#helpKeepTheLightsOn").click(function () {
            if ($('#whyDonate').is(':visible')) {
                $("#whyDonate").fadeOut("slow", function () {
                });

            } else {
                $("#whyDonate").fadeIn("slow", function () {
                });
            }
        });

        $("#whyDonate").click(function () {
            $("#whyDonate").fadeOut("slow", function () {
            });
        });

        $("#refresh").click(function () {
            location.reload();
        });

        $("#dropDownTimezone").change(function () {
            $.cookie("timezone", $("#dropDownTimezone").val(), { expires: 365, path: '/' });
        });


        $(document).ready(function () {
            setInterval(function () {
                location.reload();
            }, 600000);
        });

    </script>

