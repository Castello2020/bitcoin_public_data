# cat ~root/scripts/*

#!/bin/sh
datex=$(date +'%Y%m%d-%H%M%S')
file=mysql_dump_$datex.sql.gz
echo Backup is $file
PASSWORD=`cat /etc/psa/.psa.shadow`

 mysqldump -q -u admin -p$PASSWORD --all-databases --add-drop-table | gzip &gt;
~/$file
