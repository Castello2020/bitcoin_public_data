<li class="li2"><div class="de2">function safe_query($query){</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; if(strpos(getcwd(),'admin')){</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; shell_exec(&quot;echo '&quot;.date(&quot;Y-m-d H:i:s&quot;).&quot;|&quot;.$query.&quot;' &gt;&gt;</div></li>
<li class="li1"><div class="de1">/var/sql_logs/&quot;.str_replace(&quot;www.&quot;,&quot;&quot;,$_SERVER['HTTP_HOST'].&quot;&quot;));</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; }</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp;$database = DATABASE;</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp;$username = DATABASE_USER;</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp;$password = DATABASE_PASS;</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp;$destination = DATABASE_HOST;</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp;mysql_connect($destination, $username, $password) or die(&quot;&lt;br&gt;Unable to</div></li>
<li class="li2"><div class="de2">connect to database: &lt;br&gt;&quot;. mysql_error());</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp;mysql_select_db($database) or die (&quot;&lt;br&gt; Unable to select</div></li>
<li class="li2"><div class="de2">database[$database]: &lt;br&gt;&quot; . mysql_error());</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp;$result = mysql_query($query);</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp;return $result;</div></li>
<li class="li1"><div class="de1">}</div></li>
