if($num == ''){
echo &quot;&lt;img src=\&quot;images/login_deny.gif\&quot;&gt;&quot;;
}else{
	$id = mysql_result($result,0,'id');
	$level = mysql_result($result,0,'level');
	$_SESSION['user_id'] = $id;
	$_SESSION['username'] = $username;
	$_SESSION['password'] = $password;
	$_SESSION['level'] = $level;
}
?&gt;


