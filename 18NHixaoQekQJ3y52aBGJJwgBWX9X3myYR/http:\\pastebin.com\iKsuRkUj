<li class="li2"><div class="de2">'$booking_number'&quot;;</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $result = safe_query($query);</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $num = mysql_num_rows($result);</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; if($num == ''){return false;}else{return true;}</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; }</div></li>
