<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; function build_new_inmates(){</div></li>
<li class="li1"><div class="de1">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $inmates = array();</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $file =</div></li>
<li class="li1"><div class="de1">file('/var/www/vhosts/jocomosheriff.org/home/Export/RosterExport.txt');</div></li>
<li class="li2"><div class="de2">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; foreach($file as $line =&gt; $data){</div></li>
