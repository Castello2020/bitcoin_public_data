function LoadImageAsync(placeHolder)
 {
if (reploaded) return; reploaded = true;
var img = new Image();
$(img)
.load(function () {
$(this).hide();
$('#'+placeHolder)
.css('background', '')
.html(this);
$(this).fadeIn();
})
.error(function () {
$(placeHolder).append('<div class="error">An error occurred while loading the reputation graph.</div>');
})
.attr('src', '/script/Reputation/ReputationGraph.aspx?mid=4273880');
}

