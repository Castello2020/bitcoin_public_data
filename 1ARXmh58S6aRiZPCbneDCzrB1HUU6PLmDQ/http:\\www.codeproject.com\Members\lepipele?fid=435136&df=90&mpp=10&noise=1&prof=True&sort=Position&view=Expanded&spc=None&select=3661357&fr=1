var currentMenuItem = null;
function EnsureMenuVisible() {
    if (!currentMenuItem) return;
    var webkitWeird = document.documentElement.scrollTop < document.body.scrollTop;
    var scrollContainer = document.documentElement;
    var top = getRealMenuPos(currentMenuItem, 'Top');
    var bottom = getRealMenuPos(currentMenuItem, 'Top') + currentMenuItem.offsetHeight;
    var scrollTop = webkitWeird ? scrollTop = document.body.scrollTop : document.documentElement.scrollTop;
    if (scrollTop + scrollContainer.clientHeight < bottom)
        scrollTop = bottom - scrollContainer.clientHeight;
    if (scrollTop > top)
        scrollTop = top - scrollContainer.clientHeight / 10;
    if (webkitWeird) $(document.body).animate({ 'scrollTop': scrollTop }, 'fast');
    else $(scrollContainer).animate({ 'scrollTop': scrollTop }, 'fast');
}

function getRealMenuPos(i, which) {
    iPos = 0;
    while (i != null) {
        iPos += i['offset' + which];
        i = i.offsetParent;
    }
    return iPos;
} 
function ToggleMenu(id)
{
	var elm = document.getElementById(id);
	var i,others = document.getElementById('SectionMenu');
	for(i=0; i < others.childNodes.length; i++)
	{
		var other = others.childNodes[i];
		if (other.className == 'menu2-dropdown' && other != elm)
			$(other).slideUp(100); // other.style.display='none';
	}
	if (elm.style.display == 'block') $(elm).slideUp(100); //elm.style.display='none';
	else $(elm).slideDown(100); // elm.style.display='block';
 currentMenuItem = elm;
	window.setTimeout(EnsureMenuVisible, 200);
	return false;
}


