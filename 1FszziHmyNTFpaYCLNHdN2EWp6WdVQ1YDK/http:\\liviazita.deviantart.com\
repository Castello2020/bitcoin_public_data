        $this.find('.pagination a.away').click(function(e) {
            var url = this.href, // the full processed URL of the target button clicked
                re = /offset=(\d+)/g, // regex to match all offset=1 params (querystring or hash fragment)
                match, // temporary regex matches target
                offset; // final offset

            while (match = re.exec(url)) { // loop while matching offset=something
                if (match[1]) {
                    offset = match[1];
                }
            }

            if (offset) { // if found offset
                e.preventDefault();
                GMI.up(jQuery(this).get(0)).loadView('generic', {recentOffset: offset, catpath: false});
            }
        });
    }));</script>
