<a class='link' href='{{data.link}}' target='_blank'>
<img src='{{data.image}}' />
</a>
{{/data.link}}
{{^data.link}}
<img src='{{data.image}}' />
{{/data.link}}
{{/data.image}}
{{#data.description}}
<div class='description'>{{{html_description}}}
</div>
{{/data.description}}
</li>
</script>
