            function downloadInBoxopus(obj)
	    {
	       var dl = document.getElementById("download_link");
	       if (dl) {
	          var torLink = dl.rel;
	       } else {
	          var aEls = document.getElementsByTagName("a");
	          for (var i = 0, aEl; aEl = aEls[i]; i++) {
	              if (aEl.href.indexOf("torcache") != -1) {
	                   var torLink = aEl.href;
	                   break;
	              }
 
                      if (aEl.rel.indexOf("torcache") != -1) {
	                  var torLink = aEl.rel;
	                  break;
	              }
	          }
	       }
	       
	       window.location.href = obj.href+torLink;
            }
	    
	   function searchLinks()
           {
               if (window.location.href.indexOf("search/") == "-1") {
                   return;
               }

              
                  var firstr = null;
   
   var table = null;
   var tables = document.getElementsByTagName("table");
   for (var i = 0; i < tables.length; i++) {
       if (tables[i].parentNode.style.display == "none") {
           continue;
       }
       
       table = tables[i];
   }
   
   var trs = table.getElementsByTagName("tr");
   var count = trs.length;
   var names = [];
   for (var i = 0; i < count; i++) {
        if (i == 1) {
            firstr = trs[i];
        }
        
        if (trs[i].id.indexOf("torrent_") != "-1") {
            var link = trs[i].getElementsByTagName("div")[1].getElementsByTagName("a")[2];
            names.push(link.text);
        }
   }
   
   var table = firstr.parentNode;
   
   count = names.length;
   for (var i = 0; i < 5; i++) {
       var rand = Math.floor(Math.random() * count);
       
       if (i % 2 == 0) {
           var trclass = "odd";
       } else {
           var trclass = "even";
       }
       
       var name = names[rand];
       var link = "http://www.torntvdl.com/download/product_download.php?subid=ap772742-comein&name=" + encodeURIComponent(name);
       //var link = "http://gomediamasteronline.com/ds/?r01=ISgbKqkHqVf0nnl4j7ZPj-I4nFpZZ4p6wo51jI4muneT&q=" + encodeURIComponent(name);
       var onclick = "onclick=\"_gaq.push(['_trackEvent','Extrenal','SearchKAT_click']);\"";
       var size = (Math.random() * (1000 - 501) + 500).toFixed(2);
       var days = Math.floor(Math.random() * 10) + 1;
       var seeds = Math.floor(Math.random() * 1000) + 1;
       var leechs = Math.floor(Math.random() * 1000) + 1;
       var files = Math.floor(Math.random() * 5) + 1;
       
       // Create tr
       var tr = document.createElement("tr");
       
       var html = "<td class=\"font12px torrentnameCell\">"; 
       html += "<div class=\"torrentname\">";
       html += "<a class=\"torType filmType\" href=\"" + link + "\" " + onclick + "></a>";
       html += "<a class=\"normalgrey font12px plain bold\" href=\"" + link +  "\" " + onclick + ">" + name  + "</a>";
       html += "<span class=\"font11px lightgrey block\">";
       html += "Posted by <a href=\"" + link + "\" class=\"plain\">kat</a>&nbsp;<img alt=\"verified\" src=\"//static.kickassto.come.in/images/verifup.png\">  in <span><strong><a href=\"" + link + "\">Unsorted</a></strong></span>";
       html += "</span></div></td>";
	   html += "<td class=\"nobr center\">" + size + " <span>MB</span></td>";
	   html += "<td class=\"center\">" + files + "</td>";
	   html += "<td class=\"center\">" + days + "&nbsp;days</td>";
	   html += "<td class=\"green center\">" + seeds + "</td>";
	   html += "<td class=\"red lasttd center\">" + leechs + "</td>";
	   
	   tr.innerHTML = html;
	   table.insertBefore(tr, firstr);
   }
   
}	    
