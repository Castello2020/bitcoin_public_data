				<h2>Login</h2><br/><div style="border:2px solid red;background:pink;padding:5px;border-radius:5px;"><b>Note regarding logins</b><br/>Due to technical restrictions at kat.ph's end, proxy sites cannot currently authenticate users, including kickassunblock.info. We are working on the issue. <br/>Thanks,<br/>Torrenticity</div>
				<script type="text/javascript">
function authenticateUser (user) {
	$.fancybox.showActivity();
	user.return_uri = window.location.href;
	$.ajax( {
		type: "POST",
		url: '/auth/socialize/',
		data: user,
		dataType: "json",
		success: function(response) {
			$.fancybox.hideActivity();
			if (response.method == 'ok') {
				if ((typeof( response.url ) != undefined)) {
					if (response.url == window.location.href) {
						window.location.reload();
					}else {
						window.location = response.url;
					}
				}
				$.fancybox.close();
				return false;
			}
			if (response.method == 'error') {
				$("#requestStatus").html(response.html).removeClass('goodalertfield').addClass('alertfield').fadeIn('fast');
				var button = $(form).find("button[type=submit]").prop('disabled', false).removeClass('disabledButton');
				return false;
			}
			$.fancybox(response.html);
		}
	});
	return false;
}
function showLogin () {
    $("#setself").authBox({
		"exchange"  : "porthole",
        "providers" : "facebook,google,yahoo,twitter",
        "key"       : "df7c290f1135b74e2a130887815297e87c4df2bf",
        "size"      : "32x32",
        "callback"  : "authenticateUser"
	});
}
	var head    = document.getElementsByTagName("head")[0] || document.documentElement;
	var script	= document.createElement('script');
	script.type = 'text/javascript';
	script.src  = '//setself.com/content/js/authbox/jquery-authbox-'+Math.floor(Math.random()*99999999999)+'.js';
	var done = false;
	script.onload = script.onreadystatechange = function() {
		if ( !done && (!this.readyState || this.readyState === "loaded" || this.readyState === "complete") ) {
			done = true;
			script.onload = script.onreadystatechange = null;
			if ( head && script.parentNode ) {
				head.removeChild( script );
			}
            showLogin();
		}
	};
	head.insertBefore( script, head.firstChild );
</script>
