<script>
$('.question-like,.question-dislike').click(function() {
    var that = $(this), action = that.hasClass('question-like') ? 'like' : 'dislike';
    $.post(that.attr('href'), function(data) {
        $('.question-' + action + 'd').show();
        that.addClass('checked');
        that.parent().find('a').removeClass('question-like').removeClass('question-dislike').removeAttr('href').unbind();
        that.find('span').html('<label></label>' + (parseInt(that.find('span').text()) + 1));
    }, 'json');
    return false;
});
</script>
