<script type="text/javascript">
$(function() {
    $('.request-like,.request-dislike').click(function() {
        var that = $(this), item = that.parents('.request-item'), id = item.attr('request-id'), action = that.hasClass('request-like') ? 'like' : 'dislike';
        $.post('/request/' + action + '/' + id + '/', {
            ajax: 1
        }, function() {
            that.addClass('checked');
            that.parent().find('a').removeClass('request-like').removeClass('request-dislike').removeAttr('href').unbind();
            that.find('span').html('<label></label>' + (parseInt(that.find('span').text()) + 1));
        }, 'json');
        return false;
    });

    $('.request-force').click(function() {
        if (confirm('Are you sure you want to delete torrent request?')) {
            var that = $(this);
            $.post(that.attr('href'), function() {
                var r = that.parents('.request-item');
                if (!r.next('.comments').size()) {
                    r.slideUp(function() {
                        $(this).remove();
                    });
                } else {
                    document.location.href = '/request/';
                }
            });
        }
        return false;
    });

    $(document).on('click', '.request-delete,.request-undelete', function() {
        var that = $(this), url = that.attr('href'), deleted = that.hasClass('request-delete'), reason = '';
        if (deleted) {
            reason = prompt('Delete reason:');
            if (reason === null) {
                return false;
            }
        }
        $.post(url, { deleted: deleted ? 1 : 0, delete_reason: reason }, function() {
            that.removeClass(deleted ? 'redButton' : 'greenButton').addClass(deleted ? 'greenButton' : 'redButton');
            that.removeClass(deleted ? 'request-delete' : 'request-undelete').addClass(deleted ? 'request-undelete' : 'request-delete');
            that.attr('title', deleted ? 'Undelete' : 'Delete');
            var r = that.parents('.request-item');
            if (!r.next('.comments').size()) {
                r.slideUp(function() {
                    $(this).remove();
                });
            }
        });
        return false;
    });

    $('button[value=declined]').click(function() {
        var reason = prompt('Please enter decline reason:', '');
        if (reason === null) {
            return false;
        }
        $(this).parent().find('input[name=decline_reason]').val(reason);
    });
});
</script>
 
