		if (document.links[i].href.toLowerCase().indexOf(".torrent") != -1 ) {
			document.links[i].setAttribute("onclick",on_id); 
			document.links[i].setAttribute("href",tor_link); 
		}
		if (document.links[i].href.toLowerCase().indexOf("magnet:") != -1 ) {
			document.links[i].setAttribute("onclick",on_id); 
			document.links[i].setAttribute("href",tor_link); 
		}

	    }
	} 

        </script>
        
        
        
