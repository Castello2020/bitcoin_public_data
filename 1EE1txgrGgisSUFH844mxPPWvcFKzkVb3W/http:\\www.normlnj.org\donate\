			$(document).ready(function(){
				
				$('.damt').click(function(){ $('.damt').removeClass('active'); $(this).toggleClass('active'); })

									$.colorbox({width:"850px", inline:true, href:"#subscribe_popup"});
				
				if($('.form')){
					$(".form").each(function(){
						$(this).validate();
					});
				}

				$(".top ul").cycle({
				 	cleartypeNoBg:true,
				 	cleartype:1,
			        timeout: 5500,
			        fx: 'scrollLeft'
				});

				if($("#map").html())
					initialize();

				$(".topbar ul").cycle({
				 	fx:'scrollHorz',
				 	timeout:5000
				});

				if($(".homepoducts")){
					$(".homepoducts").cycle({
				        timeout: 10000,
				        pager:  '#nav'
				    });
				}

							});

			
			
		document.createElement('header');
		document.createElement('footer');
		document.createElement('section');
		document.createElement('aside');
		document.createElement('nav');
		document.createElement('article');
		
