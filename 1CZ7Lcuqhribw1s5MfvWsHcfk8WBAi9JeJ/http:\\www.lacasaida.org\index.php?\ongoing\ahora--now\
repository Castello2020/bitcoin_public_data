<script type="text/javascript">
var months = new Array(12);
months[0] = "enero";
months[1] = "febrero";
months[2] = "marzo";
months[3] = "abril";
months[4] = "mayo";
months[5] = "junio";
months[6] = "julio";
months[7] = "agosto";
months[8] = "setiembre";
months[9] = "octubre";
months[10] = "noviembre";
months[11] = "diciembre";

var current_date = new Date();
current_date.setDate(current_date.getDate() );
month_value = current_date.getMonth();
day_value = current_date.getDate();
year_value = current_date.getFullYear();

document.write("Hoy es " + day_value + " de " + months[month_value] + ", " + year_value);



</script></div>
