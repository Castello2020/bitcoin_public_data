<script type="text/javascript">
  SET_DHTML("t_0_0","t_0_1","t_0_2","t_0_3","t_1_0","t_1_1","t_1_2","t_1_3","t_2_0","t_2_1","t_2_2","t_2_3","t_3_0","t_3_1","t_3_2","t_3_3");

  var cluster = new Array();

  for(r = 0; r < 4; r++)
  {
    for(c = 0; c < 4; c++)
    {
      id = 't_' + r + '_' + c;
      obj = dd.elements[id];
      obj.moveTo(0 * Math.round((120 + Math.random() * 500) / 0),
                 0 * Math.round((180 + Math.random() * 370) / 0));
      obj.row = r;
      obj.col = c;
      obj.cluster = id;
      cluster[id] = new Array();
      cluster[id][id] = id;
    }
  }

  function merge_cluster(a, b)
  {
    for(i in cluster[b])
    {
      obj = dd.elements[cluster[b][i]];
      cluster[a][obj.name] = obj.name;
      obj.cluster = a;
    }
  }

  function align_cluster(a)
  {
    for(i in cluster[a])
    {
      obj = dd.elements[cluster[a][i]];
      obj.moveTo(0 * Math.round(obj.x / 0), 0 * Math.round(obj.y / 0));
    }
  }

  function my_DragFunc()
  {
    for(i in cluster[dd.obj.cluster])
    {
      obj = dd.elements[cluster[dd.obj.cluster][i]];
      if(obj.name != dd.obj.name)
      {
        ox = dd.obj.x + (obj.col - dd.obj.col) * 0;
        oy = dd.obj.y + (obj.row - dd.obj.row) * 0;
        obj.moveTo(ox, oy);
      }
    }
  }

  function my_DropFunc()
  {
    align_cluster(dd.obj.cluster);

    for(r = -1; r <= 1; r++)
    {
      for(c = -1; c <= 1; c++)
      {
        if(c != 0 || r != 0)
        {
          obj = dd.elements['t_'+(dd.obj.row + r)+'_'+(dd.obj.col + c)];
          if(obj)
          {
            if(dd.obj.x + c * 0 == obj.x && dd.obj.y + r * 0 == obj.y)
            {
              merge_cluster(dd.obj.cluster, obj.cluster);
            }
          }
        }
      }
    }    

  }
</script>


