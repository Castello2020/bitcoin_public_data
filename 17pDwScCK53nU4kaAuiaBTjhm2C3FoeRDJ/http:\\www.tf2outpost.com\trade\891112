        var scorpio = {
            init: function() {
                var hash = window.location.hash.substring(1);
                var $body = $("body");
                var $notes_edit = $(".notes_edit");

                if(hash.substring(0, 1) == "p") {
                    var $post = $("#p" + hash.substring(1));

                    if($post) {
                        if($post.is(":hidden")) {
                            $post.trigger("click");
                        }

                        if($post.hasClass("reply")) {
                            var $parent = $post.parent();
                            var forpost = $parent.data("forpost");
                            var $post = $("#p" + forpost);

                            $post.trigger("click");
                        }

                        $body.scrollTop($post.offset().top);
                    }
                } else if(hash == "my" && aries.user.id) {
                    var $posts = $("[data-userid=" + aries.user.id + "]");

                    if($posts.length > 0) {
                        var $post = $posts.first();

                        $body.scrollTop($post.offset().top);
                    }
                } else if(hash == "latest" && aries.user.id) {
                    var $posts = $(".offer, .reply").find(".new").parents(".offer, .reply");

                    if($posts.length > 0) {
                        var $post = $posts.first();

                        $body.scrollTop($post.offset().top);
                    }
                }
            }
        };

        $(function() {
            scorpio.init();
        });
    </script>
