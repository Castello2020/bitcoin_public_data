    function maybeCall(fn, val) { return isFunction(fn) ? fn.call(this, val) : fn; }
    try { store = JSON.parse((isFunction(root.atob)?root.atob:decode)(store)); } catch (err) { store = {}; }

    exports.Flags = function Flags(name) {
        var has = !!(store[name]);
        return function flag(accept, reject) {
            var result = maybeCall.call(this, (has ? accept : reject), has);
            return (result !== void 0) ? result : flag;
        };
    };

