function fixDiv() {
			  var cache = jQuery('#getFixed'); 
			  max = jQuery(document).height() - 1500; 
			  
			  if (jQuery(window).scrollTop() > 400 && jQuery(window).scrollTop() <= max) { 
			  jQuery(".fixtitle").show();
				cache.css({'position': 'fixed', 'top': '53px', 'border': '1px solid #dcdcdc', 'margin-left': '-10px'}); 
			  } else {
			  jQuery(".fixtitle").hide();
				cache.css({'position': 'relative', 'top': 'auto', 'border': '0px solid #dcdcdc', 'margin-left': '0px'});
				}
			}
jQuery(function($) {
			jQuery(window).scroll(fixDiv);
			fixDiv();
		});
		
		
