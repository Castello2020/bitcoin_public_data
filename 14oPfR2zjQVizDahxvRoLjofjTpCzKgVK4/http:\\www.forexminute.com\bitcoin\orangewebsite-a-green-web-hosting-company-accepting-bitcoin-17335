<script type="text/javascript" src="http://www.forexminute.com/wp-content/themes/forexminuteresponsivenew/js/jquery.bpopup-0.7.0.min.js"></script>
<script type="text/javascript">       
function setCookie(c_name,value,exdays, redirect){
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
document.cookie=c_name + "=" + c_value + "; path=/";
jQuery("#element_to_pop_up").remove();
jQuery(".__bPopup1").remove();
jQuery(".__bPopup1").hide();
if(!redirect) {
window.location.href='http://www.forexminute.com';
}
}

function getCookie(c_name){

var i,x,y,ARRcookies=document.cookie.split(";");
for (i=0;i<ARRcookies.length;i++)
{
  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
  x=x.replace(/^\s+|\s+$/g,"");
  if (x==c_name)
    {
    return unescape(y);
    }
  }
}              

var ckk = getCookie('popup');
     if(ckk != 1) {
     jQuery(document).ready(function(){
  	 jQuery('#element_to_pop_up').bPopup({modalClose: false,
            opacity: 0.6,
            positionStyle: 'fixed' //'fixed' or 'absolute'
            });  
     });       
     }
                     
</script>


                      
