							<script>
								jQuery.noConflict();
							jQuery(document).ready(function() {
								var myurl = 'http://rootzwiki.com' + window.location.pathname;
								jQuery.ajax({
								  dataType: "jsonp",
								  url: "http://urls.api.twitter.com/1/urls/count.json?url=" + myurl,
								  success: function(data) {
								  		jQuery('#twittercount').html(data.count);
								  }
								});	
								var mytitle = encodeURIComponent(escape(jQuery('.ipsType_pagetitle').html()));
								
								
								var threadlocation = document.getElementById('secondary_navigation');
								var childDiv = threadlocation.getElementsByClassName('breadcrumb');
								
								var finallocation = escape(childDiv[0].children[2].getElementsByTagName('span')[0].innerHTML);
								    
								jQuery('#thetwitterstuff').attr("href", "javascript:window.open('http://twitter.com/share?via=RootzWiki&text=" + mytitle + "%20-%20" + finallocation + "','mywindowtitle','width=500,height=285')");
								
								
								jQuery('#thegplusstuff').attr("onclick", "popUp=window.open('https://plus.google.com/share?url=" + myurl+ "', 'popupwindow', 'scrollbars=yes,width=800,height=400');popUp.focus();return false");
								
								
								jQuery('#thefacebookstuff').attr("href", "http://www.facebook.com/sharer.php?u=" + myurl);
								
								jQuery('#gpluscount').html(jQuery('#hackpluscount').html());
								jQuery('#subcount').html(jQuery('#subscribecount').html());
								jQuery('#subclick').attr('data-relid', jQuery('#subhack').attr('data-relid'));
								jQuery('#subclick').attr('data-isfave', jQuery('#subhack').attr('data-isfave'));
								
								if (jQuery('#subclick').attr('data-isfave') == 1) {
									jQuery('#subcount').addClass("subactive");
								}

							var rating = jQuery('#rating_text meta[itemprop=ratingValue]').attr("content");
							jQuery("#ratingcount").html(rating);
								
								jQuery.ajax({
								  dataType: "jsonp",
								  url: "http://graph.facebook.com/?ids=" + myurl,
								  success: function(data) {
								  
								  		jQuery.each(data, function() {
								  		  		jQuery('#fbookcount').html(this.shares);
								  		});
								  }
								});
								
								jQuery.ajax({
								  url: "http://rootzwiki.com/public/gplus.php",
								type: "POST",
								  data: {"url": myurl},
								  success: function(data) {
									jQuery('#gpluscount').html(data);
								  }
								});
								
								jQuery('#rating_crumb').click(function() { 
								var popid = 'rating';
														new ipb.Popup( popid, {  type: 'pane',
																				initial: jQuery('.rating').html(),
																				hideAtStart: false,
																				hideClose: false,
																				defer: false,
																				modal: true,
																				w: '350px' });	
								})								
							});	
							</script>
							
				

