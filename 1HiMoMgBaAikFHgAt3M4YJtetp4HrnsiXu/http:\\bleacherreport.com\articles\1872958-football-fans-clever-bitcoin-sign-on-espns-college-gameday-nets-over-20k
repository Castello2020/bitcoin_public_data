                BR.articleData.page = window.location.hash.split('/')[3] || 1;
            }
        })();
    </script>


<script>
    $.extend(BR.articleData, {"page":1,"hitCount":152144,"commentsDisabled":null,"commentsCount":"17"});
</script>


    <script>
        BR.can_edit = false;
        BR.can_recache = false;
    </script>

