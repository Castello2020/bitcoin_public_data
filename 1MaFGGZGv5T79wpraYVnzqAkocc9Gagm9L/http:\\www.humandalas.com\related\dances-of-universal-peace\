							center             : new google.maps.LatLng(32.765625,-108.281988),
							zoom               : 5,
							mapTypeId          : google.maps.MapTypeId.ROADMAP,
							mapTypeControl     : 0,
							mapTypeControlOptions : {
								mapTypeIds : [google.maps.MapTypeId.ROADMAP]
							},
