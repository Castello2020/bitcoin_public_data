function toggle() {
	var ele = document.getElementById("pfs-form-full");
	var text = document.getElementById("displayText");
	if(ele.style.display == "block") {
    		ele.style.display = "none";
		text.innerHTML = "Post Your HuMandala!";
  	}
	else {
		ele.style.display = "block";
		text.innerHTML = "Close Form";
	}
} 
