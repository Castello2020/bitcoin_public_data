							scaleControl       : 0,
							overviewMapControl : 0,
							overviewMapControlOptions : {
								opened : 0
							},
							panControl         : 1,
							zoomControl        : 1,
							zoomControlOptions : {
								
							},
							scrollwheel        : false
						};
