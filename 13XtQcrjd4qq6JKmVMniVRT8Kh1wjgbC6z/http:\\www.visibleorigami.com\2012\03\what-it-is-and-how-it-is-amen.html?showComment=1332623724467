    skin['ALTERNATE_BG_COLOR'] = "transparent";
    
    skin['CONTENT_BG_COLOR'] = "transparent";
    skin['CONTENT_LINK_COLOR'] = "#00ced1";
    skin['CONTENT_TEXT_COLOR'] = "#000000";
    skin['CONTENT_SECONDARY_LINK_COLOR'] = "#00ced1";
    skin['CONTENT_SECONDARY_TEXT_COLOR'] = "#00ced1";
    skin['CONTENT_HEADLINE_COLOR'] = "#00ced1";
