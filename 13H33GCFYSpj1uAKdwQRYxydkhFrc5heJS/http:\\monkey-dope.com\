<script type="text/javascript" charset="utf-8">
(function($) {
  var cache = [];
  $.preLoadImages = function() {
    var args_len = arguments.length;
    for (var i = args_len; i--;) {
      var cacheImage = document.createElement('img');
      cacheImage.src = arguments[i];
      cache.push(cacheImage);
    }
  }
})(jQuery)

$.preLoadImages("img/md01.png","img/md02.png","img/md03.png","img/md04.png","img/md05.png","img/md06.png","img/md07.png","img/md08.png","img/md09.png","img/md10.png","img/md11.png");

$(document).ready(function() {

	Cufon.replace("h1");
	Cufon.replace("#nav");
	Cufon.replace("#downloadlink");
	Cufon.replace("#aboutlink");
	Cufon.replace("h2");

	$("h1").css({"color":"#ffffff","font-size":"90pt","margin-top":"-20px","margin-bottom":"10px","padding":"0","letter-spacing":"5px"});
	$("#nav").css({"font-size":"50pt","margin-top":"0px","margin-bottom":"0px","padding":"0","letter-spacing":"8px"});
	$("#downloadlink").css({"color":"#ccc","font-size":"38pt","margin-top":"5px","padding":"0","letter-spacing":"5px"});
	$("#aboutlink").css({"color":"#f06","font-size":"38pt","margin-left":"20px","margin-top":"5px","padding":"0","letter-spacing":"5px"});
	$("h2").css({"color":"#fff","font-size":"40pt","letter-spacing":"3px","padding":"0","margin-top":"0px","margin-bottom":"0px"});
	
	$("a.fancylink").fancybox({
		'titlePosition'		: 'none',
		'padding'			: '10px',
		'margin'			: '20px',
		'hideOnContentClick': false,
		'hideOnOverlayClick': true,
		'enableEscapeButton':true,
		'showCloseButton':true,
		'scrolling':'auto',
		'overlayShow'		: false,
		'autoDimensions'	: true,
		'autoScale'			: true,
		'width'         	: 640, //640
		'height'        	: 'auto',
		onClosed			: function() {resetLogo();}
	});
	$("a.fancydownload").fancybox({
		'titlePosition'		: 'none',
		'padding'			: '10px',
		'margin'			: '20px',
		'modal'				: false,
		'hideOnContentClick': false,
		'hideOnOverlayClick': true,
		'enableEscapeButton':true,
		'showCloseButton':		true,
		'scrolling':'auto',
		'overlayShow'		: false,
		'autoDimensions'	: false,
		'autoScale'			: false,
		'width'         	: 640,
		'height'        	: 640,
		onStart				: function() {$("#md-l").hide();},
		onClosed			: function() {$("#md-l").show();}


	});
	
	$("a.teaser").click(function() {
		$.fancybox({
				'padding'		: 0,
				'autoScale'		: false,
				'hideOnOverlayClick': true,
				'enableEscapeButton':true,
				'overlayShow'		: false,
				'transitionIn'	: 'none',
				'transitionOut'	: 'none',
				'title'			: this.title,
				'width'			: 640,
				'height'		: 390,
				'href'			: this.href.replace(new RegExp("watch\\?v=", "i"), 'v/'),
				'type'			: 'swf',
				'swf'			: {
					 'wmode'		: 'transparent',
					'allowfullscreen'	: 'true'
				},
				onClosed			: function() {resetLogo();}

			});

		return false;
	});

	wFull = $(document).width();
	hFull = $(document).height();

	w = wFull*0.98;
	h = hFull*0.98;
	w = wFull;
	h = hFull;

	var wi = 604;
	var hi = 661;
	
	var minLeft = 0;
	var minTop = 100;
	var maxRight = w;
	var maxBottom = h-150;

	initMonkeyDope();
	startAnimation();
	resetLogo();
	
	var highlightspeed = 500;
	var hightlightopacity = 0.4;
//	highlight("#a1",highlightspeed,hightlightopacity);
//	highlight("#a2",highlightspeed,hightlightopacity);
//	highlight("#a3",highlightspeed,hightlightopacity);
//	highlight("#a4",highlightspeed,hightlightopacity);
//	highlight("#a5",highlightspeed,hightlightopacity);
	highlight("#a6",highlightspeed,hightlightopacity);
//	highlight("#a7",highlightspeed,hightlightopacity);
//	highlight("#a8",highlightspeed,hightlightopacity);
//	highlight("#a9",highlightspeed,hightlightopacity);
//	highlight("#a10",highlightspeed,hightlightopacity);
//	highlight("#a11",highlightspeed,hightlightopacity);

	function initMonkeyDope() {
		$("#md-l").css({height:hi,width:wi,left:w/2-wi/2,top:h/2-hi/2});
		$("#nav").css({opacity:0.9});

		$("#md-detail").css({height:wFull*0.58,width:wFull});
		$("#md-detail").hide();

		$("a#a1").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l01");});
		$("a#a2").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l02");});
		$("a#a3").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l03");});
		$("a#a4").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l04");});
		$("a#a5").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l05");});
		$("a#a6").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l06");});
		$("a#a7").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l07");});
		$("a#a8").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l08");});
		$("a#a9").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l09");});
		$("a#a10").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l10");});
		$("a#a11").click(function(e) {zoomLogo(6,e.pageX, e.pageY,"l11");});

	}
	function startAnimation() {
		$("#md-l").hide();
		$("#md-l").delay(1000).fadeIn(3000);
	}
	
	
	function resetLogo() {
		$("#md-detail").fadeOut(100,function(){
			$("#md-l").animate({opacity:1,height:hi,width:wi,left:w/2-wi/2,top:h/2-hi/2},1000,function(){
				$("#monkeydopemap li").delay(1100).show();
			});
		});

	}
	
	function highlight(linkId,speed,opacity) {
	var cv = $(linkId);
	cv.addClass('currentvideo');
	cv.css({"opacity":0});
	cv.delay(3000).animate({"opacity":opacity},speed,function(){
		cv.animate({"opacity":0},speed,function(){
			cv.animate({"opacity":opacity},speed,function(){
				cv.animate({"opacity":0},speed,function(){
					cv.removeClass('currentvideo');
					cv.css({opacity:1});
				});
			});
		});
	});
	}
	
	function zoomLogo(zoom,x,y,linkId) {
		zoomModified = 8;
		xModified = (x-$("#monkeydopemap").offset().left);
		yModified = (y-$("#monkeydopemap").offset().top);
		posLeft = (w/2+wi)-(xModified*zoomModified)-wi;
		posTop = (h/2+hi)-(yModified*zoomModified)-hi;
		maxWidth = Math.min(wi*zoomModified);
		maxHeight = Math.min(hi*zoomModified);
		$("#monkeydopemap li").hide();
		 		
		$("#md-l").animate({opacity:0,height:maxHeight,width:maxWidth,left:posLeft,top:posTop},2000,function(){
			$("#md-detail img").attr("src", "img/md"+linkId.substr(-2)+".png");
			$("#md-detail").fadeIn(500,function(){
				$("#"+linkId).click();
			});
		});
	}
	 


	$("#tab-download").tabs();
	$("#tab-about").tabs();
	$("#tab-digitalcoming").tabs();
	$("#tab-america").tabs();
	$("#tab-goingdown").tabs();
	$("#tab-maybe").tabs();
	$("#tab-backspace-romance").tabs();
	$("#tab-amber").tabs();
	$("#tab-on-a-mission").tabs();
	$("#tab-new-things").tabs();
	$("#tab-ripples-on-a-pond").tabs();
	$("#tab-scenes").tabs();
	$("#tab-monkey-dope").tabs();


	$("#a1").easyTooltip();
	$("#a2").easyTooltip();
	$("#a3").easyTooltip();
	$("#a4").easyTooltip();
	$("#a5").easyTooltip();
	$("#a6").easyTooltip();
	$("#a7").easyTooltip();
	$("#a8").easyTooltip();
	$("#a9").easyTooltip();
	$("#a10").easyTooltip();
	$("#a11").easyTooltip();

	}

);



</script>
