  $('#comment_message').focus(function() {
    _form = $(this).closest('.comment_form')
    if($('body').hasClass('signed_in')) {
      _form.html("");
      _form.spin();
      $.ajax({
        url: '/bitcoin/pages/15889584-bitcoin-exchange-bitstamp-chooses-gbgroup-for-identity/comments/new',
