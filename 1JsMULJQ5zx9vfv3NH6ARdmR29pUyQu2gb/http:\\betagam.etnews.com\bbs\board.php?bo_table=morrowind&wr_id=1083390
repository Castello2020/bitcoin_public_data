				function mw_comment_good(wr_id, good) {
					$.get("../skin/board/mw.basic/mw.proc/mw.comment.good.php?bo_table=morrowind&parent_id=1083390&wr_id="+wr_id+"&good="+good, function (data) {
						data = data.split('|');
						alert(data[0]);
						if (good == 'good') {
							$("#mw_comment_good_"+wr_id).html(data[1]);
						} else {
							$("#mw_comment_nogood_"+wr_id).html(data[1]);
						}
					});
				}
