<script type="text/javascript">
	function foutlogin_submit(f)
	{
		var f = document.foutlogin;
		if (!f.mb_id.value) {
			alert("ȸ�����̵� �Է��Ͻʽÿ�.");
			f.mb_id.focus();
			return;
		}
		if (!f.mb_password.value) {
			alert("�н����带 �Է��Ͻʽÿ�.");
			f.mb_password.focus();
			return;
		}

		f.action = 'https://betagam.etnews.com/bbs/login_check.php';

		f.submit();
	}

	function searchURL()
	{
		_trkEventLog('BETAGAM_���հ˻�');
		var txt = document.getElementById("DNW_search").value;
		window.open("http://search.danawa.com/dsearch.php?k1=" + txt);
	}

	$(document).ready(function() {
		if ($("#mb_id").val() != '') {
			$("#mb_id").css("background-image", "none");
		}

		$("#DNW_search").bind({
			focus : function() {
				$("#DNW_search").css("background-image", "none");
			},
			blur : function() {
				if ($("#DNW_search").val() == '') {
					$("#DNW_search").css("background-image", 'url(/img/new/input_search.gif)');
				}
			}
		});

		$("#mb_id").bind({
			focus : function() {
				$("#mb_id").css("background-image", "none");
			},
			blur : function() {
				if ($("#mb_id").val() == '') {
					$("#mb_id").css("background-image", 'url(/img/new/input_id.gif)');
				}
			}
		});

		$("#mb_password").bind({
			focus : function() {
				$("#mb_password").css("background-image", "none");
			},
			blur : function() {
				if ($("#mb_password").val() == '') {
					$("#mb_password").css("background-image", 'url(/img/new/input_pw.gif)');
				}
			}
		});

		$("#mb_password").keydown(function(event) {
			if (event.keyCode == '13') {
				foutlogin_submit();
				return false;
			}
		});

		$("#DNW_search").keydown(function(event) {
			if (event.keyCode == '13') {
				searchURL();
				return false;
			}
		});
	});
</script>

