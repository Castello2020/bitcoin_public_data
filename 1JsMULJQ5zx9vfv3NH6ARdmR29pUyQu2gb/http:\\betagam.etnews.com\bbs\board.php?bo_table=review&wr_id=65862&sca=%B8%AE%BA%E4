	function noImageCheck(point) {
		if (point.src != "/img/logo_betagame.gif") {
			point.src = "/img/logo_betagame.gif";
		}
	}

	 function getRandomInt(min, max) {
		return Math.floor(Math.random() * (max - min + 1)) + min;
	 }

	 function RandomBanner(obj) { // jQuery object
		var rb = this,
		banners = obj.find('li'),
		ban_len = banners.length,
		randnum = 0,
		old_index = -1,
		timer;
		this.change = function () {
			do {
				randnum = getRandomInt(0, (ban_len - 1));
			}
			while (randnum === old_index);
			banners.each(function() {
				var this_banner = $(this);
				this_banner.removeClass('selected');
				this_banner.hide();
			 });
				var selected = $(banners[randnum]);
				 selected.addClass('selected');
				 selected.show();
		};
		this.init = function (speed) {
			speed = speed || 7000; // ���⿡ �ð��� �Է��ϸ� �ڵ����� �ð� �����
			rb.change();
			timer = window.setInterval(function() {
				old_index = banners.index(obj.find('li.selected'));
				rb.change();
			}, speed);
			$(window).bind('unload', function() {
				clearInterval(timer);
			});
		}
	}

