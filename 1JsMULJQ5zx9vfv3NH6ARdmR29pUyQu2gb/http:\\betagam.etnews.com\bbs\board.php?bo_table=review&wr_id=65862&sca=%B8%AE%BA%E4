	<script type="text/javascript">
	function btn_singo(wr_id, parent_id) {
		//if (confirm("�� �Խù��� ���� �Ű��Ͻðڽ��ϱ�?")) {
		//hiddenframe.location.href = "../skin/board/mw.basic/mw.proc/mw.btn.singo.php?bo_table=review&wr_id=" + wr_id + "&parent_id=" + parent_id;
		win_open("../skin/board/mw.basic/mw.proc/mw.btn.singo.php?bo_table=review&wr_id=" + wr_id + "&parent_id=" + parent_id, "win_singo", "width=500,height=300,scrollbars=yes");
		//}
	}
	function btn_singo_view(wr_id) {
		var id = "singo_block_" + wr_id;

		if (document.getElementById(id).style.display == 'block')
			document.getElementById(id).style.display = 'none';
		else
			document.getElementById(id).style.display = 'block';
	}

	function btn_singo_clear(wr_id) {
		if (confirm("���� �ʱ�ȭ �Ͻðڽ��ϱ�?")) {
			$.get("../skin/board/mw.basic/mw.proc/mw.btn.singo.clear.php?bo_table=review&wr_id="+wr_id, function(msg) {
				alert(msg);
			});
		}
	}
	</script>
