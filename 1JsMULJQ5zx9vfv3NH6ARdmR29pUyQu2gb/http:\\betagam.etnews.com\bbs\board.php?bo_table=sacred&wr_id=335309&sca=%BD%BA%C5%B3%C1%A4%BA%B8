	function btn_singo(wr_id, parent_id) {
		//if (confirm("�� �Խù��� ���� �Ű��Ͻðڽ��ϱ�?")) {
		//hiddenframe.location.href = "../skin/board/mw.basic/mw.proc/mw.btn.singo.php?bo_table=sacred&wr_id=" + wr_id + "&parent_id=" + parent_id;
