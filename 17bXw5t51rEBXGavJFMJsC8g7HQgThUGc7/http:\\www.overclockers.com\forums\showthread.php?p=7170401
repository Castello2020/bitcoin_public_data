<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_6961511" class="smallfont" style="text-align: center;">
				
				<strong><a class="bigusername" href="member.php?s=47a668be3ae560150af607e32131b8ce&amp;u=120711">Charpy</a></strong>
				<script type="text/javascript"> vbmenu_register("postmenu_6961511", true); </script>
				
			</div>
			<div class="smallfont" align="center"><img src="oc_images/images/stars/star_yellow2.gif" alt="" border="" /></div>
			<div class="smallfont" id="noechotopic" style="text-align: center;"><span class="smallfont">Registered</span></div><br />
		
		            
			
                        <br /><br />
			
			
			<div id="noechotopic" class="smallfont" style="text-align: center;">
