require(['jquery'], function($) {
    $(function(){
        var uid = "deverettreed";

        $("a.removeAllCommentsAction").click(function(){
            var that = $(this);

            if (confirm("Delete all comments from this user?")) {
                $.ajax({
                    url: "http://virtualglobetrotting.com/mod_ajax/RemoveUserComments/",
                    data: { uid: uid },
                    success: function(data) {
                        that.fadeAndRemove();
                    }
                });
            }
        });

        $("a.banIPAction").click(function(){
            var that = $(this);
            var ip = $(this).data('ip');

            if (confirm("Ban this user's IP?")) {
                $.ajax({
                    url: "http://virtualglobetrotting.com/mod_ajax/BanIP/",
                    data: { ip: ip },
                    success: function(data) {
                        that.fadeAndRemove();
                    }
                });
            }
        });

        $("a.becomeUserAction").click(function(){
            var uid = $(this).data('uid');

            if (confirm("Become this user?")) {
                $.ajax({
                    url: "http://virtualglobetrotting.com/admin_functions/BecomeUser/",
                    data: { uid: uid },
                    success: function(data) {
                        window.location = '/';
                    }
                });
            }
        });
    });
});
