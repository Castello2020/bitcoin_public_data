google_ad_client="pub-9765959110898159";google_ad_width=728;google_ad_height=90;google_ad_format="728x90_as";google_ad_type="text_image";google_ad_channel="1284460242+6529286822+0712959289+4201664892+3154410553";google_color_border="FFFFFF";google_color_bg="FFFFFF";google_color_link="236CA1";google_color_url="000000";google_color_text="236CA1";
