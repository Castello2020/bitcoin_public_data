<script type="text/javascript">
    var jQmenu = jQ(".dropdown-menu");
    jQmenu.menuAim({
        activate: activateSubmenu,
        deactivate: deactivateSubmenu
    });
    function activateSubmenu(row) {
        var jQrow = jQ(row),
            submenuId = jQrow.data("submenuId"),
            jQsubmenu = jQ("#" + submenuId),
            height = jQmenu.outerHeight(),
            width = jQmenu.outerWidth();
        // Show the submenu
        jQsubmenu.css({
            display: "block",
        });
        jQrow.addClass("maintainHover");
    }
    function deactivateSubmenu(row) {
        var jQrow = jQ(row),
            submenuId = jQrow.data("submenuId"),
            jQsubmenu = jQ("#" + submenuId);
        // Hide the submenu and remove the row's highlighted look
        jQsubmenu.css("display", "none");
        jQrow.removeClass("maintainHover");
    }
</script>
				
