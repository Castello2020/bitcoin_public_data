	ipb.topic.topPid = pid;
}

	// Show multiquote for JS browsers
	if ( $('multiq_10454572') )
	{
		$('multiq_10454572').show();
	}
	
	if( $('toggle_post_10454572') )
	{
		$('toggle_post_10454572').show();
	}
	
	// Add perm data
	ipb.topic.deletePerms[10454572] = { 'canDelete' : 0, 'canSoftDelete' : 0 };

</script>
			

