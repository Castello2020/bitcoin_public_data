<style>
#gameResults
{
     text-transform: none;
     position: absolute;
     top: 25px;
     left: 12px;
     z-index: 100;
     display: none;
}
#gameResults .generic
{
     padding: 3px 0 3px 10px;
     margin-top: 0;
     -moz-box-shadow: none;
     -webkit-box-shadow: none;
     box-shadow: none;
     width: 493px;
     opacity: 0.95;
     background-color: #23252b;
     border: 1px solid #363636;
}
#gameResults .generic .title
{
     width: 485px;
}
#gameResults .generic.active
{
     background-color: #2e3137;
}
</style>
<script type="text/javascript">
var searchtimer;
var selectedResult = 0;
var keywordCache = '';
function executeGlobalSearch()
{
        var url = 'http://www.cheapassgamer.com/index.php?s=d27425c6414693ba8883609394230479&amp;app=cag&module=ajax&section=gamesearch&limit=100';
        var div = 'gameResults';
	var pars = 'keywords=' + jQ('#main_search').val() + '&md5check=' + ipb.vars['secure_hash'];
	jQ.ajax({
	   type: "POST",
	   url: url,
	   data: pars,
	   success: function(data, textStatus){
		   jQ('#' + div).html(data);
		   selectedResult = 0;
		   jQ('#gameResults').slideDown();
	   }
	 });
}
jQ('#main_search').keyup(function(){
  if (jQ('#main_search').val() != keywordCache)
  {
    clearTimeout(searchtimer); 
    searchtimer = setTimeout(executeGlobalSearch, 600);
  }
  keywordCache = jQ('#main_search').val();
});
jQ('#main_search').keydown(function (e) {
  var keyCode = e.keyCode;
  var resultCount = jQ("#gameResults .generic").size();
  
  if (keyCode == 38)
  {
    selectedResult--;
  }
  if (keyCode == 40)
  {
    selectedResult++;
  }
  if (selectedResult > resultCount)
    selectedResult = 1;
  if (selectedResult < 1)
    selectedResult = 0;
  jQ('#gameResults .generic').removeClass('active');
  jQ('#gameResults .generic:nth-child(' + selectedResult + ')').addClass('active');
  if (keyCode == 13 && selectedResult>0)
  {
    jQ("#search-box").submit(function(event) {
      event.preventDefault();
    });
     window.location = '?app=productinfo&module=overview&productid=' + jQ('#gameResults .generic:nth-child(' + selectedResult + ')').attr('data-productid');
  }
});
jQ( "#gameResults" ).delegate( ".generic", "click", function() {
  window.location = '?app=productinfo&module=overview&productid=' + jQ(this).attr('data-productid');
});
jQ( "#gameResults" ).delegate( ".generic", "mouseover", function() {
  jQ('#gameResults .generic').removeClass('active');
  jQ(this).addClass('active');
});
jQ('html').click(function() {
  jQ('#gameResults').hide();
});
</script>

</div>
				
				
				
