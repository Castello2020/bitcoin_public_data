                    <script type="text/javascript">
                        function recent_tweets(data) {
                            document.getElementById("twitterwrapper").style.display = "block";
                            for(i = 0; i < data.length; i++) {
                                document.getElementById("tweetcontainer").innerHTML = document.getElementById("tweetcontainer").innerHTML + '<a href="http://twitter.com/TheGunluva/status/' + (data[i].id_str ? data[i].id_str : data[i].id) + '"><div class="content">' + data[i].text + '</div></a>';
                            }
                        }
                    </script>
