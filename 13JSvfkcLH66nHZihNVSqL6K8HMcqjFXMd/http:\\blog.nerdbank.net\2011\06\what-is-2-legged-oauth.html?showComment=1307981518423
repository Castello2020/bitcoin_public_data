      var msgs = {'loadMore': 'Load more...', 'loading': 'Loading...', 'loaded': 'No more!', 'addComment': 'Add comment', 'reply': 'Reply', 'delete': 'Delete'};
      var config = {'blogId': '6894552', 'postId': '8291100173126770062', 'feed': 'http://blog.nerdbank.net/feeds/8291100173126770062/comments/default', 'authorName': 'Andrew Arnott', 'authorUrl': 'http://www.blogger.com/profile/13632400519774640095', 'baseUri': 'http://www.blogger.com', 'maxThreadDepth': 2};

