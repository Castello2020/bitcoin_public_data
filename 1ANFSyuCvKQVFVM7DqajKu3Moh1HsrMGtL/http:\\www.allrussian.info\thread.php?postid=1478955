	<script type="text/javascript">
	//<![CDATA[
	if (TransMenu.isSupported()) {

		//==================================================================================================
		// create a set of dropdowns
		//==================================================================================================
		// the first param should always be down, as it is here
		//
		// The second and third param are the top and left offset positions of the menus from their actuators
		// respectively. To make a menu appear a little to the left and bottom of an actuator, you could use
		// something like -5, 5
		//
		// The last parameter can be .topLeft, .bottomLeft, .topRight, or .bottomRight to inidicate the corner
		// of the actuator from which to measure the offset positions above. Here we are saying we want the 
		// menu to appear directly below the bottom left corner of the actuator
		//==================================================================================================
		var ms = new TransMenuSet(TransMenu.direction.down, 1, 0, TransMenu.reference.bottomLeft);

		//==================================================================================================
		// create a dropdown menu
		//==================================================================================================
		// the first parameter should be the HTML element which will act actuator for the menu
		//==================================================================================================
		var menu6 = ms.addMenu(document.getElementById('wcf.header.menu.usermenu'));
menu6.addItem("<img src='wcf/icon/profileM.png' alt=''/>&nbsp;Профиль", "index.php?form=UserProfileEdit&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
var submenu7 = menu6.addMenu(menu6.items[0]);
submenu7.addItem("<img src='wcf/icon/personalDetailsM.png' alt=''/>&nbsp;Личные данные", "index.php?form=UserProfileEdit&category=profile&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
submenu7.addItem("<img src='wcf/icon/passwordM.png' alt=''/>&nbsp;Пароль", "index.php?form=PasswordChange&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
submenu7.addItem("<img src='wcf/icon/signatureM.png' alt=''/>&nbsp;Подпись", "index.php?form=SignatureEdit&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
menu6.addItem("<img src='wcf/icon/settingsM.png' alt=''/>&nbsp;Установки", "index.php?form=UserProfileEdit&category=settings.general&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
var submenu13 = menu6.addMenu(menu6.items[1]);
submenu13.addItem("<img src='wcf/icon/userProfileGeneralM.png' alt=''/>&nbsp;Общее", "index.php?form=UserProfileEdit&category=settings.general&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
submenu13.addItem("<img src='wcf/icon/userProfilePrivacyM.png' alt=''/>&nbsp;Частная сфера", "index.php?form=UserProfileEdit&category=settings.privacy&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
submenu13.addItem("<img src='wcf/icon/userProfileCommunicationM.png' alt=''/>&nbsp;Коммуникация", "index.php?form=UserProfileEdit&category=settings.communication&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
submenu13.addItem("<img src='wcf/icon/userProfileDisplayM.png' alt=''/>&nbsp;Показ", "index.php?form=UserProfileEdit&category=settings.display&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
menu6.addItem("<img src='wcf/icon/groupM.png' alt=''/>&nbsp;Группа пользователей", "index.php?page=UserGroups&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
var submenu24 = menu6.addMenu(menu6.items[2]);
submenu24.addItem("<img src='wcf/icon/groupM.png' alt=''/>&nbsp;Группа пользователей", "index.php?page=UserGroups&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
submenu24.addItem("<img src='wcf/icon/groupLeaderM.png' alt=''/>&nbsp;Руководство группы", "index.php?page=UserGroupLeader&s=23c63c8eb8bce6f389ecb02c9449b3776d8efbb3");
