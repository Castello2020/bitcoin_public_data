<div id="timer_widget-3" class="widget widget_timer_widget"><div class="widget-wrap"><h4 class="widgettitle">The Hackaday Prize</h4>
	<div class="countdowntimer-header">		<strong class="event">Countdown for Entries</strong>	</div>			<div id='widget-timer_widget-3-content' class='countdowntimer-content' data-date='1408535400'>
				<a href='http://www.hackaday.io/prize'>			        <div class="countdowntimer-tminus timer-part"><div class='value'>T-</div></div>
			        <div class="countdowntimer-days timer-part"><div class='value'>28:</div><label>Days</label></div>
			        <div class="countdowntimer-hours timer-part"><div class='value'>01:</div><label>Hours</label></div>
			        <div class="countdowntimer-minutes timer-part"><div class='value'>56</div><label>Minutes</label></div>
			        <div class="countdowntimer-seconds timer-part"><div class='value'>14</div><label>Seconds</label></div>
			    </a>			</div>

			<script>
				setInterval(function() {
					function pad(num, size) {
						var s = "0" + num;
						return s.substr(s.length-size);
					}

					var fieldId = 'widget-timer_widget-3-content';

					var container = document.querySelector('#' + fieldId);
					var timestamp = container.getAttribute('data-date')*1000;
					var deadline = new Date(timestamp);

					var now = Date.now();
					var seconds = (deadline - now) / 1000;
					seconds = Math.floor(seconds);
					var minutes = seconds / 60;
					minutes = Math.floor(minutes);
					var hours = minutes / 60;
					hours = Math.floor(hours);
					var days = hours / 24;
					days = Math.floor(days);


					seconds -= (minutes * 60);
					minutes -= (hours * 60);
					hours -= (days * 24);


					document.querySelector('#' + fieldId + ' .countdowntimer-days .value').innerText = pad(days,2) + ':';
					document.querySelector('#' + fieldId + ' .countdowntimer-hours .value').innerText = pad(hours,2) + ':';
					document.querySelector('#' + fieldId + ' .countdowntimer-minutes .value').innerText = pad(minutes,2);
					document.querySelector('#' + fieldId + ' .countdowntimer-seconds .value').innerText = pad(seconds,2);
				},1000);
			</script>
		    </div></div>
