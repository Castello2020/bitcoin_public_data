<script>
  //<![CDATA[
    jQuery.getScript('http://cdn-static.liverail.com/js/companions.js', function(data, textStatus) {
      lrCompanionDisplayCallback['300x250'] = 'showLrCompanionAd();';
    });
    
    var showLrCompanionAd = function () {
      hideChannelDisplayAds();
      jQuery("#lr_comp_300x250").show();
      jQuery(".new_advertisement").toggle(jQuery("#right_col").is(":visible"));
      jQuery("#right_col").css({"top" : "306px"});
    }
    
    var googleCompanionAdExists = function () {
      hideChannelDisplayAds();
      if (!jQuery("#google_companion_300x250").is(":empty")) {
        jQuery("#google_companion_300x250").show();
        jQuery("#right_col").css({"top": "306px"});
      } else {
        jQuery("#google_companion_300x60").show();
        jQuery("#right_col").css({"top": "116px"});
      }
      jQuery(".new_advertisement").toggle(jQuery("#right_col").is(":visible"));
    }
    
    var isGoogleCompanionAd = function (provider) {
      return _.contains(["twitch_dfp"], provider);
    }
    
    var hideChannelDisplayAds = function () {
      jQuery("#channel_default_display_ad").hide();
      jQuery("#google_companion_300x250").hide();
      jQuery("#google_companion_300x60").hide();
      jQuery("#lr_comp_300x250").hide();
      jQuery(".new_advertisement").hide();
      jQuery("#right_col").css({"top" : "0px"});
    }
    
    var userHideAd = function () {
      Twitch.tracking.mixpanel.track_with_swf('channel-adclose');
      hideChannelDisplayAds();
    }
  //]]>
</script>
